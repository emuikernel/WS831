$('#guide_wifi24_ssid').live("change",function() {
$("#guide_ssid5g").html($("#guide_wifi24_ssid").val().substr(0,29)+"_5G");
});
$('#guide_wifi24_ssid').live("keyup",function() {
$("#guide_ssid5g").html($("#guide_wifi24_ssid").val().substr(0,29)+"_5G");
});
$('#guide_pppoe_password').live("change",function() {
$("#guide_pppoe_password_text").val($("#guide_pppoe_password").val());
});
$('#guide_pppoe_password_text').live("change",function() {
$("#guide_pppoe_password").val($("#guide_pppoe_password_text").val());
});
function syncPassword(val) {
var src = $("#guide_sync_checkbox").attr("src");
var user = $('#guide_user_password, #guide_user_text_password');
var key = $("#guide_wifi_text_password,#guide_wifi_password").filter(":visible").val();
val = val ? val : key;
if(src.indexOf("off")<0){
user.val(val);
}
}
$('#guide_wifi_password').live("change",function() {
$("#guide_wifi_text_password").val($(this).val());
});
$('#guide_wifi_text_password').live("change",function() {
$("#guide_wifi_password").val($(this).val());
});
$('#guide_user_password').live("change",function() {
$("#guide_user_text_password").val($(this).val());
});
$('#guide_user_text_password').live("change",function() {
$("#guide_user_password").val($(this).val());
});
$('#guide_wifi_password').live("keyup",function() {
var val = $(this).val();
utilGetPasswordStrength("guide_password_strength", val, "guide_pwd_strengthinfo", "guide_pwd_strength_signal", $("#guide_wifi24_ssid").val());
});
$('#guide_wifi_text_password').live("keyup",function() {
var val = $(this).val();
utilGetPasswordStrength("guide_password_strength", val, "guide_pwd_strengthinfo", "guide_pwd_strength_signal", $("#guide_wifi24_ssid").val());
});
$('#guide_user_password').live("keyup",function() {
utilGetPasswordStrength("guide_user_password_strength", $(this).val(), "guide_user_pwd_strengthinfo", "guide_user_pwd_strength_signal", "", 6);
});
$('#guide_user_text_password').live("keyup",function() {
utilGetPasswordStrength("guide_user_password_strength", $(this).val(), "guide_user_pwd_strengthinfo", "guide_user_pwd_strength_signal", "", 6);
});
$('#offline_pppoe_password').live("change",function() {
$("#offline_pppoe_password_text").val($("#offline_pppoe_password").val());
});
$('#offline_pppoe_password_text').live("change",function() {
$("#offline_pppoe_password").val($("#offline_pppoe_password_text").val());
});
$(document).ready(function(){
stopStatusTime = false;
Atp.UserProtocalController.load(function(data){
console.log("UserProtocalController data",data);
if(data["guidefinish"] == true){
$("#userprotocalcheck").hide();
}else{
$("#userprotocalcheck").show();
}
});
$("#guide_begin_div > div").touchstart(function(){
$("#guide_begin_div > div").css({"background-color":"#0f73b9","border":"1px solid #0f78be"});
});
$("#guide_begin_div > div").touchend(function(){
$("#guide_begin_div > div").css({"background-color":"#2785c6","border":"1px solid #57a0d2"});
});
$(".logout-mini").hide();
});
$(function(){
Atp.InternetStatusController = Atp.SingleObjController.extend({
objName: "system/diagnose_internet",
connectCount: 4,
disconnectCount: 0,
connecttimer: null,
disconnecttimer: null,
connectImgChange: function(){
if (this.connecttimer){
return;
}
this.connecttimer = setInterval('Atp.InternetStatusController.connectOntimeOut()', 1000);
},
connectOntimeOut: function(){
Atp.WanHomeStatusController.load();
var location = window.location.hash;
if(location != "" && location != "#home_page"){
return ;
}
var self = Atp.InternetStatusController;
var strHead = "../res/connect_";
var strEnd = ".png";
var strpath = strHead + self.connectCount + strEnd;
if (self.content.Status != "Connected"){
self.connecttimer = null;
return;
}
$("#internet_status_imgs").attr("src", strpath);
self.connectCount++;
if (self.connectCount > 6){
self.connectCount = 4;
}
if(Atp.WanHomeStatusController.content.ConnectionStatus != Atp.InternetStatusController.content.ConnectionStatus){
Atp.InternetStatusController.load();
}
},
disconnectImgChange: function(){
if (this.disconnecttimer){
return;
}
this.disconnecttimer = setInterval('Atp.InternetStatusController.disconnectOntimeOut()', 1000);
},
disconnectOntimeOut: function(){
Atp.WanHomeStatusController.load();
var location = window.location.hash;
if(location != "" && location != "#home_page"){
return ;
}
var self = Atp.InternetStatusController;
var strHead = "../res/connect_";
var strEnd = ".png";
var strpath = strHead + self.disconnectCount + strEnd;
if (self.content.Status == "Connected"){
self.disconnecttimer = null;
return;
}
$("#internet_status_imgs").attr("src", strpath);
self.disconnectCount++;
if (self.disconnectCount > 3){
self.disconnectCount = 0;
}
if(Atp.WanHomeStatusController.content.ConnectionStatus != Atp.InternetStatusController.content.ConnectionStatus){
Atp.InternetStatusController.load();
}
},
afterLoad: function(){
console.log("Atp.InternetStatusController load over ...");
if (this.content.Status == "Connected"){
$("#internet_status_connected_link").show();
$("#internet_status_disconnected_link").hide();
$("#internet_status_image").html('<a href="/small/html/app.html#status_page"><img id="internet_status_imgs" alt="alt..." src="../res/connect_4.png" width="420px" height="420px" /></a>');
this.connectImgChange();
}else{
$("#internet_status_connected_link").hide();
$("#internet_status_disconnected_link").show();
$("#internet_status_image").html('<a href="/small/html/app.html#diagnose_page"><img id="internet_status_imgs" alt="alt..." src="../res/connectfailed_blue.png" width="420px" height="420px" /></a>');
console.log("dis connected....");
}
$("#wifi_open_warning").hide();
for (var i = 0; i < Atp.WifiHomeBasicController.content.length; i++){
if (Atp.WifiHomeBasicController.content[i].BeaconType == "None"){
$("#wifi_open_warning").show();
break;
}
}
if (this.content.StatusCode == "ErrLayer2Down"){
$("#internet_wizard").attr("href","/small/html/app.html#wan_wizard_page_layer2down");
}else if(this.content.StatusCode == "Connected"){
$("#internet_wizard").attr("href","/small/html/app.html#wan_wizard_page_connectiontype");
}
else{
$("#internet_wizard").attr("href","/small/html/app.html#wan_wizard_page_connectionauto");
}
$("#internet_wizard").touchstart(function(){
$('#home_wizardimg').attr("src","../res/wizard_press.png");
});
$("#internet_wizard").touchend(function(){
$('#home_wizardimg').attr("src","../res/wizard.png");
});
$("#wlan_setting").touchstart(function(){
$('#home_wlandimg').attr("src","../res/wifisetting_press.png");
});
$("#wlan_setting").touchend(function(){
$('#home_wlandimg').attr("src","../res/wifisetting.png");
});
}
});
Atp.WifiHomeBasicController = Atp.MultiObjController.extend({
objName: "ntwk/WlanBasic"
});
Atp.WanHomeStatusController = Atp.SingleObjController.extend({
objName: "ntwk/wandetect"
});
$(document).ready(function(){
Atp.WifiHomeBasicController.load(function(){
Atp.InternetStatusController.load();
});
});
$("#home_page").bind("pagebeforeshow", function(event, prevPage){
restoredefaultSign = false;
Atp.WifiHomeBasicController.load(function(){
Atp.InternetStatusController.load();
});
});
});
var isrefresh = true;
var href = window.location.href;
var isguide = href.indexOf("guide")>=0;
$(function(){
Atp.WanRepeaterDetectController = Atp.SingleObjController.extend({
objName: "ntwk/wandetect"
});
Atp.RepeaterinfoController = Atp.SingleObjController.extend ({
objName: 'ntwk/repeaterinfo'
});
Atp.WlanDiagController = Atp.SingleObjController.extend({
objName: 'ntwk/repeaterdiag'
});
Atp.RepeaterStateController = Atp.SingleObjController.extend({
objName: 'ntwk/repeaterstate'
});
Atp.WifiApDialController = Atp.SingleObjController.extend({
objName: 'ntwk/RepeaterDial',
contentType:"application/json;charset=UTF-8;enp",
isguidewifiConnecting:false,
issplit:false,
connectfaild:function(){
if(!isguide){
utilchangePage("wan_wizard_failed");
}
},
connectsuccess:function(){
if(!isguide){
utilchangePage("wan_wizard_success");
}
},
checkProc: function() {
var dialcontent = this.content;
if(!isguide || (isguide && !this.issplit)){
if(("NONE" != dialcontent["EncryptionType"]) || ("Open" != dialcontent["WifiAuthMode"])){
if("WEP" == dialcontent["WifiAuthMode"] || "WEP" == dialcontent["EncryptionType"]){
var keylen = dialcontent["WepKey"].length;
if ((keylen != 5) && (keylen != 13) && (keylen != 10) && (keylen != 26)) {
$("#wpakey_pwderr_warning").show();
$("#wpakey_pwderr_warninginfo").text(Em.I18n.t("Touch.wan.wizard.repeater.wepkey.err"));
return true;
}
}else{
var maxlen = 63;
var maxlentip = "8~63";
if(isguide){
maxlen = 32;
maxlentip = "8~32";
}
if ((dialcontent["wifiwisprpwd"].length < 8) || (dialcontent["wifiwisprpwd"].length > maxlen)) {
$("#wpakey_pwderr_warning").show();
$("#wpakey_pwderr_warninginfo").text(Em.I18n.t("Touch.wan.wizard.repeater.passerr",{arg1:maxlentip}));
return true;
}
if (/.*[^\x20-\x7e]+.*$/.exec(dialcontent["wifiwisprpwd"]) || utilcheckInputCharacters(dialcontent["wifiwisprpwd"])) {
$("#wpakey_pwderr_warning").show();
$("#wpakey_pwderr_warninginfo").text(Em.I18n.t("Touch.wan.wizard.repeater.passworderror"));
return true;
}
}
}
}
if(isguide && this.issplit){
if(dialcontent["userpassword"].length < 6 || dialcontent["userpassword"].length > 32){
$("#wlan_user_passworderror_login").show();
$("#wlan_user_warninginfo_login").html(Em.I18n.get("User.Set.error"));
return true;
}
if(/.*[^\x20-\x7e]+.*$/.exec(dialcontent["userpassword"]) || utilcheckInputCharacters(dialcontent["userpassword"])){
$("#wlan_user_passworderror_login").show();
$("#wlan_user_warninginfo_login").html(Em.I18n.get("User.Set.error"));
return true;
}
}
$("#wlan_user_passworderror_login").hide();
$("#wpakey_pwderr_warning").hide();
return false;
},
SetWifiNetworkData:function(){
this.issplit = window.location.href.indexOf("set_account")>=0;
if(!isguide || (isguide && !this.issplit)){
if(Atp.WifiApListController.activeItem.EncryptionType == "WEP"){
Atp.WifiApListController.activeItem.WepEncyptionIndex = $("#repeater_wpakey_index").val();
Atp.WifiApListController.activeItem.WepEncyptionMode = $("#repeater_enyp_mode").val();
Atp.WifiApListController.activeItem.WepKey = $("#wpakey_wizard_repeater").val();
}else{
Atp.WifiApListController.activeItem.wifiwisprpwd = $("#wpakey_wizard_repeater").val();
}
}
this.content = Atp.WifiApListController.activeItem;
if(isguide && this.issplit){
var userpassword = "";
if(Atp.WifiApListController.activeItem.EncryptionType == "NONE"){
userpassword = $("#guide_user_password_login").val();
}else{
var src = $("#guide_sync_checkbox_login").attr("src");
if(src.indexOf("off") < 0){
userpassword = $("#wpakey_wizard_repeater").val();
}else{
userpassword = $("#guide_user_password_login").val();
}
}
this.content.userpassword = userpassword;
}
console.log("wifidial content",this.content);
if(this.checkProc()){
return ;
}
if(isguide && !this.issplit){
utilchangePage("wan_wizard_page_repeater_set_account");
return ;
}
this.content.isguide = isguide;
if(this.content.EncryptionType == "WEP"){
this.content.WepKey = this.enc(this.content.WepKey);
}else{
this.content.wifiwisprpwd = this.enc(this.content.wifiwisprpwd);
}
if(isguide && this.issplit){
this.content.userpassword = this.enc(this.content.userpassword);
}
utilchangePage("wan_wizard_page_repeater_connecting");
var self  = this;
console.log("goto submit wifi dial....");
self.submitItem("update", this.content, function(result){
console.log("goto submit wifi dial result....",result);
if(result["errcode"] == 0){
var count = 10;
var interval=setInterval(function(){
count = count-1;
Atp.WlanDiagController.load(function(diagresult){
console.log("diagresult...",diagresult);
console.log("count",count);
if(diagresult["DiagPskError"] != 0){
self.connectfaild();
clearInterval(interval);
}else{
Atp.RepeaterStateController.load(function(result){
console.log("wifi connect statues...",result);
if(result["connstate"] == "Connected"){
self.connectsuccess();
clearInterval(interval);
}
});
}
});
if(count <= 0){
console.log("goto connectfaild function...");
self.connectfaild();
clearInterval(interval);
}
},4000);
}else{
self.connectfaild();
}
});
}
});
Atp.WifiApListController = Atp.MultiObjController.extend({
objName: 'ntwk/wifiscanresult',
WifiSignal:function(val){
var WifiSignal = val.WifiSignal;
var secmode = val.WifiAuthMode;
if(WifiSignal <= 50){
if(secmode != "Open"){
return "/res/ic_wifilock3.png";
}
return "/res/ic_wifiopen3.png";
}else if(WifiSignal > 50 && WifiSignal < 70){
if(secmode != "Open"){
return "/res/ic_wifilock2.png";
}
return "/res/ic_wifiopen2.png";
}else{
if(secmode != "Open"){
return "/res/ic_wifilock1.png";
}
return "/res/ic_wifiopen1.png";
}
},
showWifiNetwork: function(){
var repeaterinfohtml = '';
var WifiSignal = '';
var Encty ='';
var connbssid = "";
if("Connected" == Atp.RepeaterStateController.content.connstate){
connbssid = Atp.RepeaterStateController.content.bssid;
}
console.log("wifi scan result ------",connbssid);
for(var i = 0; i < this.content.length; i++){
if (this.content[i] == null) {
continue;
}
WifiSignal = this.WifiSignal(this.content[i]);
Encty = (this.content[i].EncryptionType == "WEP" ? "WEP" : this.content[i].WifiAuthMode);
var tag = "&nbsp;";
var connectColor = '#242424';
if(this.content[i].BSSID == connbssid){
tag = "&radic;";
connectColor = '#28cbc0';
Encty = Em.I18n.t("Touch.repeater.wizard.connected");
}
repeaterinfohtml += '<div class="ui-title-h2" style="height:128px;" align="center">'
repeaterinfohtml += '<a href="javascript:void(0)" onclick="showDetails(\''+this.content[i].BSSID+'\')">' ;
repeaterinfohtml += '<table style="width:100%;cursor:pointer;height:127px;"><tr>';
repeaterinfohtml += '<td style="word-break:break-all;word-wrap:break-word;width:20px;"><div style="font-size:28px;cursor:pointer;height:64px;line-height:90px;color:#28cbc0;">'+tag+'</div><div style="font-size:24px;height: 64px;line-height: 50px;color:#545454;"></div></td>';
repeaterinfohtml += '<td style="word-break:break-all;word-wrap:break-word; width:100%;">';
repeaterinfohtml += '<div style="font-size:28px;cursor:pointer;height:64px;line-height:90px;color:'+connectColor+';">';
repeaterinfohtml += utilTransfToShortStr(this.content[i].WifiSsid, 13);
repeaterinfohtml += '</div><div style="font-size:24px;height: 64px;line-height: 50px;color:#545454;">'+Encty+'</div></td>';
repeaterinfohtml += '<td><img src="'+WifiSignal+'" style="padding-top:20px;width:60px;height:60px;" /></td></tr>';
repeaterinfohtml += '<tr style="height:0px;"><td colspan="3"></td></tr></table></a>';
repeaterinfohtml += '</div>';
}
if(connbssid != ""){
$("#scanwifi_btn_div").hide();
$("#connected_btn").show();
}
utilchangePage("wan_wizard_repeater_detecting_result");
$("#wifidetailsinfo").html(repeaterinfohtml);
},
detect: function(){
utilchangePage("wan_wizard_page_repeater_detecting");
console.log("in Atp.WifiApListController detect...");
var self = this;
isrefresh = false;
Atp.RepeaterinfoController.load();
Atp.RepeaterStateController.load(function(){
Atp.WifiApListController.load(function(){
Atp.WifiApListController.showWifiNetwork();
});
});
}
});
});
function submit_repeater_detect(){
isrefresh = false;
stopStatusTime = true;
Atp.WanRepeaterDetectController.load(function(){
var ConnectionStatus = Atp.WanRepeaterDetectController.content.ConnectionStatus;
var AccessType = Atp.WanRepeaterDetectController.content.AccessType;
console.log("------",ConnectionStatus,AccessType)
if(ConnectionStatus == "Connected" && AccessType == "Ethernet"){
utilchangePage("wan_wizard_page_repeater_scanfail");
}else{
Atp.WifiApListController.detect();
}
});
}
function showDetails(bssid){
utilchangePage("wan_wizard_page_repeater_edit_auto");
$("#wpakey_pwderr_warning").hide();
Atp.WifiApListController.isEdit = true;
for(var i = 0; i < Atp.WifiApListController.content.length; i++){
if(Atp.WifiApListController.content[i].BSSID == bssid){
Atp.WifiApListController.activeItem = Atp.WifiApListController.content[i];
break;
}
}
if(isguide){
if(Atp.WifiApListController.activeItem.EncryptionType == "NONE"){
$("#wifiwpakeyency_login").hide();
$('#repeater_pwd_login').show();
$('#guide_user_password_strength_login').hide();
}else{
$("#wifiwpakeyency_login").show();
}
}
var shortWiFiSsid = utilTransfToShortStr(Atp.WifiApListController.activeItem.WifiSsid, 13);
$("#Touch_wan_wizard_repeater_help").text(Em.I18n.get("Touch.wan.wizard.repeater.help",{arg1:shortWiFiSsid}));
$("#repeater_wifissid_auto").empty().text(shortWiFiSsid);
var wifisecurity = Atp.WifiApListController.activeItem.WifiAuthMode;
if(Atp.WifiApListController.activeItem.EncryptionType == "WEP"){
wifisecurity = "WEP";
$("#repeater_wpakey_wep_div").show();
$("[value='Open']").text(Em.I18n.get("Touch.wan.wizard.repeater.Open"));
$("[value='Shared']").text(Em.I18n.get("Touch.wan.wizard.repeater.Shared"));
$("#wpakey_fieldcontain_wizard_repeater_div").show();
}else{
$("#repeater_wpakey_wep_div").hide();
if(Atp.WifiApListController.activeItem.WifiAuthMode == "Open"){
$("#wpakey_fieldcontain_wizard_repeater_div").hide();
}else{
$("#wpakey_fieldcontain_wizard_repeater_div").show();
}
}
$("#repeater_wifisecurity_auto").empty().text(wifisecurity);
}
function GuideSyncCheckBoxOnClick_login(id){
var ischecked = setWlanEnableImage("guide_sync_checkbox_login");
utilShowEyePassword('guide_wifi_eyeshowpwd_img','guide_user_password_div_login','guide_user_password_text_div_login');  
if(ischecked == false){
console.log('unchecked.................................')
$("#guide_user_password_login").removeAttr("disabled");
$("#guide_user_text_password_login").removeAttr("disabled");
$("#sync_pwd_repeater").css("color","#242424");
$("#sync_pwd_text_repeater").css("color","#242424");
$('#guide_user_password_login, #guide_user_text_password_login').val("");
$("#guide_user_password_login").attr("placeholder", Em.I18n.get("Touch.user.passwordinfo.oninput"));
$("#guide_user_text_password_login").attr("placeholder", Em.I18n.get("Touch.user.passwordinfo.oninput"));
$('#guide_user_password_strength_login').hide();
}
else{
syncLoginPassword();
console.log('checked.................................')
$("#guide_user_password_login").attr("disabled","disabled");
$("#guide_user_text_password_login").attr("disabled","disabled");
$("#sync_pwd_repeater").css("color","#999");
$("#sync_pwd_text_repeater").css("color","#999");
$("#guide_user_password_login").removeAttr("placeholder");
$("#guide_user_text_password_login").removeAttr("placeholder");
$('#wlan_user_passworderror_login').hide();
var originpwd = $("#wpakey_wizard_repeater").val();
$("#guide_user_password_login, #guide_user_text_password_login").val(originpwd);
}
}
function syncLoginPassword(val) {
var src = $("#guide_sync_checkbox_login").attr("src");
var user = $('#guide_user_password_login, #guide_user_text_password_login');
var key = $("#guide_wifi_text_password_login,#guide_wifi_password_login").filter(":visible").val();
val = val ? val : key;
if(src.indexOf("off")<0){
user.val(val);
}
}
