var TimeZoneValues=["-12:00","-11:00","-10:00","-09:00","-08:00","-07:00","-07:00","-07:00","-06:00","-06:00","-06:00","-06:00","-05:00","-05:00","-05:00","-04:00","-04:00","-04:00","-03:30","-03:00","-03:00","-03:00","-02:00","-01:00","-01:00","00:00","00:00","+01:00","+01:00","+01:00","+01:00","+01:00","+02:00","+02:00","+02:00","+02:00","+02:00","+02:00","+03:00","+03:00","+03:00","+03:00","+03:30","+04:00","+04:00","+04:30","+05:00","+05:00","+05:30","+05:45","+06:00","+06:00","+06:00","+06:30","+07:00","+07:00","+08:00","+08:00","+08:00","+08:00","+08:00","+09:00","+09:00","+09:00","+09:30","+09:30","+10:00","+10:00","+10:00","+10:00","+10:00","+11:00","+11:00","+12:00","+12:00","+13:00"];var gmt="";var offsetArr=new Array();var g_zonediff=0;var dstcfgenable=0;var dstoffset=[0,0,0];var dststart=[0,0,0,0,0,0];var dstend=[0,0,0,0,0,0];function setInitSelect(){var a=false;if(Atp.SntpController.content.get("TimeZoneIdx")==-1){a=true}else{var b=Atp.SntpController.content.get("TimeZoneIdx");if(gmt!=TimeZoneValues[b]){a=true}}if(a){for(var c=0;c<TimeZoneValues.length;c++){if(TimeZoneValues[c]==gmt){Em.Logger.log("gmt",gmt,TimeZoneValues[c]);Atp.SntpController.content.set("TimeZoneIdx",c);break}}}}function getzonediff(c){if(c==""||c=="GMT"||c=="00:00"||c=="+00:00"||c=="-00:00"){return 0}var b=c.substr(1,2);var d=c.substr(4,2);var a=0;if(b.charAt(0)=="0"){a+=parseInt(b.charAt(1))*3600}else{a+=parseInt(b)*3600}if(d.charAt(0)=="0"){a+=parseInt(d.charAt(1))*60}else{a+=parseInt(d)*60}if(c.charAt(0)=="-"){a=a*(-1)}return a}function setoffset(e){var a=e.length;var d=0;var c=0;var b="";offsetArr[0]=0;offsetArr[1]=0;offsetArr[2]=0;for(c=0;c<a;c++){if((e.charAt(c)>="0")&&(e.charAt(c)<="9")){b+=e.charAt(c);c++}if((e.charAt(c)>="0")&&(e.charAt(c)<="9")){b+=e.charAt(c);c++}if(e.charAt(c)==":"){if(b.length==2&&b.charAt(0)=="0"){offsetArr[d]=parseInt(b.charAt(1))}else{offsetArr[d]=parseInt(b)}d++;b=""}if(((e.charAt(c)<"0")||(e.charAt(c)>"9"))&&(e.charAt(c)!=":")){break}}if(b!=""){offsetArr[d]=parseInt(b)}e=e.substring(c,a);return e}function setDSToffset(g){var b=g.length;var f=0;var e=0;var d="";var c=0;var a=0;offsetArr[0]=0;offsetArr[1]=0;offsetArr[2]=0;for(e=0;e<b;e++){if(g.charAt(e)=="+"||g.charAt(e)=="-"){continue}if((g.charAt(e)>="0")&&(g.charAt(e)<="9")){d+=g.charAt(e);e++}if((g.charAt(e)>="0")&&(g.charAt(e)<="9")){d+=g.charAt(e);e++}if(g.charAt(e)==":"){if(d.length==2&&d.charAt(0)=="0"){offsetArr[f]=parseInt(d.charAt(1))}else{offsetArr[f]=parseInt(d)}f++;d=""}if(((g.charAt(e)<"0")||(g.charAt(e)>"9"))&&(g.charAt(e)!=":")){break}}if(d!=""){offsetArr[f]=parseInt(d)}a=offsetArr[0]*3600+offsetArr[1]*60+offsetArr[2];if(g.charAt(0)=="-"){a=a*(-1)}c=0-g_zonediff-a;offsetArr[0]=parseInt(c/3600);offsetArr[1]=parseInt((c%3600)/60);offsetArr[2]=(c%3600)%60;g=g.substring(e,b);dstoffset[0]=offsetArr[0];dstoffset[1]=offsetArr[1];dstoffset[2]=offsetArr[2];return g}function getdayoffset(e){var a=e.length;var d=0;var c=-1;var b=0;if(e.charAt(0)=="M"){e=e.substring(1,a)}else{return e}a=e.length;offsetArr[0]=0;offsetArr[1]=0;offsetArr[2]=0;for(b=0;b<a;b++){if((e.charAt(b)>="0")&&(e.charAt(b)<="9")){c=parseInt(e.charAt(b));b++}if((e.charAt(b)>="0")&&(e.charAt(b)<="9")){c=c*10+(e.charAt(b)-"0");b++}if(e.charAt(b)=="."){offsetArr[d]=c;d++;c=-1}if(((e.charAt(b)<"0")||(e.charAt(b)>"9"))&&(e.charAt(b)!=".")){break}}if(c!=-1){offsetArr[d]=c}e=e.substring(b,a);return e}function parse_time_zone_name(d){var e="";var b=d.length;var c=0;var a=0;if(d.charAt(0)=="<"){for(c=0;c<b;c++){if(d.charAt(c)==">"){e=d.substr(c+1,b-c);break}}}else{for(c=0;c<b;c++){if(((d.charAt(c)<"A")||(d.charAt(c)>"Z"))&&((d.charAt(c)<"a")||(d.charAt(c)>"z"))){e=d.substr(c,b-c);break}}}if((e.charAt(0)!="-")&&(e.charAt(0)!="+")){e="+"+e}if(e.charAt(0)=="-"){gmt="+"}else{gmt="-"}if((e.charAt(1)>="0")&&(e.charAt(1)<="9")){if((e.charAt(2)>="0")&&(e.charAt(2)<="9")){gmt+=e.charAt(1);gmt+=e.charAt(2)}else{gmt+="0";gmt+=e.charAt(1)}}if(e.charAt(3)!=":"){gmt+=":";gmt+="0";gmt+="0"}else{gmt+=e.charAt(3);if((e.charAt(4)>="0")&&(e.charAt(4)<="9")){if((e.charAt(5)>="0")&&(e.charAt(5)<="9")){gmt+=e.charAt(4);gmt+=e.charAt(5)}else{gmt+="0";gmt+=e.charAt(5)}}}for(c=1;c<gmt.length;c++){if(gmt.charAt(c)=="0"){continue}}if(c==gmt.length-1){gmt="GMT"}if((gmt=="+00:00")||(gmt=="-00:00")||gmt=="00:00"){gmt="GMT"}g_zonediff=getzonediff(gmt);for(c=0;c<e.length;c++){if((e.charAt(c)=="-")||(e.charAt(c)=="+")||(e.charAt(c)==":")||((e.charAt(c)>="0")&&(e.charAt(c)<="9"))){continue}break}a=e.length;e=e.substr(c,a);if(e==""){dstcfgenable=0;return}dstcfgenable=1;if(e.charAt(0)=="<"){for(c=0;c<b;c++){if(e.charAt(c)==">"){e=e.substr(c+1,b-c);break}}}else{for(c=0;c<e.length;c++){if(((e.charAt(c)>="A")&&(e.charAt(c)<="Z"))||((e.charAt(c)>="a")&&(e.charAt(c)<="z"))){continue}a=e.length;e=e.substr(c,a-c);break}}if(e.charAt(0)==","){offsetArr[0]=1;offsetArr[1]=0;offsetArr[2]=0}else{e=setDSToffset(e)}if(e.charAt(0)!=","){return}e=e.substring(1,e.length);if(e.charAt(0)!="M"){return}e=getdayoffset(e);dststart[0]=offsetArr[0]-0;dststart[1]=offsetArr[1]-0;dststart[2]=offsetArr[2]-0;if(e.charAt(0)==","){offsetArr[0]=2;offsetArr[1]=0;offsetArr[2]=0}else{if(e.charAt(0)=="/"){e=e.substring(1,e.length);e=setoffset(e)}else{return}}dststart[3]=offsetArr[0];dststart[4]=offsetArr[1];dststart[5]=offsetArr[2];if(e.charAt(0)!=","){return}e=e.substring(1,e.length);if(e.charAt(0)!="M"){return}e=getdayoffset(e);dstend[0]=offsetArr[0]-0;dstend[1]=offsetArr[1]-0;dstend[2]=offsetArr[2]-0;if(e.charAt(0)==","){offsetArr[0]=2;offsetArr[1]=0;offsetArr[2]=0}else{if(e.charAt(0)=="/"){e=e.substring(1,e.length);e=setoffset(e)}}dstend[3]=offsetArr[0];dstend[4]=offsetArr[1];dstend[5]=offsetArr[2]}Atp.SntpModel=Em.Object.extend({TimeZone:function(a,b){if(1==arguments.length){return gmt}else{gmt=b;this.set("LocalTimeZoneName",build_time_zone_name());return b}}.property("LocalTimeZoneName"),setTimeZone:function(){this.set("TimeZone",TimeZoneValues[this.get("TimeZoneIdx")]);this.set("LocalTimeZoneName",build_time_zone_name())}.observes("TimeZoneIdx"),sntpStatusStr:function(){return Em.I18n.t("sntp.status_"+this.get("Status"))}.property("Status")});function build_offset_time(){var d="";var g=0;var f=0;var b,h,c;var a;if(Atp.SntpController.content){if("00:00"==Atp.SntpController.content.get("TimeZone")){return"00:00:00"}else{var e="GMT"+Atp.SntpController.content.get("TimeZone");g=getzonediff(e.substr(3,6))}}b=dstoffset[0];h=dstoffset[1];c=dstoffset[2];f=b*3600+h*60+c;a=0-(g+f);if(a<0){d+="-"}var e=Math.abs(a);b=parseInt(e/3600);h=parseInt((e%3600)/60);c=(e%3600)%60;if(b<10){d+="0"}d+=b;d+=":";if(h<10){d+="0"}d+=h;d+=":";if(c<10){d+="0"}d+=c;return d}function build_time_zone_name(){var b="GMT";var a="";Em.Logger.log("Build time now ...");if("GMT"!=gmt){b+=gmt}else{b+="00:00:00"}if(b.charAt(3)=="+"){b=b.replace("+","-")}else{if(b.charAt(3)=="-"){b=b.replace("-","+")}}if(0==dstcfgenable){return b}a=build_offset_time();b+="DST";b+=a;b+=",";b+=("M"+dststart[0]+"."+dststart[1]+"."+dststart[2]+"/");b+=(dststart[3]+":"+dststart[4]+":"+dststart[5]);b+=",";b+=("M"+dstend[0]+"."+dstend[1]+"."+dstend[2]+"/");b+=(dstend[3]+":"+dstend[4]+":"+dstend[5]);return b}Atp.SntpController=Atp.SingleObjController.create({objName:"ntwk/sntp",modelClass:Atp.SntpModel,curDateStr:"",curDate:null,isLoadFinish:false,timerInterval:1000,postsntpData:function(d){var b=utilGetCsrf();var a=this;var c={csrf:b,data:a.createSubmitData()};a.post($.toJSON(c),function(){if(d){d()}})},postData:function(){var b=utilGetCsrf();var a=this;utilStartSubmitDialog();var c={csrf:b,data:a.createSubmitData()};a.post($.toJSON(c),function(){setTimeout(function(){a.pageload();utilStopSubmitDialog()},5000)})},timerCallback:function(){var b=Atp.SntpController.get("curDate");if(b){b.setSeconds(b.getSeconds()+1);var j=b.getFullYear();var a=b.getMonth()+1;a=""+a;if(a.length<2){a="0"+a}var g=""+b.getDate();if(g.length<2){g="0"+g}var f=""+b.getHours();if(f.length<2){f="0"+f}var c=""+b.getMinutes();if(c.length<2){c="0"+c}var e=""+b.getSeconds();if(e.length<2){e="0"+e}this.set("curDateStr",j+"-"+a+"-"+g+" "+f+":"+c+":"+e)}},afterLoad:function(){var b=this.content.get("CurrentLocalTime").split("T");var a=b[0].split("-");var c=b[1].split(":");parse_time_zone_name(this.content.get("LocalTimeZoneName"));this.content.set("LocalTimeZoneName",build_time_zone_name());setInitSelect();this.set("curDate",new Date(a[0],a[1]-1,a[2],c[0],c[1],c[2]));this.set("curDateStr",b[0]+" "+b[1]);this.set("isLoadFinish",true)}});function init_TimeZone_values(d,c,a){var b=c;while(b<a){d[b]=b++}}var TimeZoneselectValues=new Array();init_TimeZone_values(TimeZoneselectValues,0,76);Atp.TimeZoneController=Atp.SelectController.create({prefixName:"sntp.TimeZone",values:TimeZoneselectValues});Atp.SntpSelectBoxScrollItem=Atp.SelectBasicItem.extend({SntpSelectBoxScrollItemID:function(){return this.content.get("value")+"_SntpSelectBoxScrollItemID"}.property(),template:Em.Handlebars.compile('    {{#if view.isfirstone}}      {{#if view.isfirstSelected}}        <div class="sntpselect_top_scroll sntpselect_topselected">       {{else}}        <div class="sntpselect_top_scroll">       {{/if}}    {{else}}      {{#if view.islastone}}        {{#if view.islastSelected}}          <div class="sntpselect_bottom_scroll sntpselect_bottomselected">         {{else}}          <div class="sntpselect_bottom_scroll">         {{/if}}      {{else}}         {{#if view.isSelectItem}}           <div class="sntpselect_mid_scroll sntpselect_mid_selected">         {{else}}          <div class="sntpselect_mid_scroll">         {{/if}}      {{/if}}     {{/if}}       <div style="margin-left:15px;padding-top:10px;overflow:hidden;width:415px;white-space:nowrap;" {{bindAttr id="view.SntpSelectBoxScrollItemID"}}>{{view.content.name}}</div>     </div>  ')});Atp.SntpSelectBox=Atp.SelectBasic.extend({currentselectid:function(){return this.get("selectlistid")+"_parenselect"}.property(),currentcontentid:function(){return this.get("selectlistid")+"_childselect"}.property(),template:Em.Handleba