$("#guide_wlan_page").bind("pagebeforeshow", function(event, data){
isAgreeProtocal();
setButtonI18n("guide_wifi_next_btn", 'guide.btn.next');
$("#guide_wifi24_ssid").attr("placeholder", Em.I18n.get("Touch.wifi.ssidinfo.oninput"));
$("#guide_wifi_password").attr("placeholder", Em.I18n.get("Touch.wifi.passwordinfo.oninput",{arg1:"8~32"}));
$("#guide_wifi_text_password").attr("placeholder", Em.I18n.get("Touch.wifi.passwordinfo.oninput",{arg1:"8~32"}));
console.log("load wlansetting_page...");
});
var wlandata = {};
$("#guide_wifi_form").on("submit",function (e){
var ssidMaxLen = 32;
var $this = $(this);
e.preventDefault();
wlandata = $this.serializeObject();
wlandata["guide_wifi24_ssid"] = $.trim(wlandata["guide_wifi24_ssid"]);
var pwdLen = wlandata["guide_wifi_password"].length;
var ssiddata = wlandata["guide_wifi24_ssid"];
var w=/^[\x20-\x7e]*$/;
var u=/.*[\x20-\x7e]$/;
var patrn_key=/.*[^\x20-\x7e]+.*$/;
$("#wlan_passworderror").hide();
if(ssiddata.length < 1 || ssiddata.length > ssidMaxLen){
$("#wlan_passworderror").show();
$("#wlan_warninginfo").html(Em.I18n.get("Wlan.Set.error"));
return ;
}
if(!( w.test(ssiddata) && u.test(ssiddata)) || utilcheckInputCharacters(ssiddata)){
$("#wlan_passworderror").show();
$("#wlan_warninginfo").html(Em.I18n.get("Wlan.Set.error"));
return ;
}
if((pwdLen < 8) || (pwdLen > 32)){
$("#wlan_passworderror").show();
$("#wlan_warninginfo").html(Em.I18n.get("Touch.wifi.passwordinfo",{arg1:"8~32"}));
return ;
}
if(patrn_key.exec($("#guide_wifi_password").val())){
$("#wlan_passworderror").show();
$("#wlan_warninginfo").html(Em.I18n.get("Wlan.Set.error"));
return ;
}
utilchangePage("guide_account_page");
});
$("#guide_account_page").on("pagecreate", function( event, ui ) {
var aimhash = "#guide_account_page";
var currentHash = window.location.hash;
if(currentHash == aimhash){
utilchangePage("guide_wlan_page");
}
});
$("#guide_account_form").on("submit",function (e){
var ssidMaxLen = 32;
var $this = $(this);
e.preventDefault();
var patrn_key=/.*[^\x20-\x7e]+.*$/;
var data = $this.serializeObject();
data.guide_wifi24_ssid = wlandata["guide_wifi24_ssid"];
data.guide_wifi_password = wlandata["guide_wifi_password"];
data.guide_wifi_text_password = wlandata["guide_wifi_text_password"];
var ssiddata = wlandata["guide_wifi24_ssid"];
var src = $("#guide_sync_checkbox").attr("src");
if(src.indexOf("off")>0){
var userpwdLen = data["guide_user_password"].length;
if((userpwdLen < 6) || (userpwdLen > 32)){
$("#wlan_user_passworderror").show();
$("#wlan_user_warninginfo").html(Em.I18n.get("Touch.user.passwordinfo"));
return ;
}
if(patrn_key.exec($("#guide_user_password").val())){
$("#wlan_user_passworderror").show();
$("#wlan_user_warninginfo").html(Em.I18n.get("User.Set.error"));
return ;
}
}
else{
data["guide_user_password"] = wlandata["guide_wifi_password"];
}
utilchangePage("guide_wifi_submiting_page");
Atp.GuideBeginController.load(function(result){
if(result["ConnectionStatus"] == "Connected"){
if(2 != result["HttpStatus"] && ("IP_Routed" == result["ConnectionType"])){
$("#finshed_failed").show();
$("#finshed_failed_info").show();
$("#finshed_down_info").hide();
$("#finshed_success").hide();
$("#finshed_success_info").hide();
}else{
$("#finshed_failed").hide();
$("#finshed_failed_info").hide();
$("#finshed_down_info").hide();
$("#finshed_success").show();
$("#finshed_success_info").show();
}
}else{
$("#finshed_success").hide();
$("#finshed_success_info").hide();
$("#finshed_failed").show();
if(isLineDown){
$("#finshed_failed_info").hide();
$("#finshed_down_info").show();
}else{
$("#finshed_failed_info").show();
$("#finshed_down_info").hide();
}
}
});
$("#new-ssid-2G").html(ssiddata);
data.ssid5g = data.guide_wifi24_ssid.substr(0,29) +"_5G";
$("#new-ssid-5G").html(data.ssid5g);
console.log("wifi data",data);
Atp.GuideWifiController.submit(data, function(result){
console.log("post wifi data ...");
if(result["errcode"] == 0){
console.log("wifi set success...");
}
});
});
