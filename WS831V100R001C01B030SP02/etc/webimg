local tonumber = tonumber
local string = string

local errcode, values = dm.GetParameterValues("InternetGatewayDevice.LANDevice.1.WLANEndPoint.1.LinkInfo.", 
    {"SSID", "BSSID", "Status", "Channel", "Signal", "Speed"});


local newObj = {}
local objs = {} 

if values ~= nil then
    objs = values["InternetGatewayDevice.LANDevice.1.WLANEndPoint.1.LinkInfo."]
end


if objs ~= nil  and objs["Status"] == "Connected" and objs["BSSID"] ~= "" then 
    newObj["ssid"] = dm.ScanIconvTryConvert(objs["SSID"])
    
    newObj["bssid"] = string.upper(objs["BSSID"])
    newObj["connstate"] = "Connected"
    newObj["signal"] = objs["Signal"] - 100
    newObj["channel"] = objs["Channel"]
    newObj["speed"] = objs["Speed"]
else
    newObj["ssid"] = ""
    newObj["bssid"] = ""
    newObj["signal"] = ""
    newObj["channel"] = ""
    newObj["speed"] = ""
    newObj["connstate"] = "Disconnected"  
end

local errcode,wlanBasic = dm.GetParameterValues("InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.",{"X_OperatingFrequencyBand","SSID"});
function calc_min(frequency)
    local array = {}
    i = 1
    if wlanBasic ~= nil then
        for k,v in pairs(wlanBasic) do
            if frequency == v["X_OperatingFrequencyBand"] then
                local wlan_instance_id, wlan_suffix = utils.findLastPoint(k)
                array[i] = tonumber(wlan_instance_id)
                i = i+1
            end
        end
        table.sort(array)
        if 1 == i  then
            return -1
        else
            return array[1]
        end
    end
    return -1
end
local wlan_index = calc_min("2.4GHz")
local wlan_5gindex = calc_min("5GHz")
function isValidIndex(k)
    local  wlan_instance_id, wlan_suffix = utils.findLastPoint(k)    
    if wlan_index == tonumber(wlan_instance_id) or wlan_5gindex == tonumber(wlan_instance_id)  then
        return true
    end
    return false
end

for k,v in pairs(wlanBasic) do
    if isValidIndex(k) then
        if v["X_OperatingFrequencyBand"] == "2.4GHz" then
            newObj["Wlan24SSID"] = v["SSID"]
        end
        if v["X_OperatingFrequencyBand"] == "5GHz" then
            newObj["Wlan5SSID"] = v["SSID"]
        end
    end
end
sys.print(json.encode(newObj))
require('dm')
require('web')
require('json')
require('utils')

local string = string
local sort, strlen = table.sort, string.len



function compMAC(a, b)
    if a.MACAddress >= b.MACAddress then
        return false
    else
        return true
    end
end

function compMACFilter(a, b)
    if a.X_WlanSrcMacAddress >= b.X_WlanSrcMacAddress then
        return false
    else
        return true
    end
end

function comp(a, b)
    if a.Active46 == b.Active46 then
        return compMAC(a, b)
    end   
    if a.Active46 == true and b.Active46 == false then
        return true
    end
    return false
end

function Find_host(objs, MAC)
    local UpperMac = string.upper(MAC)
    if objs ~= nil then
        for k,v in pairs(objs) do
            if  UpperMac == string.upper(v["MACAddress"]) then
                return v
            end
        end
    end
    return nil
end


local errcode, lanVals = dm.GetParameterValues("InternetGatewayDevice.Layer2Bridging.AvailableInterface.{i}.",
    {"InterfaceReference", "X_InterfaceAlias"})

local lanarray = {}

for k, v in pairs(lanVals) do
    lanarray[v["InterfaceReference"]] = v["X_InterfaceAlias"]
end

local hosts = {}

local errcode, objs = dm.GetParameterValues("InternetGatewayDevice.LANDevice.{i}.Hosts.Host.{i}.", 
    {"X_DeviceName", "X_DeviceType", "HostName", "MACAddress", "Active",
    "Layer2Interface",  "VendorClassID"});

for k,v in pairs(objs) do
    local newObj = {}
        newObj["ID"] = k
        newObj["MACAddress"] = v["MACAddress"]       
        newObj["Active"] = utils.toboolean(v["Active"])
        newObj["Active46"] = false
        if utils.toboolean(v["Active"]) then
            newObj["Active46"] = true
        end        
        newObj["IconType"] = v["X_DeviceType"]
        newObj["ActualName"] = v["X_DeviceName"]
        newObj["domain"] = v["Layer2Interface"]
        newObj["HostName"] = v["HostName"]
        newObj["Layer2Interface"] = lanarray[v["Layer2Interface"]]
        newObj["VendorClassID"] = v["VendorClassID"]

        table.insert(hosts, newObj)
end
-- sys.print(json.encode(hosts));

local errcode, values = dm.GetParameterValues("InternetGatewayDevice.X_WIFISAFEKEY.X_WlanMacFilter.{i}.", 
        {"X_WlanSrcMacAddress"});
-- sys.print(json.encode(values));

sort(hosts, comp)
local results = {}
local host = {}
local defaulthost = {}
local i
if nil ~= values then
    for k,v in pairs(values) do
        host = Find_host(hosts, v["X_WlanSrcMacAddress"])
        if nil ~= host then
            host.ID = k
            table.insert(results, host)
        else    
            defaulthost = {}
            defaulthost.ID = k
            defaulthost.ActualName,i = string.gsub(v["X_WlanSrcMacAddress"], ":", "-")
            defaulthost.HostName =  defaulthost.ActualName
            defaulthost.MACAddress = v["X_WlanSrcMacAddress"]
            defaulthost.IconType = "unknown"
            defaulthost.Active = false
            table.insert(results, defaulthost)
        end 
    end
end
sort(results, comp)
sys.print(json.encode(results))

   local utils = require('utils')
require('dm')

local string = string

function Find_host_macaddr_unique(MAC)
    local UpperMac = string.upper(MAC)
    local errcode, objs = dm.GetParameterValues("InternetGatewayDevice.LANDevice.1.Hosts.Host.{i}.", 
        {"MACAddress"})
    if objs ~= nil then
        for k,v in pairs(objs) do
            if  UpperMac == string.upper(v["MACAddress"]) then
                return k
            end
        end
    end

    return ""
end

function check_macaddr_unique(ID, MAC)
    local UpperMac = string.upper(MAC)
    local errcode, objs = dm.GetParameterValues("InternetGatewayDevice.X_WIFISAFEKEY.X_WlanMacFilter.{i}.", 
        {"X_WlanSrcMacAddress"});
    if objs ~= nil then
        for k,v in pairs(objs) do
            if k ~= ID and UpperMac == string.upper(v["X_WlanSrcMacAddress"]) then
                return true
            end
        end
    end

    return false
end

function DelBlackList( mac )
    print("enter delete black list",mac )
    local UpperMac = string.upper(mac)
    local errcode,wlanMac = dm.GetParameterValues("InternetGatewayDevice.LANDevice.1.WLANConfiguration.1.X_WlanMacFilter.{i}.",
        {
            "X_WlanSrcMacAddress"
        }
    )
    if wlanMac ~= nil then
    	for k,v in pairs(wlanMac) do
    		for k1, value in pairs(v) do
    		    if string.upper(value) == UpperMac then
    		        dm.DeleteObject(k)
    		    end 
    	    end
	    end
    end 
end

local maps = {
    ActualName="X_DeviceName",
    IconType = "X_DeviceType",
    MACAddress = "MACAddress"
}

local filtermaps = {
    MACAddress = "X_WlanSrcMacAddress"
}

--ActualName  
local paras = {{"HostName", data["ActualName"]}, 
        {"X_DeviceName", data["ActualName"]}, 
        {"X_DeviceType", data["IconType"]},
        {"MACAddress", data["MACAddress"]},
        {"Active", false}}
 
local para = {"X_WlanSrcMacAddress", data["MACAddress"]}  

function create()
    -- add 
    if check_macaddr_unique("", data["MACAddress"])  then
        utils.appenderror("landev.mac", "landev.mac_err")
        return 9006
    end
    --first add to hosts node to check if the hostname is unique
    local domain = Find_host_macaddr_unique(data["MACAddress"])
    if  "" == domain then
        local err1,instnum1,NeedReboot1,errs1 = dm.AddObjectWithValues("InternetGatewayDevice.LANDevice.1.Hosts.Host.", paras);
        utils.responseErrorcode(err1, errs1, maps)
        if 0 ~= err1 then 
            return err1
        end    
    else
        local values = {{domain.."X_DeviceName", data["ActualName"]}, 
                {domain.."HostName", data["ActualName"]},
                {domain.."X_DeviceType", data["IconType"]}}

        local errcode, NeedReboot, paramerr = dm.SetParameterValues(values)
        utils.responseErrorcode(errcode, paramerr, maps)
        if 0 ~= errcode then
            return errcode
        end 
    end

    local param = {}
    local inputs = {}
    param["key"] = "X_WlanSrcMacAddress"
    param["value"] = data["MACAddress"]
    table.insert(inputs, param)
    --delete black node first
    DelBlackList(data["MACAddress"])
    local errcode1, instnum, NeedReboot, paramerr = dm.AddObjectWithValues("InternetGatewayDevice.X_WIFISAFEKEY.X_WlanMacFilter.", inputs)
    if 0 ~= errcode1 then
        --utils.appenderror("landev.mac", "landev.mac_err")
        return errcode1
    end 
    return  errcode1  
end

function delete()
    return dm.DeleteObject(data["ID"])
end

function update()
    local domain = Find_host_macaddr_unique(data["MACAddress"])
    local err,needreboot, paramerror ,instnum,errs
    if "" ~= domain then
        local values = {{domain.."X_DeviceName", data["ActualName"]}, 
                        {domain.."HostName", data["ActualName"]},
                        {domain.."X_DeviceType", data["IconType"]}}
        err,needreboot, paramerror = dm.SetParameterValues(values);
        utils.responseErrorcode(err, paramerror, maps)
    else
        err,instnum,needreboot,errs = dm.AddObjectWithValues("InternetGatewayDevice.LANDevice.1.Hosts.Host.", paras);
        utils.responseErrorcode(err, errs, maps)
    end
    
    return err
end
local para_err = 0
if action == 'create' then
    para_err = create()
elseif action == 'update' then
    para_err = update()
elseif action == 'delete' then
    para_err = delete() 
end
print("leave safelock.lua", para_err)
utils.appenderror("errcode", para_err)

