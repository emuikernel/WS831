function Iconpath( signal )
    if signal < 50 then
        return "/res/signal_05.gif"
    end
    if signal < 61 then
        return "/res/signal_04.gif"
    end
    if signal < 70 then
        return "/res/signal_03.gif"
    end
    if signal < 77 then
        return "/res/signal_02.gif"
    end
    if signal < 84 then
        return "/res/signal_01.gif"
    end
    return "/res/signal_00.gif"
end

local errcode, values = dm.GetParameterValues("InternetGatewayDevice.LANDevice.1.WLANEndPoint.1.LinkInfo.", 
    {"SSID", "BSSID", "Status", "Channel", "Signal", "Speed"})

local objslinkinfo = {} 
local ConnectedSSSID = ""
if values ~= nil then
    objslinkinfo = values["InternetGatewayDevice.LANDevice.1.WLANEndPoint.1.LinkInfo."]
end

if objslinkinfo ~= nil  and objslinkinfo["Status"] == "Connected" and objslinkinfo["BSSID"] ~= "" then 
    ConnectedSSSID = string.lower(objslinkinfo["BSSID"])
end

local errcode, objs = dm.GetParameterValues("InternetGatewayDevice.LANDevice.1.WLANEndPoint.1.APList.{i}.", 
   {"SSID", "BSSID", "Channel", "SecurityMode", "EncryptionType", "BWControl", "Signal"});

local aplist = {}
local activeItem = {}
if objs ~= nil then
    for k,v in pairs(objs) do
        local newObj = {}
        newObj["ID"] = k
        newObj["WifiSsid"] = dm.ScanIconvTryConvert(v["SSID"])
        newObj["WifiSignal"] = v["Signal"]
        newObj["WifiAuthMode"] = v["SecurityMode"]--Open, Shared, WPA-PSK, WPA2-PSK 
        newObj["WifiSecMode"] = v["SecurityMode"]
        if v["SecurityMode"] == "Open" then
            newObj["WifiNeedPassword"] = 0
        else
            newObj["WifiNeedPassword"] = 1
        end
        newObj["WepEncyptionMode"] = ""
        newObj["WepEncyptionIndex"] = 1
        newObj["WepKey"] = ""
        newObj["wifiwisprenable"] = 1
        newObj["wifiwispruser"] = ""
        newObj["wifiwisprpwd"] = ""
        newObj["Channel"] = v["Channel"]
        newObj["Signalimg"] = Iconpath(newObj["WifiSignal"])
        newObj["BSSID"] = v["BSSID"]
        newObj["BWControl"] = v["BWControl"]
        newObj["AddManuel"] = 0
        newObj["EncryptionType"] = v["EncryptionType"]
        if ConnectedSSSID == string.lower(v["BSSID"]) then
            newObj["profileenable"] = 1
            newObj["isConnect"] = true
            activeItem = newObj
        else
            newObj["profileenable"] = 0
            newObj["isConnect"] = false
        end
        table.insert(aplist, newObj)
    end
end

function compID(a, b)
    if strlen(a.ID) > strlen(b.ID) then
        return false
    elseif strlen(a.ID) < strlen(b.ID) then
        return true
    end
    if a.ID >= b.ID then
        return false
    else
        return true
    end
end

function comp(a, b)
    if a.isConnect == b.isConnect then
        return compID(a, b)
    end
    if a.isConnect == true and b.isConnect == false then
        return true
    end
    return false
end
sort(aplist, comp)

if isgetactiveitem then
    sys.print(json.encode(activeItem))
else
    sys.print(json.encode(aplist))
endrequire('dm')
require('json')
require('utils')

local CONST_WIFI_STATUS_EXCELLENT = 0
local CONST_WIFI_STATUS_MEDIUM    = 10
local CONST_WIFI_STATUS_POOR      = 20

local tonumber = tonumber

function print_para(RssiArray)
	if RssiArray == nil then
		return
	end
	for k,v in pairs(RssiArray) do 
		print(k,v["Channel"], v["TotalRssi"])
	end
end

function IsSignalStrong(RssiArray, channel)
	table.sort(RssiArray, compTotalRssi)
	--print_para(RssiArray)
	for i = 1, 3 do
		if RssiArray[i].Channel == channel then
			return CONST_WIFI_STATUS_EXCELLENT
		end	
	end

	for i = table.getn(RssiArray) - 2, table.getn(RssiArray) do
		if RssiArray[i].Channel == channel then
			return CONST_WIFI_STATUS_POOR
		end
	end

	return CONST_WIFI_STATUS_MEDIUM
end

function compTotalRssi(a, b)
    if a.TotalRssi >= b.TotalRssi then
        return false
    else
        return true
    end
end

function compChannel(a, b)
    if a.Channel >= b.Channel then
        return false
    else
        return true
    end
end

function InitRssiArray(RssiArray, MaxChannel)
	for i = 1, MaxChannel do
		local newObj = {}
		newObj.Channel = i
		newObj.TotalRssi = 0
		table.insert(RssiArray, newObj)
	end
end

function ComputeTotalRssi(aplist)
	if nil == aplist then
		return nil
	end

	local channel = 0
	local objs = {}

    InitRssiArray(objs, 11)
	for k,v in pairs(aplist) do
		channel = v["Channel"]
		objs[channel].TotalRssi = objs[channel].TotalRssi + tonumber(v["Rssi"]) 
	end
	return objs	
end

function ComputeWifiStatus(domain, type, channel)
	local newObj = {}
	newObj.ID = domain
	newObj.Channel = channel
	newObj.FrequencyBand = type
	newObj.Status = CONST_WIFI_STATUS_EXCELLENT
          
	local errcode, aplist = dm.GetParameterValues("InternetGatewayDevice.X_WiFi.Radio.{i}.X_EnvironmentAP.{i}.", {"Channel", "Rssi"})

	local ResultObjs = ComputeTotalRssi(aplist)
	
	if nil ~= ResultObjs then
		--print_para(ResultObjs)
		if 0 == ResultObjs[channel].TotalRssi then
			newObj.Status = CONST_WIFI_STATUS_EXCELLENT
		else
			newObj.Status = IsSignalStrong(ResultObjs, channel)
		end
    end
	return newObj
end

local wifistatus = {}

local errcode,wifiChannel = dm.GetParameterValues("InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}",
    {
        "Channel"
    }
)

--WLANConfiguration.1 present for 2.4GHz 
if nil ~= wifiChannel then
	local lp2g = ""
	for k,v in pairs(wifiChannel) do
		local newObj = {}
		lp2g = string.find(k, "WLANConfiguration.1")
		if nil ~= lp2g then
			newObj = ComputeWifiStatus(k, "2.4GHz", v["Channel"])
			table.insert(wifistatus, newObj)
		end	
	end
end

sys.print(json.encode(wifistatus))

