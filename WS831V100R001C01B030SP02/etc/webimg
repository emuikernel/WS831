local utils = require('utils')

local domain = "InternetGatewayDevice.X_VPN.L2TP_LAC."

local maps = {
	LACEnable="Enable",
	LNSAddress = "LNSAddress",
	HostName = "HostName",
	WanInterface = "WanInterface",
	TunnelPassword = "PassWord",
	KeepAliveTime = "KeepAliveTime",
	PPPUsername = "PppUser",
	PPPPassword = "PppPass",
	ConnectionMode = "ConnectionTrigger",
}

if true == utils.toboolean(web.getparaenc()) then
    local newpara = web.decodepara(data["TunnelPassword"])
    if "" ~= newpara then
        data["TunnelPassword"] = newpara
    end  
    newpara = web.decodepara(data["PPPPassword"])
    if "" ~= newpara then
        data["PPPPassword"] = newpara
    end 
end

if _G["defaultPasswd"] == data["TunnelPassword"] then
	local errcode,values = dm.GetParameterValues(domain, {"PassWord"})
	if values ~= nil then
		local obj = values[domain]
		data["TunnelPassword"] = obj["PassWord"]
	else
		data["TunnelPassword"] = ''
	end
end

if _G["defaultPasswd"] == data["PPPPassword"] then
	local errcode,values = dm.GetParameterValues(domain, {"PppPass"})
	if values ~= nil then
		local obj = values[domain]
		data["PPPPassword"] = obj["PppPass"]
	else
		data["PPPPassword"] = ''
	end
end

local param = utils.GenSetObjParamInputs(domain, data, maps)
local err,needreboot, paramerror = dm.SetParameterValues(param);
utils.responseErrorcode(err, paramerror, maps)require('sys')

local err = sys.gmsgsend("xl2tpd", 1, {Trigger=data["Trigger"]})
utils.appenderror("errcode", err)

