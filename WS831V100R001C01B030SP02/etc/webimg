local sort, strlen = table.sort, string.len

local addresstype = "All"

if FormData["type"] ~= nil then
    addresstype = FormData["type"]
end

local statustype = "All"
local active = true
if FormData["status"] ~= nil then
    statustype = FormData["status"]
    if "Inactive" == FormData["status"] then
        active = false
    end
end

local devicetype = nil
if FormData["devicetype"] ~= nil then
    devicetype = FormData["devicetype"]
end

--get active wlan device
local wlanobj = sys.gmsgget('wlancms', 2, {})
local wlanmacList = utils.split(wlanobj.macs,',')

function isActiveWlanDevice(mac)
    for k,v in pairs(wlanmacList) do
        if mac ~= "" and string.upper(v) == string.upper(mac) then
            return true
        end
    end
    return false
end
-- get qos info ---------------------------------------------------------------------------------------------
local qosclasses = {}

local errcode,policerVals = dm.GetParameterValues("InternetGatewayDevice.QueueManagement.Policer.{i}.", 
    {"X_DownPeakRate"
    ,"X_UpRatePercent"
    ,"X_RatePercent"
    ,"PeakRate"
    });

local array = {}
i = 1
for k,v in pairs(policerVals) do
    local p,e = utils.findLastPoint(k)
    array[i] = tonumber(p)
    i = i+1
end
table.sort(array)


function apendPolicerData(classval, policerIndex)
    local domain = 'InternetGatewayDevice.QueueManagement.Policer.'..array[policerIndex]..'.'
    for k,v in pairs(policerVals) do
        if k == domain then
            classval["DownPeakRate"] = v["X_DownPeakRate"]
            classval["UpPeakRate"] = v["PeakRate"]
            classval["PolicerID"] = k
            return
        end
    end
end

local errcode, classVals = dm.GetParameterValues("InternetGatewayDevice.QueueManagement.Classification.{i}.", 
    {"ClassificationEnable", "SourceMACAddress", "ClassPolicer",  "X_Type", "ClassQueue"});

for k,v in pairs(classVals) do
    if 2 == v["X_Type"] then
    local newClass = {}
        newClass["ID"] = k
        newClass["SourceMACAddress"] = v["SourceMACAddress"]
        newClass["Enable"] = utils.toboolean(v["ClassificationEnable"])
        newClass["ClassQueue"] = v["ClassQueue"]
        apendPolicerData(newClass, v["ClassPolicer"])
        table.insert(qosclasses, newClass)
    end
end

function set_qos_info(newobj, mac)
    for k, v in pairs(qosclasses) do
        if mac == v["SourceMACAddress"] then
            newobj["DeviceMaxDownLoadRate"] = v["DownPeakRate"]
            if nil ~= v["UpPeakRate"] then
                newobj["DeviceMaxUpLoadRate"] = v["UpPeakRate"]
            end
            newobj["DeviceDownRateEnable"] = v["Enable"]
            newobj["QosclassID"] = v["ID"]
            newobj["PolicerID"] = v["PolicerID"]
            newobj["ClassQueue"] = v["ClassQueue"]
            return 
        end
    end
    
       newobj["DeviceMaxDownLoadRate"] = 0
       newobj["DeviceMaxUpLoadRate"] = 0
    newobj["DeviceDownRateEnable"] = false
    newobj["QosclassID"] = ''
    newobj["PolicerID"] = ''
    newobj["ClassQueue"] = -1
end

-- get parent control info ----------------------------------------------------------------------------------
function parsemacs(srcmac)
    local start = 1
    local devices = {}
    while true do
        ip, fp = string.find(srcmac, "|", start)
        if not ip then
            local mac = {}
            mac["MACAddress"] = string.sub(srcmac, start)
            table.insert(devices, mac)
            break
        end
        local mac = {}
        mac["MACAddress"] = string.sub(srcmac, start, ip-1)
        table.insert(devices, mac)
        start = fp +1
    end

    return devices
end

local errcode, objs = dm.GetParameterValues("InternetGatewayDevice.X_FireWall.TimeRule.{i}.", {"DevMac"});
local macfilter = {}

if objs ~= nil then
    for k,v in pairs(objs) do
        local newObj = {}
        newObj["ID"] = k
        newObj["Devices"] = parsemacs(v["DevMac"])
        table.insert(macfilter, newObj)
    end
end

function set_parentControl_info(newObj, mac)
    for k, v in pairs(macfilter) do
        for k1, v1 in pairs(v["Devices"]) do
            if v1["MACAddress"] == mac then
                newObj["MacFilterID"] = v["ID"]
                newObj["ParentControlEnable"] = true
                return
            end
        end
    end
    newObj["MacFilterID"] = ""
    newObj["ParentControlEnable"] = false
    return 
end



----------------------------------------------------------------------------------------------------------

local errcode, lanVals = dm.GetParameterValues("InternetGatewayDevice.Layer2Bridging.AvailableInterface.{i}.",
    {"InterfaceReference", "X_InterfaceAlias"})

local lanarray = {}

for k, v in pairs(lanVals) do
    lanarray[v["InterfaceReference"]] = v["X_InterfaceAlias"]
end

local hosts = {}


-----------------------------------updateVendorAndHostName By vendorClassID--------------------
local g_friendlyNameList = {}

local lang = "zh"
if "zh"== lang then 
    g_friendlyNameList = json.decode('[{"Vendor":"HUAWEI-Honor","FriendlyName":"荣耀路由"},{"Vendor":"HUAWEI-WS831","FriendlyName":"荣耀路由"},{"Vendor":"HUAWEI-WS832","FriendlyName":"WS832"},{"Vendor":"HUAWEI-WS851","FriendlyName":"荣耀路由Pro"}]')
end

function updateVendorAndHostName(obj)
    if nil == obj["VendorClassID"] then 
        return
    end

    local temp = utils.split(obj["VendorClassID"], ":")
    if 3 > #(temp) then 
        return 
    end
    
    obj["VendorClassID"] = temp[2]
    local hostName = temp[1].."-"..temp[3]	
        for k,newObj in pairs(g_friendlyNameList) do
        if string.upper(newObj.Vendor) == string.upper(hostName) then
            if 4 <= #(temp) then
                obj["HostName"] = newObj.FriendlyName.."_"..temp[4]
            else
                obj["HostName"] = newObj.FriendlyName
            end
            return
        end
    end
    
    obj["HostName"] = hostName
    return       
end

local errcode, objs = dm.GetParameterValues("InternetGatewayDevice.LANDevice.{i}.Hosts.Host.{i}.", 
    {"X_DeviceName", "X_DeviceType", "HostName", "MACAddress", "IPAddress", "InterfaceType", "Active",
    "X_ParentControl",
    "ShowDeviceRealRate",
    "UpRate","DownRate",
    "Layer2Interface", "AddressSource", "LeaseTimeRemaining","VendorClassID"});

for k,v in pairs(objs) do
    local newObj = {}
        newObj["ID"] = k
        newObj["MACAddress"] = v["MACAddress"]
        newObj["IPAddress"] = v["IPAddress"]        
        newObj["Active"] = utils.toboolean(v["Active"])
        newObj["Active46"] = false
        newObj["WlanActive"] = false
        newObj["ParentControl"] = utils.toboolean(v["X_ParentControl"])

        newObj["ShowDeviceRealRate"] = v["ShowDeviceRealRate"]
        newObj["UpRate"] = v["UpRate"]
        newObj["DownRate"] = v["DownRate"]


        if utils.toboolean(v["Active"]) then
            newObj["Active46"] = true
        end        
	--if isActiveWlanDevice(v["MACAddress"]) == true then
    --        newObj["Active46"] = true
    --end
        newObj["IconType"] = v["X_DeviceType"]
        newObj["ActualName"] = v["X_DeviceName"]
        newObj["domain"] = v["Layer2Interface"]
        newObj["HostName"] = v["HostName"]
        newObj["Layer2Interface"] = lanarray[v["Layer2Interface"]]
        newObj["AddressSource"] = v["AddressSource"]
        --We must change to unsigned int, due to Permanent Lease duration, LeaseTimeRemaining = 0XFFFFFFFF - SysGetUpTime()
        newObj["LeaseTime"] = string.format("%u", v["LeaseTimeRemaining"])
        newObj["VendorClassID"] = v["VendorClassID"]
        updateVendorAndHostName(newObj)


        local errcode, wlan_objs = dm.GetParameterValues(v["Layer2Interface"]..".", 
            {"X_OperatingFrequencyBand","X_Is_Guest_Network"});

        if wlan_objs ~= nil then
            local tmp_obj = wlan_objs[v["Layer2Interface"].."."]
            newObj["Frequency"] = tmp_obj["X_OperatingFrequencyBand"]
            newObj["IsGuest"] = utils.toboolean(tmp_obj["X_Is_Guest_Network"])
        end
        set_parentControl_info(newObj, v["MACAddress"])
        set_qos_info(newObj, v["MACAddress"])
        if devicetype == "wireless" then
            if nil ~= lanarray[v["Layer2Interface"]]  then
                local start = string.find(lanarray[v["Layer2Interface"]], "SSID")
                if nil ~= start then
                        table.insert(hosts, newObj)
                end
            else
                table.insert(hosts, newObj)
            end
        elseif devicetype == "notguest" then
            if (nil == newObj["Layer2Interface"]) or ((newObj["Layer2Interface"] ~= "SSID2") and (newObj["Layer2Interface"] ~= "SSID6")
                 and (newObj["Layer2Interface"] ~= "SSID3") and (newObj["Layer2Interface"] ~= "SSID7")) then
                table.insert(hosts, newObj)
            end
        else
            if addresstype == "All" or addresstype == newObj["AddressSource"] then
                if statustype == "All" or active == newObj["Active46"] then
                    table.insert(hosts, newObj)
                end
            end
        end
end

function compID(a, b)
    if strlen(a.ID) > strlen(b.ID) then
        return false
    elseif strlen(a.ID) < strlen(b.ID) then
        return true
    end

    if a.ID >= b.ID then
        return false
    else
        return true
    end
end

function comp(a, b)
    if a.Active46 == b.Active46 then
        return compID(a, b)
    end
    
    if a.Active46 == true and b.Active46 == false then
        return true
    end

    return false
end
sort(hosts, comp)
sys.print(json.encode(hosts))
local utils = require('utils')
require('dm')

local maps = {
    ActualName="X_DeviceName",
    IconType = "X_DeviceType",
    MACAddress = "MACAddress"
}

function check_macaddr_unique(ID, MAC)
    local errcode, objs = dm.GetParameterValues("InternetGatewayDevice.LANDevice.{i}.Hosts.Host.{i}.", 
        {"MACAddress"});

    for k,v in pairs(objs) do
        if k ~= ID and MAC == v["MACAddress"] then
            return true
        end
    end

    return false
end

function create()
    -- add 
    local paras = {{"X_DeviceName", data["ActualName"]}, 
        {"X_DeviceType", data["IconType"]},
        {"IPAddress", data["IPAddress"]},
        {"MACAddress", data["MACAddress"]},
        {"Active", false}}

    if check_macaddr_unique("", data["MACAddress"]) then
        utils.appenderror("landev.mac", "landev.mac_err")
        return 9006
    end
    local err,instnum,NeedReboot,errs = dm.AddObjectWithValues("InternetGatewayDevice.LANDevice.1.Hosts.Host.", paras);
    utils.responseErrorcode(err, errs, maps)
    return err
end

function delete()
    local errcode,macfilters = dm.GetParameterValues("InternetGatewayDevice.LANDevice.1.WLANConfiguration.1.X_WlanMacFilter.{i}.",
        {
            "X_WlanSrcMacAddress",
        }
    );

    local ret_del_macfilter
    if macfilters ~= nil then
        for kmac, vmac in pairs(macfilters) do
            if vmac["X_WlanSrcMacAddress"] == data["MACAddress"] then
                dm.DeleteObject(kmac)
            end
        end
    end
	
    return dm.DeleteObject(data["ID"])
end

function update()
    local domain = data["ID"]

    if check_macaddr_unique(domain, data["MACAddress"]) then
        utils.appenderror("landev.mac", "landev.mac_err")
        return 9006
    end
    
    local paras = {{domain.."X_DeviceName", data["ActualName"]}, 
        {domain.."X_DeviceType", data["IconType"]},
        {domain.."IPAddress", data["IPAddress"]},
        {domain.."MACAddress", data["MACAddress"]}}

    local err,needreboot, paramerror = dm.SetParameterValues(paras);
    utils.responseErrorcode(err, paramerror, maps)
    return err
end

if action == 'create' then
    err = create()
elseif action == 'update' then
    err = update()
elseif action == 'delete' then
    err = delete()
    
end
utils.appenderror("errcode", err)
require('dm')
require('web')
require('json')
require('utils')
require('sys')

