function Resizewindow(){\
var height;\
if(!!(window.attachEvent && !window.opera))\
{\
height = document.documentElement.clientHeight;\
}else{\
height = window.innerHeight;\
}\
$("body").css("height",height-5);\
}\
function isMustToBeSetViewport(){\
var ua = navigator.userAgent.toLowerCase();\
if ((ua.indexOf("android") >= 0) && (ua.indexOf("mediapad") < 0) && (ua.indexOf("firefox") < 0) && (ua.indexOf("huawei_c8860e") < 0) ){\
return true;\
}\
return false;\
}\
\
function checkInit(){\
if (isMustToBeSetViewport()) {\
if (Math.abs(window.orientation) != 90) {\
adaptUILayout.adapt(720);\
} else {\
adaptUILayout.adapt(1280);\
}\
}else{\
return;\
}\
}\
\
$(window).bind("orientationchange", function( event ) {\
if(isMustToBeSetViewport()){\
if(event.orientation){\
if(event.orientation == \'portrait\'){\
adaptUILayout.adapt(720);\
}\
else if(event.orientation == \'landscape\') {\
adaptUILayout.adapt(1280);\
}\
}\
}\
});\
\
var readyOnceTime = 0;\
$(document).ready(function(){\
var ua = navigator.userAgent.toLowerCase();\
if ((ua.indexOf("mediapad") < 0) && (ua.indexOf("mqqbrowser") < 0) && (ua.indexOf("firefox") < 0) && (ua.indexOf("huawei_c8860e") < 0)){\
$(\'head\').prepend(\'<meta name="viewport" content="user-scalable=no" />\');\
}\
checkInit();\
setInterval(Resizewindow,200);\
if (0 == readyOnceTime){\
readyOnceTime = 1;\
$.mobile.page.prototype.options.domCache = true;\
');  html.setlabel()  sys.print('\
$.mobile.ajaxEnabled = false;\
if(window.location.href.indexOf("guide.html") >=0){\
Atp.GuideHeartbeatController.ontime();\
}else\
{\
if(window.location.href.indexOf("wizard") >= 0){\
Atp.HeartbeatController.bRefresh = false;\
}else{\
Atp.HeartbeatController.bRefresh = true;\
Atp.HeartbeatController.ontime();\
}\
}\
setTimeout(function(){$(".startsUgly").show();}, 200);\
}\
document.title = Em.I18n.get("Touch.page.title");\
Atp.LogOutController = Atp.SingleObjController.extend ({\
objName : \'system/user_logout\'\
});\
$(\'#logout, .logout-btn, .logout-mini\').bind("click",function(){\
var csrf = utilGetCsrf();\
Atp.LogOutController.postData(csrf,function(){\
window.location.href = "/small/html/index.html";\
});\
});\
$(\'#logout, .logout-mini\').text(Em.I18n.get(\'Touch.logout\'));\
$(\'.change-pc-bottom, .change-pc\').bind("click",function(){\
if(window.location.href.indexOf("guide.html") >=0)\
window.location.href = "/html/guide/guide.html";\
else{\
if(g_userLevel == 0){\
window.location.href = "/html/index.html";\
}else{\
window.location.href = "/html/content.html";\
}\
}\
})\
$(\'.change-pc-bottom, .change-pc\').text(Em.I18n.get(\'Touch.pcUI\'));\
});\
</script>\
\
');  html.yield('script_tag')  sys.print('\
\
</html>');
