local errcode,values= dm.GetParameterValues("InternetGatewayDevice.Services.X_DDNSConfiguration.{i}.",
        {"Enable", "Provider", "Server", "ServicePort", "Protocol", "Username", "Password", "DomainName", "HostName", "WanPath"
