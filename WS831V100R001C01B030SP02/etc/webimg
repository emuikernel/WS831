local errcode, objs = dm.GetParameterValues("InternetGatewayDevice.LANDevice.{i}.Hosts.Host.{i}.", 
    {"MACAddress", "Active", 
    "IPAddress"});

local localMac = {}
localMac.ipaddress = ipaddress
localMac.macaddress = ""
for k,v in pairs(objs) do
    if v["IPAddress"] == ipaddress and utils.toboolean(v["Active"]) == true then
            localMac.macaddress = v["MACAddress"]
        break;
    end
end

sys.print(json.encode(localMac))
