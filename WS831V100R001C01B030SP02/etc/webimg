    local wifi_Fre = v["X_OperatingFrequencyBand"]
    if "2.4GHz" == wifi_Fre then
        wlan.ID = k
        wlan.enable = utils.toboolean(v["CanUserForCover"])
        if true == wlan.enable then 
        	break
        end
    end
end 

