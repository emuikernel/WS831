print(json.encode(HostInfos))

------------------4.更新根节点Device下的Topo图结构
print("===================4========================")
for key, Device in pairs(HostInfos) do
	-------parse the devcie
	if Device.HiLinkType == "Device" then
		FilterDevice(Device)
	end
end
