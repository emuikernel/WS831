local tostring = tostring
local tonumber = tonumber

if true == utils.toboolean(web.getparaenc()) then
    if nil ~= data["WpaPreSharedKey"] then
        local pass = web.decodepara(data["WpaPreSharedKey"])
        if nil ~= pass then
            data["WpaPreSharedKey"] = pass
        end
    end
    if nil ~= data["CurrentWepKey"] then
        local pass = web.decodepara(data["CurrentWepKey"])
        if nil ~= pass then
            data["CurrentWepKey"] = pass
        end
    end
end
    
data["HideBroadcast"] = utils.opsiteBoolVal(data["HideBroadcast"])

local ssid_map = {
    Ssid = "SSID",
    Enable = "Enable",
    SecMode = "BeaconType",
    BasicAuthMode = "BasicAuthenticationMode",
    WepEncLevel = "WEPEncryptionLevel",
    WepKeyIndex = "WEPKeyIndex",

    WpaEncModes = "WPAEncryptionModes",
    WpaIEEEEncModes = "IEEE11iEncryptionModes",
    WpaMixEncModes = "X_MixedEncryptionModes",

    HideBroadcast = "SSIDAdvertisementEnabled",
    IsolateControl = "X_WlanIsolateControl"
}

if data["SecMode"] == "None" then
    data["SecMode"] = "Basic"
    data["Basicencmode"] = "None"
    ssid_map["Basicencmode"] = "BasicEncryptionModes"
else 
    if data["SecMode"] == "Basic" then
        data["Basicencmode"] = "WEPEncryption"
        ssid_map["Basicencmode"] = "BasicEncryptionModes"
    end
end

local param = utils.GenSetObjParamInputs(data["ID"], data, ssid_map)

-- set wep key according to wep key index.
if data["CurrentWepKey"] and _G["defaultPasswd"] ~= data["CurrentWepKey"] then
    local wepKey = {}
    wepKey["key"] = data["ID"].."WEPKey."..data["WepKeyIndex"]..".WEPKey"
    wepKey["value"] = data["CurrentWepKey"]
    table.insert(param, wepKey)
end

-- set wpa pre shared key.
if data["WpaPreSharedKey"] and _G["defaultPasswd"] ~= data["WpaPreSharedKey"] then
    local wpa_preshared_key = {}
    wpa_preshared_key["key"] = data["ID"].."PreSharedKey.1.KeyPassphrase"
    wpa_preshared_key["value"] = data["WpaPreSharedKey"]
    table.insert(param, wpa_preshared_key)
end

local err,needreboot, paramerror = dm.SetParameterValues(param);
utils.responseErrorcode(err, paramerror, ssid_map)require('dm')
