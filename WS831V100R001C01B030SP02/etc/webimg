require('utils')
require('dm')

local paras = {}
function add_one_parameter(paras, name, value)
    if nil == value then 
        return
    end
    table.insert(paras, {name, value})
end

for k,v in pairs(data) do 
    add_one_parameter(paras,v["ID"].."X_DeviceName",v["ActualName"])
end

print("==== device name submit data=============")
for k,v in pairs(paras) do
    print("=============================")
    for idx,val in pairs(v) do
        print(val)
    end
end

local err,needreboot, paramerror = dm.SetParameterValues(paras);
utils.appenderror("errcode", err)
