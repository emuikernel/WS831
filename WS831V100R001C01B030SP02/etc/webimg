    <script type="text/javascript" language="javascript" src="/lib/cat_jquery.js.jgz"></script>
    <script type="text/javascript" language="javascript" src="/js/detectmobilebrowser.js"></script>
    <script type="text/javascript" language="javascript" src="/js/redirectutil.js"></script>
    <script type="text/javascript">
        <!-- fake for iphone internet detect -->
        <!-- <HTML><HEAD><TITLE>Success</TITLE></HEAD><BODY>Success</BODY></HTML> -->
        var isToguide = isRestoreGuide();
        var wlanModel = getWlanModel();
        var firstLogin = isFirstLogin();
        function isRepeater(){
            if((2 == wlanModel) || (3 == wlanModel)){
                return true;
            }
            return false;
        }
        $(document).ready(function() {
            if(typeof isToSmallSystem != "undefined"){
                if(isToSmallSystem()){
                    window.location.href = "/html/smallsystem.html";
                    return ;
                }
            }
            if(isToguide == false){
                if ($.browser.mobile){
                    window.location.href = "/small/html/guide.html";
                }else{
                    window.location.href = "/html/guide/guide.html";
                }
            }else{ 
                if ($.browser.mobile && (false == isRepeater())){
                    if(0 == firstLogin){
                        window.location.href = "/small/html/index.html";
                    }else{
                        window.location.href = "/small/html/wizard.html";
                    }
                }else{
                    if((true == isRepeater()) || (0 == firstLogin)){
                        window.location.href = "/html/index.html";
                    }else{
                        window.location.href = "/html/pub/redirect.html";
                    }
                }
            }
        });
    </script>
    </body>
</html>
