local domain = ""

local errcode, profile_values = dm.GetParameterValues("InternetGatewayDevice.LANDevice.1.WLANEndPoint.1.Profile.{i}.",
    {"SSID","BSSID"})


local flag = 1

if nil ~= profile_values then
    flag = 0
    for k, v in pairs(profile_values) do
	    print(k)
        domain = k
        flag = 1
    end
else
    flag = 0
end

local errcode, NeedReboot, paramerr, instnum
if nil ~= profile_values and 1 == flag then
	-- update
    domain = "InternetGatewayDevice.LANDevice.1.WLANEndPoint.1.Profile.1."
	local paras = utils.GenSetObjParamInputs(domain, data, maps)
    errcode, NeedReboot, paramerr = dm.SetParameterValues(paras)
    utils.responseErrorcode(errcode, paramerr, maps)
    print("update" ,errcode)
else
	-- Add
    domain = "InternetGatewayDevice.LANDevice.1.WLANEndPoint.1.Profile."
    local paras = utils.GenAddObjParamInputs(data, maps)
    errcode, instnum, NeedReboot, paramerr= dm.AddObjectWithValues(domain, paras);
    print("addd" ,errcode)
 end

if 0 ~= errcode then
    print("set repeater profile error", errcode)
    utils.responseErrorcode(errcode, paramerr, maps)
else
    domain = "InternetGatewayDevice.LANDevice.1.WLANEndPoint.1."
	local param = {{domain.."ConnectType", data["ConnectType"]}}
    local err,needreboot, paramerror = dm.SetParameterValues(param)
    print("set wlan ConnectType", err)
    utils.responseErrorcode(err, paramerror, typemaps)
end       require('dm')
