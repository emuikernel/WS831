local aplist = {}
if errcode == 0 then
    local eapcode, environment = dm.GetParameterValues("InternetGatewayDevice.X_WiFi.Radio.1.X_EnvironmentAP.{i}.", {"SSID","BSSID","Channel","Rssi"})
    for k,v in pairs(environment) do
        local aplistitem = {}
        aplistitem["ID"] = k
        aplistitem["SSID"] = v["SSID"]
        aplistitem["BSSID"] = v["BSSID"]
        aplistitem["Rssi"] = getNumber(v["Rssi"])
        aplistitem["IconPath"] = Iconpath(v["Rssi"])
        aplistitem["Channel"] = v["Channel"]
        insert(aplist, aplistitem)
    end
    sort(aplist, compID)
end

