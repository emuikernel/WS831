local tostring = tostring


local errcode,wanconndevs= dm.GetParameterValues("InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.",
        {"WANPPPConnectionNumberOfEntries"})

local errcode,vlans = dm.GetParameterValues("InternetGatewayDevice.X_VLANTermination.{i}.", 
            {"LowerLayers", "VLANID", "802-1pMark"});

local errcode,accessdevs= dm.GetParameterValues("InternetGatewayDevice.WANDevice.{i}.WANCommonInterfaceConfig.",
        {"WANAccessType", "PhysicalLinkStatus"})


local errcode, atmlink = dm.GetParameterValues("InternetGatewayDevice.WANDevice.1.WANConnectionDevice.{i}.WANDSLLinkConfig.",
            {"LinkStatus"})
local errcode, ptmlink = dm.GetParameterValues("InternetGatewayDevice.WANDevice.2.WANConnectionDevice.{i}.WANDSLLinkConfig.",
            {"LinkStatus"})

local errcode,pppCon = dm.GetParameterValues("InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANPPPConnection.{i}.", 
    {
    "ConnectionType", "Enable",
    "X_ServiceList", "X_LowerLayers","ConnectionStatus","X_Default"});

local errcode,ipCon = dm.GetParameterValues("InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANIPConnection.{i}.", 
    {
    "ConnectionType", "X_ServiceList", "X_LowerLayers","Enable", 
    "AddressingType", "ConnectionStatus","X_Default"});

function get_lower_layer(ID, lowerlayer)
    if nil == lowerlayer or "" == lowerlayer then
        local start = string.find(ID, ".WANPPPConnection.")
        if not start then
            start = string.find(ID, ".WANIPConnection.")
        end
        return string.sub(ID, 0, start)
    end

    return lowerlayer.."."
end

-- get currently active default wan

local activeDftWan = ""
activeDftWan = utils.getDefaultWan(pppCon, ipCon, accessdevs)

--[[
]]
function get_str_before(str, symbol)
    if str == nil then
        return nil
    end
    local pos = string.find(str, symbol)
    if pos then
        return string.sub(str, 0, pos-1)
    end
    return str
end

--get link info
function get_access_type_by_wanconndev(wanconndev, accessdevs)
    local start = string.find(wanconndev, ".WANConnectionDevice.")
    local wandev = string.sub(wanconndev, 1, start)
    wandev = wandev.."WANCommonInterfaceConfig."
    for k,v in pairs(accessdevs) do
        if wandev == k then
            return v["WANAccessType"]
        end
    end
    return ""
end

function get_wandev_by_link_intf(intf)
    local start = string.find(intf, ".WANCommonInterfaceConfig.")
    return string.sub(intf, 1, start)
end

function get_dsl_params_by_wanconndev(wanconndev, link, accesstype)
    local getValues = nil;
    local linkconfig = ""
    if "DSL" == accesstype then
        local errcode
        linkconfig = get_wandev_by_link_intf(wanconndev).."WANDSLLinkConfig."
        errcode,getValues= dm.GetParameterValues(linkconfig,{"LinkType", "DestinationAddress"})
    end
    link.AccessType = accesstype
    if nil == getValues then
        link.PVC = ""
        link.LinkType = "EoA"
        return
    end
    local obj = getValues[linkconfig]
    link.PVC = obj["DestinationAddress"]
    link.LinkType = obj["LinkType"]
end

local links = {}
for k,v in pairs(wanconndevs) do
    local link = {}
    link.LowerLayer = ""
    link.ID = k
    get_dsl_params_by_wanconndev(k, link, get_access_type_by_wanconndev(k, accessdevs))
    link.VLANId = 1
    link.VLAN1p = 0
    table.insert(links, link)
end

for k,v in pairs(vlans) do
    local link = {}
    link.ID = k
    link.LowerLayer = v["LowerLayers"].."."
    get_dsl_params_by_wanconndev(link.LowerLayer, link, get_access_type_by_wanconndev(link.LowerLayer, accessdevs))
    link.VLANId = v["VLANID"]
    link.VLAN1p = v["802-1pMark"]
    table.insert(links, link)
end

--get link info over
--find current wan link
function getCurrentWanLinkData(currentwanLowerLayer,linklist,con)
    if nil ~= currentwanLowerLayer then
        for k,v in pairs(linklist) do
            if currentwanLowerLayer == v["ID"] then
                local currentlink = {}
                currentlink.PVC = v["PVC"]
                currentlink.ID = v["ID"]
                currentlink.VLANId = v["VLANId"]
                currentlink.VLAN1p = v["VLAN1p"]
                currentlink.LinkType = v["LinkType"]
                con.linkdata = currentlink
                return ;
            end
        end
    end
end

for k,v in pairs(pppCon) do
    local con = {}
    con.ID = k
    utils.fill_access_info_by_ID(k, con, accessdevs);
    if con.AccessType == "DSL" then
        utils.fill_xdls_linkstatus_by_ID(k, con, atmlink)
    elseif con.AccessType == "VDSL" then
        utils.fill_xdls_linkstatus_by_ID(k, con, ptmlink)
    end
    con.ConnectionType = utils.get_ip_conn_type(k, v["ConnectionType"])
    con.LowerLayer = get_lower_layer(k, v["X_LowerLayers"])
    cur_user,level = web.getuserinfo()
    con.Enable = utils.toboolean(v["Enable"])
    con.ConnectionStatus = v["ConnectionStatus"]
    con.ServiceList = v["X_ServiceList"]
    if ("" ~= activeDftWan and k == activeDftWan) then
        getCurrentWanLinkData(con.LowerLayer,links,con)
        sys.print(json.encode(con))
        return
    end
end

for k,v in pairs(ipCon) do
    local con = {}
    con.ID = k
    utils.fill_access_info_by_ID(k, con, accessdevs);
    if con.AccessType == "DSL" then
        utils.fill_xdls_linkstatus_by_ID(k, con, atmlink)
    elseif con.AccessType == "VDSL" then
        utils.fill_xdls_linkstatus_by_ID(k, con, ptmlink)
    end
    con.Enable = utils.toboolean(v["Enable"])

    con.ConnectionType = utils.get_ip_conn_type(k, v["ConnectionType"])
    con.LowerLayer = get_lower_layer(k, v["X_LowerLayers"])
    con.ConnectionStatus = v["ConnectionStatus"]
    con.APN = ""
    con.DialNum = ""
    con.ServiceList = v["X_ServiceList"]
    con.IPv4AddrType = v["AddressingType"]
    if ("" ~= activeDftWan and k == activeDftWan) then
        getCurrentWanLinkData(con.LowerLayer,links,con)
        sys.print(json.encode(con))
        return
    end
endrequire('dm')
