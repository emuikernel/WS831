--lua_content_for__ script_tag begin
sys.print('\r\
<script type="text/javascript">\r\
$(function(){\r\
"use strict";\r\
function parseBoolean(str){\r\
if (str == "true"){\r\
return true;\r\
}\r\
return false;\r\
}\r\
\r\
Atp.HeartbeatController.bRefresh = false;\r\
$("#submit").prev(\'span\').find(\'span.ui-btn-text\').text(Em.I18n.get(\'Touch.login.login\'));\r\
\r\
Atp.UserLoginController = Atp.SingleObjController.extend({\r\
objName: "system/user_login",\r\
retryTimes: 0,\r\
\r\
errCsrfProc: function(result){\r\
\r\
},\r\
\r\
createSubmitData: function(action, data){\r\
var post_data = {\r\
"csrf": this.getCsrf(),\r\
"action": action,\r\
"data": data\r\
};\r\
return $.toJSON(post_data);\r\
},\r\
\r\
login: function(data){\r\
var self = this;\r\
if (self.retryTimes > 5){\r\
self.retryTimes = 0;\r\
$("#login_warning").html(Em.I18n.get("Touch.login.notice"));\r\
$("#first_login").show();\r\
return;\r\
}\r\
self.retryTimes = self.retryTimes + 1;\r\
this.postData(data, function(result){\r\
if (result.errcode == 1){\r\
Atp.UserLoginController.updateCsrf(result);\r\
self.login(data);\r\
}\r\
else{\r\
self.retryTimes = 0;\r\
if ("ok" == result[\'errorCategory\']){\r\
window.location.href = "/small/html/home.html#home_page";\r\
}\r\
else{\r\
var fail_wait_time = result["WaitSecond"];\r\
var fail_lock_time = 3;\r\
$("#Password").val("");\r\
if(result[\'errorCategory\'] == "user_pass_err"){\r\
$("#login_warning").html(Em.I18n.get("Touch.login." + result[\'errorCategory\'], {arg1: fail_lock_time-result["count"]}));\r\
}else{\r\
$("#login_warning").html(Em.I18n.get("Touch.login." + result[\'errorCategory\'], {arg1: fail_wait_time/60}));\r\
}\r\
$("#first_login").show();\r\
}\r\
}\r\
});\r\
}\r\
});\r\
\r\
Atp.UserCountController = Atp.MultiObjController.extend({\r\
objName: "system/useraccount",\r\
ADMIN_LEVEL: 2,\r\
\r\
getLoginUserName: function(){\r\
for (var i = 0; i < this.content.length; i++){\r\
if (this.content[i].userlevel == this.ADMIN_LEVEL){\r\
return this.content[i].username;\r\
}\r\
}\r\
return "admin";\r\
},\r\
\r\
afterLoad: function(){\r\
if (this.content[0].FirstLogin == 0){\r\
$("#login_warning").html(Em.I18n.get("Touch.login.defaultpwd"));\r\
$("#Password").val("admin");\r\
$("#first_login").show();\r\
}\r\
if(true == this.content[0].LoginWifiKeySame){\r\
$(".login_wifi_samepass_info").show();\r\
}else{\r\
$(".login_wifi_samepass_info").hide();\r\
}\r\
}\r\
});\r\
Atp.UserCountController.load();\r\
showPrivatePage();\r\
});\r\
\r\
$(\'form\').on(\'submit\', function (e) {\r\
var $this = $(this);\r\
e.preventDefault();\r\
$("#login_warning").html("");\r\
$("#first_login").hide();\r\
var data = $this.serializeObject();\r\
data.UserName = Atp.UserCountController.getLoginUserName();\r\
var csrf_obj = utilGetCsrf();\r\
var plaintPwd = data.UserName + base64Encode(SHA256(data.Password)) + csrf_obj.csrf_param +csrf_obj.csrf_token;\r\
data.Password = SHA256(plaintPwd);\r\
data.LoginFlag = 1;\r\
if(Atp.UserCountController.content[0].FirstLogin == 0){\r\
data.SetFirst = 1;\r\
}\r\
Atp.UserLoginController.login(data);\r\
});\r\
\r\
function showPrivatePage(){\r\
var lang = "en";\r\
switch(g_userLang){\r\
case "ar":\r\
lang = "ae";\r\
break;\r\
case "es-rUS":\r\
lang = "sla";\r\
break;\r\
case "ja":\r\
lang = "jp";\r\
break;\r\
case "pt-rPT":\r\
lang = "pt";\r\
break;\r\
case "zh":\r\
lang = "cn";\r\
break;\r\
case "zh-TW":\r\
lang = "tw";\r\
break;\r\
default:\r\
lang = g_userLang;\r\
break;\r\
}\r\
var path = "http://consumer.huawei.com/minisite/worldwide/privacy-policy/"+lang+"/index.htm?languagetag="+lang;\r\
$("#go_private_page").attr("href",path);\r\
}\r\
\r\
</script>\r\
');
