--print(json.encode(data))

function parseURI(api)
		local indexStart, indexEnd = string.find(api , '?');
		if nil == indexStart or nil == indexEnd then 
			return api
		end

		local path = string.sub(api, 1, indexStart-1)
		local param = string.sub(api, indexEnd+1, -1)
    	local kv = utils.split(param, '=');
    	_G["FormData"][kv[1]] = kv[2]
		return path
end

function doGetProc(api)
	local apicontent = "";
	local path = parseURI(api);
	if "cloudc" == cfgtool then
		--print("===cloudc======"..path)
		require("cloudc")
		apicontent = cloudc.getapicontent("/html"..path..".json.lua")
	else
		--print("===web======"..path)
		require("web")
		apicontent = web.getrawfile("/html"..path..".json.lua")
	end
	--print(api.."======================================start=======================================")
	
	if apicontent ~= nil then
		--print(apicontent)
		local prog = loadstring(apicontent)
		if prog then
			sys.print("{\"api\":\""..api.."\",")
			sys.print("\"response\":")
			prog()
			sys.print("}")
			return false
		else
			print(api.." format error.")
		end
	end
	--print(api.."======================================end=======================================")
	return true
end

--print("===================")

if "cloudc" == cfgtool then
	_G["FormData"]["cfgtool"] = "cloudc"
else 
	_G["FormData"]["cfgtool"] = "web"
end 
if nil ~= data and nil ~= data.RequestList then
	--print(json.encode(data.RequestList))
	local requestList = data.RequestList;
	local isFirst = true;

	sys.print("{\"data\":[")

	for k,obj in  pairs(requestList) do
	   -- print(k.."value:"..obj.api)
		if false == isFirst then
			sys.print(", ");
		end

	    isFirst = doGetProc(obj.api)

	    --print(obj.api.. "process ok".."    isFirst:")
	    --print(isFirst)
	end
	sys.print("]")
	sys.print(", \"errcode\":0}")
end 



