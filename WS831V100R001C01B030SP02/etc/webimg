var Ember = {};
var Em = Ember;
var Atp = {};
var CLDR = {
defaultLanguage: "en"
};
if (!window.console || !console.firebug){
var names = ["log", "debug", "info", "warn", "error", "assert", "dir", "dirxml", "group", "groupEnd", "time", "timeEnd", "count", "trace", "profile", "profileEnd"];
window.console = {};
for (var i = 0; i < names.length; ++i)
window.console[names[i]] = function() {}
}
var I18n = {
translations: {},
addTranslations: function (input) {
for (var key in input)
{
this.translations[key] = input[key];
}
},
t: function(key, attrs){
return this.get(key, attrs);
},
get: function(key, attrs){
if( !key || typeof( key ) !== "string" ) return;
var keyStr = this.translations[key];
if (null == keyStr){
return "Missing translation: " + key;
}
keyStr = this.findEmbededAttrs(keyStr, attrs);
return keyStr;
},
findEmbededAttrs: function(keyStr, attrs){
var self = this;
var parts = keyStr.split("{{");
if (0 == parts.length) {
return [keyStr, false];
}
var newKeyStr = "";
$.each(parts, function (i, part) {
var endPos = part.indexOf("}}");
if (-1 == endPos) {
newKeyStr += part;
} else {
var newKey = part.substring(0, endPos);
if (-1 != newKey.indexOf(".")) {
newKeyStr += self.get(newKey);
} else {
newKeyStr += attrs[newKey];
}
newKeyStr += part.substring(endPos+2);
}
});
return newKeyStr;
}
};
Ember.I18n = I18n;
$.fn.serializeObject = function()
{
var o = {};
var a = this.serializeArray();
$.each(a, function() {
if (o[this.name]) {
if (!o[this.name].push) {
o[this.name] = [o[this.name]];
}
o[this.name].push(this.value || '');
} else {
o[this.name] = this.value || '';
}
});
return o;
};
function utilGetTimeString(timeVal) {
var timeString = "";
var tmpVal = 0;
if(typeof timeVal == "undefined"){
return timeString;
}
tmpVal = parseInt(timeVal / (60*60*24));
timeString += parseInt(tmpVal) + " " + Em.I18n.get('Touch.day', {count: tmpVal})+ " ";
timeVal %= 60*60*24;
tmpVal =parseInt(timeVal / (60*60));
timeString += parseInt(tmpVal) + " " + Em.I18n.get('Touch.hour', {count: tmpVal})+ " ";
timeVal %= 60*60;
tmpVal = parseInt(timeVal / (60));
timeString += parseInt(tmpVal) + " "+ Em.I18n.get('Touch.minute', {count: tmpVal}) + " ";
return timeString;
}
var g_csrf_obj = null;
function utilUpdateCsrf(param, token) {
if (typeof(token) === 'undefined'){
return;
}
g_csrf_obj = {};
g_csrf_obj.csrf_param = param;
g_csrf_obj.csrf_token = token;
}
function utilGetCsrf() {
if (g_csrf_obj){
return g_csrf_obj;
}
var csrf_obj = {};
var metas = document.getElementsByTagName("meta");
var m;
for(m = 0 ; m < metas.length; m++) {
if (metas[m].getAttribute('name') === 'csrf_param') {
csrf_obj.csrf_param = metas[m].getAttribute('content');
break;
}
}
for(m = 0 ; m < metas.length; m++){
if (metas[m].getAttribute('name') === 'csrf_token') {
csrf_obj.csrf_token = metas[m].getAttribute('content');
break;
}
}
return csrf_obj;
}
$(function(){
var COMMON_JSON_PATH='/api/';
Atp.Object = {};
function cloneObject(inObj){
var newObj = (inObj instanceof Array) ? [] : {};
for (var i in inObj) {
if (inObj[i] && typeof inObj[i] == "object") {
newObj[i] = cloneObject(inObj[i]);
}
else
{
newObj[i] = inObj[i];
}
}
return newObj;
}
function setProperties(dest,source){
for (var property in source){
dest[property] = source[property];
}
}
function  _utf8_encode(string) {
string = string.replace(/\r\n/g,"\n");
var utftext = "";
for (var n = 0; n < string.length; n++) {
var c = string.charCodeAt(n);
if (c < 128) {
utftext += String.fromCharCode(c);
}
else if((c > 127) && (c < 2048)) {
utftext += String.fromCharCode((c >> 6) | 192);
utftext += String.fromCharCode((c & 63) | 128);
}
else {
utftext += String.fromCharCode((c >> 12) | 224);
utftext += String.fromCharCode(((c >> 6) & 63) | 128);
utftext += String.fromCharCode((c & 63) | 128);
}
}
return utftext;
}
function base64_encode (input) {
_keyStr = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/=";
var output = "";
var chr1, chr2, chr3, enc1, enc2, enc3, enc4;
var i = 0;
input = _utf8_encode(input);
while (i < input.length) {
chr1 = input.charCodeAt(i++);
chr2 = input.charCodeAt(i++);
chr3 = input.charCodeAt(i++);
enc1 = chr1 >> 2;
enc2 = ((chr1 & 3) << 4) | (chr2 >> 4);
enc3 = ((chr2 & 15) << 2) | (chr3 >> 6);
enc4 = chr3 & 63;
if (isNaN(chr2)) {
enc3 = enc4 = 64;
} else if (isNaN(chr3)) {
enc4 = 64;
}
output = output +
this._keyStr.charAt(enc1) + this._keyStr.charAt(enc2) +
this._keyStr.charAt(enc3) + this._keyStr.charAt(enc4);
}
return output;
}
Atp.Object.extend = function(source){
var temp = cloneObject(this);
temp._super = this;
for (var property in source){
temp[property] = source[property];
}
return temp;
};
Atp.ObjController = Atp.Object.extend({
objName: '',
contentType:"application/json;charset=UTF-8",
enc: function(encstring) {
var metas = document.getElementsByTagName("meta");
var rasn = ''
var rsae = ''
for(m = 0 ; m < metas.length; m++) {
if (metas[m].getAttribute('name') === 'n') {
rasn = metas[m].getAttribute('content');
}
if (metas[m].getAttribute('name') === 'e') {
rase = metas[m].getAttribute('content');
}
}
var rsa = new RSAKey();
rsa.setPublic(rasn, rase);
var arr = base64_encode(encstring);
var num = arr.length / 245;
var restotal = '';
for (i = 0; i < num; i++)
{
var encdata = arr.substr(i * 245, 245);
var res = rsa.encrypt(encdata);
if(res.length!=rasn.length)
{
i--;
continue;
}
restotal += res;
}
return restotal;
},
encdata: function(mydata) {
if(this.get("contentType").indexOf("enc") < 0){
return mydata;
}
return this.enc(mydata);
},
updateCsrf: function(result){
if (typeof(result) == 'undefined' || typeof(result.csrf_token) == 'undefined'){
return ;
}
utilUpdateCsrf(result.csrf_param,result.csrf_token);
},
getCsrf: function(){
return utilGetCsrf();
},
errCsrfProc: function(result){},
ajaxGetJson: function(path, successProc, errorProc, bAsync){
var isAsyncLoad = true;
if (typeof(bAsync) != 'undefined'){
isAsyncLoad = bAsync;
}
$.ajax({
url: path,
dataType: 'json',
async: isAsyncLoad,
dataFilter: function(data, type){
var i = data.indexOf('while(1); /*')
if (i == 0){
data = data.substr('while(1); /*'.length);
}
i = data.lastIndexOf('*/');
if (i + 2 == data.length){
data = data.substr(0, i);
}
return data;
},
success: function(data) {
successProc(data);
},
error:function(err, textStatus){
if (errorProc){
errorProc(err, textStatus);
}
}
});
},
createSubmitData: function(action, data){
var v, json = {}
for (var key in data){
if (data.hasOwnProperty(key)){
v = data[key];
if (typeof(v) === "function"){
continue;
}
json[key] = v;
}
}
var post_data = {
"csrf": this.getCsrf(),
"action": action,
"data": json
};
return $.toJSON(post_data);
},
ajaxPostJson: function(path, action, data, callback, errproc){
var self = this;
jQuery.ajax({
url: path,
type: "POST",
data: self.createSubmitData(action, data),
dataType: "json",
contentType:self.contentType,
dataFilter: function(data, type){
var i = data.indexOf('while(1); /*')
if (i == 0){
data = data.substr('while(1); /*'.length);
}
i = data.lastIndexOf('*/');
if (i + 2 == data.length){
data = data.substr(0, i);
}
return data;
},
success: function(result) {
if (null == result){
return;
}
if (1 == result.errcode){
self.errCsrfProc(result);
}
else
{
self.updateCsrf(result);
}
if (callback) {
callback(result);
}
},
error: function(er) {
if (errproc) {
errproc(er);
}
}
});
},
submitItem: function(action, data, callback){
var path = COMMON_JSON_PATH + this.objName;
this.ajaxPostJson(path, action, data, callback);
}
});
Atp.SingleObjController = Atp.ObjController.extend({
content: {},
afterLoad: function(){},
load: function(callback){
var self = this;
var path = COMMON_JSON_PATH + this.objName;
this.ajaxGetJson(
path,
function(data){
if (null == data){
return;
}
setProperties(self.content,data);
if (self.afterLoad){
self.afterLoad();
}
if (callback) {
callback(data, self);
}
},
function(err, textStatus){
if (self.loadErrProc){
self.loadErrProc(err, textStatus);
}
}
);
},
postData: function(data, callback){
this.submitItem("update", data, callback);
}
});
Atp.MultiObjController = Atp.ObjController.extend({
content: [],
afterLoad: function(){},
load: function(callback){
var self = this;
var path = COMMON_JSON_PATH + this.objName;
this.ajaxGetJson(
path,
function(data){
var i;
if (null == data){
return;
}
self.content=[];
for (i = 0; i < data.length; i++){
self.content[i] = {};
setProperties(self.content[i],data[i]);
}
if (self.afterLoad){
self.afterLoad();
}
if (callback) {
callback(data, self);
}
},
function(err, textStatus){
if (self.loadErrProc){
self.loadErrProc(err, textStatus);
}
}
);
},
updateItem: function(data, callback){
this.submitItem("update", data, callback);
},
addItem: function(data, callback){
this.submitItem("create", data, callback);
},
deleteItem: function(data, callback){
this.submitItem("delete", data, callback);
}
});
Atp.HeartbeatController = Atp.SingleObjController.extend({
objName: 'system/heartbeat',
bRefresh: true,
isNotLoginPage: function(){
return (window.location.pathname != "/small/html/index.html");
},
redirect2Login: function(){
if (this.isNotLoginPage()){
window.location.href = "/small/html/index.html";
}
},
loadErrProc: function(err, textStatus){
var nu =navigator.userAgent.toLowerCase();
if ("error" == textStatus){
if($.browser.opera)
{
this.redirect2Login();
}else if(nu.indexOf("mqqbrowser") >= 0){
this.redirect2Login();
}
else if(err.status == 404){
this.redirect2Login();
}
}
else{}
},
ontime: function(interval){
var t = 5000;
if (interval){
t = interval;
}
var self = this;
setInterval(function(){
self.refresh();
}, t);
},
refresh: function(){
if (this.bRefresh && this.isNotLoginPage()){
var r = Math.random();
this.objName ='system/heartbeat?r=' + r;
this.load();
}
}
});
Atp.GuideHeartbeatController = Atp.SingleObjController.extend({
objName: 'system/guideheartbeat',
ontime: function(interval){
var t = 5000;
if (interval){
t = interval;
}
var self = this;
setInterval(function(){
self.refresh();
}, t);
},
refresh: function(){
console.log("Atp.HeartbeatController refresh");
var r = Math.random();
this.objName ='system/guideheartbeat?r=' + r;
this.load();
}
});
});
}
