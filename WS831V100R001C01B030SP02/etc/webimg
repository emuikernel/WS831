Atp.WanWizardController.load(function(){
Atp.WanSetupDetectController.load(function(){
submit_auto();
});
});
},100);
});
var aimpage = "";
function setCheckBoxSetValue(id, val){
if (typeof(id) === "undefined"){
return;
}
var currentcheck = $("#"+id).attr("src");
if ("true" == val){
if(currentcheck.indexOf("checked")>=0){
return ;
}
$("#"+id).attr("src","../res/checked.png");
}
else{
if(currentcheck.indexOf("uncheck")>=0){
return ;
}
$("#"+id).attr("src","../res/uncheck.png");
}
}
function show_connecting_page(){
console.log("In show_connecting_page...");
utilchangePage("wan_wizard_page_connecting");
}
$(function(){
"use strict";
Atp.UserCountController = Atp.MultiObjController.extend({
objName: "system/useraccount",
ADMIN_LEVEL: 2,
getLoginUserName: function(){
for (var i = 0; i < this.content.length; i++){
console.log(this.content[i]);
if (this.content[i].userlevel == this.ADMIN_LEVEL){
return this.content[i].username;
}
}
return "admin";
},
afterLoad:function(){
if(true == this.content[0].LoginWifiKeySame){
$(".login_wifi_samepass_info").show();
}else{
$(".login_wifi_samepass_info").hide();
}
}
});
Atp.UserLoginController = Atp.SingleObjController.extend({
objName: "system/user_login",
retryTimes: 0,
errCsrfProc: function(result){
},
createSubmitData: function(action, data){
var post_data = {
"csrf": this.getCsrf(),
"action": action,
"data": data
};
return $.toJSON(post_data);
},
login: function(data){
console.log("UserLoginController login.....");
var self = this;
if (self.retryTimes > 5){
self.retryTimes = 0;
$.mobile.hidePageLoadingMsg();
$("#login_warning").html(Em.I18n.get("Touch.login.notice"));
$("#first_login").show();
return;
}
self.retryTimes = self.retryTimes + 1;
this.postData(data, function(result){
if (result.errcode == 1){
Atp.UserLoginController.updateCsrf(result);
self.login(data);
}
else{
$.mobile.hidePageLoadingMsg();
console.log("login result=",result,aimpage);
self.retryTimes = 0;
if ("ok" == result['errorCategory']){
Atp.WanWizardController.objName = "ntwk/wan?type=active";
g_userLevel = 2;
console.log("WanWizardController getCsrf",Atp.WanWizardController.getCsrf());
Atp.HeartbeatController.ontime();
if(aimpage == "wan_wizard_page_connecting"){
console.log("login to aimpage",aimpage);
DiagnoseAgain();
}else{
Atp.WanWizardController.load(function(result){
console.log("result====",result);
console.log("aimpage",aimpage);
if(aimpage == "wan_wizard_page_connecting"){
console.log("login to aimpage",aimpage);
DiagnoseAgain();
}else if(aimpage.indexOf("small") >= 0){
window.location.href = aimpage;
}else if(aimpage == "auto"){
submit_auto();
}else if(aimpage == "pppoe"){
submitpppoe();
}else{
window.location.href = "/small/html/home.html#home_page";
}
});
}
}
else{
var fail_wait_time = result["WaitSecond"];
var fail_lock_time = 3;
$("#Password").val("");
if(result['errorCategory'] == "user_pass_err"){
$("#login_warning").html(Em.I18n.get("Touch.login." + result['errorCategory'], {arg1: fail_lock_time-result["count"]}));
}else{
$("#login_warning").html(Em.I18n.get("Touch.login." + result['errorCategory'], {arg1: fail_wait_time/60}));
}
$("#first_login").show();
}
}
});
}
});
Atp.CurrentMacShowController = Atp.SingleObjController.extend({
objName: 'ntwk/getmacaddress'
});
Atp.WanWizardController = Atp.SingleObjController.extend({
objName: "ntwk/swan",
contentType:"application/json;charset=UTF-8;enp",
submit: function(data, callback){
console.log("WanWizardController.content=",Atp.WanWizardController.content);
if(Atp.WanWizardController.objName == "ntwk/swan"){
Atp.WanWizardController.objName = "ntwk/wan?type=active";
var self = this;
Atp.WanWizardController.load(function(){
var content = Atp.WanWizardController.content;
content.linkdata = data.linkdata;
content.NATType  = 1;
if (data.type == "pppoe"){
content.Password = self.enc(data.wan_pppoe_password);
content.Username = data.wan_pppoe_username;
content.MACColoneEnable = data.mac_clnone_enable;
content.MACColone = (data.macclone_mac).toUpperCase();
content.ConnectionType = "PPP_Routed";
}
else if (data.type == "static"){
content.ConnectionType = "IP_Routed";
content.IPv4AddrType   = "Static";
content.IPv4Addr       = data.wan_wizard_ipaddress;
content.IPv4Mask       = data.wan_wizard_ipmask;
content.IPv4Gateway    = data.wan_wizard_gateway;
content.IPv4DnsServers = data.wan_wizard_dns1 + "," + data.wan_wizard_dns2;
}
else{
content.ConnectionType = "IP_Routed";
content.IPv4AddrType   = "DHCP";
}
Atp.WanWizardController.submitItem("update", content, callback);
});
return ;
}
var content = Atp.WanWizardController.content;
content.linkdata = data.linkdata;
content.NATType  = 1;
if (data.type == "pppoe"){
content.Password = this.enc(data.wan_pppoe_password);
content.Username = data.wan_pppoe_username;
content.MACColoneEnable = data.mac_clnone_enable;
content.MACColone = (data.macclone_mac).toUpperCase();
content.ConnectionType = "PPP_Routed";
}
else if (data.type == "static"){
content.ConnectionType = "IP_Routed";
content.IPv4AddrType   = "Static";
content.IPv4Addr       = data.wan_wizard_ipaddress;
content.IPv4Mask       = data.wan_wizard_ipmask;
content.IPv4Gateway    = data.wan_wizard_gateway;
content.IPv4DnsServers = data.wan_wizard_dns1 + "," + data.wan_wizard_dns2;
}
else{
content.ConnectionType = "IP_Routed";
content.IPv4AddrType   = "DHCP";
}
Atp.WanWizardController.submitItem("update", content, callback);
}
});
Atp.WanSetupDetectController = Atp.SingleObjController.extend({
objName: "ntwk/wandetect",
detectCnt: 0,
detectStatus: 0,
showppponce:0,
detectauto:false,
finishDetect: function(result){
console.log("In finishDetect...");
Atp.WanSetupDetectController.detectStatus = 0;
console.log("set self.detectStatus=",self.detectStatus);
console.log("come in finish detect..");
if (result.Status == "Connected"){
utilchangePage("wan_wizard_success");
}
else{
utilchangePage("wan_wizard_failed");
}
},
isConnecting: function(result){
if ("Connecting" == result["ConnectionStatus"]) {
return true;
}
if ("Authenticating" == result["ConnectionStatus"]) {
return true;
}
if ("Disconnecting" == result["ConnectionStatus"]) {
return true;
}
if ("PendingDisconnecting" == result["ConnectionStatus"]) {
return true;
}
return false;
},
isConnected: function(result){
if ("Bridged" == result["ConnectionType"]) {
if ("Connected" == result["ConnectionStatus"]) {
return true;
}
return false;
}
if ("Connected" != result["ConnectionStatus"]) {
return false;
}
return true;
},
detectWanConnected: function(result, self){
console.log("in detectWanConnected...",result);
console.log("self.detectStatus=",self.detectStatus);
self.detectStatus = self.detectStatus + 1;
var detailStr =  Em.I18n.get("guide.wan.detail");
if(result["ConnectionType"] == "PPP_Routed"){
if ("Unknown" == result.ErrReason){
Atp.WanSetupDetectController.detectStatus = 0;
Atp.WanSetupDetectController.showppponce++;
utilchangePage("wan_wizard_page_pppoe");
$("#wan_pppoe_error_info").html(Em.I18n.get("guide.diagnose.unknownerr"));
return;
}
}
if ("ErrAuthFail" == result.ErrReason){
Atp.WanSetupDetectController.detectStatus = 0;
Atp.WanSetupDetectController.showppponce++;
utilchangePage("wan_wizard_page_pppoe");
$("#wan_pppoe_error_info").html(Em.I18n.get("wan.pppoe.error"));
return;
}
if ("ErrAuthUserLimit" == result.ErrReason){
Atp.WanSetupDetectController.detectStatus = 0;
Atp.WanSetupDetectController.showppponce++;
utilchangePage("wan_wizard_page_pppoe");
$("#wan_pppoe_error_info").html(Em.I18n.get("guide.userlimit.error")+detailStr);
$("#wan_pppoe_error_info").attr("name","userlimit");
return;
}
if ("ErrAuthMacLimit" == result.ErrReason){
Atp.WanSetupDetectController.detectStatus = 0;
Atp.WanSetupDetectController.showppponce++;
utilchangePage("wan_wizard_page_pppoe");
$("#wan_macclone_info").show();
$("#wan_pppoe_error_info").html(Em.I18n.get("guide.macclone.error")+detailStr);
$("#wan_pppoe_error_info").attr("name","macclone");
return;
}
if (false == self.isConnected(result)){
if (self.detectStatus <= 5){
setTimeout(function() {
Atp.WanSetupDetectController.load(Atp.WanSetupDetectController.detectWanConnected);
}, 2000);
}
}
if (self.detectStatus > 5 || self.isConnected(result)){
self.finishDetect(result);
}
},
scanningWanType: function(result, self){
console.log("In scanningWanType...");
var flag = false;
console.log("scanningWanType",result);
console.log("self.detectCnt=",self.detectCnt);
if(Atp.WanSetupDetectController.detectauto){
if ("ErrAuthFail" == result.ErrReason){
self.detectCnt = 0;
utilchangePage("wan_wizard_page_pppoe");
$("#wan_pppoe_error_info").html(Em.I18n.get("wan.pppoe.error"));
$("#wan_pppoe_error_info").attr("name","");
if("" != Atp.WanSetupDetectController.content.DetailErr){
$("#wan_pppoe_error_info").html(Em.I18n.get("guide.diagnose.pppaccounterror")+Em.I18n.get("guide.wan.detail"));
}
return;
}
Atp.WanSetupDetectController.detectauto = false;
}
if ("Finished" == result.SearchingStatus){
self.detectCnt = 0;
if ("1" == result["WanResult"]){
Atp.WanWizardController.content.ConnectionType = "PPP_Routed";
}
else if("2" == result["WanResult"]){
Atp.WanWizardController.content.ConnectionType = "IP_Routed";
}
flag = true;
if("Connected" ==result.ConnectionStatus){
self.finishDetect(result);
}else{
console.log("WanWizardController submit....");
console.log(Atp.WanWizardController.content);
Atp.WanWizardController.content.Password = "";
console.log("WanWizardController post content....",Atp.WanWizardController.content);
Atp.WanWizardController.submit(Atp.WanWizardController.content, function(){
console.log("post data then detect.....");
self.detectStatus = 0;
Atp.WanSetupDetectController.load(Atp.WanSetupDetectController.detectWanConnected);
});
}
return;
}
if (self.detectCnt > 10 || flag){
self.detectCnt = 0;
self.finishDetect(result);
return;
}
setTimeout(function() {
var detectCount = self.detectCnt;
self.detectCnt = detectCount + 1;
Atp.WanSetupDetectController.load(Atp.WanSetupDetectController.scanningWanType);
}, 1000);
},
detect: function(){
console.log("In detect...");
var self = this;
console.log("detect.............");
var data = {};
data.Type = "connType";
data.wan  = Atp.WanWizardController.content.ID;
console.log(data);
self.submitItem("trigger", data, function(){
self.detectCnt = 0;
Atp.WanSetupDetectController.load(Atp.WanSetupDetectController.scanningWanType);
});
}
});
Atp.OnlineUpgStateController = Atp.SingleObjController.extend({
objName: 'system/onlinestate'
});
});
function submit_auto(){
console.log("In submit_auto...");
if(g_userLevel == 0){
utilchangePage("login_page");
aimpage = "auto";
return ;
}
show_connecting_page();
Atp.WanSetupDetectController.detect();
Atp.WanSetupDetectController.detectauto = true;
}
function utilTochangePage(topage){
console.log("In utilTochangePage...");
aimpage = "/small/html/app.html#"+topage;
if(g_userLevel == 0){
utilchangePage("login_page");
}else{
window.location.href = aimpage;
}
}
function DiagnoseAgain(){
console.log("In DiagnoseAgain...");
Atp.OnlineUpgStateController.load(function(data, self){
console.log(data);
if(19 == self.content["UpdateState"] && self.content["DownloadProcess"] >= 0 && self.content["DownloadProcess"] < 100){
utilchangePage("hota_download_page");
}else if(19 == self.content["UpdateState"] && self.content["DownloadProcess"] == 100){
utilchangePage("hota_upgrade_page");
}else{
Atp.WanWizardController.load();
Atp.UserCountController.load();
console.log("load wan wizard data",Atp.WanWizardController.content);
Atp.WanSetupDetectController.load(function(data, self){
console.log(data);
if (self.content["ErrReason"] == "ErrLayer2Down"){
console.log("no cable....");
utilchangePage("wan_wizard_page_layer2down");
}
else if(self.content["ErrReason"] == "ErrAuthFail"){
console.log("ErrAuthFail...");
utilchangePage("wan_wizard_page_pppoe");
}else if(self.content["ErrReason"] == "Success"){
console.log("Success...");
utilchangePage("wan_wizard_success");
}else{
console.log("cabled....");
console.log("wan_wizard_failed...");
utilchangePage("wan_wizard_failed");
}
});
}
});
}
function quick_auto_connect(){
$("#wan_wizard_page_layer2down_content").hide();
$("#wizard_detecting_div").show();
Atp.WanSetupDetectController.load(function(data, self){
console.log(data);
if (self.content["ErrReason"] == "ErrLayer2Down"){
console.log("quick_auto_connect no cable....");
setTimeout(function(){
utilchangePage("wan_wizard_page_layer2down");
$("#wizard_detecting_div").hide();
$("#wan_wizard_page_layer2down_content").show();
},2000);
}else{
console.log("quick_auto_connect cabled....");
submit_auto();
}
});
}
$(document).ready(function(){
console.log("In document ready...");
DiagnoseAgain();
$('#logout, .logout-mini').hide();
});
$("#wan_wizard_page_connecting").bind("pagebeforeshow", function(event, data){
if(Atp.WanSetupDetectController.content.ErrReason == "Success"){
return ;
}
if(g_userLevel == 0){
console.log("wan_wizard_page_connecting  g_userLevel is 0..... ");
utilchangePage("login_page");
aimpage = "wan_wizard_page_connecting";
$("#loginsubmit").prev('span').find('span.ui-btn-text').text(Em.I18n.get('UserLogin.LoginLabel'));
}
});
$('#login_form').on('submit', function (e) {
var $this = $(this);
e.preventDefault();
$("#login_warning").html("");
$("#first_login").hide();
$.mobile.showPageLoadingMsg();
var data = $this.serializeObject();
data.UserName = Atp.UserCountController.getLoginUserName();
var csrf_obj = utilGetCsrf();
var plaintPwd = data.UserName + base64Encode(SHA256(data.Password)) + csrf_obj.csrf_param +csrf_obj.csrf_token;
data.Password = SHA256(plaintPwd);
data.LoginFlag = 1;
Atp.UserLoginController.login(data);
});
$("#wan_mac-choose").click(function(){
$(".choose-more").css("overflow","visible");
if($(".mac-list").hasClass("hide")){
$(".mac-list").removeClass("hide").addClass("show");
}else{
$(".mac-list").removeClass("show").addClass("hide");
}
});
$(".mac-list .mac-item").click(function(){
var cur_item = $(".mac-list .active");
cur_item.removeClass("active");
$(this).addClass("active");
$("#wan_mac-choose").attr("name",$(this).index());
$("#wan_mac-choose").html($(this).html());
$(".mac-list").removeClass("show").addClass("hide");
if($(".mac-list .otherclone").hasClass("active")){
$("#wan_macconet_type_manual").show();
}
else{
$("#wan_macconet_type_manual").hide();
}
});
$("#wan_pppoe_error").click(function(){
console.log("more_detail.......",$(".more_detail").parent().attr("name"));
if("macclone" == $(".more_detail").parent().attr("name")){
$("#wan_pppoe_error_info").html(Em.I18n.get("guide.macclone.error")+Em.I18n.get("guide.wan.detail"));
$("#wan_pppoe_error_help").html(Em.I18n.get("guide.macclone.error.help"));
$("#wan_pppoe_error_help").toggle();
}
if("userlimit" == $(".more_detail").parent().attr("name")){
$("#wan_pppoe_error_info").html(Em.I18n.get("guide.userlimit.error")+Em.I18n.get("guide.wan.detail"));
$("#wan_pppoe_error_help").html(Em.I18n.get("guide.userlimit.error.help"));
$("#wan_pppoe_error_help").toggle();
}
});
function submitpppoe(){
var data = $("#wan_setup_pppoe_form").serializeObject();
$("#wan_pppoe_error").hide();
$("#wan_pppoe_error_info").html("");
if(data){
var max_ppp_password_length = 64;
if(data.wan_pppoe_password.length > max_ppp_password_length){
$("#wan_pppoe_error").show();
$("#wan_pppoe_error_info").html(Em.I18n.get("wan.pppoe.passworderror"));
return;
}
if(0 == $("#wan_mac-choose").attr("name")){
data.mac_clnone_enable = false;
data.macclone_mac = Atp.CurrentMacShowController.content.macaddress;
}else if(1 == $("#wan_mac-choose").attr("name")){
data.mac_clnone_enable = true;
data.macclone_mac = Atp.CurrentMacShowController.content.macaddress;
}else{
data.mac_clnone_enable = true;
data.macclone_mac = $("#wan_macconet_manual").val();
if(!isValidMacAddr(data.macclone_mac)){
$("#wan_pppoe_error").show();
$("#wan_pppoe_error_info").html(Em.I18n.get("guide.mac.error"));
return;
}
}
}
console.log("wan pppoe submit data,",data);
show_connecting_page();
Atp.WanWizardController.submit(data, function(result){
console.log("wan pppoe submit result=",result);
if(result["errcode"] != 0){
utilchangePage("wan_wizard_page_pppoe");
$("#wan_pppoe_error").show();
$("#wan_pppoe_error_info").html(Em.I18n.get("wan.pppoe.error"));
}
else{
$("#wan_pppoe_error").hide();
$("#wan_pppoe_error_info").html("");
Atp.WanWizardController.load(function(){
Atp.WanSetupDetectController.load(Atp.WanSetupDetectController.detectWanConnected);
console.log("PPPOE DISCONNECT!");
});
}
});
}
$('#button_wan_pppoe').bind('click', function (e) {
console.log("In button_wan_pppoe click...");
if(g_userLevel == 0){
utilchangePage("login_page");
aimpage = "pppoe";
return ;
}
e.preventDefault();
submitpppoe();
});
function exitDiagnose(){
if(g_userLevel == 0){
window.location.href = "/small/html/index.html";
}else{
window.location.href = "/small/html/home.html#home_page";
}
}
$("#wan_wizard_page_pppoe").on("pagecreate", function( event, ui ) {
setButtonI18n("pppoe_skip_input_btn", 'Touch.wan.wizard_exit');
});
$("#wan_wizard_page_pppoe").bind("pagebeforeshow", function(event, data){
setButtonI18n("button_wan_pppoe", 'Touch.connect');
setButtonI18n("pppoe_skip_input_btn", 'Touch.wan.wizard_exit');
$("#pppoe_wansetup_text_fieldcontain").hide();
$("#pppoe_wansetup_password_fieldcontain").show();
$("#pppoe_shownewpassword_img").attr("src","../res/uncheck.png");
$("#wan_macconet_manual").attr("placeholder", Em.I18n.get("guide.mac.format"));
$("#wan_mac-choose").html(Em.I18n.get("guide.internet.macconenone"));
$(".mac-list .noclone").html(Em.I18n.get("guide.internet.macconenone"));
$(".mac-list .defaultclone").html(Em.I18n.get("guide.internet.macconecurrent"));
$(".mac-list .otherclone").html(Em.I18n.get("guide.internet.macconemanual"));
Atp.WanWizardController.load(function(){
$("#wan_pppoe_error").hide();
$("#wan_pppoe_error_help").hide();
var wizard_content = Atp.WanWizardController.content;
$("#wan_pppoe_username").val(wizard_content.Username);
$("#wan_wizard_pppoe_setup_password").val(wizard_content.Password);
$("#wan_wizard_pppoe_setup_password_text").val(wizard_content.Password);
if(Atp.WanSetupDetectController.showppponce > 0){
$("#wan_pppoe_error").show();
var err = Atp.WanSetupDetectController.content.ErrReason;
if("ErrNoAnswer" == err){
$("#wan_pppoe_error_info").html(Em.I18n.get("guide.noanswer.error"));
}else if("Unknown" == err){
$("#wan_pppoe_error_info").html(Em.I18n.get("guide.diagnose.unknownerr"));
}else if(("ErrAuthFail" != err) && ("ErrAuthUserLimit" != err) && ("ErrAuthMacLimit" != err)){
$("#wan_pppoe_error_info").html(Em.I18n.get("guide.diagnose.failed"));
}
}
Atp.CurrentMacShowController.load(function(result){
if("undefined" == typeof Atp.WanWizardController.content.MACColoneEnable){
$("#wan_macclone_info").hide();
}else{
if(false == Atp.WanWizardController.content.MACColoneEnable){
$(".mac-list .noclone").addClass("active");
$("#wan_mac-choose").attr("name",0);
$("#wan_mac-choose").html($(".mac-list .noclone").html());
}else if(result["macaddress"] == Atp.WanWizardController.content.MACColone){
$("#wan_macclone_info").show();
$(".mac-list .defaultclone").addClass("active");
$("#wan_mac-choose").attr("name",1);
$("#wan_mac-choose").html($(".mac-list .defaultclone").html());
}else{
$("#wan_macclone_info").show();
$(".mac-list .otherclone").addClass("active");
$("#wan_mac-choose").attr("name",2);
$("#wan_mac-choose").html($(".mac-list .otherclone").html());
$("#wan_macconet_type_manual").show();
$("#wan_macconet_manual").val(Atp.WanWizardController.content.MACColone);
}
}
});
});
});
$("#wan_wizard_page_layer2down").on("pagebeforeshow", function( event, ui ) {
setButtonI18n("diagnose_skip_input_btn", 'Touch.wan.wizard_exit');
});
$("#login_page").bind("pagebeforeshow", function( event, data  ){
setButtonI18n("loginsubmit", "Touch.login.login");
});
$('#wan_wizard_pppoe_setup_password').bind("change",function() {
$("#wan_wizard_pppoe_setup_password_text").val($("#wan_wizard_pppoe_setup_password").val());
});
$('#wan_wizard_pppoe_setup_password_text').bind("change",function() {
$("#wan_wizard_pppoe_setup_password").val($("#wan_wizard_pppoe_setup_password_text").val());
});
function updateImageSize()
{
Atp.OnlineUpgStateController.load(function(data, self){
var process = self.content["DownloadProcess"];
$("#hota_totalimage").html(parseFloat((self.content["ImageSize"]/(1024*1024)).toFixed(2)));
$("#hota_currentimage").html(parseFloat(($("#hota_totalimage").html()*process/100).toFixed(2)));
$("#downprocess").css("width",process*0.7 + "%");
if(process == 100){
utilchangePage("hota_upgrade_page");
return ;
}
setTimeout(function(){
updateImageSize();
},2000);
});
}
$("#hota_download_page").bind("pagebeforeshow", function( event, data  ){
updateImageSize();
});
var fromWan = false;
var isCloseWlan2g = false;
var isCloseWlan5g = false;
var hasPostWifiErr = false;
function initImage(enable,imageid){
if(enable == true){
$("#"+imageid).attr("src","/small/res/on.png");
}else{
$("#"+imageid).attr("src","/small/res/off.png");
}
}
function getCurrentModeStatus(openbuttonid){
var wlanimage = $("#"+openbuttonid).attr("src");
if( wlanimage.indexOf("uncheck.png")>=0 ){
return "WPAand11i";
}
return "Basic";
}
function getCurrentEnableStatus(openbuttonid){
var wlanimage = $("#"+openbuttonid).attr("src");
if( wlanimage.indexOf("on.png")>=0 ){
return true;
}
return false;
}
function setWlanEnableImage(id){
var currentimage = $("#"+id).attr("src");
if(currentimage.indexOf("on.png") >= 0){
$("#"+id).attr("src","/small/res/off.png");
return false;
}else{
$("#"+id).attr("src","/small/res/on.png");
return true;
}
}
function wifi245gOperation(wifi245genable,displayid){
if(wifi245genable == true){
$("#"+displayid).show();
}else{
$("#"+displayid).hide();
}
}
function isShowErrorinfo(val){
var wifi245g = getCurrentEnableStatus("wlan"+val+"_wifiopen_img");
if(wifi245g == false){
$("#wlan_ssidempty_warning").hide();
$("#wlan_passwordempty_warning").hide();
$("#wlan_open_warning").hide();
$("#wifiwpakeyency").hide();
$("#wpakey_checkbox").hide();
$("#wpakey_text_fieldcontain_"+val).hide();
$("#wpakey_fieldcontain_"+val).hide();
}else if(wifi245g == true){
$("#wifiwpakeyency").show();
$("#wlan_open_warning").hide();
}
}
function show_submit_wlansetting(){
var wifi2genableCur = getCurrentEnableStatus("wlan2g_wifiopen_img");
var wifi5genableCur = getCurrentEnableStatus("wlan5g_wifiopen_img");
if(wifi2genableCur||wifi5genableCur){
$("#submit_wlansetting").show();
}else{
$("#submit_wlansetting").hide();
}
}
function renewdata(val){
if (val == "2g") {
var initObj = Atp.WifiBasicController.wlan2gObj;
}else{
var initObj = Atp.WifiBasicController.wlan5gObj;
}
if (!getCurrentEnableStatus("wlan"+val+"_wifiopen_img")) {
$("#ssid"+val).val(initObj.WifiSsid);
$("#wpakey"+val).val(initObj.WpaPreSharedKey);
$("#wpakey_text_"+val).val(initObj.WpaPreSharedKey);
}
}
function setWlan2gStatus_OnClick(){
var wificontrol = Atp.WifiBasicController;
var initImgStatus = wificontrol.wlan2gObj.WifiEnable && wificontrol.wlan2gObj.WifiSsidEnable;
var wifi24g = setWlanEnableImage("wlan2g_wifiopen_img");
var beaconType_2g = getCurrentModeStatus("wifisetting2g_open_img");
wifi245gOperation(wifi24g,"wifi_setting2g_content");
if(wifi24g == true){
$("#password_strength_2g_line").show();
$("#wifiwpakeyency2g").show();
$("#wlan2g_open_warning_2g_line").show();
if(beaconType_2g == "Basic"){
$("#wlan2g_open_warning").show();
$("#wpakey_fieldcontain_2g").hide();
}else{
$("#wpakey_fieldcontain_2g").show();
$("#wlan2g_open_warning").hide();
}
isCloseWlan2g = false;
$("#submit_wlansetting").show();
}else{
$("#password_strength_2g").hide();
$("#password_strength_2g_line").hide();
$("#wifiwpakeyency2g").hide();
$("#wlan2g_open_warning").hide();
$("#wlan2g_open_warning_2g_line").hide();
isCloseWlan2g = true;
$("#wpakey_fieldcontain_2g").show();
if(initImgStatus != wifi24g){
$("#submit_wlansetting").show();
}else{
$("#submit_wlansetting").hide();
show_submit_wlansetting();
}
}
renewdata("2g");
isShowErrorinfo("2g");
}
function setWlan5gStatus_OnClick(){
var wificontrol = Atp.WifiBasicController;
var initImgStatus = wificontrol.wlan5gObj.WifiEnable && wificontrol.wlan5gObj.WifiSsidEnable;
var wifi5g = setWlanEnableImage("wlan5g_wifiopen_img");
var beaconType_5g = getCurrentModeStatus("wifisetting5g_open_img");
wifi245gOperation(wifi5g,"wifi_setting5g_content");
if(wifi5g == true){
$("#password_strength_5g_line").show();
$("#wifiwpakeyency5g").show();
$("#wlan5g_open_warning_5g_line").show();
if(beaconType_5g == "Basic"){
$("#wlan5g_open_warning").show();
$("#wpakey_fieldcontain_5g").hide();
}else{
$("#wlan5g_open_warning").hide();
$("#wpakey_fieldcontain_5g").show();
}
isCloseWlan5g = false;
$("#submit_wlansetting").show();
}else{
$("#password_strength_5g").hide();
$("#password_strength_5g_line").hide();
$("#wifiwpakeyency5g").hide();
$("#wlan5g_open_warning").hide();
$("#wlan5g_open_warning_5g_line").hide();
isCloseWlan5g = true;
$("#wpakey_fieldcontain_5g").show();
if(initImgStatus != wifi5g){
$("#submit_wlansetting").show();
}else{
$("#submit_wlansetting").hide();
show_submit_wlansetting();
}
}
renewdata("5g");
isShowErrorinfo("5g");
}
function allClosed(wifiallenable,open245g,val){
wifi245gOperation(open245g,"wifi_setting"+val+"_content");
initImage(open245g,"wlan"+val+"_wifiopen_img");
if(false == wifiallenable){
$("#submit_wlansetting").hide();
$("#wpakey_fieldcontain_"+val).hide();
}else {
$("#submit_wlansetting").show();
$("#wpakey_fieldcontain_"+val).show();
}
}
function utilShowEyeWifiPassword(checkboxid,passwordinput,textinput,inputid,type){
var src = $("#"+checkboxid).attr("src");
if (typeof(src) === "undefined"){
return;
}
if(src.indexOf("eyehide") >= 0){
if("********" == $("#"+inputid).val()){
Atp.WifiBasicShowPassController.objName = "ntwk/wlantouch?type="+type;
console.log("objName:","ntwk/wlantouch?type="+type);
Atp.WifiBasicShowPassController.load(function(){
if(type.indexOf("24") >= 0){
$("#"+inputid).val(Atp.WifiBasicShowPassController.wlan2gObj.WpaPreSharedKey);
}
else{
$("#"+inputid).val(Atp.WifiBasicShowPassController.wlan5gObj.WpaPreSharedKey);
}
$("#"+passwordinput).hide();
$("#"+textinput).show();
});
}else{
$("#"+passwordinput).hide();
$("#"+textinput).show();
}
return true;
}
else{
$("#"+passwordinput).show();
$("#"+textinput).hide();
return false;
}
}
$(function(){
Atp.WifiBasicShowPassController = Atp.MultiObjController.extend({
objName: "ntwk/wlantouch",
wlan2gObj: null,
wlan5gObj: null,
afterLoad:function(){
for (var i = 0; i < this.content.length; i++){
if (this.content[i].wlanSign == 0){
this.wlan2gObj  = this.content[i];
}
if(this.content[i].wlanSign == 1){
this.wlan5gObj = this.content[i];
}
}
}
});
Atp.WifiBasicController = Atp.MultiObjController.extend({
objName: "ntwk/wlantouch",
contentType:"application/json;charset=UTF-8;enp",
wlan2gObj: null,
wlan5gObj: null,
showdata: function(obj,val){
var wifiallenable = (this.wlan2gObj.WifiEnable && this.wlan2gObj.WifiSsidEnable)||(this.wlan5gObj.WifiEnable && this.wlan5gObj.WifiSsidEnable);
var wifi245genable = obj.WifiEnable && obj.WifiSsidEnable;
var beaconType = obj.BeaconType;
$("#ssid"+val).val(obj.WifiSsid);
$("#wpakey_text_"+val).val(obj.WpaPreSharedKey);
$("#wpakey"+val).val(obj.WpaPreSharedKey);
allClosed(wifiallenable,wifi245genable,val);
if (wifi245genable) {
if(beaconType == "None"){
$("#wifisetting"+val+"_open_img").attr("src", "/small/res/checked.png");
$("#wpakey_fieldcontain_"+val).hide();
$("#password_strength_"+val).hide();
$("#password_strength_"+val+"_line").hide();
$("#wlan"+val+"_open_warning").show();
if(val == "5g"){
$("#password_strength_5g_line").hide();
}
}else{
$("#wifisetting"+val+"_open_img").attr("src", "/small/res/uncheck.png");
$("#wpakey_fieldcontain_"+val).show();
$("#wpakey_text_fieldcontain_"+val).hide();
$("#password_strength_"+val).hide();
$("#password_strength_"+val+"_line").show();
$("#wlan"+val+"_open_warning").hide();
if(val == "5g"){
$("#password_strength_5g_line").show();
}
}
}else{
$("#wifiwpakeyency"+val).hide();
$("#wlan"+val+"_open_warning").hide();
$("#password_strength_"+val+"_line").hide();
$("#wlan"+val+"_open_warning_"+val+"_line").hide();
}
if(obj.WpaPreSharedKey == "" && wifi245genable == true){
$("#wlan_open_warning").show();
}
if(obj.WpaPreSharedKey == "" || wifi245genable == false){
$("#wpakey_text_fieldcontain_"+val).hide();
$("#wpakey_fieldcontain_"+val).hide();
}else{
$("#wpakey_text_fieldcontain_"+val).hide();
$("#wpakey_fieldcontain_"+val).show();
}
},
afterLoad: function(){
for (var i = 0; i < this.content.length; i++){
if (this.content[i].wlanSign == 0){
this.wlan2gObj  = this.content[i];
}
if(this.content[i].wlanSign == 1){
this.wlan5gObj = this.content[i];
}
}
this.showdata(this.wlan2gObj,"2g");
this.showdata(this.wlan5gObj,"5g");
},
createSsidSubmitData: function(wlanobj,ssid,password,enable){
console.log("in createSsidSubmitData");
var ssiddata = wlanobj;
ssiddata.WifiSsid = ssid;
ssiddata.WifiEnable = enable;
ssiddata.WifiSsidEnable = enable;
console.log("enable..WifiEnable..",ssid,enable);
if ("WPAand11i" == wlanobj.BeaconType){
ssiddata.WepKey = this.enc(password);
ssiddata.WpaEncryptionMode = 'AES';
}
else{
ssiddata.BasicEncryptionMode  = "None";
ssiddata.WepKey = this.enc("");
}
console.log("wifi content=",ssiddata);
return ssiddata;
},
submit: function(data, callback){
console.log("submit data",data);
if (null == data){
return;
}
var wlandata = {};
this.wlan2gObj.BeaconType = data.beaconType2g;
wlandata.wifi24g = this.createSsidSubmitData(this.wlan2gObj,data.ssid2g,data.wpakey_text_2g,data.wifi24genable);
wlandata.wifi24g["notsetwebpassword"] = true;
this.wlan5gObj.BeaconType = data.beaconType5g;
wlandata.wifi5g = this.createSsidSubmitData(this.wlan5gObj,data.ssid5g,data.wpakey_text_5g,data.wifi5genable);
wlandata.wifi5g["notsetwebpassword"] = true;
console.log("wlan submit ",wlandata);
this.submitItem("SsidSettings", wlandata, callback);
}
});
});
function jumpIfchangeData(change){
if (fromWan){
if (change){
utilchangePage("wlansetting_confirm_page");
}
else{
utilchangePage("wan_wizard_page_over_wifi_nochange");
Atp.WanSetupDetectController.load(function(){
if("Connected" == Atp.WanSetupDetectController.content.ConnectionStatus){
$("#wan_wizard_to_wifi_nochange_wansuccess_img").show();
$("#wan_wizard_to_wifi_nochange_wansuccess").show();
$("#wan_wizard_to_wifi_nochange_wanfail_img").hide();
$("#wan_wizard_to_wifi_nochange_wanfail").hide();
}else{
$("#wan_wizard_to_wifi_nochange_wansuccess_img").hide();
$("#wan_wizard_to_wifi_nochange_wansuccess").hide();
$("#wan_wizard_to_wifi_nochange_wanfail_img").show();
$("#wan_wizard_to_wifi_nochange_wanfail").show();
}
})
}
fromWan = false;
}
else{
if (change){
utilchangePage("wlansetting_confirm_page");
}
else{
window.location.href = "/small/html/home.html#home_page";
}
}
}
function warninginfoInit(){
$("#wlan_ssidempty_warning").hide();
$("#wlan_passwordempty_warning").hide();
$("#wlan_open_warning").hide();
$("#password_strength_2g").hide();
$("#password_strength_2g").hide();
$("#password_strength_5g").hide();
$("#password_strength_5g").hide();
}
function wlan_cancel(){
var ischange = judgedatachange();
console.log("change...",ischange);
if(ischange){
warninginfoInit();
Atp.WifiBasicController.load();
}else{
jumpIfchangeData(false);
}
}
function submitwlan(val,ssid_id,keyid,ssidemptywarningid,ssidwarninginfoid,passwordemptywaringid,passwordwaringid,checkboxid,openmodeimgid){
var ssidMaxLen = 32;
var ssidLen = $("#"+ssid_id).val().length;
var pwdLen = $("#"+keyid).val().length;
var errSign = 0;
var w=/^[\x20-\x7e]*$/;
var u=/.*[\x20-\x7e]$/;
var patrn_key=/.*[^\x20-\x7e]+.*$/;
$("#"+ssidemptywarningid).hide();
$("#"+passwordemptywaringid).hide();
var ssidvalue = "";
ssidvalue = $.trim($("#" + ssid_id).val());
var openmode = getCurrentModeStatus(openmodeimgid);
var wifiopen = getCurrentEnableStatus(checkboxid);
if(wifiopen){
if(ssidvalue == ""){
$("#"+ssidemptywarningid).show();
$("#"+ssidwarninginfoid).html(val+Em.I18n.get("Touch.wifi.ssidempty"));
errSign = 1;
}else if(ssidLen > ssidMaxLen){
$("#"+ssidemptywarningid).show();
$("#"+ssidwarninginfoid).html(val+Em.I18n.get("Touch.wifi.ssidinfo", {arg1:ssidMaxLen}));
errSign = 1;
}else if(!( w.test(ssidvalue) && u.test(ssidvalue))){
$("#"+ssidemptywarningid).show();
$("#"+ssidwarninginfoid).html(val+Em.I18n.get("Wlan.Set.error"));
errSign = 1;
}else if(openmode != "Basic"){
if((pwdLen < 8) || (pwdLen > 63)){
$("#"+passwordemptywaringid).show();
$("#"+passwordwaringid).html(val+Em.I18n.get("Touch.wifi.passwordinfo",{arg1:"8~63"}));
errSign = 2;
}else{
if(patrn_key.exec($("#"+keyid).val())){
$("#"+passwordemptywaringid).show();
$("#"+passwordwaringid).html(val+Em.I18n.get("Wlan.Set.error"));
errSign = 2;
}
}
}
}
return errSign;
}
function getSubmitData(){
var submitdata = $("#wlan_setting_form").serializeObject();
submitdata.ssid2g = $.trim(submitdata.ssid2g);
submitdata.wifi24genable = getCurrentEnableStatus("wlan2g_wifiopen_img");
submitdata.beaconType2g = getCurrentModeStatus("wifisetting2g_open_img");
submitdata.ssid5g = $.trim(submitdata.ssid5g);
submitdata.wifi5genable = getCurrentEnableStatus("wlan5g_wifiopen_img");
submitdata.beaconType5g = getCurrentModeStatus("wifisetting5g_open_img");
return submitdata;
}
function judgedatachange(){
var submitdata = getSubmitData();
console.log("submitdata=",submitdata);
var changesign = false;
var wifi24g = Atp.WifiBasicController.wlan2gObj;
if(wifi24g["BeaconType"] == "None"){
wifi24g["BeaconType"] = "Basic";
}
var wifi5g = Atp.WifiBasicController.wlan5gObj;
if(wifi5g["BeaconType"] == "None"){
wifi5g["BeaconType"] = "Basic";
}
if((submitdata["ssid2g"] != wifi24g["WifiSsid"]) || (submitdata["wifi24genable"] != (wifi24g["WifiEnable"] && wifi24g["WifiSsidEnable"])) || ((wifi24g["WpaPreSharedKey"] != submitdata["wpakey_text_2g"]) && (submitdata["beaconType2g"] != "Basic")) || (wifi24g["BeaconType"] != submitdata["beaconType2g"])
|| (submitdata["ssid5g"] != wifi5g["WifiSsid"]) || (submitdata["wifi5genable"] != (wifi5g["WifiEnable"] && wifi5g["WifiSsidEnable"])) || ((wifi5g["WpaPreSharedKey"] != submitdata["wpakey_text_5g"]) && (submitdata["beaconType5g"] != "Basic")) || (wifi5g["BeaconType"] != submitdata["beaconType5g"])
){
console.log("change 2.4 ");
changesign = true;
}
return changesign;
}
function wlan_submit(){
console.log("in wlan_submit...");
var ssid245gerrinfo = 0;
var ischange = judgedatachange();
ssid245gerrinfo = submitwlan("2.4G ","ssid2g","wpakey_text_2g","wlan_ssidempty_warning","wlan_ssid_warninginfo","wlan_passwordempty_warning","wlan_pwd_warninginfo","wlan2g_wifiopen_img","wifisetting2g_open_img")
||submitwlan("5G ","ssid5g","wpakey_text_5g","wlan_ssidempty_warning","wlan_ssid_warninginfo","wlan_passwordempty_warning","wlan_pwd_warninginfo","wlan5g_wifiopen_img","wifisetting5g_open_img");
if(ssid245gerrinfo != 0){
console.log("error happend");
return ;
}
jumpIfchangeData(ischange);
}
function wlan_confirm(){
$.mobile.showPageLoadingMsg();
var submitdata = getSubmitData();
$("#wlan_ssidempty_warning").hide();
Atp.WifiBasicController.submit(submitdata,function(result){
console.log("post result=",result);
$.mobile.hidePageLoadingMsg();
if (0 != result.errcode) {
hasPostWifiErr = true;
if(typeof result["WifiSsid"] != "undefined" ){
$("#wlan_ssid_warninginfo").html(Em.I18n.get("Wlan.Set.error"));
$("#wlan_ssidempty_warning").show();
}
utilchangePage("wlansetting_page");
}else{
hasPostWifiErr = false;
utilchangePage("wlansetting_take_effect_page");
}
})
}
$("#wlansetting_page").bind("pagebeforeshow", function( event, data){
setButtonI18n("wlansetting_submit", 'Touch.save');
setButtonI18n("wlansetting_cancel", 'Touch.cancel');
$("#ssid2g").attr("placeholder", Em.I18n.get("Touch.wifi.ssidinfo.oninput"));
$("#wpakey2g").attr("placeholder", Em.I18n.get("Touch.wifi.passwordinfo.oninput",{arg1:"8~63"}));
$("#wpakey_guestnetwork_password_2g").attr("placeholder", Em.I18n.get("Touch.wifi.passwordinfo.oninput",{arg1:"8~63"}));
$("#ssid5g").attr("placeholder", Em.I18n.get("Touch.wifi.ssidinfo.oninput"));
$("#wpakey5g").attr("placeholder", Em.I18n.get("Touch.wifi.passwordinfo.oninput",{arg1:"8~63"}));
$("#wpakey_guestnetwork_password_5g").attr("placeholder", Em.I18n.get("Touch.wifi.passwordinfo.oninput",{arg1:"8~63"}));
if(false == hasPostWifiErr){
warninginfoInit();
setTimeout(function(){
Atp.WifiBasicController.load();
},100);
console.log("load wlansetting_page...");
}
});
$("#wlansetting_confirm_page").on("pagecreate", function( event, ui ) {
$("#wlan_set_error").hide();
var aimhash = "#wlansetting_confirm_page";
var currentHash = window.location.hash;
if(currentHash == aimhash){
utilchangePage("wlansetting_page");
}
});
