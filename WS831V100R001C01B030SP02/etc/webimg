local result = {}

function file_exists(path)
  local file = io.open(path, "rb")
  if file then 
  	file:close()
  end
  return (file ~= nil)
end

local resultState = file_exists("/var/crash.tar")

local errcode,values = dm.GetParameterValues("InternetGatewayDevice.CrashDiagnostics.", 
  {"DiagnosticsState", "AuthorizeEnable"});
local obj = values["InternetGatewayDevice.CrashDiagnostics."];


result["DiagnosticsState"] = obj["DiagnosticsState"];
result["AuthorizeEnable"]  = utils.toboolean(obj["AuthorizeEnable"]);
result["ResultState"]      = resultState
sys.print(json.encode(result))

