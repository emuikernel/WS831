function submit_auto(){
Atp.WanSetupDetectController.detect();
Atp.WanSetupDetectController.detectauto = true;
}
function quick_auto_connect(){
setTimeout(function(){
Atp.WanWizardController.load(function(){
Atp.WanSetupDetectController.detectauto = true;
utilchangePage("wan_wizard_page_connecting");
Atp.WanSetupDetectController.load(function(){
if("ErrLayer2Down" == Atp.WanSetupDetectController.content.ErrReason){
utilchangePage("wan_wizard_page_layer2down");
}else {
submit_auto();
}
});
});
},100);
}
