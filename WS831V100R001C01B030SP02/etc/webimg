local domain2G = ''
local domain5G = ''
local isRestore = false

function setIsRestore(Enable,str)
    Enable = utils.toboolean(Enable)
    if Enable ~= data[str] then
        isRestore = true
    end
end

for k, v in pairs(wifiConf) do
    if "2.4GHz" == v["OperatingFrequencyBand"] then
        domain2G = k
        setIsRestore(v["Enable"],"Enable2G")
    end
    if "5GHz" == v["OperatingFrequencyBand"] then
        domain5G = k
        setIsRestore(v["Enable"],"Enable5G")
    end
end


if data["ConfigTool"] ~= "PC" then
    data["InitEnable2G"] = true
    data["InitEnable5G"] = true
end
local paras = {{domain2G..'Enable', data["Enable2G"]},
		{domain2G..'X_InitEnable', data["InitEnable2G"]},
               {domain2G..'WLANPowerMode', data["PowerMode"]},
                {domain5G..'WLANPowerMode', data["PowerMode"]},
                {domain5G..'Enable', data["Enable5G"]},
		{domain5G..'X_InitEnable', data["InitEnable5G"]}
            }

local maps = {
    Enable2G = domain2G..'Enable',
    InitEnable2G = domain2G..'X_InitEnable',
    PowerMode = domain2G..'WLANPowerMode',
    Enable5G = domain5G..'Enable',
    InitEnable5G = domain5G..'X_InitEnable',
}

function add_parameter(paras, name, value)
    if nil == value then
        return
    end
    table.insert(paras, {name, value})
end

if data["Enable2G"] == true and data["2GID"] ~= nil then
    add_parameter(paras,data["2GID"].."Enable",true)
end
if data["Enable5G"] == true and data["5GID"] ~= nil then
    add_parameter(paras,data["5GID"].."Enable",true)
end

if isRestore == false then
    utils.appenderror("wlan", "wlan.ssid.noreconncet")
end

local errcode , NeedReboot, paramerr = dm.SetParameterValues(paras)
utils.responseErrorcode(errcode, paramerr, maps)

