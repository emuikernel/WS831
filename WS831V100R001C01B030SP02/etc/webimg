local wlaneffective = {}
wlaneffective.haseffective = true

	if  _G["wlantimes"] > 10 then
			wlaneffective.haseffective = false
			_G["wlantimes"] = _G["wlantimes"] + 1
			local result = {}
			local status = true
			local WifisyncStatusFile = io.open("/var/wifisync.conf","r")
			if nil ~= WifisyncStatusFile then
				print("/var/wifisync.conf file is exited ")
				status = false
				WifisyncStatusFile:close() 
			else
				print("/var/wifisync.conf file is not exited ")
				status = true
				io.close()
			end
			if status == true then
				wlaneffective.haseffective = true
				_G["wlantimes"] = 0
			else
				if _G["wlantimes"] > 120 then
					os.remove("/var/wifisync.conf")
					_G["wlantimes"] = 0
					wlaneffective.haseffective = true
				end		
			end	
	else
		wlaneffective.haseffective = false
		_G["wlantimes"] = _G["wlantimes"] + 1
	end


