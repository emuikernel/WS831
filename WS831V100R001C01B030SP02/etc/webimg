local account_data = {}

local sdinformation = {}

--local errcode,value = web.getSdShareMode()

function checkUsbDevice()
	-- body
	local errcode,usbPhysicalStorage = dm.GetParameterValues("InternetGatewayDevice.Services.StorageService.{i}.PhysicalMedium.{i}.", {"Name","Capacity"})
	for pk,pv in pairs(usbPhysicalStorage) do
		return true
	end

	return false
end


--[[
-- 1 represents usb share mode 0:web share mode
account_data.SDShareMode = value["SDShareMode"]
-- 1:enable 0:diable
account_data.SDCardShareStatus = value["SDCardShareStatus"]
-- 0:user define dir 1: whole dir
account_data.SDShareFileMode = value["SDShareFileMode"]
-- 0:read-only 1:read/write
account_data.SDAccessType = value["SDAccessType"]
-- user define share dir 
account_data.SDSharePath = value["SDSharePath"]
-- 0:sd card not insert 1:insert]]--
-- 1 represents usb share mode 0:web share mode
account_data.SDShareMode = 1
-- 1:enable 0:diable
account_data.SDCardShareStatus = 1
-- 0:user define dir 1: whole dir
account_data.SDShareFileMode = 1
-- 0:read-only 1:read/write
account_data.SDAccessType = 1
-- user define share dir 
account_data.SDSharePath = 1

if checkUsbDevice() then
	account_data.SDCardStatus = 1
else
	account_data.SDCardStatus = 0
end

sdinformation.sdcard = account_data

sys.print(json.encode(sdinformation))require('json')
