function add_one_wlan_parameter(paras, name, value)
    if nil == value then 
        return
    end     
    table.insert(paras, {name, value})
end

function update()
    local params = {}
    local domain = ""
    if nil ~= data["config2g"] and nil ~= data["config2g"]["Trigger"] then
        domain = data["config2g"]["ID"].."X_AutoChannelRefresh"
        add_one_wlan_parameter(params, domain, data["config2g"]["Trigger"])
    end  
    local  errcode, NeedReboot, paramerr = dm.SetParameterValues(params)
    return errcode
end

