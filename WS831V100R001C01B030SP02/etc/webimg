local tostring = tostring

local errcode, lanVals = dm.GetParameterValues("InternetGatewayDevice.Layer2Bridging.AvailableInterface.{i}.", {"InterfaceReference", "X_InterfaceAlias"})
for k, v in pairs(lanVals) do
    local lanarray = {}
    lanarray[v["X_InterfaceAlias"]] = v["InterfaceReference"]
end

local lanifs = {}

function add_lanif_lanname(lanname)
    for k,v in pairs(lanVals) do
        if v["X_InterfaceAlias"] == lanname then
           local lanif = {}
           lanif.ID = v["InterfaceReference"]
           lanif.lanif = lanname
           table.insert(lanifs, lanif)
           return
        end
    end
end

function add_ssid_by_dm(domain_prefix)
    local domain = domain_prefix.."."
    local errcode, values = dm.GetParameterValues(domain, { "SSID" });
    if values ~= nil then
        local obj
        local ssid = {}
        obj = values[domain]
        ssid.ID = domain_prefix
        ssid.lanif = obj["SSID"]
        table.insert(lanifs, ssid)
    end
end
-- LAN1
add_lanif_lanname("LAN1")

-- LAN2
add_lanif_lanname("LAN2")

-- LAN3
add_lanif_lanname("LAN3")

-- LAN4
add_lanif_lanname("LAN4")
-- 2.4GHz wifi
add_ssid_by_dm("InternetGatewayDevice.LANDevice.1.WLANConfiguration.1")
-- 5GHz wifi
add_ssid_by_dm("InternetGatewayDevice.LANDevice.1.WLANConfiguration.2")

-- 2.4GHz guest network
add_ssid_by_dm("InternetGatewayDevice.LANDevice.2.WLANConfiguration.1")

-- 5GHz guest network
add_ssid_by_dm("InternetGatewayDevice.LANDevice.2.WLANConfiguration.2")

sys.print(json.encode(lanifs))
local utils = require('utils')
local errcode, classVals = dm.GetParameterValues("InternetGatewayDevice.QueueManagement.Classification.{i}.", 
    {"ClassificationEnable","ClassificationName","SourceMACAddress","ClassQueue", "ClassPolicer"});

local errcode, hostobjs = dm.GetParameterValues("InternetGatewayDevice.LANDevice.{i}.Hosts.Host.{i}.",{"MACAddress"});

local macqospriority = {}
print("come in to get mac qos....")
for kk,vv in pairs(hostobjs) do
	local inmacqos = false
	local Mac = ""
	Mac = vv["MACAddress"]
	local priorityItem = {}
	for k,v in pairs(classVals) do
		if v["SourceMACAddress"] == vv["MACAddress"] then
			inmacqos = true
			priorityItem.ID = k
			priorityItem.Name = v["ClassificationName"]
			priorityItem.ClassEnable = utils.toboolean(v["ClassificationEnable"])
			priorityItem.MACAddress = vv["MACAddress"]
			priorityItem.ClassQueue = v["ClassQueue"]
			priorityItem.ClassPolicer = v["ClassPolicer"]
			table.insert(macqospriority,priorityItem)
			break;
		end
	end
	if inmacqos == false then
		priorityItem.ID = ""
		priorityItem.ClassEnable = false
		priorityItem.Name = (string.gsub(Mac,":","_"))
		priorityItem.MACAddress = Mac
		priorityItem.ClassQueue = -1
		priorityItem.ClassPolicer = -1
		table.insert(macqospriority,priorityItem)
	end
end

sys.print(json.encode(macqospriority))local utils = require('utils')


local qosEnableparas = {}
function add_one_parameter(paras, name, value)
    if nil == value then 
        return
    end
    table.insert(paras, {name, value})
end
print("QosEnable=",data["QosEnable"])
add_one_parameter(qosEnableparas,"InternetGatewayDevice.QueueManagement.Enable",data["QosEnable"])
local err = dm.SetParameterValues(qosEnableparas)

print("qosenable err=",err)
function CreateDeviceQos(data)
    local paras = {{"ClassificationEnable", data["ClassEnable"]}, 
        {"X_QosLinkType", 3},
        {"X_Type",2},
        {"ClassificationName", data["Name"]},
        {"SourceMACAddress", data["MACAddress"]},
        {"ClassQueue", data["ClassQueue"]},
        {"ClassPolicer", data["ClassPolicer"]}}
        print("==== create macqos data=============")
        for k,v in pairs(paras) do
            print("=============================",k)
            for idx,val in pairs(v) do
                print(idx,val)
            end
        end
    local err,instnum,NeedReboot,errs = dm.AddObjectWithValues("InternetGatewayDevice.QueueManagement.Classification.", paras);
    print("create macqos err=",err)
    return err,instnum
end

function UpdateDeviceQos(data)
    local domain = data["ID"]    
    local paras = {{domain.."ClassificationEnable", data["ClassEnable"]}, 
        {domain.."X_QosLinkType", 3},
        {domain.."X_Type",2},
        {domain.."ClassificationName", data["Name"]},
        {domain.."SourceMACAddress", data["MACAddress"]},
        {domain.."ClassQueue", data["ClassQueue"]},
        {domain.."ClassPolicer", data["ClassPolicer"]}}
        print("==== update macqos data=============")
        for k,v in pairs(paras) do
            print("=============================",k)
            for idx,val in pairs(v) do
                print(idx,val)
            end
        end
    local err,needreboot, paramerror = dm.SetParameterValues(paras);
    print("update "..domain.." err=",err)
    return err,domain
end

print("|||||||||||||||||||||||||||||||||||||||||||||||||||||||")
print("operate macqos:"..data["ID"])
local operateErrCode = 0
local operateErrDomain = ""
if data["ID"] == "" then
	operateErrCode,operateErrDomain = CreateDeviceQos(data)
else
    if data["ClassQueue"] == -1 then
        print("delete macqos...")
        dm.DeleteObject(data["ID"])
    else
	   operateErrCode,operateErrDomain = UpdateDeviceQos(data)
    end
end
if operateErrCode ~=0 then
	utils.appenderror("errcode", operateErrCode)
	utils.appenderror("operateErrDomain", operateErrDomain)
else
    utils.appenderror("errcode", 0)
end
print("|||||||||||||||||||||||||||||||||||||||||")require('dm')
require('utils')

local maps = {
    Enable="Enable",
    DownBandWidth = "DownBandWidth",
    WmmEnable = "X_WmmEnable",
    SpeedTestFlag = "X_SpeedTestFlag",

    SmartQosEnable="X_SmartQosEnable",

    UpBandWidth = "X_BandWidth"
}

local errcode,values = dm.GetParameterValues("InternetGatewayDevice.QueueManagement.",maps);
local obj = values["InternetGatewayDevice.QueueManagement."]
local qosCfg = {}

qosCfg.Enable = utils.toboolean(obj["Enable"])
qosCfg.WmmEnable = utils.toboolean(obj["X_WmmEnable"])
qosCfg.inputUpBandWidth = obj["X_BandWidth"]

qosCfg.SpeedTestFlag =utils.toboolean(obj["X_SpeedTestFlag"])

if utils.toboolean(obj["X_SpeedTestFlag"]) == false then
    qosCfg.inputDownBandWidth = ""
else
    qosCfg.inputDownBandWidth = obj["DownBandWidth"]
end

qosCfg.UpBandWidth = obj["X_BandWidth"]


qosCfg.DownBandWidth = obj["DownBandWidth"]

local ismartqosclose = false
local errcode,SmartQosvalues = dm.GetParameterValues("InternetGatewayDevice.QueueManagement.",{"X_SmartQosEnable"});
local smartEnable = SmartQosvalues["InternetGatewayDevice.QueueManagement."]
if utils.toboolean(smartEnable["X_SmartQosEnable"]) == false then
	ismartqosclose = true
end

if qosCfg.Enable == false or ismartqosclose  then
    qosCfg.currentmode = "equal"
    qosCfg.beforemode = "equal"

else
    local errcode,Povalues = dm.GetParameterValues("InternetGatewayDevice.QueueManagement.Policer.{i}.",{"X_PolicerPrio"});
    for k ,v in pairs(Povalues) do
        for i in string.gmatch(k, "%d+") do
            if tonumber(i) == 2 then
                if v["X_PolicerPrio"] == 1 then
                    qosCfg.currentmode = "game"
                    qosCfg.beforemode = "game"
                end
            elseif tonumber(i) == 3 then
                if v["X_PolicerPrio"] == 1 then
                    qosCfg.currentmode = "video"
                    qosCfg.beforemode = "video"
                end
            elseif tonumber(i) == 4 then
                if v["X_PolicerPrio"] == 1 then
                    qosCfg.currentmode = "internet"
                    qosCfg.beforemode = "internet"
                end
            end
        end
    end
end

function GetSpeedtestResult(qosCfg)
    qosCfg["DownBandWidth_test"] = 0
    qosCfg["UpBandWidth_test"] = 0

    local errcode,speedvalues = dm.GetParameterValues("InternetGatewayDevice.X_SPEEDTEST.",{"SpeedtestStatus","DownBandwidth","UpBandwidth"})
    local speedvaluesObj = speedvalues["InternetGatewayDevice.X_SPEEDTEST."]
    if speedvaluesObj then
        isFinish = utils.toboolean(speedvaluesObj["SpeedtestStatus"])
        if (nil ~= speedvalues) and (false == isFinish) then
            if speedvaluesObj["DownBandwidth"] <= 3000 and speedvaluesObj["DownBandwidth"] >0 or speedvaluesObj["UpBandwidth"] <=400 and speedvaluesObj["UpBandwidth"] >0 then
            else
                qosCfg["DownBandWidth_test"] = speedvaluesObj["DownBandwidth"]
                qosCfg["UpBandWidth_test"] = speedvaluesObj["UpBandwidth"]
            end        
        end
    end
end
GetSpeedtestResult(qosCfg)

sys.print(json.encode(qosCfg))
local utils = require('utils')
local maps = {
	Enable="Enable",
	DownBandWidth = "DownBandWidth",
	WmmEnable = "X_WmmEnable",
	SpeedTestFlag = "X_SpeedTestFlag",
    SmartQosEnable="X_SmartQosEnable",
    currentmode = "X_SmartQosCurrentMode",
	UpBandWidth = "X_BandWidth"
}
--get current smartqos enable
local errcode,SmartQosvalues = dm.GetParameterValues("InternetGatewayDevice.QueueManagement.",{"X_SmartQosEnable"});
--is macqos all disable
local errcode, classVals = dm.GetParameterValues("InternetGatewayDevice.QueueManagement.Classification.{i}.", {"ClassificationEnable", "SourceMACAddress", "X_Type"});
local qosclasses = {}
for k,v in pairs(classVals) do
    if 2 == v["X_Type"] and v["SourceMACAddress"] ~= "" then
        local newClass = {}
        newClass["Enable"] = utils.toboolean(v["ClassificationEnable"])
        table.insert(qosclasses, newClass)
    end
end
function isDisableAllMACQos()
    for k, v in pairs(qosclasses) do
        if v["Enable"] == true then
            return false
        end
    end
    return true
end
local isCloseAllMac = isDisableAllMACQos()
if isCloseAllMac == false then --存在MACQOS
	--页面过来的
	if data["beforemode"]  ~= nil and data["beforemode"] ~= "equal" and data["currentmode"] == "equal" then
		data["Enable"] = true
		data["SmartQosEnable"] = false
	elseif data["beforemode"]  ~= nil and  data["currentmode"] ~= "equal"  then
		data["Enable"] = true
		data["SmartQosEnable"] = true
	else
		--测速过来的
		local smartEnable = SmartQosvalues["InternetGatewayDevice.QueueManagement."]
		if utils.toboolean(smartEnable["X_SmartQosEnable"]) == false then
			data["Enable"] = true
			data["SmartQosEnable"] = false
		else
			data["Enable"] = true
			data["SmartQosEnable"] = true
		end
	end
elseif data["currentmode"] ~= "equal" then
	data["Enable"] = true
	data["SmartQosEnable"] = true
else
	data["Enable"] = false
	data["SmartQosEnable"] = false
end


function add_one_parameter(paras, name, value)
    table.insert(paras, {name, value})
end
   
--获取但前的状态 如果是从equal切换到其他的模式，或者用户没有输入任何的带宽值，就进入此分支
local upbandwidthresult = 0
local isFinish = true
local errcode,speedvalues = dm.GetParameterValues("InternetGatewayDevice.X_SPEEDTEST.",{"SpeedtestStatus","DownBandwidth","UpBandwidth"})
local speedvaluesObj = speedvalues["InternetGatewayDevice.X_SPEEDTEST."]
if speedvaluesObj then
    isFinish = utils.toboolean(speedvaluesObj["SpeedtestStatus"])
    upbandwidthresult = speedvaluesObj["UpBandwidth"]
end

if data["beforemode"] ~= nil and data["beforemode"] ~= "equal" and data["beforemode"] ~= "game" and data["beforemode"] ~= "internet" and data["beforemode"] ~= "video" then
    utils.appenderror("errcode", 9007)
    return ;
end

if data["currentmode"] ~= nil and data["currentmode"] ~= "equal" and data["currentmode"] ~= "game" and data["currentmode"] ~= "internet" and data["currentmode"] ~= "video" then
    utils.appenderror("errcode", 9007)
    return ;
end

if data["beforemode"] == "equal" and data["currentmode"] ~= "equal" then
    print("before is equal currentmode is not equal")
    --如果之前已经有了测试结果，那么不再测速了
    --没有结果，那么去测速 保存但前的模式
    if upbandwidthresult == 0 and isFinish == false then
        print("go to speedtest....")
        local speeterr = dm.SetParameterValues("InternetGatewayDevice.X_SPEEDTEST.SpeedtestStatus", "1")
        --这个时候把几个节点的信息写进去
        local domain = "InternetGatewayDevice.QueueManagement."
        local paras = {{"X_SmartQosCurrentMode", data["currentmode"]}}
        local errcode =  dm.SetObjToDB(domain, paras)
    end
end
if data["currentmode"] ~= "equal" then
    print("QueueManagement set ok")

    if data["currentmode"] == "game" then
        print("PolicerParas game...............................................")

        local domain = "InternetGatewayDevice.QueueManagement.Policer.2."
        local paras = {{"X_PolicerPrio", 1}}
        local errcode =  dm.SetObjToDB(domain, paras)
        local domain = "InternetGatewayDevice.QueueManagement.Policer.3."
        local paras = {{"X_PolicerPrio", 2}}
        local errcode =  dm.SetObjToDB(domain, paras)
        local domain = "InternetGatewayDevice.QueueManagement.Policer.4."
        local paras = {{"X_PolicerPrio", 3}}
        local errcode =  dm.SetObjToDB(domain, paras)

    elseif data["currentmode"] == "internet" then
        print("PolicerParas internet...............................................")
        local domain = "InternetGatewayDevice.QueueManagement.Policer.2."
        local paras = {{"X_PolicerPrio", 2}}
        local errcode =  dm.SetObjToDB(domain, paras)
        local domain = "InternetGatewayDevice.QueueManagement.Policer.3."
        local paras = {{"X_PolicerPrio", 3}}
        local errcode =  dm.SetObjToDB(domain, paras)
        local domain = "InternetGatewayDevice.QueueManagement.Policer.4."
        local paras = {{"X_PolicerPrio", 1}}
        local errcode =  dm.SetObjToDB(domain, paras)
    elseif data["currentmode"] == "video" then
        print("PolicerParas video...............................................")

        local domain = "InternetGatewayDevice.QueueManagement.Policer.2."
        local paras = {{"X_PolicerPrio", 2}}
        local errcode =  dm.SetObjToDB(domain, paras)
        local domain = "InternetGatewayDevice.QueueManagement.Policer.3."
        local paras = {{"X_PolicerPrio", 1}}
        local errcode =  dm.SetObjToDB(domain, paras)
        local domain = "InternetGatewayDevice.QueueManagement.Policer.4."
        local paras = {{"X_PolicerPrio", 3}}
        local errcode =  dm.SetObjToDB(domain, paras)
    end
end
local paras = {}
add_one_parameter(paras, "InternetGatewayDevice.QueueManagement.X_SmartQosCurrentMode", data["currentmode"])
add_one_parameter(paras, "InternetGatewayDevice.QueueManagement.Enable", data["Enable"])
add_one_parameter(paras, "InternetGatewayDevice.QueueManagement.X_SmartQosEnable", data["SmartQosEnable"])

if speedvaluesObj and utils.toboolean(data["SpeedTestFlag"]) == false then
    if (speedvaluesObj["DownBandwidth"] < 1000 and speedvaluesObj["DownBandwidth"] > 0) or (speedvaluesObj["UpBandwidth"] < 400 and speedvaluesObj["UpBandwidth"] > 0) then
    else
        add_one_parameter(paras, "InternetGatewayDevice.QueueManagement.X_BandWidth", speedvaluesObj["UpBandwidth"])
        add_one_parameter(paras, "InternetGatewayDevice.QueueManagement.DownBandWidth", speedvaluesObj["DownBandwidth"])
    end
else
    if (tonumber(data["inputDownBandWidth"]) < 1000 and tonumber(data["inputDownBandWidth"]) > 0) or (tonumber(data["inputUpBandWidth"]) < 400 and tonumber(data["inputUpBandWidth"]) > 0) then
    else
        add_one_parameter(paras, "InternetGatewayDevice.QueueManagement.X_BandWidth", data["inputUpBandWidth"])
        add_one_parameter(paras, "InternetGatewayDevice.QueueManagement.DownBandWidth", data["inputDownBandWidth"])
    end
end
add_one_parameter(paras, "InternetGatewayDevice.QueueManagement.X_SpeedTestFlag", data["SpeedTestFlag"])

local errcode,needreboot, paramerr = dm.SetParameterValues(paras);
utils.appenderror("errcode", errcode)require('dm')
