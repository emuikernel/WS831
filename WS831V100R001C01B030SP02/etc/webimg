function reTesting(){
$("#guide_layer2down_page_content").hide();
$("#guide_detecting_div").show();
Atp.guideInternetController.load(function(){
if(Atp.guideInternetController.content.AccessStatus == "Up"){
isLineup = true;
utilchangePage("guide_lineup_page");
}else{
setTimeout(function(){
utilchangePage("guide_layer2down_page");
$("#guide_detecting_div").hide();
$("#guide_layer2down_page_content").show();
},2000);
}
});
}
