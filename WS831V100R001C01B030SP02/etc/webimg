local tostring = tostring

local searchWanOnly = false
local res = {}

--we get result from file
local resultfile = "/var/pvcsearch/wanresult"
function parse_line_info(line)
	print(string.len(line))
	if 1 == string.find(line, "ID: ") then
		res.ID = string.sub(line, string.len("ID: ")+1, string.len(line))
	end
	if 1 == string.find(line, "Connectiontype: ") then
		res.ConnectionType = string.sub(line, string.len("Connectiontype: ")+1, string.len(line))
	end
	if 1 == string.find(line, "AccessType: ") then
		res.AccessType = string.sub(line, string.len("AccessType: ")+1, string.len(line))
	end
	if 1 == string.find(line, "WanResult: ") then
		res.WanResult = string.sub(line, string.len("WanResult: ")+1, string.len(line))
	end
	if 1 == string.find(line, "SearchingStatus: ") then
		res.SearchingStatus = string.sub(line, string.len("SearchingStatus: ")+1, string.len(line))
	end
end

function getDetectResultFromfile()
	local fh = nil
	local iCnt = 0
	local iLineCnt = 0
	--max 3.4 seconds
	while iCnt < 17 do
		fh = io.open(resultfile, "r")
		if fh then
			break
		end
		iCnt = iCnt + 1
		sys.usleep(200*1000)
	end
	print("DetectResultFromfile after count", iCnt)
	if nil == fh then
		return false
	end
	line = fh:read("*line")
	while line ~= nil and line ~= "" do
		iLineCnt = iLineCnt + 1
		parse_line_info(line)
		line = fh:read("*line")
	end
	fh:close()
	--read again, we must get all content
	if 5 ~= iLineCnt then
		sys.usleep(100*1000)
		iLineCnt = 0
		fh = io.open(resultfile, "r")
		if fh then
			while line ~= nil and line ~= "" do
				iLineCnt = iLineCnt + 1
				parse_line_info(line)
				line = fh:read("*line")
			end
			fh:close()
		end
	end
	if 5 ~= iLineCnt then
		return false		
	end
    return true
end

function GetPreDetectresult()
	local obj = sys.gmsgget_ex("pvcsearchcms", 0, 300, {})
	if nil == obj then
		print("can not get result from cms")
		if true == getDetectResultFromfile() then
			if true == utils.toboolean(res.SearchingStatus) then
				res.SearchingStatus = "Finished"
			else
				res.SearchingStatus = "Scanning"
			end
			res.AccessStatus = "Up"
			res.AutoFlag = 0
			res.BackupStatus = 1
			res.DNSServers = ""
			res.DefaultGateway = ""
			res.ErrReason = "Success"
			res.ExternalIPAddress = ""
			res.PVCResult = ""
			res.Status = "Connected"
			res.ConnectionStatus = "Connected"
			res.Uptime = 1
			return true
		end
	end
	return false
end

if true == GetPreDetectresult() then
	sys.print(json.encode(res))
else

local errcode,httpstatus = dm.GetParameterValues("InternetGatewayDevice.Services.X_HttpDetect.", {"DetectStatus"});

res.HttpStatus = httpstatus["InternetGatewayDevice.Services.X_HttpDetect."]["DetectStatus"]

