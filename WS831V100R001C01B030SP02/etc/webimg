require('utils')

local timetype = ""
if FormData and FormData["type"] ~= nil then
	timetype = FormData["type"]
end

local weekday = {
	[0]="Daily", "Monday", "Tuesday", "Wednesday", "Thursday", "Friday", "Saturday", "Sunday"
}

function StringSplit(sourString, szSep)
	local nStartIndex = 1
	local first = ""
	local second = ""
	while true do
	   local nLastIndex = string.find(sourString, szSep, nStartIndex)
	   if not nLastIndex then
	    second = string.sub(sourString, nStartIndex, string.len(sourString))
	    break
	   end
	   first = string.sub(sourString, nStartIndex, nLastIndex - 1)
	   nStartIndex = nLastIndex + string.len(szSep)
	end
	return first,second
end

function setweektime(newObj, key, times)
	local start = 1
	local index = 1
	while true do
		ip, fp = string.find(times, "|", start)
		if not ip then
			newObj[weekday[index]..key.."Hour"],newObj[weekday[index]..key.."Minute"] = StringSplit(string.sub(times, start),":")
			break
		end
		local mac = {}
		newObj[weekday[index]..key.."Hour"],newObj[weekday[index]..key.."Minute"] = StringSplit(string.sub(times, start,ip-1),":")
		index = index + 1
		start = fp +1
	end
end

function setweekenable(newObj)
	local index = 1
	while index < 8 do
		if newObj[weekday[index].."FromHour"] == "00" and newObj[weekday[index].."FromMinute"] == "00" and newObj[weekday[index].."ToHour"] == "00" and newObj[weekday[index].."ToMinute"] == "00" then
			newObj[weekday[index].."enable"] = false
			newObj[weekday[index].."FromHour"] = "08"
			newObj[weekday[index].."FromMinute"] = "00"
			newObj[weekday[index].."ToHour"] = "23"
			newObj[weekday[index].."ToMinute"] = "00"
		else
			newObj[weekday[index].."enable"] = true
		end
		index = index + 1
	end
end

function parsetime(newObj, dmObj)
	if dmObj["TimeMode"] == 0 then
		for k, v in pairs(weekday) do
			newObj[v.."FromHour"],newObj[v.."FromMinute"] = StringSplit(dmObj["WeekTimeStart"],":")
			newObj[v.."ToHour"],newObj[v.."ToMinute"]  = StringSplit(dmObj["WeekTimeEnd"],":")
		end
	else
		local from = dmObj["WeekTimeStart"]
		local to = dmObj["WeekTimeEnd"]
		setweektime(newObj, "From", from)
		setweektime(newObj, "To", to)
		newObj["DailyFromHour"] = "08"
		newObj["DailyFromMinute"] = "00"
		newObj["DailyToHour"] = "23"
		newObj["DailyToMinute"] = "00"
	end
end

local errcode, objs = dm.GetParameterValues("InternetGatewayDevice.X_FireWall.TimeRule.{i}.", 
    {"Enable", "TimeMode", "WeekTimeStart", "WeekTimeEnd", "DevMac"});

local timefilter = {}
local activetimefilter = {}

if objs ~= nil then
	for k,v in pairs(objs) do
	    local newObj = {}
	    newObj["ID"] = k
	    newObj["Enable"] = utils.toboolean(v["Enable"])
	    newObj["TimeMode"] = v["TimeMode"]
	    newObj["Device"] = v["DevMac"]
	    parsetime(newObj, v)
	    setweekenable(newObj)

	    local index = 1
		while index < 8 do
			if newObj[weekday[index].."enable"] then
				newObj["fromhour"] = newObj[weekday[index].."FromHour"]
				newObj["fromminute"] = newObj[weekday[index].."FromMinute"]
				newObj["tohour"] = newObj[weekday[index].."ToHour"]
				newObj["tominute"] = newObj[weekday[index].."ToMinute"]
				break
			end
			index = index + 1
		end

	    if timetype ~= "" and timetype == v["DevMac"] then
			table.insert(activetimefilter, newObj)
	    else
			table.insert(timefilter, newObj)
		end
	end
end

if timetype ~="" then
	utils.multiObjSortByID(activetimefilter)
	sys.print(json.encode(activetimefilter))
else
	table.sort(timefilter,function(x,y) return x.Device < y.Device end)
	sys.print(json.encode(timefilter))
endlocal utils = require('utils')
require('dm')
local string = string

local maps = {
	Enable = "Enable",
	TimeMode = "TimeMode",
	Device = "DevMac",
	WeekTimeStart = "WeekTimeStart",
	WeekTimeEnd = "WeekTimeEnd"
}

local weekday = {
	[0]="Daily", "Monday", "Tuesday", "Wednesday", "Thursday", "Friday", "Saturday", "Sunday"
}

function InitWeekDaytimes()
	if tonumber(data["TimeMode"]) == 0 then
		data["TimeMode"] = 0
		data["WeekTimeStart"] = data["DailyFromHour"]..":"..data["DailyFromMinute"]
		data["WeekTimeEnd"] = data["DailyToHour"]..":"..data["DailyToMinute"]
	else
		data["TimeMode"] = 1
		if data["Mondayenable"] == false then
			data[weekday[1]..'FromHour'] = "00"
			data[weekday[1]..'FromMinute'] = "00"
			data[weekday[1]..'ToHour'] = "00"
			data[weekday[1]..'ToMinute'] = "00"
		end
		if data["Tuesdayenable"] == false then
			data[weekday[2]..'FromHour'] = "00"
			data[weekday[2]..'FromMinute'] = "00"
			data[weekday[2]..'ToHour'] = "00"
			data[weekday[2]..'ToMinute'] = "00"
		end
		if data["Wednesdayenable"] == false then
			data[weekday[3]..'FromHour'] = "00"
			data[weekday[3]..'FromMinute'] = "00"
			data[weekday[3]..'ToHour'] = "00"
			data[weekday[3]..'ToMinute'] = "00"
		end
		if data["Thursdayenable"] == false then
			data[weekday[4]..'FromHour'] = "00"
			data[weekday[4]..'FromMinute'] = "00"
			data[weekday[4]..'ToHour'] = "00"
			data[weekday[4]..'ToMinute'] = "00"
		end
		if data["Fridayenable"] == false then
			data[weekday[5]..'FromHour'] = "00"
			data[weekday[5]..'FromMinute'] = "00"
			data[weekday[5]..'ToHour'] = "00"
			data[weekday[5]..'ToMinute'] = "00"
		end
		if data["Saturdayenable"] == false then
			data[weekday[6]..'FromHour'] = "00"
			data[weekday[6]..'FromMinute'] = "00"
			data[weekday[6]..'ToHour'] = "00"
			data[weekday[6]..'ToMinute'] = "00"
		end
		if data["Sundayenable"] == false then
			data[weekday[7]..'FromHour'] = "00"
			data[weekday[7]..'FromMinute'] = "00"
			data[weekday[7]..'ToHour'] = "00"
			data[weekday[7]..'ToMinute'] = "00"
		end
	end
end

function StringSplit(sourString, szSep)
	local nStartIndex = 1
	local first = ""
	local second = ""
	while true do
	   local nLastIndex = string.find(sourString, szSep, nStartIndex)
	   if not nLastIndex then
	    second = string.sub(sourString, nStartIndex, string.len(sourString))
	    break
	   end
	   first = string.sub(sourString, nStartIndex, nLastIndex - 1)
	   nStartIndex = nLastIndex + string.len(szSep)
	end
	return first,second
end

function createtime()
	if tonumber(data["TimeMode"]) == 0 then
		data["TimeMode"] = 0
		data["WeekTimeStart"] = data["DailyFromHour"]..":"..data["DailyFromMinute"]
		data["WeekTimeEnd"] = data["DailyToHour"]..":"..data["DailyToMinute"]
	else
		data["TimeMode"] = 1
		InitWeekDaytimes()
		if data["Mondayenable"]then
			data[weekday[1]..'FromHour'] = data["fromhour"]
			data[weekday[1]..'FromMinute'] = data["fromminute"]
			data[weekday[1]..'ToHour'] = data["tohour"]
			data[weekday[1]..'ToMinute'] = data["tominute"]
		end
		if data["Tuesdayenable"] then
			data[weekday[2]..'FromHour'] = data["fromhour"]
			data[weekday[2]..'FromMinute'] = data["fromminute"]
			data[weekday[2]..'ToHour'] = data["tohour"]
			data[weekday[2]..'ToMinute'] = data["tominute"]
		end
		if data["Wednesdayenable"] then
			data[weekday[3]..'FromHour'] = data["fromhour"]
			data[weekday[3]..'FromMinute'] = data["fromminute"]
			data[weekday[3]..'ToHour'] = data["tohour"]
			data[weekday[3]..'ToMinute'] = data["tominute"]
		end
		if data["Thursdayenable"] then
			data[weekday[4]..'FromHour'] = data["fromhour"]
			data[weekday[4]..'FromMinute'] = data["fromminute"]
			data[weekday[4]..'ToHour'] = data["tohour"]
			data[weekday[4]..'ToMinute'] = data["tominute"]
		end
		if data["Fridayenable"] then
			data[weekday[5]..'FromHour'] = data["fromhour"]
			data[weekday[5]..'FromMinute'] = data["fromminute"]
			data[weekday[5]..'ToHour'] = data["tohour"]
			data[weekday[5]..'ToMinute'] = data["tominute"]
		end
		if data["Saturdayenable"] then
			data[weekday[6]..'FromHour'] = data["fromhour"]
			data[weekday[6]..'FromMinute'] = data["fromminute"]
			data[weekday[6]..'ToHour'] = data["tohour"]
			data[weekday[6]..'ToMinute'] = data["tominute"]
		end
		if data["Sundayenable"] then
			data[weekday[7]..'FromHour'] = data["fromhour"]
			data[weekday[7]..'FromMinute'] = data["fromminute"]
			data[weekday[7]..'ToHour'] = data["tohour"]
			data[weekday[7]..'ToMinute'] = data["tominute"]
		end
		data["WeekTimeStart"] = data["MondayFromHour"]..":"..data["MondayFromMinute"]
		data["WeekTimeEnd"] = data["MondayToHour"]..":"..data["MondayToMinute"]
		for i=2, 7 do	
			data["WeekTimeStart"] = data["WeekTimeStart"].."|"..data[weekday[i]..'FromHour']..":"..data[weekday[i]..'FromMinute']
			data["WeekTimeEnd"] = data["WeekTimeEnd"].."|"..data[weekday[i]..'ToHour']..":"..data[weekday[i]..'ToMinute']
		end
	end
end

function create()
	-- add 
	data["Enable"] = utils.booleantoint(data["Enable"])
	createtime()
	local paras = utils.GenAddObjParamInputs(data, maps)
	print("==== timecontrol create submit data=============")
	for k,v in pairs(paras) do
	    print("=============================")
	    for idx,val in pairs(v) do
	        print(val)
	    end
	end
	local errcode, instnum, NeedReboot, paramerr= dm.AddObjectWithValues("InternetGatewayDevice.X_FireWall.TimeRule.", paras);
	utils.responseErrorcode(errcode, paramerr, maps)
	return errcode
end

function delete()
	print("delete id=",data["ID"])
	return dm.DeleteObject(data["ID"])
end

function update()
	local domain = data["ID"]
	data["Enable"] = utils.booleantoint(data["Enable"])
	createtime()
	local paras = utils.GenSetObjParamInputs(domain, data, maps)

	print("==== timecontrol update submit data=============")
	for k,v in pairs(paras) do
	    print("=============================")
	    for idx,val in pairs(v) do
	        print(val)
	    end
	end
	local errcode, NeedReboot, paramerr = dm.SetParameterValues(paras)
	utils.responseErrorcode(errcode, paramerr, maps)
	return errcode
end

if action == 'create' then
	err = create()
elseif action == 'update' then
	err = update()
elseif action == 'delete' then
	err = delete()
else
	return
end

utils.appenderror("errcode", err)
