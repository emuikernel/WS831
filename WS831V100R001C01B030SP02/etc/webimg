Atp.AccountController=Atp.MultiObjController.create({objName:"system/useraccount",contentType:"application/json;charset=UTF-8;enc",afterLoad:function(){var b=this.content;for(var a=0;a<b.length;a++){if(b[a]["userlevel"]==2){this.set("activeItem",null);this.set("activeItem",b[a]);this.activeItem.set("curpwd","");this.activeItem.set("newpwd","");this.activeItem.set("confirmpwd","")}}},checkProc:function(a){if(this.activeItem.curpwd==""){showErr("useraccount_view_data_edit_useraccount_curpwd_input_error_id","useraccount_view_data_edit_useraccount_curpwd_input_error_info",Em.I18n.t("login.notempty",{arg1:Em.I18n.t("useraccount.curpwd")}));return true}$("#useraccount_view_data_edit_useraccount_curpwd_input_error_id").hide();if(this.activeItem.newpwd==""){showErr("useraccount_view_data_edit_useraccount_newpwd_input_error_id","useraccount_view_data_edit_useraccount_newpwd_input_error_info",Em.I18n.t("login.notempty",{arg1:Em.I18n.t("useraccount.newpwd")}));return true}$("#useraccount_view_data_edit_useraccount_newpwd_input_error_id").hide();if((this.activeItem.newpwd.length<6)||(this.activeItem.newpwd.length>32)){showErr("useraccount_view_data_edit_useraccount_newpwd_input_error_id","useraccount_view_data_edit_useraccount_newpwd_input_error_info",Em.I18n.t("useraccount.newpwd")+Em.I18n.t("Menu.lenrange_err",{arg1:"6-32"}));return true}$("#useraccount_view_data_edit_useraccount_newpwd_input_error_id").hide();if(this.activeItem.newpwd!=this.activeItem.confirmpwd){showErr("useraccount_view_data_edit_useraccount_confirmpwd_input_error_id","useraccount_view_data_edit_useraccount_confirmpwd_input_error_info",Em.I18n.t("Menu.pwd_nomatch_err"));return true}$("#useraccount_view_data_edit_useraccount_confirmpwd_input_error_id").hide();return false},postData:function(b,e){if(this.checkProc()){return}var a=this;var c=utilGetCsrf();this.activeItem.set("enableprompt",false);this.activeItem.set("promptinfo","");var d={csrf:c,action:"update",data:a.createSubmitData()};utilStartSubmitDialog();a.post($.toJSON(d),function(f){if(0!=f.errcode){if(typeof f.curpwd!="undefined"){showErr("useraccount_view_data_edit_useraccount_curpwd_input_error_id","useraccount_view_data_edit_useraccount_curpwd_input_error_info",Em.I18n.t(f.curpwd))}utilStopSubmitDialog()}else{window.location.href="/"}})}});Atp.AccountEditView=Em.View.extend({attributeBindings:["style"],style:"margin-bottom:100px;",contentBinding:"Atp.AccountController.activeItem",controller:Atp.AccountController,curpwd:"",newpwd:"",confirmpwd:"",setcurpwd:function(){if(this.get("curpwd")){Atp.AccountController.activeItem.set("curpwd",SHA256(this.get("curpwd")))}Atp.AccountController.activeItem.set("confirmpwd",this.get("confirmpwd"))}.observes("curpwd","confirmpwd"),observe_newpwd:function(){var c=0;if(""==this.get("newpwd")){$("#passwdstrength").hide();return}Atp.AccountController.activeItem.set("newpwd",this.get("newpwd"));var b=this.get("newpwd");if(""==b||b.length<6||b.length>32){$("#passwdstrength").hide();return}var a=8;$("#passwdstrength").show();if(b.split("").reverse().join("")==Atp.AccountController.activeItem.get("username")){c=5}else{if(b.length<a){c=10}else{if(b.match(/[a-z]/)){c+=25}if(b.match(/[A-Z]/)){c+=25}if(b.match(/[0-9]/)){c+=25}if(b.match(/\W/)){c+=25}}}if(c<=35){$("#pwdstrengthinfo").html(Em.I18n.t("index.password.weak"));$("#pwdfirstId").css("background-color","#EC6217");$("#pwdsecondId").css("background-color","#D1D1D1");$("#pwdthirdId").css("background-color","#D1D1D1")}else{if(c>35&&c<=60){$("#pwdstrengthinfo").html(Em.I18n.t("index.password.mid"));$("#pwdfirstId").css("background-color","#FB980E");$("#pwdsecondId").css("background-color","#FB980E");$("#pwdthirdId").css("background-color","#D1D1D1")}else{if(c>60){$("#pwdstrengthinfo").html(Em.I18n.t("index.password.strong"));$("#pwdfirstId").css("background-color","#88DA33");$("#pwdsecondId").css("background-color","#88DA33");$("#pwdthirdId").css("background-color","#88DA33")}}}}.observes("newpwd"),template:Em.Handlebars.compile('         <div class="control-group margintop_10 width_100p" style="margin-top:40px;">             {{ view Atp.LabelView labelID="useraccount.curpwd" isPasswordAndNotShowPassword=true valueBinding="view.curpwd" }}             {{ view Atp.LabelView labelID="useraccount.newpwd" isPasswordAndNotShowPassword=true valueBinding="view.newpwd" }}             <div class="control-group hide" id="passwdstrength">                <div class="control-label">&nbsp;</div>                <div class="controls" style="margin-left:22px;">                    <div style="width:140px;padding-top:12px;">                         <table style="width:100%;height:3px;border-spacing:0px 0px;background-color:transparent;clear:both;" border="0" bordercolor="white" {{bindAttr id="view.tableId"}}><tr>                          <td id="pwdfirstId" style="width:38px;"></td>                          <td style="width:13px;"></td>                          <td id="pwdsecondId" style="width:38px;"></td>                          <td style="width:13px;"></td>                          <td id="pwdthirdId" style="width:38px;"></td></tr></table>                    </div>                    <div style="text-align:left;width:245px;font-size:12px;color:#3F3F3F;margin-top:8px;margin-left:2px;">                        <div id="pwdstrengthinfo"></div>                    </div>                </div>            </div>            {{ view Atp.LabelView labelID="useraccount.confirmpwd" isPasswordAndNotShowPassword=true valueBinding="view.confirmpwd" }}         </div>    ')});Atp.AccountCollapse=Atp.SingleObjView.extend({prefixName:"useraccount",controller:Atp.AccountController,EditView:Atp.AccountEditView});
function generateSecondMenu(a){var d="";var f=a.length;for(var b=0;b<f;b++){var c=a[b];var e="ic-menu_point_gray";if(!c.show){continue}d+='<div id="'+c.ID+'_menuId" class="menucolordiv accordion-toggle" style="height:50px;text-align:left;" onclick="Atp.MenuController.clickSubmenu(\''+c.ID+"');\">";e=(b==0&&(Atp.MenuController.activeSubmenu.ID==c.ID))?"ic-menu_point_blue":"ic-menu_point_gray";d+='<div style="padding-top:18px;padding-bottom:18px;padding-left:25px;font-size:14px;"><div id="'+c.ID+'_pointid" class="'+e+'" style="float:left;width:20px;margin-top:5px;"></div><div style="float:left;">'+Em.I18n.t("menu.config."+c.ID)+"</div></div></div>"}return d}Atp.SubmenuModel=Em.Object.extend({view:null});Atp.MenuModel=Em.Object.extend({});function ReloadPage(){var e=window.location.hash;if(e==""){return}if(e.indexOf("?")>=0){e=e.substring(0,e.indexOf("?"))}var c=Atp.MenuController.activeMenu.Submenus;var d=0;for(var b=0;b<c.length;b++){if(e=="#"+c[b].ID){d=1;break}}if(d==0){Ember.Logger.log("ReloadPage......",e);var a=$.cookie("activeSubmenuID");Atp.MenuController.hidePage();Atp.MenuController.loadUrl();Atp.MenuController.SetActiveMenuCookie();Atp.MenuController.SetActiveSubMenuCookie();if(e!="#"+a){Atp.MenuController.clickTopMenu($.cookie("activeMenuID"))}Atp.MenuController.showPage()}else{Ember.Logger.log("changeSubPage:",e.substring(1,e.length));Atp.MenuController.changeSubPage(e.substring(1,e.length));Atp.MenuController.SetActiveSubMenuCookie()}}function CheckHashExist(c){var b=Atp.MenuController.content;if(b==null){return false}var a=c.substring(c.indexOf("#")+1,c.length);if(a.indexOf("?")>=0){a=a.substring(0,a.indexOf("?"))}for(var e=0;e<b.length;e++){if(b[e].Page.indexOf("advance.html")>=0){for(var d=0;d<b[e].Submenus.length;d++){if(a==b[e].Submenus[d].ID){return true}}}}return false}var oldHref=window.location.href;function ListenHashChange(){var a=window.location.href;if(oldHref!==a){if(CheckHashExist(a)){oldHref=a;utilStopSubmitDialog();ReloadPage()}}}functio