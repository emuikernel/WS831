    other.guide = utils.toboolean(restoreobj["InternetGatewayDevice.UserInterface.X_Web."]["ChannelWanGuide"]) 
end

---------It's for APP only, if any user has login, we will not go to the guide page
other.FirstLogin = 1
local errcode,values = dm.GetParameterValues("InternetGatewayDevice.UserInterface.X_Web.UserInfo.{i}.",
    {
        "X_IsFirst"
    }
);

if nil ~= values then
    local IsNotFirstLogin = 0
    for k, v in pairs(values) do
        if 1 == v["X_IsFirst"] then 
            IsNotFirstLogin = 1
        end
    end

    if 0 == IsNotFirstLogin then
        other.FirstLogin = 0
    end
end


other.IsNeedSalt = false
other.IsNeedSalt = true

Device.other = other
