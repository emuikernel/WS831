require('utils')
local dhcpmaps = {
    FristIP = "IPInterfaceIPAddress",
    MinIP = "MinAddress",
    MaxIP = "MaxAddress",
    DevName = "DeviceName",
    DomainName = "DomainName"
}
local paras = {}

function add_one_parameter(paras, name, value)
    table.insert(paras, {name, value})
end

add_one_parameter(paras,data["ID"].."IPInterfaceIPAddress",data["FristIP"])
local err,needreboot, paramerror = dm.SetParameterValues(paras)
add_one_parameter(paras,data["dhcpID"].."MinAddress",data["MinIP"])
add_one_parameter(paras,data["dhcpID"].."MaxAddress",data["MaxIP"])
add_one_parameter(paras,data["dhcpID"].."DeviceName ",data["DevName"])
add_one_parameter(paras,data["dhcpID"].."DomainName",data["DomainName"])

print("==== submit lan data=============")
for k,v in pairs(paras) do
    print("=============================")
    for idx,val in pairs(v) do
        print(val)
    end
end

local err,needreboot, paramerror = dm.SetParameterValues(paras)
    sys.sleep(4)
if nil ~= data["needReboot"] and data["needReboot"] == true then
	sys.rebootdevice()
end
