local tostring = tostring

function add_parameter(paras, name, value)
    if nil == value then 
        return
    end
    table.insert(paras, {name, value})
end

local paras = {}

add_parameter(paras, "InternetGatewayDevice.X_WiFi.Radio.1.WLANMode","4")

print("pc set ChannelWanGuide")
add_parameter(paras, "InternetGatewayDevice.UserInterface.X_Web.ChannelWanGuide", "1")

local errcode, NeedReboot, paramerr = dm.SetParameterValues(paras)

print("set password...",errcode)
if errcode == 0 then
    print("set password success then delete iptables and ebtables")
    sys.exec("iptables -D FORWARD -j DROP")
    sys.exec("ebtables -D FORWARD 1 -o !br+ -j DROP")
    sys.exec("ebtables -D FORWARD 1 -i !br+ -j DROP")
    print("iptables and ebtables delete over...")
end

utils.appenderror("errcode", errcode)local dm = require('dm')
local web = require('web')
local json = require('json')
local utils = require('utils')
local sys = require('sys')

local tostring = tostring
local GetParameterValues, toboolean, insert = dm.GetParameterValues, utils.toboolean, table.insert

local wlanobj = {}

local wlanmode = 0

local numbers = 0
local obj = sys.gmsgget('wlancms', 2, {})
numbers = obj.number

local errcode,wifiConf = GetParameterValues("InternetGatewayDevice.X_WiFi.Radio.{i}.",{
     "WLANMode",
    "Enable","OperatingFrequencyBand"})
function get_WifiEnable(frequency)
    for k, v in pairs(wifiConf) do
        if frequency == v["OperatingFrequencyBand"] then
            wlanmode = v["WLANMode"]
            return toboolean(v["Enable"])
        end
    end
    return false
end

local errcode,WlanSSID = GetParameterValues("InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.",{"SSID","X_OperatingFrequencyBand"})

function calc_min(frequency)
    local array = {}
    local i = 1
    if WlanSSID ~= nil then
        for k,v in pairs(WlanSSID) do
            if frequency == v["X_OperatingFrequencyBand"] then
                local wlan_instance_id, wlan_suffix = utils.findLastPoint(k)
                array[i] = tonumber(wlan_instance_id)
                i = i+1
            end
        end
        table.sort(array)
        if 1 == i  then
            return -1
        else
            return array[1]
        end
    end
    return -1
end

wlan_index = calc_min("2.4GHz")
wlan_5gindex = calc_min("5GHz")

for k,v in pairs(WlanSSID) do
    local wlan_instance_id, wlan_suffix = utils.findLastPoint(k)
    local wlanInstance = {}
    if wlan_index == tonumber(wlan_instance_id) then
        wlanInstance.WifiFrequency = 2
        wlanInstance.WifiEnable = get_WifiEnable(v["X_OperatingFrequencyBand"])
        wlanInstance.WLANMode = wlanmode
        wlanInstance.WifiSsid = v["SSID"]
        wlanInstance.Numbers = numbers
        insert(wlanobj, wlanInstance)
    end
        if wlan_5gindex == tonumber(wlan_instance_id) then
            wlanInstance.WifiFrequency = 5
            wlanInstance.WifiEnable = get_WifiEnable(v["X_OperatingFrequencyBand"])
            wlanInstance.WifiSsid = v["SSID"]
            wlanInstance.Numbers = numbers
            insert(wlanobj, wlanInstance)
        end
end

sys.print(json.encode(wlanobj))
local utils = require('utils')
local print = print

require('dm')
require('json')
require('utils')

local newObj = {}

local errcode, objs = dm.GetParameterValues("InternetGatewayDevice.X_WiFi.Radio.1.",
    {"WLANMode"});

local obj = objs["InternetGatewayDevice.X_WiFi.Radio.1."]
if objs ~= nil then 
    newObj["WLANMode"] = obj["WLANMode"]   
end

local errcode2, statusObj = dm.GetParameterValues("InternetGatewayDevice.LANDevice.1.WLANEndPoint.1.LinkInfo.", 
    {"Status","BSSID"});
if statusObj ~= nil then 
    statusObj = statusObj["InternetGatewayDevice.LANDevice.1.WLANEndPoint.1.LinkInfo."]
end
if statusObj ~= nil and objs["Status"] ~= "Connected" and statusObj["BSSID"] ~= "" then 
	newObj["ConnectState"] = "Connected"
else
	newObj["ConnectState"] = "Disconnected"
end

newObj["HilinkStatus"] = 0
local errcode1, hilinkObj = dm.GetParameterValues("InternetGatewayDevice.X_HiLink.Controller.",
    {"Status"});

if hilinkObj ~= nil then 
	hilinkObj = hilinkObj["InternetGatewayDevice.X_HiLink.Controller."]
end
if hilinkObj ~= nil then 
    if 1 ==  hilinkObj["Status"]  or 3 ==  hilinkObj["Status"]  then 
	newObj["HilinkStatus"] = hilinkObj["Status"] 
    end
end

sys.print(json.encode(newObj))
