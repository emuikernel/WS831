local tonumber = tonumber
local tostring = tostring

local portmappingwan = {}

local errcode,PPPPortMappingwan = dm.GetParameterValues("InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANPPPConnection.{i}.PortMapping.{i}.", 
    {"PortMappingEnabled","PortMappingProtocol","RemoteHost","ExternalPort","ExternalPortEndRange","InternalClient","InternalPort",
        "PortMappingDescription"});

local errcode,IPPortMappingwan = dm.GetParameterValues("InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANIPConnection.{i}.PortMapping.{i}.", 
    {"PortMappingEnabled","PortMappingProtocol","RemoteHost","ExternalPort","ExternalPortEndRange","InternalClient","InternalPort",
    "PortMappingDescription"});

function get_WANConID( PMID )
    local index = string.find(PMID,".PortMapping")
    return string.sub(PMID, 1, index-1)
end

function get_PMWan_data(PMobj)
    if PMobj then
        for k,v in pairs(PMobj) do
            local Obj = {}
            Obj.ID = k
            Obj.WanID = get_WANConID(k)
            Obj.PortMappingDescription = v["PortMappingDescription"]
            Obj.PortMappingEnabled = utils.toboolean(v["PortMappingEnabled"])
            Obj.PortMappingProtocol = v["PortMappingProtocol"]
            Obj.RemoteHost = v["RemoteHost"]
            Obj.ExternalPort = v["ExternalPort"]
            Obj.ExternalPortEndRange = v["ExternalPortEndRange"]
            if (0 == Obj.ExternalPortEndRange) and (0 ~= Obj.ExternalPort) then
                Obj.ExternalPortEndRange = Obj.ExternalPort
            end
            Obj.InternalClient = v["InternalClient"] 
            Obj.InternalPort = v["InternalPort"]

            table.insert(portmappingwan, Obj)
        end
    end
end
get_PMWan_data(PPPPortMappingwan)
get_PMWan_data(IPPortMappingwan)

utils.multiObjSortByID(portmappingwan)
sys.print(json.encode(portmappingwan))local utils = require('utils')
require('dm')
local print = print

local pm_maps = {
    PortMappingDescription = "PortMappingDescription",
    PortMappingEnabled = "PortMappingEnabled",
    PortMappingProtocol = "PortMappingProtocol",
    RemoteHost = "RemoteHost",
    InternalClient = "InternalClient",
    ExternalPort = "ExternalPort",
    ExternalPortEndRange = "ExternalPortEndRange",
        InternalPort = "InternalPort"
}

function update_portpara()
    if nil ~= data["ExternalPort"] then
        data["ExternalPort"] = math.floor(data["ExternalPort"])
    end

    if nil ~= data["ExternalPortEndRange"] then
        data["ExternalPortEndRange"] = math.floor(data["ExternalPortEndRange"])
    end

    if nil ~= data["InternalPort"] then
        data["InternalPort"] = math.floor(data["InternalPort"])
    end


    if (data["ExternalPort"] == data["ExternalPortEndRange"]) and (0 ~= data["ExternalPort"]) then
        data["ExternalPortEndRange"] = 0
    end

end

function create()
    update_portpara()
    local paras = {
        {"PortMappingDescription", data["PortMappingDescription"]},
        {"PortMappingEnabled", utils.booleantoint(data["PortMappingEnabled"])},
        {"PortMappingProtocol", data["PortMappingProtocol"]},
        {"RemoteHost", data["RemoteHost"]},
        {"InternalClient", data["InternalClient"]},
        {"ExternalPort", data["ExternalPort"]},
        {"ExternalPortEndRange", data["ExternalPortEndRange"]},
        {"InternalPort", data["InternalPort"]}
    }

    local errcode, instnum, NeedReboot, errs = dm.AddObjectWithValues(data["WanID"]..".PortMapping.", paras);

    if errcode ~= 0 then
        utils.responseErrorcode(errcode, errs, pm_maps)
        return errcode
    end
    utils.appenderror("errcode", 0)
    return errcode
end

function delete()
    return dm.DeleteObject(data["ID"])
end

function update()
    update_portpara()
    local domain = data["ID"]

    local param = utils.GenSetObjParamInputs(domain, data, pm_maps)
    local err,needreboot, paramerror = dm.SetParameterValues(param);
    if err ~= 0 then
        utils.responseErrorcode(err, paramerror, pm_maps)
    end
    return err
end

if action == 'create' then
    err = create()
elseif action == 'update' then
    err = update()
elseif action == 'delete' then
    err = delete()
else
    return
end

utils.appenderror("errcode", err)require('dm')
