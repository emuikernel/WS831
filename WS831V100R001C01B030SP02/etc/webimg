print("************** usb capacity**************** \n")


--local errcode,value = usb.getCapacity()
local account_data = {}
local sd_capacity = {}
--datacard
--usb--
local errcode,usbPhysicalStorage = dm.GetParameterValues("InternetGatewayDevice.Services.StorageService.{i}.PhysicalMedium.{i}.", {"Name","Capacity"})

if usbPhysicalStorage ~= nil then
    local errcode,usbStorage = dm.GetParameterValues("InternetGatewayDevice.Services.StorageService.{i}.LogicalVolume.{i}.", {"PhysicalReference","UsedSpace"})
    if usbStorage ~= nil then
        local totalCapacity = 0
        local usedSpace = 0    
        for pk,pv in pairs(usbPhysicalStorage) do   
            totalCapacity = totalCapacity + pv["Capacity"] * 1024 * 1024
            local used = 0;
            for k,v in pairs(usbStorage) do
                if v["PhysicalReference"].."." == pk then
                    used = used + v["UsedSpace"]
                end
            end
            usedSpace = usedSpace + used * 1024 * 1024
        end
        account_data.AvailableSize = totalCapacity - usedSpace
        account_data.TotalSize = totalCapacity
        sd_capacity.sdcapacity = account_data
    end
end
sys.print(json.encode(sd_capacity))
require('json')
