$('#wan_wizard_page_connectiontype').bind("pagebeforeshow", function(event, data){
setTimeout(function(){
Atp.WanWizardController.load(function(){
var self = Atp.WanWizardController;
if(self.content.ConnectionStatus == "Connected"){
if(self.content.ConnectionType == "PPP_Routed"){
$("#wan_selectppp>div>div:first").css("color","#29c0c6");
$("#wan_selectppp>div>div:last").css("color","#29c0c6");
$("#wan_selectdhcp>div>div:first").css("color","#242424");
$("#wan_selectdhcp>div>div:last").css("color","#545454");
$("#wan_selectstatic>div>div").css("color","#242424");
}else if(self.content.ConnectionType == "IP_Routed"){
if(self.content.IPv4AddrType == "DHCP"){
$("#wan_selectppp>div>div:first").css("color","#242424");
$("#wan_selectppp>div>div:last").css("color","#545454");
$("#wan_selectdhcp>div>div:first").css("color","#29c0c6");
$("#wan_selectdhcp>div>div:last").css("color","#29c0c6");
$("#wan_selectstatic>div>div").css("color","#242424");
}else{
$("#wan_selectppp>div>div:first").css("color","#242424");
$("#wan_selectppp>div>div:last").css("color","#545454");
$("#wan_selectdhcp>div>div:first").css("color","#242424");
$("#wan_selectdhcp>div>div:last").css("color","#545454");
$("#wan_selectstatic>div>div").css("color","#29c0c6");
}
}
}else{
$("#wan_selectppp>div>div:first").css("color","#242424");
$("#wan_selectppp>div>div:last").css("color","#545454");
$("#wan_selectdhcp>div>div:first").css("color","#242424");
$("#wan_selectdhcp>div>div:last").css("color","#545454");
$("#wan_selectstatic>div>div").css("color","#242424");
}
});
},100);
});
