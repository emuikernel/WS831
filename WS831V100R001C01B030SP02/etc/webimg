local errcode, values = dm.GetParameterValues("InternetGatewayDevice.Services.X_Power.ManualPowerSave.{i}.", 
    {"Enable", "Name", "State", "StartTime", "EndTime"});
print(errcode, values)

for k, v in pairs(values) do
	if v["Name"] == 'led' then
		newObj["LedState"] = utils.toboolean(v["State"])
		newObj["LedEnable"] = utils.toboolean(v["Enable"])
		newObj["LedStartTime"] = v["StartTime"]
		newObj["LedEndTime"]   = v["EndTime"]
		newObj["LedID"]		   = k
	elseif v["Name"] == 'wifi' then
		newObj["WlanState"] = utils.toboolean(v["State"])
		newObj["WlanEnable"] = utils.toboolean(v["Enable"])
		newObj["WlanStartTime"] = v["StartTime"]
		newObj["WlanEndTime"]   = v["EndTime"]
		newObj["WlanID"]        = k
	end
end

