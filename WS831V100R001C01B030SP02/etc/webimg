%2==1){c+='<div class="inline-block unit_vertical_line"></div>'}else{c+='<div class="inline-block unit_line"></div>'}}c+='<div class="inline-block ic_line_right_topo"></div></div>';c+='<div style="width:842px;margin: 0 auto;margin-left:-361px;padding-bottom:30px;overflow:hidden;"><div id="slidePane" style="width:'+(a*120+2)+'px;margin: 0 auto;margin-left:0px;">'}}}}return c}function launchNode(c){var g=$(c).attr("name");for(var h=0;h<nodeList.length;h++){if(nodeList[h].indexOfParentNode==nodeList[g].indexOfParentNode){if(nodeList[h].isLeaf==false){if(h==g){$(c).removeClass("show").addClass("hide")}else{var a="#"+nodeList[h].id;var f=$(a).find(".ic_add_normal_topo");f.removeClass("hide").addClass("show").nextAll().hide()}}}}var b=nodeList[g].childrenSum;var j='<div class="unit_vertical_line" style="margin-top:28px;"></div>';j+=drawChildrenLine(b);for(var d=0;d<nodeList.length;d++){if(nodeList[d].indexOfParentNode==g){j+='<div class="inline-block" style="width:120px;vertical-align: top;" id="'+nodeList[d].id+'">';j+='<div class="'+nodeList[d].iconPath+' ic_tag">';if(nodeList[d].linkQuality=="Low"){var e=Em.I18n.t("topology.warning");if(nodeList[d].iconPath=="ic_ws831_small_normal_topo"){j+='<div class="ic_warning" style="right:-12px;"></div>'}else{if(nodeList[d].iconPath=="ic_conputer_normal_topo"){j+='<div class="ic_warning" style="right:-15px;"></div>'}else{j+='<div class="ic_warning"></div>'}}j+='<div class="tag_detail">'+e+"</div>"}j+='</div><div style="margin-top:20px;font-size: 14px;color:#444;">'+nodeList[d].friendlyName+"</div>";if(nodeList[d].isLeaf==true){j+="</div>"}else{j+='<div class="ic_add_normal_topo" onclick="launchNode(this)" name="'+d+'"></div></div>'}}}j+="</div>";$(c).after(j)}function drawTopology(){var c="";if(levelNodeNum(1)>7){c+='<div id="leftBtn" class="ic_prev_disabled_topo disabled"></div><div id="rightBtn" class="ic_next_topo active"></div>'}c+=drawChildrenLine(levelNodeNum(1));for(var b=0;b<nodeList.length;b++){if(nodeList[b].level==1){c