    "X_WanAlias",
    "ConnectionType", "X_ServiceList", "NATEnabled",
    });

local connections = {}

for k,v in pairs(pppCon) do
    if CheckWan(k, v) then
        local con = {}
        con.ID = string.sub(k, 1, string.len(k)-1)
        con.Name = v["Name"]
        if v["X_WanAlias"] and "" ~= v["X_WanAlias"] then
            con.Name = v["X_WanAlias"]
        end
        table.insert(connections, con)
    end
end

for k,v in pairs(ipCon) do
    if CheckWan(k, v) then
        local con = {}
        con.ID = string.sub(k, 1, string.len(k)-1)
        con.Name = v["Name"]
        if v["X_WanAlias"] and "" ~= v["X_WanAlias"] then
            con.Name = v["X_WanAlias"]
        end
        table.insert(connections, con)
    end
end

local con = {}
con.ID = ""
con.Name = "Auto"
if type ~= "routewan" and type ~= "allwan" and type ~= "tunnelwan" and type ~= "mldwan" and type ~= "spoof" and type ~= "routebalancewan" then
    table.insert(connections, con)
end
