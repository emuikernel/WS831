local maps = {
	ParentControl = "X_ParentControl"
}

function update()
    local domain = data["ID"]
    
    if nil == domain
        return 9003
    end

    local paras = {
		{domain.."X_ParentControl", data["ParentControl"]}}

    local err,needreboot, paramerror = dm.SetParameterValues(paras);
    utils.responseErrorcode(err, paramerror, maps)
    return err
end

if action == 'update' then
    err = update()  
end

utils.appenderror("errcode", err)


