isAgreeProtocal();
if(getCurrentType == 1){
$("#connect_submit_info").text(Em.I18n.get("Touch.wan.wizard_ppp"));
}else if (getCurrentType == 2){
$("#connect_submit_info").text(Em.I18n.get("Touch.wan.wizard_dhcp"));
}else{
$("#connect_submit_info").text(Em.I18n.get("guide.diagnose.scaninfo"));
}
});
$("#mac-choose").click(function(){
$(".choose-more").css("overflow","visible");
if($(".mac-list").hasClass("hide")){
$(".mac-list").removeClass("hide").addClass("show");
}else{
$(".mac-list").removeClass("show").addClass("hide");
}
});
$(".mac-list .mac-item").click(function(){
var cur_item = $(".mac-list .active");
cur_item.removeClass("active");
$(this).addClass("active");
$("#mac-choose").attr("name",$(this).index());
$("#mac-choose").html($(this).html());
$(".mac-list").removeClass("show").addClass("hide");
if($(".mac-list .otherclone").hasClass("active")){
$("#guide_macconet_type_manual").show();
}
else{
$("#guide_macconet_type_manual").hide();
}
});
$("#guide_pppoe_error").click(function(){
if("macclone" == $(".more_detail").parent().attr("name")){
$("#guide_pppoe_error_info").html(Em.I18n.get("guide.macclone.error")+Em.I18n.get("guide.wan.detail"));
$("#guide_pppoe_error_help").html(Em.I18n.get("guide.macclone.error.help"));
$("#guide_pppoe_error_help").toggle();
}
if("userlimit" == $(".more_detail").parent().attr("name")){
$("#guide_pppoe_error_info").html(Em.I18n.get("guide.userlimit.error")+Em.I18n.get("guide.wan.detail"));
$("#guide_pppoe_error_help").html(Em.I18n.get("guide.userlimit.error.help"));
$("#guide_pppoe_error_help").toggle();
}
});
$("#guide_dhcpfinish_page").bind("pagebeforeshow",function(){
isAgreeProtocal();
Atp.WanGuideDetectController.load(function(result){
var self = Atp.WanGuideDetectController;
if("ErrNoAnswer" == result.ErrReason){
$("#connect_result_info").html(Em.I18n.get("guide.noanswer.error"));
}else if("Unknown" == result.ErrReason){
$("#connect_result_info").html(Em.I18n.get("guide.diagnose.unknownerr"));
}else if(false == self.isConnected(result)){
$("#connect_result_info").html(Em.I18n.get("guide.diagnose.dhcpfailed"));
}else{
$("#connect_result_info").html(Em.I18n.get("guide.noanswer.error"));
}
});
});
$("#guide_pppoe_page").bind("pagebeforeshow", function(event, data){
isAgreeProtocal();
getCurrentType = 1;
$("#guide_macconet_manual").attr("placeholder", Em.I18n.get("guide.mac.format"));
$("#mac-choose").html(Em.I18n.get("guide.internet.macconenone"));
$(".mac-list .noclone").html(Em.I18n.get("guide.internet.macconenone"));
$(".mac-list .defaultclone").html(Em.I18n.get("guide.internet.macconecurrent"));
$(".mac-list .otherclone").html(Em.I18n.get("guide.internet.macconemanual"));
setButtonI18n("guide_pppoe_btn", 'guide.btn.next');
Atp.GuideWanController.load(function(){
$("#guide_pppoe_error").hide();
$("#guide_pppoe_error_help").hide();
if(Atp.GuideWanController.isSubmitAccount>=1){
$("#guide_pppoe_error").show();
var err = Atp.WanGuideDetectController.content.ErrReason;
if("ErrNoAnswer" == err){
$("#guide_pppoe_error_info").html(Em.I18n.get("guide.noanswer.error"));
}else if("Unknown" == err){
$("#wan_pppoe_error_info").html(Em.I18n.get("guide.diagnose.unknownerr"));
}else if(("ErrAuthFail" != err) && ("ErrAuthUserLimit" != err) && ("ErrAuthMacLimit" != err)){
$("#guide_pppoe_error_info").html(Em.I18n.get("guide.diagnose.failed"));
}
}else{
$("#guide_pppoe_error").hide();
}
if(Atp.GuideWanController.content){
if(Atp.GuideWanController.content.Username){
$("#guide_pppoe_username").val(Atp.GuideWanController.content.Username);
$("#guide_pppoe_password").val("");
$("#guide_pppoe_password_text").val("");
}
}
Atp.CurrentMacController.load(function(result){
if(false == Atp.GuideWanController.content.MACColoneEnable){
$(".mac-list .noclone").addClass("active");
$("#mac-choose").attr("name",0);
$("#mac-choose").html($(".mac-list .noclone").html());
}else if(result["macaddress"] == Atp.GuideWanController.content.MACColone){
$(".mac-list .defaultclone").addClass("active");
$("#mac-choose").attr("name",1);
$("#mac-choose").html($(".mac-list .defaultclone").html());
}else{
$(".mac-list .otherclone").addClass("active");
$("#mac-choose").attr("name",2);
$("#mac-choose").html($(".mac-list .otherclone").html());
$("#guide_macconet_type_manual").show();
$("#guide_macconet_manual").val(Atp.GuideWanController.content.MACColone);
}
});
});
});
$("#guide_pppoe_form").on("submit",function (e){
$("#guide_pppoe_error").hide();
var $this = $(this);
e.preventDefault();
var data = $this.serializeObject();
console.log("data",data);
if(data){
var max_ppp_password_length = 64;
if(data.guide_pppoe_password.length > max_ppp_password_length){
$("#guide_pppoe_error").show();
$("#guide_pppoe_error_info").html(Em.I18n.get("wan.pppoe.passworderror"));
return;
}
if(0 == $("#mac-choose").attr("name")){
data.mac_clnone_enable = false;
data.guide_macclone_manual = Atp.CurrentMacController.content.macaddress;
}else if(1 == $("#mac-choose").attr("name")){
data.mac_clnone_enable = true;
data.guide_macclone_manual = Atp.CurrentMacController.content.macaddress;
}else{
data.mac_clnone_enable = true;
data.guide_macclone_manual = $("#guide_macconet_manual").val();
if(!isValidMacAddr(data.guide_macclone_manual)){
$("#guide_pppoe_error").show();
$("#guide_pppoe_error_info").html(Em.I18n.get("guide.mac.error"));
return;
}
}
}
getCurrentType = 1;
utilchangePage("guide_submiting_page");
Atp.GuideWanController.submit(0,data, function(){
console.log("ppp account data to detectWanConnected...");
Atp.WanGuideDetectController.detectStatus = 0;
Atp.WanGuideDetectController.load(Atp.WanGuideDetectController.detectWanConnected);
});
});
