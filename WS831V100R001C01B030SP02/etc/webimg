local qosclasses = {}
for k,v in pairs(classVals) do
    if 2 == v["X_Type"] and v["SourceMACAddress"] ~= "" then
        local newClass = {}
        newClass["SourceMACAddress"] = v["SourceMACAddress"]
        newClass["Enable"] = utils.toboolean(v["ClassificationEnable"])
        table.insert(qosclasses, newClass)
    end
end

function isDisableAllMACQos()
    for k, v in pairs(qosclasses) do
        if data["MACAddress"] ~= v["SourceMACAddress"] and v["Enable"] == true then
            return false
        end
    end
    if data["DeviceDownRateEnable"] == true then
        return false
    end
    return true
end

function isSelfMACQos()
    for k, v in pairs(qosclasses) do
        if data["MACAddress"] ~= v["SourceMACAddress"] and v["Enable"] == true then
            return false
        end
    end
    return true
end

function calc_index(instanceId)
    local errcode,policerVals = dm.GetParameterValues("InternetGatewayDevice.QueueManagement.Policer.{i}.", {"MeterType"});
    local array = {}
    i = 1
    if policerVals ~= nil then
        for k,v in pairs(policerVals) do
            local p,e = utils.findLastPoint(k)
            array[i] = tonumber(p)
            i = i+1
        end
    end
    table.sort(array)
    for j=1, i-1 do
        if array[j] == instanceId then
            return j
        end
    end
    return -1
end

-- new class
function create_class()
    local errcode
    local name = nil
    if data["DeviceDownRateEnable"] == false then
        utils.appenderror("errcode", 0)
        utils.appenderror("QosclassID","")
        utils.appenderror("PolicerID","")
        return 0
    end
    name = data["MACAddress"]
    -- add policer
    data["MeterType"] = "TwoRateThreeColor"


    local PlicerNumber = 0;
    local errcode,policervalues = dm.GetParameterValues("InternetGatewayDevice.QueueManagement.Policer.{i}.",{"PolicerEnable"});
    for k,v in pairs(policervalues) do 
        PlicerNumber = PlicerNumber + 1;
    end
    if PlicerNumber == 15 then
        utils.appenderror("errcode",9004)
        return errcode
    end
    local err = dm.SetParameterValues("InternetGatewayDevice.QueueManagement.Enable", "0")
    local paras = utils.GenAddObjParamInputs(data, policerMaps)
    local errcode, newPolicer, NeedReboot, errparams = dm.AddObjectWithValues("InternetGatewayDevice.QueueManagement.Policer.", paras);
    if 0 ~= errcode then
        utils.responseErrorcode(errcode, errparams, policerMaps)
        utils.appenderror("QosclassID","")
        utils.appenderror("PolicerID","")
        return errcode
    end
    local policerIdex = calc_index(newPolicer)
    -- mac class
    paras = {{"ClassificationEnable", data["DeviceDownRateEnable"]}, 
         {"ClassificationName", name},    
         {"X_QosLinkType", 3},
         {"X_ShowFlag", 1},
         {"SourceMACAddress", data["MACAddress"]}, 
         {"X_Type", 2}, 
         {"ClassQueue", -1},
         {"ClassPolicer", policerIdex}}
    --第二条的时候放到上面
    if false == isSelfMACQos() then
        local errcode, newClass, NeedReboot,paramerr = dm.AddObjectWithValues("InternetGatewayDevice.QueueManagement.Classification.", paras);
        if errcode ~= 0 then
            dm.DeleteObject("InternetGatewayDevice.QueueManagement.Policer."..newPolicer)
        end
        local err = dm.SetParameterValues("InternetGatewayDevice.QueueManagement.Enable", "1")
        utils.responseErrorcode(errcode, paramerr, classMaps)
        if errcode == 0 then
            utils.appenderror("QosclassID", "InternetGatewayDevice.QueueManagement.Classification."..newClass..".")
            utils.appenderror("PolicerID", "InternetGatewayDevice.QueueManagement.Policer."..newPolicer..".")
        else
            utils.appenderror("QosclassID", "")
            utils.appenderror("PolicerID", "")
        end

        return errcode
    end

    local errcode, newClass, NeedReboot,paramerr = dm.AddObjectWithValues("InternetGatewayDevice.QueueManagement.Classification.", paras);
    if errcode ~= 0 then
        dm.DeleteObject("InternetGatewayDevice.QueueManagement.Policer."..newPolicer)
        if utils.toboolean(data["DeviceDownRateEnable"]) and isSelfMACQos() then
            local err = dm.SetParameterValues("InternetGatewayDevice.QueueManagement.Enable", "0")
        end
        utils.responseErrorcode(errcode, paramerr, classMaps)
    else
        --第一条的时候放到下面
        if isSelfMACQos() and utils.toboolean(data["DeviceDownRateEnable"]) then
            local err = dm.SetParameterValues("InternetGatewayDevice.QueueManagement.Enable", "1")
        end
        utils.appenderror("errcode", 0)
        utils.appenderror("QosclassID", "InternetGatewayDevice.QueueManagement.Classification."..newClass..".")
        utils.appenderror("PolicerID", "InternetGatewayDevice.QueueManagement.Policer."..newPolicer..".")
    end
    return errcode
end

function delete_class(PolicerID)
    local isAlldisable = isDisableAllMACQos()
    errcode = dm.DeleteObject(PolicerID)
    print("isAlldisable",isAlldisable,objband["X_SmartQosEnable"] )
    --如果macqos全关，smartqos 是关闭的，那么把qos也关闭掉
    if isAlldisable then
        if utils.toboolean(objband["X_SmartQosEnable"]) == false then
            local err = dm.SetParameterValues("InternetGatewayDevice.QueueManagement.Enable", "0")
            print("all mac qos close ,smart qos is close then close qos")
        end
    end
    utils.appenderror("errcode", errcode)
    return errcode
end

function update_class()
    if data["DeviceDownRateEnable"] == false then
        return delete_class(data["PolicerID"])
    end
    -- body     
    local domain 
    local paras
    domain = data["QosclassID"]
    local name = nil
    name = data["MACAddress"]
    -- mac class
    paras = {{domain.."ClassificationEnable", data["DeviceDownRateEnable"]},{domain.."ClassQueue", data["ClassQueue"]},{domain.."ClassificationName", name}};
    local errcode, NeedReboot, paramerr = dm.SetParameterValues(paras)
    utils.responseErrorcode(errcode, paramerr, classMaps)
     
    domain = data["PolicerID"]
    paras = {{domain.."PolicerEnable", data["DeviceDownRateEnable"]}, 
    {domain.."X_DownPeakRate",data["DeviceRPercent"]},
    {domain.."PeakRate",data["DeviceUpRPercent"]},
    {domain.."X_UpRatePercent",data["DeviceMaxUpLoadRate"]},
    {domain.."X_RatePercent",data["DeviceMaxDownLoadRate"]}}
    errcode , NeedReboot, paramerr = dm.SetParameterValues(paras)
    utils.responseErrorcode(errcode, paramerr, policerMaps)
    return errcode
end

if data["QosclassID"] == '' then
    err = create_class()
else
    err = update_class()
end
