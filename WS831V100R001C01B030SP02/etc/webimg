}

function SetWanResultWhenDhcpPvcSearchClosed(wanvalue)
	local OpendhcpPvcSearch = false
	OpendhcpPvcSearch = true
	if false == OpendhcpPvcSearch then
		for k,v in pairs(wanvalue) do
			local start = string.find(k, "WANPPPConnection")
		    if not start then
		        res.WanResult = "2"
		    else
		    	res.WanResult = "1"
		    end
		end
	end    
end

local errcode, values = dm.GetParameterValues(wan, maps)
if values ~= nil then	
	SetWanResultWhenDhcpPvcSearchClosed(values)
	local wanStatusObj = values[wan]
	wanStatusObj.ConnectionType = utils.get_ip_conn_type(wan, wanStatusObj.ConnectionType)
	utils.appendSingleObj(res, wanStatusObj, maps)
end

sys.print(json.encode(res))
end
