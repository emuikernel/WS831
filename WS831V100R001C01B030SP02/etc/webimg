require('utils')
local  sort, insert, GetParameterValues,SetParameterValues = table.sort, table.insert, dm.GetParameterValues,dm.SetParameterValues

local searchtype = "before"
if FormData["type"] ~= nil then
    searchtype = FormData["type"]
end
function getNumber(Rssi)
    local signaltemp = 0
    for i in string.gmatch(Rssi, "%d+") do
        signaltemp = i
    end
    return "-"..signaltemp
end
function Iconpath(Rssi)
    local signaltemp = 0
    for i in string.gmatch(Rssi, "%d+") do
        signaltemp = tonumber(i)
    end

    local signal = 113 - signaltemp
    if signal > 80 then
        return "/res/signal_05.gif"
    end
    if signal > 70 then
        return "/res/signal_04.gif"
    end
    if signal > 60 then
        return "/res/signal_03.gif"
    end
    if signal > 40 then
        return "/res/signal_02.gif"
    end
    if signal > 20 then
        return "/res/signal_01.gif"
    end
    return "/res/signal_00.gif"
end

function compID(a, b)
    local a_id_table = {}
    local b_id_table = {}
    for i in string.gmatch(a.Rssi, "%d+") do
        insert(a_id_table, tonumber(i))
    end
    for i in string.gmatch(b.Rssi, "%d+") do
        insert(b_id_table, tonumber(i))
    end

    local size = 0
    if table.getn(a_id_table) > table.getn(b_id_table) then
        size = table.getn(b_id_table)
    else
        size = table.getn(a_id_table)
    end

    if size < 1 then
        return false
    end
    for i=1,size do
        if a_id_table[i] > b_id_table[i] then
            return false
        elseif a_id_table[i] < b_id_table[i] then
            return true
        end
    end
    return false
end

local errinfo = {}
if searchtype ~= "before" then
    local errcode,wifiRadio = dm.GetParameterValues("InternetGatewayDevice.X_WiFi.Radio.1.",{"Enable"})
    local wifiradioobj = wifiRadio["InternetGatewayDevice.X_WiFi.Radio.1."]
    if false == utils.toboolean(wifiradioobj["Enable"]) then
        errinfo.aperror = "diagnose_wlanaplist.radioerror"
        sys.print(json.encode(errinfo))
        return
    end

    local errcode,wifiobj = dm.GetParameterValues("InternetGatewayDevice.LANDevice.1.WLANConfiguration.1.",{"Enable"})
    local wifiEnableobj = wifiobj["InternetGatewayDevice.LANDevice.1.WLANConfiguration.1."]
    if false == utils.toboolean(wifiEnableobj["Enable"]) then
        errinfo.aperror = "diagnose_wlanaplist.ssiderror"
        sys.print(json.encode(errinfo))
        return 
    end
end

