local errcode,bandvalues = dm.GetParameterValues("InternetGatewayDevice.QueueManagement.",{"X_BandWidth","DownBandWidth","X_SmartQosEnable"});
local objband = bandvalues["InternetGatewayDevice.QueueManagement."]
if objband["DownBandWidth"] == 0  then
   objband["DownBandWidth"] = 100000
end

if objband["X_BandWidth"] == 0  then
   objband["X_BandWidth"] = 100000
end

--data["DeviceRPercent"] = math.floor(data["DeviceMaxDownLoadRate"] * objband["DownBandWidth"] * 0.01)
--data["DeviceUpRPercent"] = math.floor(data["DeviceMaxUpLoadRate"] * objband["X_BandWidth"] * 0.01)
