setButtonI18n("button_wlan_confirm", "Touch.confirm");
$("#confirm_changewifi").show();
});
$("#wlansetting_take_effect_page").bind("pagebeforeshow", function( event, prevPage  ){
var submitdata = getSubmitData();
var ssid2g_str = submitdata.ssid2g;
var ischange = judgedatachange();
$("#wifi_effect2g").html(ssid2g_str);
var ssid5g_str = submitdata.ssid5g;
$("#wifi_effect5g").html(ssid5g_str);
$("#wlan_switch_open").show();
if(getCurrentEnableStatus("wlan2g_wifiopen_img")){
$("#wifi_effect2g").parent().show();
}else{
$("#wifi_effect2g").parent().hide();
}
if (getCurrentEnableStatus("wlan5g_wifiopen_img")) {
$("#wifi_effect5g").parent().show();
}else{
$("#wifi_effect5g").parent().hide();
}
});
function liveFunction(val){
$("#wpakey"+val).live("change",function(){
$("#wpakey_text_"+val).val($("#wpakey"+val).val());
});
$("#wpakey_text_"+val).live("change",function(){
$("#wpakey"+val).val($("#wpakey_text_"+val).val());
});
$("#wpakey"+val).live("keyup",function(){
var str = "********";
if(0 == $("#wpakey"+val).val().indexOf(str)){
if(val.indexOf("2g") >= 0){
$("#wpakey"+val).val($("#wpakey"+val).val().substr(str.length, $("#wpakey"+val).val().length));
}
else{
$("#wpakey"+val).val($("#wpakey"+val).val().substr(str.length, $("#wpakey"+val).val().length));
}
}
$("#wpakey_text_"+val).attr("placeholder", Em.I18n.get("Touch.wifi.passwordinfo.oninput",{arg1:"8~63"}));
$("#wlan_passwordempty_warning").hide();
utilGetPasswordStrength("password_strength_"+val, $("#wpakey"+val).val(), "pwd_strengthinfo_"+val, "pwd_strength_signal_"+val, $("#ssid"+val).val());
});
$("#wpakey_text_"+val).live("keyup",function() {
var str = "********";
if(0 == $("#wpakey_text_"+val).val().indexOf(str)){
if(val.indexOf("2g") >= 0){
$("#wpakey_text_"+val).val($("#wpakey_text_"+val).val().substr(str.length, $("#wpakey_text_"+val).val().length));
}
else{
$("#wpakey_text_"+val).val($("#wpakey_text_"+val).val().substr(str.length, $("#wpakey_text_"+val).val().length));
}
}
$("#wpakey"+val).attr("placeholder", Em.I18n.get("Touch.wifi.passwordinfo.oninput",{arg1:"8~63"}));
$("#wlan_passwordempty_warning").hide();
utilGetPasswordStrength("password_strength_"+val, $("#wpakey_text_"+val).val(), "pwd_strengthinfo_"+val, "pwd_strength_signal_"+val, $("#ssid"+val).val());
});
}
liveFunction("2g");
liveFunction("5g");
function wlanSetOpenCheckBoxOnClick_2g(){
var src = $("#wifisetting2g_open_img").attr("src");
if (src.indexOf("uncheck") >= 0){
$("#wifisetting2g_open_img").attr("src", "/small/res/checked.png");
$("#wpakey_fieldcontain_2g").hide();
$("#wpakey_text_fieldcontain_2g").hide();
$("#password_strength_2g").hide();
$("#password_strength_2g_line").hide();
$("#wlan2g_open_warning").show();
}else{
$("#wifisetting2g_open_img").attr("src", "/small/res/uncheck.png");
$("#wpakey_fieldcontain_2g").show();
$("#password_strength_2g").show();
$("#password_strength_2g_line").show();
$("#wlan2g_open_warning").hide();
}
}
function wlanSetOpenCheckBoxOnClick_5g(){
var src = $("#wifisetting5g_open_img").attr("src");
if (src.indexOf("uncheck") >= 0){
$("#wifisetting5g_open_img").attr("src", "/small/res/checked.png");
$("#wpakey_fieldcontain_5g").hide();
$("#wpakey_text_fieldcontain_5g").hide();
$("#password_strength_5g").hide();
$("#password_strength_5g_line").hide();
$("#wlan5g_open_warning").show();
}else{
$("#wifisetting5g_open_img").attr("src", "/small/res/uncheck.png");
$("#wpakey_fieldcontain_5g").show();
$("#password_strength_5g").show();
$("#password_strength_5g_line").show();
$("#wlan5g_open_warning").hide();
}
}
