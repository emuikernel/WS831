    MeterType = "MeterType",
    DeviceDownRateEnable = "PolicerEnable",
    DeviceMaxDownLoadRate = "X_RatePercent",
    DeviceMaxUpLoadRate = "X_UpRatePercent",
    DeviceRPercent        = "X_DownPeakRate",
    DeviceUpRPercent        = "PeakRate"
}

if (nil == data["DeviceMaxUpLoadRate"]) and (nil ~= data["DeviceMaxDownLoadRate"]) then
    data["DeviceMaxUpLoadRate"] = data["DeviceMaxDownLoadRate"]
