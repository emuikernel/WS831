local utils = require('utils')

print("************** list file**************** \n")

local currentpath = data["CurrentPath"]

if nil == currentpath then
	currentpath = "/"
end

print("currentpath is "..currentpath.."\n")

local dirtbl = {}
local filetbl = {}


local errcode, array = usb.listFiles(currentpath)
print("errcode is  "..errcode.."\n")

function sortFunc(a,b)
	-- body
	local firstLength = string.len(a.Name)
	local secondLength = string.len(b.Name)
	if firstLength ~= secondLength then 
		return firstLength < secondLength
	else
		return a.Name < b.Name
	end
end

-- Type 0: dir 1:file 2:partion
if 0 == errcode then
	if array ~= nil then
	    for i=1,table.getn(array),3 do
	        local element = {}
	        element.Type = array[i]
	        element.Name = array[i+1]
	        element.Size = array[i+2]

	        if element.Type == 0 then
	        	table.insert(dirtbl,element)
	        elseif element.Type == 1 then
	        	table.insert(filetbl,element)
	        elseif element.Type == 2 then
	        	table.insert(dirtbl,element)
	        end 
	    end

	    -- this is not partion
        if 0 ~= table.getn(filetbl) then
        	table.sort(dirtbl, sortFunc)
        	table.sort(filetbl, sortFunc)
        	for i=1,table.getn(filetbl) do
        		table.insert(dirtbl,filetbl[i])
        	end
        else
        	table.sort(dirtbl, sortFunc)
        end
	end
end


utils.appenderror("FileList", dirtbl)require('json')
