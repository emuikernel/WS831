local errcode, wizardwancom = dm.GetParameterValues("InternetGatewayDevice.Services.X_WizardWanSavedConfig.", {"PPPUsername", "PPPPassword"});

function getwizardwanconfigpara(con)
    if nil == wizardwancom then
        return
    end
    local wizardcommvalue = wizardwancom["InternetGatewayDevice.Services.X_WizardWanSavedConfig."]
    if nil == wizardcommvalue then
        return
    end
    if "" == con.Username and "" == con.Password and 1 == con.IsDefault then
        con.Username = wizardcommvalue["PPPUsername"]
        con.Password = wizardcommvalue["PPPPassword"]
    end
end

function get_lower_layer(ID, lowerlayer)
    if nil == lowerlayer or "" == lowerlayer then
        local start = string.find(ID, ".WANPPPConnection.")
        if not start then
            start = string.find(ID, ".WANIPConnection.")
        end
        return string.sub(ID, 0, start)
    end

    return lowerlayer.."."
end

local connections = {}

-- Support three mode:
-- 0: get all wan
-- 1: get all default wan
-- 2: get currently active default wan
local wantype = 0

if FormData and FormData["type"] ~= nil then
    if "alldefault" == FormData["type"] then
        wantype = 1
    elseif "active" == FormData["type"] then
        wantype = 2
    elseif "firstinternet" == FormData["type"] then
        wantype = 3
    else
        return
    end
end

local activeDftWan = ""
local isAuthFail = false  
if 2 == wantype then
activeDftWan = utils.getDefaultWan(pppCon, ipCon, accessdevs)
--[[
]]
end

function get_str_before(str, symbol)
    if str == nil then
        return nil
    end
    local pos = string.find(str, symbol)
    if pos then
        return string.sub(str, 0, pos-1)
    end
    return str
end
local testflag = 0
function wan_get_layer1maxrate(accessdevs, con)
    local UpMaxBitRate = 0
    local DownMaxBitRate = 0
    if ("Ethernet" == con.AccessType) then
        local start_find = string.find(con.ID, "WANConnectionDevice")
        local res_find = string.sub(con.ID, 1, start_find - 1)
        res_find = res_find.."WANCommonInterfaceConfig."
        for kkk,vvv in pairs(accessdevs) do
            if res_find == kkk then
                --bps
                UpMaxBitRate = vvv["Layer1UpstreamMaxBitRate"]
                DownMaxBitRate = vvv["Layer1DownstreamMaxBitRate"]
                break
            end
        end
    end
    return DownMaxBitRate,UpMaxBitRate
end

function wan_GetKByterate_after_compare_layer1rate(RecordByteRate, Layer1BitRate)
    local KByterate = 0
    KByterate = math.floor(RecordByteRate/(1024))
    if 0 ~= Layer1BitRate then
        if KByterate > math.floor(Layer1BitRate/1000*1024/1000/8) then
            KByterate = math.floor(Layer1BitRate/1000*1024/1000/8)
        end
    end
    return KByterate
end
function wan_rate_Split(szFullString, szSeparator)
    local nFindStartIndex = 1
    local nSplitIndex = 1
    local nSplitArray = {}
    local singlerate
    if ((nil == szFullString) or (nil == szSeparator)) then
        return nSplitArray
    end
    while true do
        local nFindLastIndex = string.find(szFullString, szSeparator, nFindStartIndex)
        if not nFindLastIndex then
            singlerate = string.sub(szFullString, nFindStartIndex, string.len(szFullString))
            table.insert(nSplitArray, singlerate)
            break
        end
        singlerate = string.sub(szFullString, nFindStartIndex, nFindLastIndex - 1)
        table.insert(nSplitArray, singlerate)
        nFindStartIndex = nFindLastIndex + string.len(szSeparator)
    end
    return nSplitArray
end
for k,v in pairs(pppCon) do
    local con = {}
    con.ID = k
    con.Name = v["Name"]
    con.Alias = v["X_WanAlias"]
    if  "" == v["X_WanAlias"] then
        con.Alias = v["Name"]
    end     
    if v["X_WanAlias"] and "" ~= v["X_WanAlias"] then
        con.Name = v["X_WanAlias"]
    end
    con.Enable = utils.toboolean(v["Enable"])
    utils.fill_access_info_by_ID(k, con, accessdevs);
    if con.AccessType == "DSL" then
        utils.fill_xdls_linkstatus_by_ID(k, con, atmlink)
    elseif con.AccessType == "VDSL" then
        utils.fill_xdls_linkstatus_by_ID(k, con, ptmlink)
    end

    con.ConnectionType = utils.get_ip_conn_type(k, v["ConnectionType"])
    con.WanType = "PPP_Routed"
    con.LowerLayer = get_lower_layer(k, v["X_LowerLayers"])
    con.IsDefault = v["X_Default"]
    con.Username = v["Username"]
    con.Password = v["Password"]
    getwizardwanconfigpara(con)
    con.Password = _G["defaultPasswd"]
    con.ConnectionStatus = v["ConnectionStatus"]
    con.ServiceList = v["X_ServiceList"]
    con.MRU = v["MaxMRUSize"]
    con.MTU = 1500
    con.MSS = v["X_TCP_MSS"]
    con.NATType = 0
    if utils.toboolean(v["NATEnabled"]) then
        if "full cone" == v["X_NATType"] then
            con.NATType = 2
        else
            con.NATType = 1
        end
    end

    con.PPPDialIpMode = "dynamic"
    con.PPPDialIpAddr = ""
    con.PPPAuthMode = v["PPPAuthenticationProtocol"]
    con.PPPTrigger = v["ConnectionTrigger"]
    con.PPPIdletime = v["IdleDisconnectTime"]
    con.IPv4Enable = true
    con.IPv4AddrType = "DHCP"
    con.IPv4Addr = v["ExternalIPAddress"]
    con.IPv4Mask = "255.255.255.0"
    con.IPv4Gateway = v["DefaultGateway"]
    con.IPv4DnsServers = v["DNSServers"]
    
    con.DNSOverrideAllowed = utils.toboolean(v["DNSOverrideAllowed"])
    if con.DNSOverrideAllowed then
        con.DNSOverrideAllowed = false
    else
        con.DNSOverrideAllowed = true
    end
    con.MACColone = v["MACAddress"]
    con.MACColoneEnable = utils.toboolean(v["MACAddressOverride"])
    con.PPPoEServiceName = v["PPPoEServiceName"]
    con.PPPoEACName     = v["X_ServerACName"]
    if  (v["ConnectionStatus"] == "Connected" and 0 == testflag) then
        sys.gmsgget("wan", 12, {})
        testflag = 1
    end
    --DownBandwidth KBps
    local layer1DownMaxBitRate,layer1UpMaxBitRate = wan_get_layer1maxrate(accessdevs, con)
    if con.ConnectionType ~= nil and "Bridged" == con.ConnectionType then
        con.DownBandwidth = 0
        con.UpBandwidth = 0
    else
        con.DownBandwidth = wan_GetKByterate_after_compare_layer1rate(v["X_NowDownBandwidth"], layer1DownMaxBitRate)
        con.UpBandwidth =  wan_GetKByterate_after_compare_layer1rate(v["X_NowUpBandwidth"], layer1UpMaxBitRate)
    end
    local wan_hisrate = {}
    con.DownBandwidthMax = wan_GetKByterate_after_compare_layer1rate(v["X_DownBandwidthMax"], layer1DownMaxBitRate)
    con.UpBandwidthMax = wan_GetKByterate_after_compare_layer1rate(v["X_UpBandwidthMax"], layer1UpMaxBitRate)    
    con.BandwidthTime = v["X_BandwidthTime"]
    wan_hisrate = wan_rate_Split(v["X_DownBandwidthHistory"], ",")
    con.DownBandwidthHistory = ""    
    for k,v in pairs(wan_hisrate) do
        if con.ConnectionType ~= nil and "Bridged" == con.ConnectionType then
            wan_hisrate[k] = 0
        end
        if ("" == con.DownBandwidthHistory) then
            con.DownBandwidthHistory = con.DownBandwidthHistory..wan_GetKByterate_after_compare_layer1rate(wan_hisrate[k], layer1DownMaxBitRate)
        else
            con.DownBandwidthHistory = con.DownBandwidthHistory..","..wan_GetKByterate_after_compare_layer1rate(wan_hisrate[k], layer1DownMaxBitRate)
        end
    end
    wan_hisrate = wan_rate_Split(v["X_UpBandwidthHistory"], ",")
    con.UpBandwidthHistory = ""
    for k,v in pairs(wan_hisrate) do
        if ("" == con.UpBandwidthHistory) then
            con.UpBandwidthHistory = con.UpBandwidthHistory..wan_GetKByterate_after_compare_layer1rate(wan_hisrate[k], layer1UpMaxBitRate)
        else
            con.UpBandwidthHistory = con.UpBandwidthHistory..","..wan_GetKByterate_after_compare_layer1rate(wan_hisrate[k], layer1UpMaxBitRate)
        end
    end
    if ("" ~= activeDftWan and k == activeDftWan) then
        sys.print(json.encode(con))
        return
    end

    if 3 == wantype then
        local wanstart = string.find(v["X_ServiceList"], "INTERNET")
        if utils.is_same_mode(CurrentMode, con.AccessType) then 
            if wanstart then
                sys.print(json.encode(con))
                return
            end
        end
    end

    if (0 == wantype) or
       (1 == wantype and utils.toboolean(v["X_Default"])) then
        if utils.is_same_mode(CurrentMode, con.AccessType)
         then
        table.insert(connections, con)
        end
    end
end

for k,v in pairs(ipCon) do
    local con = {}
    con.ID = k
    con.Name = v["Name"]
    con.Alias = v["X_WanAlias"]
    if "" == v["X_WanAlias"] then
        con.Alias = v["Name"]
    end     
    if v["X_WanAlias"] and "" ~= v["X_WanAlias"] then
        con.Name = v["X_WanAlias"]
    end
    con.Enable = utils.toboolean(v["Enable"])
    utils.fill_access_info_by_ID(k, con, accessdevs);
    if con.AccessType == "DSL" then
        utils.fill_xdls_linkstatus_by_ID(k, con, atmlink)
    elseif con.AccessType == "VDSL" then
        utils.fill_xdls_linkstatus_by_ID(k, con, ptmlink)
    end
    con.ConnectionType = utils.get_ip_conn_type(k, v["ConnectionType"])
    con.LowerLayer = get_lower_layer(k, v["X_LowerLayers"])
    con.IsDefault = v["X_Default"]
    con.Username = ""
    con.Password = ""
    getwizardwanconfigpara(con)
    con.Password = _G["defaultPasswd"]
    con.PPPoEServiceName = ""
    con.ConnectionStatus = v["ConnectionStatus"]
    con.APN = ""
    con.DialNum = ""
    con.ServiceList = v["X_ServiceList"]
    con.MRU = 1492
    con.MTU = v["MaxMTUSize"]
    con.MSS = v["X_TCP_MSS"]
    con.NATType = 0
    if utils.toboolean(v["NATEnabled"]) then
        if "full cone" == v["X_NATType"] then
            con.NATType = 2
        else
            con.NATType = 1
        end
    end
    con.PPPDialIpMode = "dynamic"
    con.PPPDialIpAddr = ""
    con.PPPAuthMode = "AUTO"
    con.PPPTrigger = "AlwaysOn"
    con.PPPIdletime = 60
    con.IPv4Enable = true
    con.IPv4AddrType = v["AddressingType"]
    if v["AddressingType"] == "Static" then
        con.WanType = "Static"
    else
        con.WanType = "IP_Routed"
    end
    con.IPv4Addr = v["ExternalIPAddress"]
    if v["SubnetMask"] == "" then
        con.IPv4Mask = "255.255.255.0"
    else
        con.IPv4Mask = v["SubnetMask"]
    end
    con.IPv4Gateway = v["DefaultGateway"]
    con.IPv4DnsServers = v["DNSServers"]
    con.DNSOverrideAllowed = utils.toboolean(v["DNSOverrideAllowed"])

    if con.DNSOverrideAllowed then
        con.DNSOverrideAllowed = false
    else
        con.DNSOverrideAllowed = true
    end
    con.WANDHCPOption60 = v["X_WANDHCPOption60"]
    con.MACColone = v["MACAddress"]
    con.MACColoneEnable = utils.toboolean(v["MACAddressOverride"])
    if  (v["ConnectionStatus"] == "Connected" and 0 == testflag) then
        sys.gmsgget("wan", 12, {})
        testflag = 1
    end
    --DownBandwidth KBps
    local layer1DownMaxBitRate,layer1UpMaxBitRate = wan_get_layer1maxrate(accessdevs, con)
    if con.ConnectionType ~= nil and "Bridged" == con.ConnectionType then
        con.DownBandwidth = 0
        con.UpBandwidth = 0
    else
        con.DownBandwidth = wan_GetKByterate_after_compare_layer1rate(v["X_NowDownBandwidth"], layer1DownMaxBitRate)
        con.UpBandwidth =  wan_GetKByterate_after_compare_layer1rate(v["X_NowUpBandwidth"], layer1UpMaxBitRate)
    end
    local wan_hisrate = {}
    con.DownBandwidthMax = wan_GetKByterate_after_compare_layer1rate(v["X_DownBandwidthMax"], layer1DownMaxBitRate)
    con.UpBandwidthMax = wan_GetKByterate_after_compare_layer1rate(v["X_UpBandwidthMax"], layer1UpMaxBitRate)    
    con.BandwidthTime = v["X_BandwidthTime"]
    wan_hisrate = wan_rate_Split(v["X_DownBandwidthHistory"], ",")
    con.DownBandwidthHistory = ""
   
    for k,v in pairs(wan_hisrate) do
        if con.ConnectionType ~= nil and "Bridged" == con.ConnectionType then
            wan_hisrate[k] = 0
        end
        if ("" == con.DownBandwidthHistory) then
            con.DownBandwidthHistory = con.DownBandwidthHistory..wan_GetKByterate_after_compare_layer1rate(wan_hisrate[k], layer1DownMaxBitRate)
        else
            con.DownBandwidthHistory = con.DownBandwidthHistory..","..wan_GetKByterate_after_compare_layer1rate(wan_hisrate[k], layer1DownMaxBitRate)
        end
    end

    wan_hisrate = wan_rate_Split(v["X_UpBandwidthHistory"], ",")
    con.UpBandwidthHistory = ""
    for k,v in pairs(wan_hisrate) do
        if ("" == con.UpBandwidthHistory) then
            con.UpBandwidthHistory = con.UpBandwidthHistory..wan_GetKByterate_after_compare_layer1rate(wan_hisrate[k], layer1UpMaxBitRate)
        else
            con.UpBandwidthHistory = con.UpBandwidthHistory..","..wan_GetKByterate_after_compare_layer1rate(wan_hisrate[k], layer1UpMaxBitRate)
        end
    end
    if ("" ~= activeDftWan and k == activeDftWan) then
        sys.print(json.encode(con))
        return
    end

    if 3 == wantype then
        local wanstart = string.find(v["X_ServiceList"], "INTERNET")
        if utils.is_same_mode(CurrentMode, con.AccessType) then
            if wanstart then
                sys.print(json.encode(con))
                return
            end
        end
    end

    if (0 == wantype) or
       (1 == wantype and utils.toboolean(v["X_Default"])) then

        if utils.is_same_mode(CurrentMode, con.AccessType)
         then
        table.insert(connections, con)
        end
    end
end

utils.multiObjSortByID(connections)
sys.print(json.encode(connections))
local utils = require('utils')
local sys = require('sys')
local tostring = tostring

if nil ~= data["Password"] then
    if true == utils.toboolean(web.getparaenc()) then
        local parapass = web.decodepara(data["Password"])
        if "" ~= parapass then
            data["Password"] = parapass
        end
    end
end

if nil ~= data["Username"] and nil ~= data["EncodeUsername"] and true == data["EncodeUsername"] then
	data["Username"] = web.decodeURIComponent(data["Username"])
end

if nil ~= data["PPPoEServiceName"] and nil ~= data["EncodePPPoEServiceName"] and true == data["EncodePPPoEServiceName"] then
	data["PPPoEServiceName"] = web.decodeURIComponent(data["PPPoEServiceName"])
end


if nil ~= data['DNSOverrideAllowed'] then
	data['DNSOverrideAllowed'] = utils.toboolean(data['DNSOverrideAllowed'])
	if true == data['DNSOverrideAllowed'] then
		data['DNSOverrideAllowed'] = false
	else
		data['DNSOverrideAllowed'] = true
		if 'Static' ~= data['IPv4AddrType'] then
			data['IPv4DnsServers'] = ""
		end
	end
else
	data['DNSOverrideAllowed'] = true;
end

local errcode, wizardwancom = dm.GetParameterValues("InternetGatewayDevice.Services.X_WizardWanSavedConfig.", {"PPPUsername", "PPPPassword"});

function setwizardwanconfigpara(wandata)
    if nil == wizardwancom then
        return
    end
    local wizardcommvalue = wizardwancom["InternetGatewayDevice.Services.X_WizardWanSavedConfig."]
    if nil == wizardcommvalue then
        return
    end
    if ("" == wizardcommvalue["PPPUsername"]) and ("" == wizardcommvalue["PPPPassword"]) then
        return
    end

    if (_G["defaultPasswd"] == wandata.Password) and (1 == wandata.IsDefault) then
        wandata.Password = wizardcommvalue["PPPPassword"]
    end
end

