<script language="JavaScript" type="text/javascript">\
function pageload(){\
Atp.ContentInternetController.load(function(){\
var wlanmode  = Atp.WlanModeController.content.get("WLANMode");\
var connectStatus = Atp.ContentInternetController.content;\
if(wlanmode != 3  || (connectStatus.get("ConnectionStatus") == "Connected") && (connectStatus.get("AccessType") != "WIFI")){\
Atp.WizardUsbController.load();\
Atp.WlanObjs.load();\
Atp.HostsController.load(function(){\
Atp.WanErrorController.load(function(){\
Atp.ContentInternetController.load(function(){\
Atp.WlanRadioController.load();\
Atp.DeviceInfoController.load();\
Atp.WanDiagnoseController.load();\
Atp.HotaVersionController.load();\
});\
});\
});\
}\
else{\
Atp.HostsController.load(function(){\
Atp.RepeaterStateController.load();\
});\
}\
});\
}\
$(document).ready(function() {\
pageload();\
});\
</script>\
');
