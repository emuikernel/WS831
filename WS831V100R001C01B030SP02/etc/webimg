$("#diagnose_page_authfail").bind("pagebeforeshow", function( event, prevPage ){
setButtonI18n("button_diagnose_pppoe", "Touch.connect");
$("#diagnose_pppoe_text_fieldcontain").hide();
$("#diagnose_pppoe_password_fildcontain").show();
$("#diagnose_pppoe_error").hide();
$("#diagnose_pppoe_error_help").hide();
$("#diagnose_macconet_manual").attr("placeholder", Em.I18n.get("guide.mac.format"));
$("#diagnose_mac-choose").html(Em.I18n.get("guide.internet.macconenone"));
$(".mac-list .noclone").html(Em.I18n.get("guide.internet.macconenone"));
$(".mac-list .defaultclone").html(Em.I18n.get("guide.internet.macconecurrent"));
$(".mac-list .otherclone").html(Em.I18n.get("guide.internet.macconemanual"));
Atp.WanWizardController.load(function(){
var wizard_content = Atp.WanWizardController.content;
$("#wan_diagnose_pppoe_username").val(wizard_content.Username);
$("#wan_wizard_diagnose_pppoe_password").val(wizard_content.Password);
$("#wan_wizard_diagnose_pppoe_text").val(wizard_content.Password);
if(Atp.WanDetectController.showppponce > 0){
$("#diagnose_pppoe_error").show();
var err = Atp.WanDetectController.content.ErrReason;
if("ErrNoAnswer" == Atp.WanDetectController.content.ErrReason){
$("#diagnose_pppoe_error_info").html(Em.I18n.get("guide.noanswer.error"));
}else if("Unknown" == err){
$("#wan_pppoe_error_info").html(Em.I18n.get("guide.diagnose.unknownerr"));
}else if(("ErrAuthFail" != err) && ("ErrAuthUserLimit" != err) && ("ErrAuthMacLimit" != err)){
$("#diagnose_pppoe_error_info").html(Em.I18n.get("guide.diagnose.failed"));
}
}
Atp.CurrentMacShowController.load(function(result){
if(false == Atp.WanWizardController.content.MACColoneEnable){
$(".mac-list .noclone").addClass("active");
$("#diagnose_mac-choose").attr("name",0);
$("#diagnose_mac-choose").html($(".mac-list .noclone").html());
}else if(result["macaddress"] == Atp.WanWizardController.content.MACColone){
$("#diagnose_macclone_info").show();
$(".mac-list .defaultclone").addClass("active");
$("#diagnose_mac-choose").attr("name",1);
$("#diagnose_mac-choose").html($(".mac-list .defaultclone").html());
}else{
$(".mac-list .otherclone").addClass("active");
$("#diagnose_macclone_info").show();
$("#diagnose_mac-choose").attr("name",2);
$("#diagnose_mac-choose").html($(".mac-list .otherclone").html());
$("#diagnose_macconet_type_manual").show();
$("#diagnose_macconet_manual").val(Atp.WanWizardController.content.MACColone);
}
});
});
});
