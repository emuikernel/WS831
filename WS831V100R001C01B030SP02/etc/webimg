﻿var isLineup = false;
var stopStatusTime = false;
var getCurrentType = 0; 
var isLineDown = false;
$(function(){
Atp.UserProtocalController =  Atp.SingleObjController.extend({
objName: 'system/userprotocal',
submit: function(data, callback){
console.log("UserProtocalController post_data",data);
this.postData(data, callback);
}
});
Atp.CurrentMacController = Atp.SingleObjController.extend({
objName: 'ntwk/getmacaddress'
});
Atp.guideInternetController = Atp.SingleObjController.extend({
objName: 'ntwk/swan',
LoadInTime:function(){
setTimeout(function(){
if(isLineup){
return ;
}else{
Atp.guideInternetController.load();
}
},1500);
},
afterLoad:function(){
console.log("afterload content...");
var self = this;
var currenthref = window.location.href;
if(isLineup){
return ;
}
if(self.content.AccessStatus == "Down"){
if(!stopStatusTime){
if(currenthref.indexOf("guide_layer2down_page") >= 0){
self.LoadInTime();
}else{
utilchangePage("guide_layer2down_page");
}
}
}else{
isLineup = true;
utilchangePage("guide_lineup_page");
}
}
});
Atp.GuideWanController = Atp.SingleObjController.extend({
objName: "ntwk/wan?type=active",
contentType:"application/json;charset=UTF-8;enp",
isSubmitAccount:0,
submit: function(flag,data, callback){
var self = this;
var content = this.content;
content.guide = true;
content.NATType  = 1;
if (flag==0){
self.isSubmitAccount = 1;
content.Password = this.enc(data.guide_pppoe_password);
content.Username = data.guide_pppoe_username;
if(typeof data.mac_clnone_enable != "undefined"){
content.MACColoneEnable = data.mac_clnone_enable;
}
if(typeof data.guide_macclone_manual != "undefined"){
content.MACColone = (data.guide_macclone_manual).toUpperCase();
}
content.ConnectionType = "PPP_Routed";
}else if(flag==1){
content.IPv4AddrType   = data.IPv4AddrType;
content.ConnectionType = "IP_Routed";
}else{
content = data;
}
self.postData(content, callback);
}
});
Atp.GuideBeginController =  Atp.SingleObjController.extend({
objName: "ntwk/wandetect"
});
Atp.WanGuideDetectController = Atp.SingleObjController.extend({
objName: "ntwk/wandetect",
detectCnt: 0,
detectStatus: 0,
finishHttpDetect:function(result){
if(2 == result.HttpStatus){
connectedPagenext();
}else{
console.log("dhcp http fail...");
utilchangePage("guide_dhcpfinish_page");
}
},
httpDetect:function(result){
var self = Atp.WanGuideDetectController;
self.detectStatus++;
console.log("In httpDetect...",self.detectStatus);
console.log("In httpDetect...",result);
if ((2 != result.HttpStatus) || (3 != result.HttpStatus)){
if (self.detectStatus <= 5){
setTimeout(function() {
Atp.WanGuideDetectController.load(function(result){
Atp.WanGuideDetectController.httpDetect(result);
});
}, 1500);
}
}
if (self.detectStatus > 5 || (2 == result.HttpStatus) || (3 == result.HttpStatus)){
Atp.WanGuideDetectController.detectStatus = 6;
self.finishHttpDetect(result);
return;
}
},
finishDetect: function(result){
var self = this;
console.log("In finishDetect...");
Atp.WanGuideDetectController.detectStatus = 0;
if("Connected" == self.content.ConnectionStatus){
console.log("in finishDetect......connected...",self.content.ConnectionType);
if("IP_Routed" == self.content.ConnectionType){
self.httpDetect(result);
}else{
connectedPagenext();
}
}else{
if(2 == getCurrentType){
console.log("dhcp fail...");
utilchangePage("guide_dhcpfinish_page");
}else{
utilchangePage("guide_pppoe_page");
}
}
return ;
},
isConnecting: function(result){
if ("Connecting" == result["ConnectionStatus"]) {
return true;
}
if ("Authenticating" == result["ConnectionStatus"]) {
return true;
}
if ("Disconnecting" == result["ConnectionStatus"]) {
return true;
}
if ("PendingDisconnecting" == result["ConnectionStatus"]) {
return true;
}
return false;
},
isConnected: function(result){
if ("Bridged" == result["ConnectionType"]) {
if ("Connected" == result["ConnectionStatus"]) {
return true;
}
return false;
}
if ("Connected" != result["ConnectionStatus"]) {
return false;
}
return true;
},
detectWanConnected: function(result, self){
console.log("in detectWanConnected...",result);
console.log("self.detectStatus=",self.detectStatus);
var detectCount = self.detectStatus;
self.detectStatus = detectCount + 1;
if ("Down" == result.AccessStatus) {
self.detectCnt = 0;
utilchangePage("guide_layer2down_page");
return;
}
var detailStr = Em.I18n.get("guide.wan.detail");
if ("Unknown" == result.ErrReason){
Atp.WanGuideDetectController.detectStatus = 0;
if(result["ConnectionType"] == "PPP_Routed"){
utilchangePage("guide_pppoe_page");
$("#guide_pppoe_error_info").html(Em.I18n.get("guide.diagnose.unknownerr"));
}else{
utilchangePage("guide_dhcpfinish_page");
$("#connect_result_info").html(Em.I18n.get("guide.diagnose.unknownerr"));
}
return;
}
if ("ErrAuthFail" == result.ErrReason){
Atp.WanGuideDetectController.detectStatus = 0;
utilchangePage("guide_pppoe_page");
$("#guide_pppoe_error_info").html(Em.I18n.get("guide.diagnose.pppaccounterror"));
return;
}
if ("ErrAuthUserLimit" == result.ErrReason){
Atp.WanGuideDetectController.detectStatus = 0;
utilchangePage("guide_pppoe_page");
$("#guide_pppoe_error_info").html(Em.I18n.get("guide.userlimit.error") + detailStr);
$("#guide_pppoe_error_info").attr("name","userlimit");
return;
}
if ("ErrAuthMacLimit" == result.ErrReason){
Atp.WanGuideDetectController.detectStatus = 0;
$("#macclone_info").show();
utilchangePage("guide_pppoe_page");
$("#guide_pppoe_error_info").html(Em.I18n.get("guide.macclone.error") + detailStr);
$("#guide_pppoe_error_info").attr("name","macclone");
return;
}
if (false == self.isConnected(result)){
if (self.detectStatus <= 5){
setTimeout(function() {
Atp.WanGuideDetectController.load(Atp.WanGuideDetectController.detectWanConnected);
}, 1500);
}
}
if (self.detectStatus > 5 || self.isConnected(result)){
self.finishDetect(result);
return;
}
},
scanningWanType: function(result, self){
var flag = false;
console.log("scanningWanType",result);
console.log("self.detectCnt=",self.detectCnt);
if ("Down" == result.AccessStatus) {
self.detectCnt = 0;
utilchangePage("guide_layer2down_page");
return;
}
if ("ErrAuthFail" == result.ErrReason){
self.detectCnt = 0;
utilchangePage("guide_pppoe_page");
$("#guide_pppoe_error_info").html(Em.I18n.get("guide.diagnose.pppaccounterror"));
$("#guide_pppoe_error_info").attr("name","");
return;
}
if ("Finished" == result.SearchingStatus){
self.detectCnt = 0;
console.log("WanResult=",result["WanResult"]);
if ("1" == result["WanResult"]){
getCurrentType = 1;
console.log("WanResult..fsdafs.......",getCurrentType);
Atp.GuideWanController.content.ConnectionType = "PPP_Routed";
}
else if("2" == result["WanResult"]){
getCurrentType = 2;
console.log("WanResult.........",getCurrentType);
Atp.GuideWanController.content.ConnectionType = "IP_Routed";
}
flag = true;
if("Connected" ==result.ConnectionStatus && Atp.UserProtocalController.content.agree){
self.finishDetect(result);
}else{
console.log("GuideWanController submit....",Atp.GuideWanController.content);
Atp.GuideWanController.submit(-1,Atp.GuideWanController.content, function(){
console.log("post data to detectWanConnected...");
self.detectStatus = 0;
Atp.WanGuideDetectController.load(Atp.WanGuideDetectController.detectWanConnected);
});
}
return;
}
if (self.detectCnt > 10 || flag){
self.detectCnt = 0;
self.finishDetect(result);
return;
}
setTimeout(function() {
var detectCount = self.detectCnt;
self.detectCnt = detectCount + 1;
Atp.WanGuideDetectController.load(Atp.WanGuideDetectController.scanningWanType);
}, 1500);
},
detect: function(){
var self = this;
var data = {};
data.Type = "connType";
data.wan  = Atp.GuideWanController.content.ID;
console.log("detect.............",data);
isLineup = false;
self.submitItem("trigger", data, function(){
self.detectCnt = 0;
Atp.WanGuideDetectController.load(Atp.WanGuideDetectController.scanningWanType);
});
}
});
Atp.GuideWifiController = Atp.MultiObjController.extend({
objName: "ntwk/wlantouch",
contentType:"application/json;charset=UTF-8;enp",
wlan2gObj: null,
wlan5gObj: null,
wlan2guestObj:null,
showdata:function(){
$("#guide_wifi24_ssid").val(this.wlan2gObj.WifiSsid);
$("#guide_ssid5g").html(this.wlan2gObj.WifiSsid+"_5G");
$("#guide_wifi_password, #guide_wifi_text_password").val(this.wlan2gObj.WpaPreSharedKey);
},
afterLoad: function(){
for (var i = 0; i < this.content.length; i++){
if (this.content[i].wlanSign == 0){
this.wlan2gObj = this.content[i];
}
if(this.content[i].wlanSign == 1){
this.wlan5gObj = this.content[i];
}
if(this.content[i].wlanSign == 2){
this.wlan2guestObj  = this.content[i];
}
this.wlan2gObj.upassword = this.wlan2gObj.WpaPreSharedKey;
}
this.showdata();
},
createSsidSubmitData: function(wlanobj,ssid,password,upassword){
console.log("in createSsidSubmitData");
var ssiddata = wlanobj;
ssiddata.WifiSsid = ssid;
ssiddata.WepKey = this.enc(password);
ssiddata.upassword = this.enc(upassword);
console.log("wifi content=",ssiddata);
return ssiddata;
},
createGuestGuideSubmitData: function(wlanobj,ssid){
console.log("in createGuestGuideSubmitData");
var ssiddata = wlanobj;
ssiddata.WifiSsid = ssid.substr(0,26) + "_Guest";
ssiddata.SecOpt  = "none";
console.log("guestnetwork content=",ssiddata);
return ssiddata;
},
submit: function(data, callback){
console.log("submit wlan data",data);
if (null == data){
return;
}
var wlandata = {};
wlandata.guide = true;
this.wlan2gObj.BeaconType = 'WPAand11i';
this.wlan2gObj.WpaEncryptionMode = 'AES';
wlandata.wifi24g = this.createSsidSubmitData(this.wlan2gObj,data.guide_wifi24_ssid,data.guide_wifi_password,data.guide_user_password);
this.wlan5gObj.BeaconType = 'WPAand11i';
this.wlan5gObj.WpaEncryptionMode = 'AES';
wlandata.wifi5g = this.createSsidSubmitData(this.wlan5gObj,data.ssid5g,data.guide_wifi_password,data.guide_user_password);
wlandata.wifiguest24g = this.createGuestGuideSubmitData(this.wlan2guestObj,data.guide_wifi24_ssid);
console.log("wlan submit ",wlandata);
$("#wlan_passworderror").hide();
this.submitItem("SsidSettings", wlandata, function(result){
console.log("-------------------------------------",result);
if (0 != result.errcode) {
utilchangePage("guide_wlan_page");
if(typeof result["WifiSsid"] != "undefined" ){
$("#wlan_passworderror").show();
$("#wlan_warninginfo").html(Em.I18n.get("Wlan.Set.error"));
}
}
});
}
});
});
function utilShowGuideEyeWifiPassword(checkboxid,passwordinput,textinput,inputid){
var src = $("#"+checkboxid).attr("src");
if (typeof(src) === "undefined"){
return;
}
if(src.indexOf("eyehide") >= 0){
if("********" == $("#"+inputid).val()){
Atp.GuideWifiController.objName = "ntwk/wlantouch?type=showpass24";
console.log("objName:","ntwk/wlantouch?type=showpass24");
Atp.GuideWifiController.load(function(){
$("#"+inputid).val(Atp.GuideWifiController.wlan2gObj.WpaPreSharedKey);
$("#"+passwordinput).hide();
$("#"+textinput).show();
});
}else{
$("#"+passwordinput).hide();
$("#"+textinput).show();
}
return true;
}
else{
$("#"+passwordinput).show();
$("#"+textinput).hide();
return false;
}
}
function changeCheckboxSrc(id, on, off){
if (typeof(id) === "undefined"){
return;
}
var src = $("#"+id).attr("src");
if (typeof(src) === "undefined"){
return;
}
if (src.indexOf("uncheck") >= 0){
$("#"+id).attr("src", "/small/res/checked.png");
off();
}
else{
$("#"+id).attr("src", "/small/res/uncheck.png");
on();
}
}
function GuideSyncCheckBoxOnClick(id){
var ischecked = setWlanEnableImage("guide_sync_checkbox");
utilShowEyePassword('guide_wifi_eyeshowpwd_img','guide_user_password_div','guide_user_password_text_div');  
if(ischecked == false){
console.log('unchecked.................................')
$("#guide_user_password").removeAttr("disabled");
$("#guide_user_text_password").removeAttr("disabled");
$("#sync_user_password").css("color","#242424");
$("#sync_user_text_password").css("color","#242424");
$('#guide_user_password, #guide_user_text_password').val("");
$('#guide-user-div').show();
$("#guide_user_password").attr("placeholder", Em.I18n.get("Touch.user.passwordinfo.oninput"));
$("#guide_user_text_password").attr("placeholder", Em.I18n.get("Touch.user.passwordinfo.oninput"));
$('#guide_user_password_strength').hide();
}
else{
syncPassword();
console.log('checked.................................')
$("#guide_user_password").attr("disabled","disabled");
$("#guide_user_text_password").attr("disabled","disabled");
$("#sync_user_password").css("color","#999");
$("#sync_user_text_password").css("color","#999");
$("#guide_user_password").removeAttr("placeholder");
$("#guide_user_text_password").removeAttr("placeholder");
$('#wlan_user_passworderror').hide();
var originpwd = $("#guide_wifi_password").val();
$("#guide_user_password, #guide_user_text_password").val(originpwd);
}
}
function GuideutilCheckBoxOnClick(id){
changeCheckboxSrc(id, function(){
$("#guide_begin_div > div").css({"background-color":"#0f73b9","border":"1px solid #0f78be"});
$("#guide_begin_div > div").touchstart(function(){
$("#guide_begin_div > div").css({"background-color":"#0f73b9","border":"1px solid #0f78be"});
});
$("#guide_begin_div > div").touchend(function(){
$("#guide_begin_div > div").css({"background-color":"#0f73b9","border":"1px solid #0f78be"});
});
}, function(){
$("#guide_begin_div > div").css({"background-color":"#2785c6","border":"1px solid #57a0d2"});
$("#guide_begin_div > div").touchstart(function(){
$("#guide_begin_div > div").css({"background-color":"#0f73b9","border":"1px solid #0f78be"});
});
$("#guide_begin_div > div").touchend(function(){
$("#guide_begin_div > div").css({"background-color":"#2785c6","border":"1px solid #57a0d2"});
});
});
}
function isAgreeProtocal(){
if(Atp.UserProtocalController.content.agree == false){
window.location.href = "/small/html/guide.html";
return ;
}
}
function connectedPagenext(){
utilchangePage("guide_wlan_page");
}
function GuideBegin(){
Atp.GuideBeginController.load(function(result){
if(result["ErrReason"] == "ErrLayer2Down"){
utilchangePage("guide_layer2down_page");
}else if(result["ConnectionStatus"] == "Connected" && (result["ConnectionType"] == "PPP_Routed")){
getCurrentType = 1;
utilchangePage("guide_submiting_page");
setTimeout(function(){
connectedPagenext();
},2000);
}else if(result["ConnectionStatus"] == "Connected" && (result["ConnectionType"] == "IP_Routed")){
getCurrentType = 2;
utilchangePage("guide_submiting_page");
Atp.WanGuideDetectController.httpDetect(result);
}else if((result["WanResult"] == 1) && (result["SearchingStatus"] == "Finished")){
utilchangePage("guide_pppoe_page");
}else{
utilchangePage("guide_connecting_page");
}
});
}
function postUsrProtacal(agress){
var content = {};
content.agree = agress;
Atp.UserProtocalController.submit(content,function(proresult){
console.log("protocal result=",proresult);
if(proresult["errcode"] == 1){
utilUpdateCsrf(proresult.csrf_param, proresult.csrf_token);
Atp.UserProtocalController.submit(content,function(){});
}
if(proresult["errcode"] == 0){
Atp.UserProtocalController.load(function(){
if(Atp.UserProtocalController.content.agree == true){
GuideBegin();
}
});
}
});
}
function guidechangePage(){
Atp.UserProtocalController.load(function(){
if($("#guide_protocal_checkbox").attr("src").indexOf("uncheck")>=0){
if(Atp.UserProtocalController.content.agree == true){
postUsrProtacal("false");
}
}else{
if(Atp.UserProtocalController.content.agree == false){
postUsrProtacal("true");
}else{
GuideBegin();
}
}
});
}
$("#guide_connecting_page").bind("pagebeforeshow", function( event, prevPage){
isAgreeProtocal();
Atp.GuideWanController.load(function(){
console.log("GuideWanController data");
Atp.WanGuideDetectController.load(function(){
Atp.WanGuideDetectController.detectStatus = 0;
Atp.WanGuideDetectController.detect();
});
});
});
