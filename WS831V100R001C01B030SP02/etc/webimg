local errcode, kval = dm.GetParameterValues("InternetGatewayDevice.LANDevice.1.WLANEndPoint.1.Profile.1.", 
    {"SSID", "WEPKey", "PreSharedKey", "SecurityMode", "EncryptionType", "WEPKeyIndex"});


local errcode, values = dm.GetParameterValues("InternetGatewayDevice.LANDevice.1.WLANEndPoint.1.LinkInfo.", 
    {"BSSID", "Status"});


local secObj = {} 
 secObj = kval["InternetGatewayDevice.LANDevice.1.WLANEndPoint.1.Profile.1."]

local newObj = {}
local objs = {} 

if values ~= nil then
    for k, v in pairs(values) do
        for k1, v1 in pairs(v) do
            print(k1, v1)
        end
    end
    objs = values["InternetGatewayDevice.LANDevice.1.WLANEndPoint.1.LinkInfo."]
end

if objs ~= nil  and objs["Status"] == "Connected" and objs["BSSID"] ~= "" then 
	newObj.SSID = secObj["SSID"]
	newObj.BSSID = objs["BSSID"]
	newObj.SecurityMode = secObj["SecurityMode"]
	newObj.WepEncyptionMode = secObj["SecurityMode"]
	newObj.EncryptionType = secObj["EncryptionType"]
	if "WEP" == secObj["EncryptionType"] then
		newObj.CurrentKey = secObj["WEPKey"]
		newObj.WEPKeyIndex = secObj["WEPKeyIndex"]
	else
		newObj.CurrentKey = secObj["PreSharedKey"]
	end
else
	newObj.SSID = ""
	newObj.BSSID = ""
	newObj.SecurityMode = "Open"
	newObj.EncryptionType = "NONE"
	newObj.CurrentKey = ""
	newObj.WEPKeyIndex = ""
end

sys.print(json.encode(newObj))

