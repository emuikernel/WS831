if data["enable"] == false then
	for k,v in pairs(wlanBasic) do
		print("close "..k)
		add_one_parameter(paras, k.."CanUserForCover", false)
	end
else
	--operate intelligent
	for k,v in pairs(wlanBasic) do
		print("open "..k)
		if v["CanUserForCoverDefault"] ~= nil and v["CanUserForCoverDefault"] == 1 then
			add_one_parameter(paras, k.."CanUserForCover", true)
		end
		add_one_parameter(paras, "InternetGatewayDevice.Services.X_UPnP.Enable", true)
	end
end
