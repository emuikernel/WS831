Atp.LoadHelper.appendJs("/js/ddns.js");
Atp.LoadHelper.loadAll();
Atp.DDNSserviceContainerView = Atp.PageContainerView.extend ({
prefixName: "ddns",
dataView: Em.View.extend({
template: Em.Handlebars.compile('\
{{view Atp.DDNSServiceView}} \
')
