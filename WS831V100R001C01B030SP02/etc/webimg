require('utils')

local type = FormData["type"]


function CheckWan(id, wan)
    if type == "spoof" then
        if "DHCP_Spoofed" == wan.ConnectionType then 
            return true
        end
        return false
    end

    if type == "l2tpwan" then

    end

    if type == "mldwan" then
        if utils.toboolean(wan.X_IPv6Enable) == false then
            return false
        end

        ip = string.find(wan.ConnectionType, "IP_Routed")
        if not ip then 
            return false
        end

        ip = string.find(wan.X_ServiceList, "INTERNET")
        ip2 = string.find(wan.X_ServiceList, "Other")
        if not ip and not ip2 then
            return false
        end

        if not wan.ConnectionTrigger then
            return true
        end

        trigger = string.find(wan.ConnectionTrigger, "OnDemand")
        if trigger then
                return false
        end

        return true
    end

    if type == "igmpwan" then
        if wan.NATEnabled == 0 then
            return false
        end

        ip = string.find(wan.ConnectionType, "IP_Routed")
        if not ip then 
            return false
        end

        ip = string.find(wan.X_ServiceList, "INTERNET")
        ip2 = string.find(wan.X_ServiceList, "Other")
        if not ip and not ip2 then
            return false
        end

        if not wan.ConnectionTrigger then
            return true
        end
        
        return true
    end

    if type == "routewan" then
        ip = string.find(wan.ConnectionType, "IP_Routed")
        if not ip then 
            return false
        end
        ip = string.find(wan.X_ServiceList, "INTERNET")
        ip2 = string.find(wan.X_ServiceList, "Other")
        if not ip and not ip2 then
            return false
        end
    end

    if type == "l2tpwan" then
        ip = string.find(wan.ConnectionType, "IP_Routed")
        if not ip then 
            return false
        end
        ip = string.find(wan.X_ServiceList, "INTERNET")
        ip2 = string.find(wan.X_ServiceList, "Other")
        if not ip and not ip2 then
            return false
        end
    end

    if type == "routebalancewan" then
        ip = string.find(wan.ConnectionType, "IP_Routed")
        if not ip then 
            return false
        end
        ip = string.find(wan.X_ServiceList, "INTERNET")
        ip2 = string.find(wan.X_ServiceList, "Other")
        if not ip and not ip2 then
            return false
        end
    end

    if type == "pptpwan" then
        ip = string.find(wan.ConnectionType, "IP_Routed")
        if not ip then 
            return false
        end
        ip = string.find(wan.X_ServiceList, "INTERNET")
        ip2 = string.find(wan.X_ServiceList, "Other")
        if not ip and not ip2 then
            return false
        end
    end

    if type == "tunnelwan" then
        if wan.X_ServiceList == "INTERNET" then
            return true
        end
        return false
    end

    if type == "ipsecwan" then
        ip = string.find(wan.ConnectionType, "IP_Routed")
        if not ip then 
            return false
        end

        ip = string.find(wan.X_ServiceList, "INTERNET")
        if not ip then
            return false
        end
    end

    return true
end

