sys.print('\r\
\r\
<script language="JavaScript" type="text/javascript">\r\
function pageload(){\r\
Atp.ContentInternetController.load(function(){\r\
var wlanmode  = Atp.WlanModeController.content.get("WLANMode");\r\
var connectStatus = Atp.ContentInternetController.content;\r\
if(wlanmode != 3  || (connectStatus.get("ConnectionStatus") == "Connected") && (connectStatus.get("AccessType") != "WIFI")){\r\
Atp.WlanObjs.load();\r\
Atp.HostsController.load(function(){\r\
Atp.WanErrorController.load(function(){\r\
Atp.ContentInternetController.load(function(){\r\
Atp.WlanRadioController.load();\r\
Atp.DeviceInfoController.load();\r\
Atp.WanDiagnoseController.load();\r\
Atp.HotaVersionController.load();\r\
});\r\
});\r\
});\r\
}\r\
else{\r\
Atp.HostsController.load(function(){\r\
Atp.RepeaterStateController.load();\r\
});\r\
}\r\
});\r\
Atp.BrandCollectionController.load(function(){\r\
Atp.RemoteHostController.load(function(){\r\
Atp.TopologyController.load(function(){\r\
Atp.TopologyController.set("isLoadFinish",true);\r\
var content = Atp.TopologyController.content;\r\
Em.Logger.log("didInsertElement=============");\r\
});\r\
});\r\
});\r\
}\r\
$(document).ready(function() {\r\
pageload();\r\
});\r\
</script>\r\
');
