$("#wpakey_wizard_repeater").live("change",function(){
$("#wpakey_text_wizard_repeater").val($("#wpakey_wizard_repeater").val());
});
$("#wpakey_text_wizard_repeater").live("change",function(){
$("#wpakey_wizard_repeater").val($("#wpakey_text_wizard_repeater").val());
});
$("#wpakey_wizard_repeater").live("keyup",function(){
if(isguide){
$("#wpakey_text_wizard_repeater").attr("placeholder", Em.I18n.get("Touch.wifi.passwordinfo.oninput",{arg1:"8~32"}));
}else{
$("#wpakey_text_wizard_repeater").attr("placeholder", Em.I18n.get("Touch.wifi.passwordinfo.oninput",{arg1:"8~63"}));
}
});
$("#wpakey_text_wizard_repeater").live("keyup",function() {
if(isguide){
$("#wpakey_wizard_repeater").attr("placeholder", Em.I18n.get("Touch.wifi.passwordinfo.oninput",{arg1:"8~32"}));
}else{
$("#wpakey_wizard_repeater").attr("placeholder", Em.I18n.get("Touch.wifi.passwordinfo.oninput",{arg1:"8~63"}));
}
});
