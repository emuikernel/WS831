require('utils')


function EncyptionName( mode )
    if mode == "Basic" then
        return "WEP"
    end    
    if mode == "11i" then 
        return "WPA2"
    end    
    if mode == "WPAAnd11i" then
        return "WPA/WPA2"        
    end
    return mode 
end
--84 77 70 61 50
function Iconpath( signal )
    if signal < 50 then
        return "/res/signal_05.gif"
    end
    if signal < 61 then
        return "/res/signal_04.gif"
    end
    if signal < 70 then
        return "/res/signal_03.gif"
    end
    if signal < 77 then
        return "/res/signal_02.gif"
    end
    if signal < 84 then
        return "/res/signal_01.gif"
    end
    return "/res/signal_00.gif"
end

function TouchIconpath( signal )
    if signal < 50 then
        return "../res/wifisignal_05.png"
    end
    if signal < 61 then
        return "../res/wifisignal_04.png"
    end
    if signal < 70 then
        return "../res/wifisignal_03.png"
    end
    if signal < 77 then
        return "../res/wifisignal_02.png"
    end
    if signal < 84 then
        return "../res/wifisignal_01.png"
    end
    return "../res/wifisignal_00.png"
end

function checkssid( ssid )
    if ssid == nil  then
        ssid = ""
    end
    return ssid
end

local errcode, objs = dm.GetParameterValues("InternetGatewayDevice.LANDevice.1.WLANEndPoint.1.APList.{i}.", 
   {"SSID", "BSSID", "Channel", "SecurityMode", "EncryptionType", "BWControl", "Signal"});

local aplist = {}


if objs ~= nil then
    for k,v in pairs(objs) do
        local newObj = {}
        newObj["ID"] = k
        newObj["SSID"] = checkssid(v["SSID"])
        newObj["BSSID"] = v["BSSID"]
        newObj["Signal"] = v["Signal"]
	newObj["BWControl"] = v["BWControl"]
        newObj["IconPath"] = Iconpath(newObj["Signal"])
        newObj["TouchIconPath"] = TouchIconpath(newObj["Signal"])
        newObj["SecurityMode"] = v["SecurityMode"]
        newObj["Encyption_for_user"] = EncyptionName(newObj["SecurityMode"])
        newObj["Channel"] = v["Channel"]
        newObj["EncryptionType"] = v["EncryptionType"]
        newObj["isConnect"] = false
        table.insert(aplist, newObj)
    end
end 

utils.multiObjSortByID(aplist)
sys.print(json.encode(aplist))require('dm')
require('web')
require('json')
require('utils')

--first get wlan 2.4G and 5G datas
--wifi enable
local errcode,wifiConf = dm.GetParameterValues("InternetGatewayDevice.X_WiFi.Radio.{i}.",{"Enable","OperatingFrequencyBand"});
function get_WifiEnable(frequency)
    for k, v in pairs(wifiConf) do
        if frequency == v["OperatingFrequencyBand"] then
            return utils.toboolean(v["Enable"])
        end
    end
    return false
end

--get wifi details
local errcode,wlanBasic = dm.GetParameterValues("InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.",
{"Enable","X_OperatingFrequencyBand","SSID","BeaconType","BasicEncryptionModes","BasicAuthenticationMode","WEPKeyIndex","WEPEncryptionLevel","WPAEncryptionModes","IEEE11iEncryptionModes","X_MixedEncryptionModes"});
local errcode,wlanGuestBasic = dm.GetParameterValues("InternetGatewayDevice.LANDevice.2.WLANConfiguration.{i}.",
{"Enable","X_OperatingFrequencyBand","SSID","BeaconType","IEEE11iEncryptionModes","X_MixedEncryptionModes"});

local isShowPass = false
local isShowPass5g = false
local isShowPassGuest = false
local isShowPassGuest5g = false
if FormData["type"] ~= nil then
    if FormData["type"]  == "showpass24" then
        isShowPass = true
    end
    if FormData["type"]  == "showpass5g" then
        isShowPass5g = true
    end
    if FormData["type"]  == "showpassguest24" then
        isShowPassGuest = true
    end
    if FormData["type"]  == "showpassguest5g" then
        isShowPassGuest5g = true
    end
    if FormData["type"]  == "showpassall" then
        isShowPass = true
        isShowPass5g = true
        isShowPassGuest = true
        isShowPassGuest5g = true
    end
end

local wlanCfgs = {}
function calc_min(index,frequency)
    local array = {}
    i = 1
    if index == 0 then
        if wlanBasic ~= nil then
            for k,v in pairs(wlanBasic) do
                if frequency == v["X_OperatingFrequencyBand"] then
                    local wlan_instance_id, wlan_suffix = utils.findLastPoint(k)
                    array[i] = tonumber(wlan_instance_id)
                    i = i+1
                end
            end
            table.sort(array)
            if 1 == i  then
                return -1
            else
                return array[1]
            end
        end
    end
    if index == 1 then
        if wlanGuestBasic ~= nil then
            for k,v in pairs(wlanGuestBasic) do
                if frequency == v["X_OperatingFrequencyBand"] then
                    local wlan_instance_id, wlan_suffix = utils.findLastPoint(k)
                    array[i] = tonumber(wlan_instance_id)
                    i = i+1
                end
            end
            table.sort(array)
            if 1 == i  then
                return -1
            else
                return array[1]
            end
        end    
    end
    return -1
end

wlan_index = calc_min(0,"2.4GHz")

wlan_5gindex = calc_min(0,"5GHz")

function isValidIndex(index,k)
    local  wlan_instance_id, wlan_suffix = utils.findLastPoint(k)
    if index == 0 then
        if wlan_index == tonumber(wlan_instance_id)  or wlan_5gindex == tonumber(wlan_instance_id)  then
            return true
        end
    end
    if index == 1 then
        if wlan_guest_index == tonumber(wlan_instance_id)  or wlan_guest_5gindex == tonumber(wlan_instance_id)  then
            return true
        end
    end
    return false
end

for k,v in pairs(wlanBasic) do
   if isValidIndex(0,k) then
        local needshowpwd = false
        if (v["X_OperatingFrequencyBand"] == "2.4GHz" and isShowPass) then
            needshowpwd = true
        end
        if (v["X_OperatingFrequencyBand"] == "5GHz" and isShowPass5g) then
            needshowpwd = true
        end
        local WepKey = {}
        wepkey_conf_str = k.."WEPKey."..tostring(v["WEPKeyIndex"]).."."
        local err_wep, wep_data = dm.GetParameterValues(k.."WEPKey.{i}.", { "WEPKey" });
        for kk, vv in pairs(wep_data) do
            wep_instance_id, wep_suffix = utils.findLastPoint(kk)
            WepKey[wep_instance_id] = _G["defaultPasswd"]
            if needshowpwd then
                WepKey[wep_instance_id] = vv["WEPKey"]
            end
        end
        wpakey_conf_str = k.."PreSharedKey.1."
        local err_wpakey, wap_key_data = dm.GetParameterValues(wpakey_conf_str, { "PreSharedKey", "KeyPassphrase" });
        for kk, vv in pairs(wap_key_data) do
            WpaPreSharedKey = _G["defaultPasswd"]
            if needshowpwd then
                WpaPreSharedKey = vv["KeyPassphrase"]
            end
            break
        end
        local wlanInstance = {}
        wlanInstance.ID = k
        wlanInstance.FrequencyBand = v["X_OperatingFrequencyBand"]
        if v["X_OperatingFrequencyBand"] == "2.4GHz" then
            wlanInstance.wlanSign = 0
        end
        if v["X_OperatingFrequencyBand"] == "5GHz" then
            wlanInstance.wlanSign = 1
        end      
        wlanInstance.WifiEnable = get_WifiEnable(v["X_OperatingFrequencyBand"])
        wlanInstance.WifiSsidEnable = utils.toboolean(v["Enable"])
        wlanInstance.WifiSsid = v["SSID"]

        if "Basic" == v["BeaconType"] and "None" == v["BasicEncryptionModes"] then
            wlanInstance.BeaconType = "None"
        else
            wlanInstance.BeaconType = v["BeaconType"]
        end
        wlanInstance.BasicEncryptionMode = v["BasicEncryptionModes"]
        wlanInstance.BasicAuthMode = v["BasicAuthenticationMode"]
        if "40-bit" == v["WEPEncryptionLevel"] then
            wlanInstance.WepKeyLen = 64
        end
        if "104-bit" == v["WEPEncryptionLevel"] then
            wlanInstance.WepKeyLen = 128  
        end 
        wlanInstance.WepKeyIndex = v["WEPKeyIndex"]
        wlanInstance.WepKey = WepKey
        wlanInstance.WpaPreSharedKey = WpaPreSharedKey
        if "TKIPEncryption" == v["WPAEncryptionModes"] then
            wlanInstance.WpaEncryptionMode = "TKIP"
        end
        if "AESEncryption" == v["WPAEncryptionModes"] then
            wlanInstance.WpaEncryptionMode = "AES"
        end
        if "TKIPandAESEncryption" == v["WPAEncryptionModes"] then
            wlanInstance.WpaEncryptionMode = "TKIP+AES"
        end    
        if "TKIPEncryption" == v["IEEE11iEncryptionModes"] then
            wlanInstance.IEEEiEncryptionMode = "TKIP"
        end
        if "AESEncryption" == v["IEEE11iEncryptionModes"] then
            wlanInstance.IEEEiEncryptionMode = "AES"
        end    
        if "TKIPandAESEncryption" == v["IEEE11iEncryptionModes"] then
            wlanInstance.IEEEiEncryptionMode = "TKIP+AES"
        end 
        if "TKIPEncryption" == v["X_MixedEncryptionModes"] then
            wlanInstance.MixedEncryptionModes = "TKIP"
        end
        if "AESEncryption" == v["X_MixedEncryptionModes"] then
            wlanInstance.MixedEncryptionModes = "AES"
        end    
        if "TKIPandAESEncryption" == v["X_MixedEncryptionModes"] then
            wlanInstance.MixedEncryptionModes = "TKIP+AES"
        end
        --just for quick set wifi 
        if wlanInstance.BeaconType == "None" then 
            wlanInstance.WpaPreSharedKey = ""
        end
        table.insert(wlanCfgs, wlanInstance)
    end
end
--second get wlan guestnetwork 2.4G and 5G datas
wlan_guest_index = calc_min(1,"2.4GHz")
wlan_guest_5gindex = calc_min(1,"5GHz")
for kk,vv in pairs(wlanGuestBasic) do
    if isValidIndex(1,kk) then
        local wlanguestInstance = {}
        wlanguestInstance["ID"] = kk

        local needshowguestpwd = false
        if (vv["X_OperatingFrequencyBand"] == "2.4GHz" and isShowPassGuest) then
            needshowguestpwd = true
        end
        if (vv["X_OperatingFrequencyBand"] == "5GHz" and isShowPassGuest5g) then
            needshowguestpwd = true
        end

        wlanguestInstance["FrequencyBand"] = vv["X_OperatingFrequencyBand"] 
        if vv["X_OperatingFrequencyBand"] == "2.4GHz" then
            wlanguestInstance.wlanSign = 2
        end
        if vv["X_OperatingFrequencyBand"] == "5GHz" then
            wlanguestInstance.wlanSign = 3
        end
        local wpakey_conf_str = kk.."PreSharedKey.1."
        local err_wpakey, wap_key_data = dm.GetParameterValues(wpakey_conf_str, { "PreSharedKey", "KeyPassphrase" });
        for kkk, vvv in pairs(wap_key_data) do
            WpaPreSharedKey = _G["defaultPasswd"]
            if needshowguestpwd then
                WpaPreSharedKey = vvv["KeyPassphrase"]
            end
            break
        end
        wlanguestInstance["WifiSsid"] = vv["SSID"]
        if "11i" == vv["BeaconType"] and "AESEncryption" == vv["IEEE11iEncryptionModes"] then
            wlanguestInstance["SecOpt"] = "aes" 
        elseif "WPAand11i" == vv["BeaconType"] and "TKIPandAESEncryption" == vv["X_MixedEncryptionModes"]  then
            wlanguestInstance["SecOpt"] = "tkip"
        else
            wlanguestInstance["SecOpt"] = "none"
                WpaPreSharedKey = ""
        end  
        wlanguestInstance["WpaPreSharedKey"] = WpaPreSharedKey 

        local WifiEnable = get_WifiEnable(vv["X_OperatingFrequencyBand"])
        local SsidEnable = utils.toboolean(vv["Enable"])
        wlanguestInstance["WifiSsidEnable"] =  false
        if true == WifiEnable and true == SsidEnable then
            wlanguestInstance["WifiSsidEnable"] = true
        end
        table.insert(wlanCfgs, wlanguestInstance)
    end   
end
utils.multiObjSortByID(wlanCfgs)
sys.print(json.encode(wlanCfgs))local utils = require('utils')
local print = print
local maps = {
    wpakey = "KeyPassphrase",
    WifiEnable = "Enable",
    WifiSsid = "SSID",
    WifiHideBroadcast = "SSIDAdvertisementEnabled",
    X_WlanIsolateControl = "X_WlanIsolateControl",
    BeaconType = "BeaconType",
    BasicEncryptionMode = "BasicEncryptionModes",
    BasicAuthMode = "BasicAuthenticationMode",
    WpaEncryptionMode = "WPAEncryptionModes",
    IEEEiEncryptionMode = "IEEE11iEncryptionModes",
    MixedEncryptionModes = "X_MixedEncryptionModes",
    RadiusServer = "RadiusServer",
    Radiusport = "RadiusPort"
};

--用户没有登录 又不是向导 这直接返回
local name,level = web.getuserinfo();
if level == 0 and data["guide"] == nil then
    print("no auth to set wlan...")
    return;
end

function add_wlan_parameter(paras, name, value)
    if nil == value then
        return
    end
    table.insert(paras, {name, value})
end

function getindexwepkey(curdata)
    local index = 0
    local wepkeys = {}
    if nil ~= curdata["WepKey"] then
        index = curdata["WepKeyIndex"]
        return curdata["WepKey"][tostring(index)]
    end
    return nil
end

function submit_setting_data(paras, curdata)
    if nil == curdata then
        return
    end
    if nil == curdata["ID"] then
        return
    end
    if 0 ~= curdata["ID"] then
        local wlancfg = curdata["ID"]
        if "cloudc" ~= cfgtool then 
            if true == utils.toboolean(web.getparaenc()) then
                if curdata["WepKey"] ~= nil then
                    local pass = web.decodepara(curdata["WepKey"])
                    if "" ~= pass then
                        curdata["WepKey"] = pass
                    end
                end
            end
        end
        add_wlan_parameter(paras, wlancfg.."SSID", curdata["WifiSsid"])
        add_wlan_parameter(paras, wlancfg.."Enable", curdata["WifiSsidEnable"])
        add_wlan_parameter(paras, wlancfg.."BeaconType", curdata["BeaconType"])

        if curdata["BeaconType"] == "Basic" then
            add_wlan_parameter(paras, wlancfg.."BasicEncryptionModes", curdata["BasicEncryptionMode"])
            if curdata["BasicEncryptionMode"] == "WEPEncryption" then
                add_wlan_parameter(paras, wlancfg.."BasicAuthenticationMode", curdata["BasicAuthMode"])
                add_wlan_parameter(paras, wlancfg.."WEPKeyIndex", curdata["WepKeyIndex"])
                add_wlan_parameter(paras, wlancfg.."WEPKey."..tostring(curdata["WepKeyIndex"])..".WEPKey", getindexwepkey(curdata))
            else
                add_wlan_parameter(paras, wlancfg.."BasicAuthenticationMode", "None")
            end
        end
        if curdata["BeaconType"] == "WPA" then
            if curdata["WpaEncryptionMode"] == "TKIP" then
                add_wlan_parameter(paras, wlancfg.."WPAEncryptionModes", "TKIPEncryption")
            end
            if curdata["WpaEncryptionMode"] == "AES" then
                add_wlan_parameter(paras, wlancfg.."WPAEncryptionModes", "AESEncryption")
            end
            if curdata["WpaEncryptionMode"] == "TKIP+AES"  then
                add_wlan_parameter(paras, wlancfg.."WPAEncryptionModes", "TKIPandAESEncryption")
            end
            if _G["defaultPasswd"] ~= curdata["WepKey"] then
                if 64 == string.len(curdata["WepKey"]) then
                    add_wlan_parameter(paras, wlancfg.."PreSharedKey.1.PreSharedKey", curdata["WepKey"])
                else
                    add_wlan_parameter(paras, wlancfg.."PreSharedKey.1.KeyPassphrase", curdata["WepKey"])
                end
            end
        end
        if curdata["BeaconType"] == "11i" then
            if curdata["WpaEncryptionMode"] == "TKIP" then
                add_wlan_parameter(paras, wlancfg.."IEEE11iEncryptionModes", "TKIPEncryption")
            end
            if curdata["WpaEncryptionMode"] == "AES" then
                add_wlan_parameter(paras, wlancfg.."IEEE11iEncryptionModes", "AESEncryption")
            end
            if curdata["WpaEncryptionMode"] == "TKIP+AES"  then
                add_wlan_parameter(paras, wlancfg.."IEEE11iEncryptionModes", "TKIPandAESEncryption")
            end
            if _G["defaultPasswd"] ~= curdata["WepKey"] then
                if 64 == string.len(curdata["WepKey"]) then
                    add_wlan_parameter(paras, wlancfg.."PreSharedKey.1.PreSharedKey", curdata["WepKey"])
                else
                    add_wlan_parameter(paras, wlancfg.."PreSharedKey.1.KeyPassphrase", curdata["WepKey"])
                end
            end
        end
        if curdata["BeaconType"] == "WPAand11i" then
            if curdata["WpaEncryptionMode"] == "TKIP" then
                add_wlan_parameter(paras, wlancfg.."X_MixedEncryptionModes", "TKIPEncryption")
            end
            if curdata["WpaEncryptionMode"] == "AES" then
                add_wlan_parameter(paras, wlancfg.."X_MixedEncryptionModes", "AESEncryption")
            end
            if curdata["WpaEncryptionMode"] == "TKIP+AES"  then
                add_wlan_parameter(paras, wlancfg.."X_MixedEncryptionModes", "TKIPandAESEncryption")
            end
            if _G["defaultPasswd"] ~= curdata["WepKey"] then
                if 64 == string.len(curdata["WepKey"]) then
                    add_wlan_parameter(paras, wlancfg.."PreSharedKey.1.PreSharedKey", curdata["WepKey"])
                else
                    add_wlan_parameter(paras, wlancfg.."PreSharedKey.1.KeyPassphrase", curdata["WepKey"])
                end
            end
        end
    end
end

function submit_change_wpsstate(paras, curdata)
    if nil == curdata then
        return
    end
    if nil == curdata["ID"] then
        return
    end
    if 0 ~= curdata["ID"] then
        local wlancfg = curdata["ID"]

        if curdata["BeaconType"] == "Basic" or curdata["BeaconType"] == "WPA" then
            add_wlan_parameter(paras, wlancfg.."WPS.Enable", false)
        end
        if curdata["BeaconType"] == "11i" or curdata["BeaconType"] == "WPAand11i" then
            if curdata["WpaEncryptionMode"] == "TKIP" then
                add_wlan_parameter(paras, wlancfg.."WPS.Enable", false)
            else
                if wlancfg == "InternetGatewayDevice.LANDevice.1.WLANConfiguration.1." then
                add_wlan_parameter(paras, wlancfg.."WPS.Enable", true)
                end
                if wlancfg == "InternetGatewayDevice.LANDevice.1.WLANConfiguration.2." then
                    add_wlan_parameter(paras, wlancfg.."WPS.Enable", true)
                end
            end
        end
    end
end

local paras = {}
if utils.toboolean(data["guide"]) then
    add_wlan_parameter(paras, "InternetGatewayDevice.X_WiFi.Radio.1.WLANMode","4")
end
curdata = data["wifi24g"]
if nil ~= curdata then
    submit_change_wpsstate(paras, curdata)
end
curdata = data["wifi5g"]
if nil ~= curdata then
    submit_change_wpsstate(paras, curdata)
end

--guestnetwork
function submit_frequency_data(paras, data)
    if data == nil then
        return
    end
    if data["ID"] == nil then
        return
    end
    if "cloudc" ~= cfgtool then 
        if true == utils.toboolean(web.getparaenc()) then
            if data["WpaPreSharedKey"] ~= nil then
                local pass = web.decodepara(data["WpaPreSharedKey"])
                if "" ~= pass then
                    data["WpaPreSharedKey"] = pass
                end
            end
        end
    end
    local wlan_str = data["ID"]
    add_wlan_parameter(paras, wlan_str.."Enable", data["Enable"])
    add_wlan_parameter(paras, wlan_str.."SSID", data["WifiSsid"])
    if "none" == data["SecOpt"] then
        add_wlan_parameter(paras, wlan_str.."BeaconType", "Basic")
        add_wlan_parameter(paras, wlan_str.."BasicEncryptionModes", "None")
        add_wlan_parameter(paras, wlan_str.."BasicAuthenticationMode", "None")
    end
    if "aes" == data["SecOpt"] then
        add_wlan_parameter(paras, wlan_str.."BeaconType", "11i")
        add_wlan_parameter(paras, wlan_str.."IEEE11iEncryptionModes", "AESEncryption")
        if _G["defaultPasswd"] ~= data["WpaPreSharedKey"] then
            add_wlan_parameter(paras, wlan_str.."PreSharedKey.1.KeyPassphrase", data["WpaPreSharedKey"])
        end
    end

    if "tkip" == data["SecOpt"] then
        add_wlan_parameter(paras, wlan_str.."BeaconType", "WPAand11i")
        add_wlan_parameter(paras, wlan_str.."X_MixedEncryptionModes", "TKIPandAESEncryption")
        if _G["defaultPasswd"] ~= data["WpaPreSharedKey"] then
            add_wlan_parameter(paras, wlan_str.."PreSharedKey.1.KeyPassphrase", data["WpaPreSharedKey"])
        end
    end
end

function submit_guest_guide_data(paras, data)
    if data == nil then
        return
    end
    if data["ID"] == nil then
        return
    end
    local wlan_str = data["ID"]
    add_wlan_parameter(paras, wlan_str.."Enable", data["Enable"])
    add_wlan_parameter(paras, wlan_str.."SSID", data["WifiSsid"])
    data["SecOpt"] = none
    add_wlan_parameter(paras, wlan_str.."BeaconType", "Basic")
    add_wlan_parameter(paras, wlan_str.."BasicEncryptionModes", "None")
    add_wlan_parameter(paras, wlan_str.."BasicAuthenticationMode", "None")
end


local wifiguest24data = data["wifiguest24g"]

local wifiguest5data = data["wifiguest5g"]
local wlan24data = data["wifi24g"]

local wlan5data = data["wifi5g"]

submit_setting_data(paras, wlan24data)

submit_setting_data(paras, wlan5data)
submit_frequency_data(paras, wifiguest24data)
submit_frequency_data(paras, wifiguest5data)

local errcode,wifiConf = dm.GetParameterValues("InternetGatewayDevice.X_WiFi.Radio.{i}.",{"OperatingFrequencyBand"})

if utils.toboolean(data["guide"]) == false then
    for k,v in pairs(wifiConf) do
        if "2.4GHz" == v["OperatingFrequencyBand"] then
                if wlan24data ~= nil then
                    add_wlan_parameter(paras, k.."Enable", wlan24data["WifiEnable"])
                    submit_setting_data(paras, wlan24data)
                end
        else
            if "5GHz" == v["OperatingFrequencyBand"] then
                if wlan5data ~= nil then
                    add_wlan_parameter(paras, k.."Enable", wlan5data["WifiEnable"])
                    submit_setting_data(paras, wlan5data)
                end
            end
        end
    end
end

if utils.toboolean(data["guide"]) then
    print("touch set ChannelWanGuide")
    add_wlan_parameter(paras,"InternetGatewayDevice.UserInterface.X_Web.ChannelWanGuide","1")
    print("touch set ChannelWanGuide return ",guideconfig)
end
add_wlan_parameter(paras,"InternetGatewayDevice.UserInterface.X_Web.QuickConfigured","1")
print("Set QuickConfigured 1")
if wlan24data["notsetwebpassword"] ~= nil and wlan24data["notsetwebpassword"] == true then
else
    if wlan24data["upassword"] ~= nil and wlan24data["upassword"] ~= "" then
        if "cloudc" ~= cfgtool then 
            if true == utils.toboolean(web.getparaenc()) then
                local pass = web.decodepara(wlan24data["upassword"])
                if "" ~= pass then
                    wlan24data["upassword"] = pass
                end
            end
        end
        local userdomain = ""
        local errcode,uservalues = dm.GetParameterValues("InternetGatewayDevice.UserInterface.X_Web.UserInfo.{i}.",{"Userlevel"})
        for u,l in pairs(uservalues) do
            if l["Userlevel"] == 2 then
                userdomain = u
                break
            end
        end
        if "" ~= userdomain then
            add_wlan_parameter(paras, userdomain.."Userpassword",wlan24data["upassword"])
        end
    else
        if wlan24data["WepKey"] then
            local userdomain = ""
            local errcode,uservalues = dm.GetParameterValues("InternetGatewayDevice.UserInterface.X_Web.UserInfo.{i}.",{"Userlevel"})
            for u,l in pairs(uservalues) do
                if l["Userlevel"] == 2 then
                    userdomain = u
                    break
                end
            end
            if "" ~= userdomain then
                add_wlan_parameter(paras, userdomain.."Userpassword",wlan24data["WepKey"])
            end
        end
    end
end

local errcode, NeedReboot, paramerr = dm.SetParameterValues(paras)
if errcode == 0 then
if utils.toboolean(data["guide"]) then
    print("set password success then delete iptables and ebtables")
    sys.exec("iptables -D FORWARD -j DROP")
    sys.exec("ebtables -D FORWARD 1 -o !br+ -j DROP")
    sys.exec("ebtables -D FORWARD 1 -i !br+ -j DROP")
    print("iptables and ebtables delete over...")
end
else
    utils.responseErrorcode(errcode, paramerr, maps)
end
utils.appenderror("errcode", errcode)require('json')
