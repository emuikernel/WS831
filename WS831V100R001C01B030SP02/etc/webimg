---------------------0.1 公用函数定义
function compMac(a, b)
	if nil == a or nil == b then 
		return false
	end

	if string.upper(a) == string.upper(b) then
		return true
	end
	return false
end

function isWlanHostObj(layer2Interface)
    if nil ~= layer2Interface then
        local start = string.find(layer2Interface, "WLAN")
        if nil ~= start then
            return true
        end
    end
    return false
end

function isWlanConnectedHost(accessType)
	if nil ~= accessType then
		if "WIFI" == accessType or "Wireless" == accessType then
			return true
		end
	end
	return false
end

function addDeviceEthInterfaceList(interface)
	if  nil == deviceEthInterfaceList then 
		deviceEthInterfaceList = GetNullArray()
	end
	table.insert(deviceEthInterfaceList, interface)
	return
end

function isDeviceEthInterface(interface)
	if nil == deviceEthInterfaceList then 
		return false
	end
	 
    for k,v in pairs(deviceEthInterfaceList) do
        if v == interface then
            return true
        end
    end

    return false
end

function addVirtualMacList(mac)
	if  nil == virtualMacList then 
		virtualMacList = GetNullArray()
	end
	table.insert(virtualMacList, mac)
	return
end

function addRepeaterVirtualMac()
	for key, Host in pairs(ConnectedHostsObj) do 
		if Host.MappedMAC ~= Host.RealMAC then 
			addVirtualMacList(Host.MappedMAC)
		end
	end
	return
end

function isVirtualMac(mac)
	if nil == virtualMacList then 
		return false
	end
	 
    for k,v in pairs(virtualMacList) do
        if true == compMac(v, mac) then
            return true
        end
    end

    return false
end

function isActiveWlanDevice(mac)
    for k,v in pairs(wlanmacList) do
        if mac ~= "" and true == compMac(v, mac) then
            return true
        end
    end
    return false
end

function IsHilinkDevice(mac)
	for key, Obj in pairs(DevicesObj) do
		if true == compMac(mac, Obj.MAC) then
			return true
		end
	end
	return false
end

function IsMacInConnectedHost(mac, Active, layer2Interface, lastAccesstime, ConnectedHosts, HostID)
	if nil == ConnectedHosts then 
		return false
	end
	for kk, vv in pairs(ConnectedHosts) do
		
		if vv.HiLinkType == "Device" then
		     if true == IsMacInConnectedHost(mac, Active, layer2Interface, lastAccesstime, vv.ConnectedDevices, HostID) then
		     	return true
		     end
		end

		if true == compMac(mac, vv.RealMAC)  then
			if nil ~= layer2Interface then
				print("layer2Interface is "..layer2Interface)
				print("mac is "..mac)
				--无线设备判断associalist，碰到connected host也是无线，判断时间
				if true == utils.toboolean(Active) then
					if false == utils.toboolean(vv.Active) then
						--有线设备在主上在线，在从上离线，需要判断接入的接口是否有别的Device接入
						--如果有别的active的Device接入，那么就不在主设备上
						if true == isDeviceEthInterface(layer2Interface) then
							return true
						end
						vv.RemoveFlag = true
					else
						--主从设备都是active的
						if true == isWlanHostObj(layer2Interface) then
							--主设备上下挂的是无线设备
							if false == isActiveWlanDevice(mac) then
								return true
							end

							if true == isWlanConnectedHost(vv.AccessType) then
								--主从设备都是无线的
								if lastAccesstime > vv.LastAccessTime then	
									vv.RemoveFlag = true
								else
									return true
								end
							else
								return true
							end
						else
							--主设备下挂有线设备
							--暂时不考虑两个有线在线的情况，多级场景后续优化
							return true
						end
					end
				else
					--主设备是不在线的
					if true == IsActiveDevice(vv.RealMAC) and nil ~= HostID  then 
						vv.RemoveFlag = true
					end
					return true
				end
			else
				--原逻辑不变化
				if true == Active and false == utils.toboolean(vv.Active) then	
					--print("will del the connectedHosts:"..vv.ID) 
					--dm.DBDeleteObject(vv.ID)
					vv.RemoveFlag = true
					-- return false
				else
					if false == Active and true == IsActiveDevice(vv.RealMAC) and nil ~= HostID  then 
						vv.RemoveFlag = true
					end
					return true
				end
			end
		end
	end
	return false
end

function GetHostIdByMac(mac, vmac)
	for key, Obj in pairs(HostInfoObj) do
		if true == compMac(mac, Obj.MACAddress) then
			return key
		end
	end

	if nil ~= vmac then 
		for key, Obj in pairs(HostInfoObj) do
			if true == compMac(mac, Obj.MACAddress) then
				return key
			end
		end
	end

	return ""
end

function IsActiveDevice(mac)
	for key, Obj in pairs(HostInfoObj) do
		if true == compMac(mac, Obj.MACAddress) then
			if true == utils.toboolean(Obj.Active) then 
				return true
			else
				return isActiveWlanDevice(mac)
			end
		end
	end

	return false
end

function GetNullArray()
	local nullArray = {}
	table.insert(nullArray, nil)
	return nullArray
end


function GetURLInfo(UrlInfo)
	local URLInfoArray = GetNullArray()
	if nil ~= UrlInfo then 
		-- print(UrlInfo)

		URLInfoArray = json.decode(UrlInfo) 

		-- print(json.encode(URLInfoArray))
	end

	return URLInfoArray
end

------High,Middle,Low
------high -40 -- -60 好     -60 -- -75 中     -75 --  差

function GetUplinkLinkQuality(accessType, RSSI)
	if nil == RSSI and nil == accessType then 
		return "Unknown"
	end
	
	if "Wireless" ~= accessType then 
		return "High"
	end

	if nil == RSSI then
		return "Unknown" 
	end 

	local rssi = RSSI - 100
	if rssi >= -50 then 
		return "High"
	else
		if rssi >= -60 then 
			return "Medium"
		end
	end

	return "Low"
end

function GetLinkQuality(accessType, RSSI)
	if nil == RSSI and nil == accessType then 
		return "Unknown"
	end

	if "WIFI" ~= accessType then 
		return "High"
	end

	if nil == RSSI then
		return "Unknown" 
	end 

	local rssi = RSSI - 100
	if rssi >= -60 then 
		return "High"
	else
		if rssi >= -75 then 
			return "Medium"
		end
	end

	return "Low"
end

function GetWlanDeviceRSSI(mac)
	local Rssi = 0
	for k,v in pairs(wlanassocList) do
		local temparray = utils.split(v, ' ')
		if nil == temparray then 
			return 0
		end

		local tempMac  = temparray[1]
		local tempRSSI = temparray[2]

		if true == compMac(mac, tempMac) and nil ~= tempRSSI then
			-- print(Rssi)
			Rssi = tonumber(tempRSSI)
			return Rssi
		end
	end

	-- print(Rssi)
	return Rssi
end

------------------Get The HilinkDevice And the hanging STA device ----------
function GetHiLinkDevices(DevicesObj, ConnectedHostsObj)
	local HilinkDevices = GetNullArray()

	for DeviceKey,DeviceObj in pairs(DevicesObj) do
		 local newObj = {}
		 newObj = DeviceObj;
		 newObj.HiLinkType = "Device"
		 newObj.Level = 0
		 newObj.ConnectedDevices = GetNullArray()
		 newObj.ID = DeviceKey
		 newObj.Active = utils.toboolean(newObj.Active)

		 if true == utils.toboolean(DeviceObj.Active) then 
			 for HostKey,HostsObj in pairs(ConnectedHostsObj) do
			 	local p = nil
			 	p = string.find(HostKey, DeviceKey, 1)
			 	if nil ~= p then
			 		HostsObj.ID = HostKey
			 		HostsObj.Active = utils.toboolean(HostsObj.Active)
			 		table.insert(newObj.ConnectedDevices, HostsObj)	
			 	 end
			 end
		end
	 		table.insert(HilinkDevices, newObj)
	end
	return HilinkDevices
end

------------Get the Relationship between HiLinkDevice---------
function GetRelationOfDevice (HilinkDevices)
	for i = 1, #(HilinkDevices) do 
		-- print(json.encode(HilinkDevices[i]))
		local DeviceMac = HilinkDevices[i].MAC
		-- print(DeviceMac)
		for j = 1, #(HilinkDevices) do 
			if j ~= i then
				local ConectedHosts = HilinkDevices[j].ConnectedDevices
				-- print(json.encode(ConectedHosts))
				for kk, tempOBj in pairs(ConectedHosts) do
				-- for k = 1, #(ConectedHosts) do 
					if  true ==  compMac(DeviceMac, tempOBj.RealMAC) then 
						-- print("=========================Device==========")
						if tempOBj.Active == HilinkDevices[i].Active then
							HilinkDevices[i].Level = HilinkDevices[i].Level + 1
						else
							-- HilinkDevices[i].Active true  ConectedHosts[k].Active false
							-- print("del the ConnectedHostsOBj:"..tempOBj.ID)
							-- dm.DBDeleteObject(tempOBj.ID)
							table.remove(ConectedHosts, kk)	
						end
					end
				end
				-- print(json.encode(ConectedHosts))
			end
		end
		-- print("==============================")
	end

	for i = 1, #(HilinkDevices) do 
		-- print(json.encode(HilinkDevices[i]))
		local DeviceMac = HilinkDevices[i].MAC
		-- print(DeviceMac)
		for j = 1, #(HilinkDevices) do 
			if j ~= i then
				local ConectedHosts = HilinkDevices[j].ConnectedDevices
				-- print(json.encode(ConectedHosts))
				for k = 1, #(ConectedHosts) do 
					if nil ~= ConectedHosts[k] and 
					    true ==  compMac(DeviceMac, ConectedHosts[k].RealMAC) then 
						-- print("=========================Device==========")
						table.remove(ConectedHosts, k)
						-- The hilinkDevice j is the parent of i
						if 1 == HilinkDevices[i].Level - HilinkDevices[j].Level then
							table.insert(ConectedHosts, HilinkDevices[i])
						end
					end
				end
				-- print(json.encode(ConectedHosts))
			end
		end
		-- print("==============================")
	end
	-- print(json.encode(HilinkDevices))
	-- print("===================2.1========================")
	

	local HilinkRootDevice = GetNullArray()
	for i = 1, #(HilinkDevices) do 
		if  0 == HilinkDevices[i].Level then 			
			table.insert(HilinkRootDevice, HilinkDevices[i])
		end
	end
	return HilinkRootDevice;
end 

----------------key:HostId value:HostObj otherInput:HiLinkDevices
----------------If the HostObj is RootDevice or RootHost, return the obj, or return nil
function GetActiveRootDeviceOrHost(HostObj, HiLinkDevices, HostId)
	local IsRootHost = true
	local newObj = {}

	if nil == HostObj or nil == HostObj.MACAddress then 
		return nil
	end
	
	if true == IsHilinkDevice(HostObj.MACAddress) then
		-- print(HostObj.MACAddress .. "Is HilinkDevice")
		for kk,Device in pairs(HiLinkDevices) do	
			if true == compMac(HostObj.MACAddress, Device.MAC) then--待考虑和优化
				print(HostObj.MACAddress.." is root hilinkDevice")
				newObj.HostID = HostId
				newObj.Active = Device.Active
				newObj.HiLinkType = Device.HiLinkType
				newObj.MACAddress = Device.MAC
				newObj.AccessType = Device.AccessType
				newObj.URLInfo    = GetURLInfo(Device.URLInfo)
				----TODO: It's Root Device，we can Only get The LinkQuality From HostInfo
                newObj.RSSI        = Device.RSSI --GetWlanDeviceRSSI(newObj.MACAddress)
                newObj.Rate        = Device.Rate
				newObj.LinkQuality = GetUplinkLinkQuality(newObj.AccessType, newObj.RSSI)
				newObj.ConnectedDevices = Device.ConnectedDevices
				return newObj
			end
		end
		
		return nil
	end
	
	------Is not HilinkDevice, just Host, judge wheter it's in The ConnectedHosts
	------if host obj active is false,then it is false; if host obj active is true, we need to judge it through IsActiveDevice
	if true == HostObj.Active then
		HostObj.Active = IsActiveDevice(HostObj.MACAddress)
	end

	print("HostObj.Active is "..HostObj.Active)
	if true == isVirtualMac(HostObj.MACAddress) or false == utils.toboolean(HostObj.Active) or false == compMac(HostObj.MACAddress, HostObj.VirtualMACAddress) then 
		IsRootHost = false
	end
	
	if true  == IsRootHost then 
		for kk,Device in pairs(HiLinkDevices) do		
			if  true == IsMacInConnectedHost(HostObj.MACAddress, HostObj.Active, HostObj.Layer2Interface, HostObj.LastAccessTime, Device.ConnectedDevices, HostId) then 
				print("HostObj.MACAddress under connected host "..HostObj.MACAddress)
				IsRootHost = false
				break
			end
		end
		
		for kk,Device in pairs(HiLinkDevices) do
			--print(json.encode(Device.ConnectedDevices))
			for kk, vv in pairs(Device.ConnectedDevices) do 
				if true == vv.RemoveFlag then 
					table.remove(Device.ConnectedDevices, kk)
				end
			end 
		end
	end


	if true == IsRootHost then
		--print(HostObj.MACAddress.." is under Root")
		newObj.HostID = HostId
		newObj.HiLinkType = "None"
		newObj.MACAddress = HostObj.MACAddress
		if true == isActiveWlanDevice(HostObj.MACAddress) then
			newObj.AccessType = "WIFI"
			newObj.RSSI        = GetWlanDeviceRSSI(HostObj.MACAddress)
		else
			newObj.AccessType = "Ethernet"
			newObj.RSSI       = 0
		end
		newObj.ConnectedDevices = GetNullArray()
		----TODO: It's Root Device，we can Only get The LinkQuality From HostInfo
		
		newObj.LinkQuality = GetLinkQuality(newObj.AccessType, newObj.RSSI)
		newObj.Active = HostObj.Active
		return newObj
	end	

	return nil
end

function GetAllActiveRootDeviceOrHost(HostInfoObjs, HiLinkDevices)
	local HostInfos = GetNullArray()
	for key, obj in pairs(HostInfoObj) do
		--if true == IsActiveDevice(obj.MACAddress) then 
		-- print("GetAllActiveRootDeviceOrHost  " ..obj.MACAddress )
		local newObj = GetActiveRootDeviceOrHost(obj, HiLinkDevices, key)
		table.insert(HostInfos, newObj)
		--end
	end
	return HostInfos
end

function IsMacInUnderDevice(mac, Active, layer2Interface, lastAccesstime, ConnectedHosts, HostID)
	if nil == mac or nil == ConnectedHosts then 
		return false
	end

	for key, Device in pairs(ConnectedHosts) do
		-- print(key)
		-- print(Device.HiLinkType)
		if Device.HiLinkType == "Device" then
			if true == IsMacInConnectedHost(mac, Active, layer2Interface, lastAccesstime, Device.ConnectedDevices, HostID) then
				-- print(mac .. " is under ".. json.encode(Device) .."============")
				return true
			else 
				-- print(mac .. " is not under ".. json.encode(Device) .."============")
			end
		end
	end
	return false
end

-------------------Filter the dummpy connected host
function FilterDevice(Device)
	if nil == Device then
		return
	end

	local ConnectedHosts = Device.ConnectedDevices 
	local newConnectedHosts = GetNullArray()

	for key, Host in pairs(ConnectedHosts) do
		local tempHost = {}
		if "Device" ==  Host.HiLinkType then 
			tempHost.HostID = GetHostIdByMac(Host.MAC, nil)
			tempHost.MACAddress = Host.MAC
			tempHost.HiLinkType = Host.HiLinkType
			tempHost.ConnectedDevices = Host.ConnectedDevices
			tempHost.AccessType = Host.AccessType
			---TODO: HiLinkDevice cannot get Quality now
			tempHost.RSSI = Host.RSSI
			tempHost.Rate        = Host.Rate
			tempHost.LinkQuality = GetUplinkLinkQuality(tempHost.AccessType, tempHost.RSSI)
			tempHost.URLInfo     = GetURLInfo(Host.URLInfo)
			tempHost.Active = Host.Active
			if "" ~= tempHost.HostID then
				table.insert(newConnectedHosts, tempHost)
				FilterDevice(tempHost)
			end
		end
	end

	for key, Host in pairs(ConnectedHosts) do
		local tempHost = {}
		if nil ==  Host.HiLinkType or "None" == Host.HiLinkType then
			-- print('=================='..Host.MappedMAC.. '===============')
			-- print(IsActiveDevice(Host.MappedMAC))	
			if false == IsMacInUnderDevice(Host.RealMAC, Host.Active, nil, nil, ConnectedHosts, Host.ID)
			   and true == Host.Active and false == isVirtualMac(Host.RealMAC) then
				-- print('==================Host==============='..Host.MappedMAC)
				tempHost.HostID = GetHostIdByMac(Host.RealMAC, Host.MappedMAC)
				tempHost.MACAddress = Host.RealMAC
				tempHost.HiLinkType = "None"
				tempHost.ConnectedDevices = {}
				tempHost.AccessType = Host.AccessType
				tempHost.LinkQuality = GetLinkQuality(Host.AccessType, Host.RSSI)
				tempHost.RSSI = Host.RSSI
				tempHost.Active = Host.Active
				if "" ~= tempHost.HostID then
					table.insert(tempHost.ConnectedDevices, nil)
					table.insert(newConnectedHosts, tempHost)
					--print("==================FilterDevice= end=============="..json.encode(tempHost))
				end
			end	
		end

	end

	Device.ConnectedDevices = newConnectedHosts
	-- print("==================FilterDevice= end=============="..json.encode(Device))
	return
end


function MacToIntArray(mac)
	if mac == nil then 
		return 0
	end

	local mac_addr_tb = utils.split(mac, ':')

	-- print(json.encode(mac_addr_tb))
	local mac_addr_last = mac_addr_tb[4]..mac_addr_tb[5]..mac_addr_tb[6]
	local mac_addr_before = mac_addr_tb[1]..mac_addr_tb[2]..mac_addr_tb[3]
	local mac_int_1 = tonumber('0x'..mac_addr_before)
	local mac_int_2 = tonumber('0x'..mac_addr_last)

	-- print("mac_int_1  ", mac, mac_int_1, string.format("%02x",tostring(mac_int_1)))
	-- print("mac_int_2   ", mac,  mac_int_2, string.format("%02x",tostring(mac_int_2)))

	return mac_int_1, mac_int_2
end


function IsSameHilinkDeviceByMac(mac1, mac2)
	
	local mac1_int1, mac1_int2 = MacToIntArray(mac1)
	local mac2_int1, mac2_int2 = MacToIntArray(mac2)

	if mac1_int1 ~= mac2_int1 then 
		return false
	end 

	local mac_diff = math.abs(mac1_int2 - mac2_int2)
	

	if mac_diff > 0 and mac_diff < SAME_HILINK_DEVICE_MAX_MAX_DIFF then 
		-- print(mac_diff, "true")
		return true
	end

	-- print(mac_diff, "false")
	return false
end

function filterDummpyHilinkDeviceByMac(DeviceObjs)

	print("====================filterDummpyHilinkDeviceByMac=================")
	print(json.encode(DeviceObjs))

	for key, newObj in pairs(DeviceObjs) do
		-- print("=---------------------------=",key, json.encode(newObj))
		local bFind = false

		for hostKey, newHostObj in pairs(HostInfoObj) do
			if true == compMac(newObj["MAC"], newHostObj["MACAddress"]) then
				newObj["Active"] = newHostObj["Active"]
				bFind = true
				if true == newObj["Active"] 
					and false == isWlanHostObj(newHostObj["Layer2Interface"]) then
					addDeviceEthInterfaceList(newHostObj["Layer2Interface"])
				end
				break
			end
		end

		if true ~= bFind then
			newObj["Active"] = false
		end

		if false == utils.toboolean(newObj["Active"]) then 
			print (newObj["MAC"] , "mac is unAcitve")
			
			for key1, newTmpObj in pairs(DeviceObjs) do
				print(key, key1)
				if key1 ~= key 
					and true == IsSameHilinkDeviceByMac(newObj["MAC"], newTmpObj["MAC"]) then
					print (newObj["MAC"] , "mac is dumppy with ", newTmpObj["MAC"])
					DeviceObjs[key] = nil
					-- table.remove(HostInfos, key)
					break
				end
			end
		end
	end
end

print("=======================0 prepare begin ============================")
function compRealMac(a, b)
	return a.RealMAC > b.RealMAC;
end

function filterDummpyNode()
	for kk, newDevice in pairs(DevicesObj) do 
		local deviceMacDevice = newDevice["MAC"]
		local deviceAcitve    = utils.toboolean(newDevice["Active"])
		local deviceIPaddr = nil
		--在connectedHost中加入他的直接接入节点信息
		for HostKey,HostsObj in pairs(ConnectedHostsObj) do
			local p = nil
			p = string.find(HostKey, kk, 1)
			if nil ~= p then
			 	HostsObj.DeviceMac = deviceMacDevice
			end
		end

		if nil ~= deviceMacDevice and true == deviceAcitve then 
			for key, newObj in pairs(HostInfoObj) do
				if true == compMac(deviceMacDevice, newObj["MACAddress"]) then 
					deviceIPaddr = newObj["IPAddress"]
					break
				end
			end

			for key, newObj in pairs(HostInfoObj) do
				if newObj["IPAddress"] == deviceIPaddr and false == compMac(deviceMacDevice,  newObj["MACAddress"]) then
					addVirtualMacList(newObj["MACAddress"])
				end
			end
		end
	end

	table.sort(ConnectedHostsObj, compRealMac)
	print("ConnectedHostsObj = " .. json.encode(ConnectedHostsObj))

	local lastKey = nil
	local lastConnectedHost = nil
	for k, newConnectedHost in pairs(ConnectedHostsObj) do 
		local IsLastKeyValid = true
		if nil == lastKey or nil == lastConnectedHost then
			IsLastKeyValid = false
		end

		if true == IsLastKeyValid then
			if newConnectedHost["RealMAC"] == lastConnectedHost["RealMAC"] then
				if true == isWlanConnectedHost(newConnectedHost["AccessType"]) and true == isWlanConnectedHost(lastConnectedHost["AccessType"]) then
					if newConnectedHost["LastAccessTime"] < lastConnectedHost["LastAccessTime"] then
						lastConnectedHost.RemoveFlag = true
						lastKey = k
						lastConnectedHost = newConnectedHost
					else
						newConnectedHost.RemoveFlag = true
					end
				end
			end
		end
	end
end

filterDummpyHilinkDeviceByMac(DevicesObj)
print(json.encode(DevicesObj))

filterDummpyNode()
addRepeaterVirtualMac()
print("virtualMacList= "..json.encode(virtualMacList))

