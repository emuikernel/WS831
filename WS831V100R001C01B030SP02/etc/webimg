local json = require('json')

local tonumber = tonumber

local result = {}
-- get version and size of image from hotainfo file
local FileData = {}
local DownloadProcess = 0
local tmpsize = 0
local imagesize = 0
local state = 0x10
local newState = 0x10
local version = ""

local hotafile = io.open("/var/hotainfo", "r")
if nil ~= hotafile then
	for line in hotafile:lines() do
    	    kv = utils.split(line, '=');
    	    if nil ~= kv and nil ~= kv[1] and nil ~= kv[2] then
		FileData[kv[1]] = kv[2];
 	    end
	end
	hotafile:close();
end

-- init the value 
if nil ~= FileData['size'] then
	imagesize = tonumber(FileData['size'])
end
if nil ~= FileData['version'] then
	version = FileData['version']
end
if nil ~= FileData['state'] then 
	state = tonumber(FileData['state'])
end

if nil ~= FileData['newState'] then
	newState = tonumber(FileData['newState'])
end
	

-- get the downloadProcess from the file size
if 0 ~= imagesize then	
	local tmpfile = io.open ("/var/upg.bin", "r")
	if nil ~= tmpfile then
		tmpsize = tmpfile:seek("end")
		tmpfile:close()
	end
	
	tmpfile = io.open ("/var/upg_tmp.bin", "r")
	if nil ~= tmpfile then
		tmpsize = tmpsize + tmpfile:seek("end")
		tmpfile:close()
	end
end

if tmpsize > imagesize then 
	tmpsize = imagesize
end

if 0 ~= imagesize then	
	DownloadProcess = math.floor(100 * tmpsize / imagesize)
end


result["NewUpdateState"] = newState

result["UpdateState"] = state
result["Version"]  = version
result["ImageSize"] = imagesize
result["DownloadSize"] = tmpsize
result["DownloadProcess"] = DownloadProcess


local user_language = "zh"
if "cloudc" ~= FormData["cfgtool"] then
	user_language = web.lang()
end

if nil ~= sys.getchangelog(user_language) then
	result["UpgradeContent"] = sys.getchangelog(user_language)
else
	result["UpgradeContent"] = ""
end
sys.print(json.encode(result))
