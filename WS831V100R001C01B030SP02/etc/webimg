local login_user = ""
local login_level = 2

if "cloudc" ~= FormData["cfgtool"] then
    login_user, login_level = web.getuserinfo()
end

local allusers = {}

local errcode,wlanBasic = dm.GetParameterValues("InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.",{"X_OperatingFrequencyBand","BeaconType"});

local WpaPreSharedKey = ""
local WpaPreSharedKey_5G = ""
local wifiData = {}
local isWpaPreSharedKeySame = false
function calc_min(frequency)
    local array = {}
    i = 1
    if wlanBasic ~= nil then
        for k,v in pairs(wlanBasic) do
            if frequency == v["X_OperatingFrequencyBand"] then
                local wlan_instance_id, wlan_suffix = utils.findLastPoint(k)
                array[i] = tonumber(wlan_instance_id)
                i = i+1
            end
        end
        table.sort(array)
        if 1 == i  then
            return -1
        else
            return array[1]
        end
    end
    return -1
end
local wlan_index = calc_min("2.4GHz")
local wlan_5gindex = calc_min("5GHz")
function isValidIndex(k)
    local  wlan_instance_id, wlan_suffix = utils.findLastPoint(k)    
    if wlan_index == tonumber(wlan_instance_id) or wlan_5gindex == tonumber(wlan_instance_id)  then
        return true
    end
    return false
end

if wlanBasic ~=nil then
    for k,v in pairs(wlanBasic) do
        if isValidIndex(k) then
            local err, wlankey = dm.GetParameterValues(k.."PreSharedKey.1.", { "KeyPassphrase" });
            local obj = wlankey[k.."PreSharedKey.1."]
            local temp_pass = obj["KeyPassphrase"]
            if v["X_OperatingFrequencyBand"] == "2.4GHz" then
                WpaPreSharedKey = temp_pass
                wifiData.BeaconType = v["BeaconType"] 
            end
            if v["X_OperatingFrequencyBand"] == "5GHz" then
                WpaPreSharedKey_5G = temp_pass
                wifiData.BeaconType_5G = v["BeaconType"] 
            end
        end
    end
end

if WpaPreSharedKey_5G ~= nil then
    if WpaPreSharedKey == WpaPreSharedKey_5G then
        isWpaPreSharedKeySame = true
    end
else
	isWpaPreSharedKeySame = true
end

if wifiData.BeaconType == "None" or wifiData.BeaconType_5G == "None" then
    isWpaPreSharedKeySame = false
end


if login_level == 1 then        
    for k, v in pairs(values) do
        if login_user == v["Username"] then
            local user = {}
            user.username = v["Username"]
            user.userlevel = v["Userlevel"]
            if "cloudc" ~= FormData["cfgtool"] and isWpaPreSharedKeySame == true and v["Userpassword"] == web.sha256Encode(WpaPreSharedKey) then
                user.LoginWifiKeySame = true
            end
            user.enableprompt =  utils.toboolean(v["EnablePasswdPrompt"])
            user.promptinfo = v["UserpasswdPrompt"]
            user.DBenableprompt =  utils.toboolean(v["EnablePasswdPrompt"])
            user.DBpromptinfo = v["UserpasswdPrompt"]
            user.ID = k
            user.FirstLogin = v["X_IsFirst"]
            table.insert(allusers, user)
            break
        end
    end
else
    for k, v in pairs(values) do
        local user = {}
        user.username = v["Username"]
        user.userlevel = v["Userlevel"]
            if "cloudc" ~= FormData["cfgtool"] and isWpaPreSharedKeySame == true and v["Userpassword"] == web.sha256Encode(WpaPreSharedKey) then
                user.LoginWifiKeySame = true
            end
        user.enableprompt =  utils.toboolean(v["EnablePasswdPrompt"])
        user.promptinfo = v["UserpasswdPrompt"]
        user.DBenableprompt =  utils.toboolean(v["EnablePasswdPrompt"])
        user.DBpromptinfo = v["UserpasswdPrompt"]
        user.ID = k
        user.FirstLogin = v["X_IsFirst"]
        table.insert(allusers, user)
    end
end

utils.multiObjSortByID(allusers)
sys.print(json.encode(allusers))local utils = require('utils')
if "cloudc" ~= cfgtool then 
	local web = require('web')
end
local tostring = tostring

local maps = {
	username = "Username",
	newpwd = "Userpassword",
	enableprompt = "EnablePasswdPrompt",
	promptinfo = "UserpasswdPrompt"
}

function get_userlevel_by_id(ID)
    local errcode,values = dm.GetParameterValues(ID, {"Userlevel"});
    if values == nil or values[ID] == nil then
        return 0
    end

    return tonumber(values[ID]["Userlevel"])
end

function get_password_by_id(ID)
    local errcode,values = dm.GetParameterValues(ID, {"Userpassword"});
    if values == nil or values[ID] == nil then
        return nil
    end

    return values[ID]["Userpassword"]
end

function add_one_parameter(paras, name, value)
	if nil == value then
		return 
	end	
	table.insert(paras, {name, value})
end

function add_submit_data(paras, domain, data, userlevel)
    add_one_parameter(paras, domain.."Userpassword", data["newpwd"])
    if userlevel ~= 2 then
        add_one_parameter(paras, domain.."Username", data["username"])
    end
end

function submit_data(paras, data)
	if nil == data then
		return
	end
	if nil == data["ID"] then
		return 
	end	
	local domain = data["ID"]
	
	if "cloudc" == cfgtool then
		---- cannot change the password by cloud
		-- add_submit_data(paras, domain, data, set_level)
		utils.appenderror("errcode", 1)
		return
	end
	
    local login_user, login_level = web.getuserinfo()
    local set_level = web.getuserlevel(get_userlevel_by_id(domain))

    -- admin change user's name and password
    if login_level == 2 and set_level == 1 then
        add_submit_data(paras, domain, data, set_level)
        return
    end

    if login_level == set_level then
        local db_pwd = get_password_by_id(domain)
        if db_pwd == nil or db_pwd ~= data["curpwd"] then
            if db_pwd == nil or db_pwd ~= web.sha256Encode(data["curpwd"]) then
                utils.appenderror("errcode", 10)
                utils.appenderror("curpwd", "useraccount.errpwd")
                return
            end    
        end
        add_submit_data(paras, domain, data, set_level)
        return
    end

    utils.appenderror("errcode", 10)
    utils.appenderror("curpwd", "useraccount.erruser")
end

function create()
    local paras = utils.GenAddObjParamInputs(data, maps)
    
    local errcode, instnum, NeedReboot, paramerr= dm.AddObjectWithValues("InternetGatewayDevice.UserInterface.X_Web.UserInfo.", paras);
    if 0 == errcode then
    	utils.appenderror("ID", "InternetGatewayDevice.UserInterface.X_Web.UserInfo."..instnum..".")
    end	
    utils.responseErrorcode(errcode, paramerr, maps)
    return errcode
end

function delete()
    return dm.DeleteObject(data["ID"])
end

function update()	
	local paras = {}
	submit_data(paras, data)
    local errcode, NeedReboot, paramerr =  dm.SetParameterValues(paras)
    utils.responseErrorcode(errcode, paramerr, maps)
    if errcode ~= 0 then
    	_G["webtimes"] = _G["webtimes"] + 1
    	if _G["webtimes"] > 2 then
    		_G["webtimes"] = 0
    		web.logout()
    	end
    else
    	_G["webtimes"] = 0
    end
    return errcode
end
-- judge data is update or delete
if action == "update" or action == "delete" then
    print("useraccount "..action,data["ID"])
    if nil == data["ID"] or nil == string.find(data["ID"], "UserInfo.") then
        utils.appenderror("errcode", 9003)
        return
    end
end
if action == 'create' then
    err = create()
elseif action == 'update' then
    err = update()
elseif action == 'delete' then
    err = delete()
else
    return
end
utils.appenderror("errcode", err)
