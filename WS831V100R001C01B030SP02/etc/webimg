local utils = require('utils')

local domain = "InternetGatewayDevice.Services.X_Mirror."

local maps = {
    Enable="Enable",
    CapturePort = "LanInterface",
    DestPort = "TypeInterface",
    EffectTime = "EffectTime"
}

function checkLanInterace(lan_interface)
    local LanInteraceAvailable = {"LAN1","LAN2","LAN3","LAN4"}
    for k,v in pairs(LanInteraceAvailable) do
        if lan_interface == v then
            return true
        end
    end
    return false
end
function checkTypeInterace(type_interface)
    local TypeInteraceAvailable = {"dsl","ethwan","wlan"}
    for k,v in pairs(TypeInteraceAvailable) do
        if type_interface == v then
            return true
        end
    end
    return false
end

if true ~= data["Enable"] and false ~= data["Enable"] then
    utils.appenderror("errcode", "9003")
    return
end

if false == checkLanInterace(data["CapturePort"]) then
    utils.appenderror("errcode", "9003")
    return
end

if false == checkTypeInterace(data["DestPort"]) then
    utils.appenderror("errcode", "9003")
    return
end

local param = utils.GenSetObjParamInputs(domain, data, maps)
local err,needreboot, paramerror = dm.SetParameterValues(param);
utils.responseErrorcode(err, paramerror, maps)
local utils = require('utils')
