print("===========>enter smartConfig.lua")
local maps = {
    ssid = "SSID",
    password = "PreSharedKey"
}

local typemaps = {
     ConnectType = "ConnectType"
}

local errcode, NeedReboot, paramerr, instnum 


local param = {{"InternetGatewayDevice.LANDevice.1.WLANEndPoint.1.ConnectType", 2}}
local err,needreboot, paramerror = dm.SetParameterValues(param)
print("set wlan ConnectType", err)
utils.responseErrorcode(err, paramerror, typemaps)

if nil ~= profile_values and 1 == flag then
    -- update
    print("333333333333333333333333333333333333")
    domain = "InternetGatewayDevice.LANDevice.1.WLANEndPoint.1.Profile.1."
    local paras = utils.GenSetObjParamInputs(domain, data, maps)
    -- print(json.encode(paras))
    errcode, NeedReboot, paramerr = dm.SetParameterValues(paras)
    utils.responseErrorcode(errcode, paramerr, maps)
    print("update" ,errcode)
else
    -- Add
    print("444444444444444444444444444444444")
    domain = "InternetGatewayDevice.LANDevice.1.WLANEndPoint.1.Profile."
    local paras = utils.GenAddObjParamInputs(data, maps)
    -- print(json.encode(paras))
    errcode, instnum, NeedReboot, paramerr= dm.AddObjectWithValues(domain, paras);
    print("addd" ,errcode)
 end

local web = require('web')

web.setLang(data["Language"])
require('dm')
require('json')
require('utils')

if "cloudc" ~= cfgtool then 
    if true == utils.toboolean(web.getparaenc()) then
        if data["WepKey"] ~= nil then
            local pass = web.decodepara(data["WepKey"])
            if "" ~= pass then
                data["WepKey"] = pass
            end
        end
        if data["wifiwisprpwd"] ~= nil then
            local pass = web.decodepara(data["wifiwisprpwd"])
            if "" ~= pass then
                data["wifiwisprpwd"] = pass
            end
        end
        if data["userpassword"] ~= nil then
            local pass = web.decodepara(data["userpassword"])
            if "" ~= pass then
                data["userpassword"] = pass
            end
        end
    end
end

if nil ~= data["WEPKey"] then
    data["WEPKey"] = web.decodeURIComponent(data["WEPKey"])
end

local maps = {
    BSSID = "BSSID",
    Channel = "Channel",
    WifiAuthMode = "SecurityMode",----WEP WPA2 open share
    EncryptionType = "EncryptionType",--AES TPIK
    WepKey = "WEPKey",
    WepEncyptionIndex = "WEPKeyIndex",
    wifiwisprpwd = "PreSharedKey",
    WifiSsid     = "SSID"
}

local typemaps = {
    AddManuel = "ConnectType"
}

local errcode, profile_values = dm.GetParameterValues("InternetGatewayDevice.LANDevice.1.WLANEndPoint.1.Profile.{i}.",{"SSID","BSSID"})

if data["WifiAuthMode"] == "WEP" or data["EncryptionType"] == "WEP" then
    data["WifiAuthMode"] = data["WepEncyptionMode"]
end

if data["EncryptionType"] == "TKIP/AES" then
    data["EncryptionType"] = "TKIPAES"
end

local domain = ""
local flag = 1

if nil ~= profile_values then
    flag = 0
    for k, v in pairs(profile_values) do
        domain = k
        flag = 1
    end
else
    flag = 0
end
local webparas = {}
function add_parameter(paras, name, value)
    if nil == value then
        return
    end
    table.insert(paras, {name, value})
end


local hasguide = true
local errcode,guideobj = dm.GetParameterValues("InternetGatewayDevice.UserInterface.X_Web.", {"ChannelWanGuide"});
if nil ~= guideobj and nil ~= guideobj["InternetGatewayDevice.UserInterface.X_Web."] then 
    hasguide = utils.toboolean(guideobj["InternetGatewayDevice.UserInterface.X_Web."]["ChannelWanGuide"]) 
end


if false == hasguide and utils.toboolean(data["isguide"]) then 
    local errcode,uservalues = dm.GetParameterValues("InternetGatewayDevice.UserInterface.X_Web.UserInfo.{i}.",{"Userlevel"})
    local userdomain = ""
    for k,v in pairs(uservalues) do
        if v["Userlevel"] == 2 then
            userdomain = k
            break
        end
    end
    
    print("wifidial set ChannelWanGuide")
    if "" ~= userdomain then
        add_parameter(webparas, userdomain.."Userpassword",data["userpassword"])
    end
    add_parameter(webparas,"InternetGatewayDevice.UserInterface.X_Web.ChannelWanGuide","1")
end

add_parameter(webparas, "InternetGatewayDevice.X_WiFi.Radio.1.WLANMode","3")
local err,needreboot, paramerror = dm.SetParameterValues(webparas);
print("wlanmode and userpassword set err=",err)

--delete iptables ebtables rules
if false == hasguide and utils.toboolean(data["isguide"]) then 
    print("set password success then delete iptables and ebtables")
    sys.exec("iptables -D FORWARD -j DROP")
    sys.exec("ebtables -D FORWARD 1 -o !br+ -j DROP")
    sys.exec("ebtables -D FORWARD 1 -i !br+ -j DROP")
    print("iptables and ebtables delete over...")
end



local errcode, NeedReboot, paramerr, instnum
if nil ~= profile_values and 1 == flag then
    -- update
    domain = "InternetGatewayDevice.LANDevice.1.WLANEndPoint.1.Profile.1."
    local paras = utils.GenSetObjParamInputs(domain, data, maps)
    errcode, NeedReboot, paramerr = dm.SetParameterValues(paras)
    utils.responseErrorcode(errcode, paramerr, maps)
    print("update profile..." ,errcode)
else
    -- Add
    domain = "InternetGatewayDevice.LANDevice.1.WLANEndPoint.1.Profile."
    local paras = utils.GenAddObjParamInputs(data, maps)
    errcode, instnum, NeedReboot, paramerr= dm.AddObjectWithValues(domain, paras);
    print("add profile..." ,errcode)
end

if 0 ~= errcode then
    print("set wifi data profile error", errcode)
    utils.responseErrorcode(errcode, paramerr, maps)
else
    domain = "InternetGatewayDevice.LANDevice.1.WLANEndPoint.1."
    local param = {{domain.."ConnectType", data["AddManuel"]}}
    local err,needreboot, paramerror = dm.SetParameterValues(param)
    utils.responseErrorcode(err, paramerror, typemaps)
end       require('dm')
require('json')
require('utils')

local tostring = tostring
local tonumber = tonumber

if FormData["frequency"] ~= nil then
    frequency = FormData["frequency"]
end

local isShowPass = true
local isShowPass5g = true
if FormData["type"] ~= nil then
    if FormData["type"]  == "notshowpass24" then
        isShowPass = false
    end
    if FormData["type"]  == "notshowpass5g" then
        isShowPass5g = false
    end

    if FormData["type"]  == "notshowpassall" then
        isShowPass = false
        isShowPass5g = false
    end
end

local errcode,wifiConf = dm.GetParameterValues("InternetGatewayDevice.X_WiFi.Radio.{i}.",
    {
        "Enable",
        "OperatingFrequencyBand"
    }
);

function get_WifiEnable(frequency)
    for k, v in pairs(wifiConf) do
        if frequency == v["OperatingFrequencyBand"] then
            return utils.toboolean(v["Enable"])
        end
    end

    return false
end

local domain = "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}."

local errcode,wlanBasic = dm.GetParameterValues(domain,
    {
        "Enable",
        "X_OperatingFrequencyBand",
        "RegulatoryDomain",
        "Channel",
        "AutoChannelEnable",
        "X_WlanStandard",
        "MaxBitRate",
        "TransmitPower",
        "SSID",
        "SSIDAdvertisementEnabled",
        "X_WlanIsolateControl",
        "WMMEnable",
        "X_Wlan11NHtMcs",
        "X_Wlan11NBWControl",
        "X_Wlan11NGIControl",
        "X_AssociateDeviceNum",
        "BeaconType",
        "BasicEncryptionModes",
        "BasicAuthenticationMode",
        "WEPEncryptionLevel",
        "WEPKeyIndex",
        "WPAEncryptionModes",
        "IEEE11iEncryptionModes",
        "X_MixedEncryptionModes",     
        "X_Wlan11NTxRxStream"
    }
);

local wlanCfgs = {}
function calc_min(frequency)
    local array = {}
    i = 1
    if wlanBasic ~= nil then 
        for k,v in pairs(wlanBasic) do
            if frequency == v["X_OperatingFrequencyBand"] then
                wlan_instance_id, wlan_suffix = utils.findLastPoint(k)
                array[i] = tonumber(wlan_instance_id)
                i = i+1
            end    
        end
        table.sort(array)
        if 1 == i  then
            return -1    
        else
            return array[1]
        end    
    end    
    return -1
end

wlan_index = calc_min("2.4GHz")

wlan_5gindex = calc_min("5GHz")

function isValidIndex(k)
    local  wlan_instance_id, wlan_suffix = utils.findLastPoint(k)
    if wlan_index == tonumber(wlan_instance_id)  or wlan_5gindex == tonumber(wlan_instance_id)  then
        return true
    end

    return false
end

for k,v in pairs(wlanBasic) do
    local needshowpwd = false
    if (v["X_OperatingFrequencyBand"] == "2.4GHz" and isShowPass) then
        needshowpwd = true
    end
    if (v["X_OperatingFrequencyBand"] == "5GHz" and isShowPass5g) then
        needshowpwd = true
    end
    if isValidIndex(k) then
        local WepKey = {}
        wepkey_conf_str = k.."WEPKey."..tostring(v["WEPKeyIndex"]).."."
        local err_wep, wep_data = dm.GetParameterValues(k.."WEPKey.{i}.", { "WEPKey" });
        for kk, vv in pairs(wep_data) do
            wep_instance_id, wep_suffix = utils.findLastPoint(kk)
            WepKey[wep_instance_id] = _G["defaultPasswd"]
            if needshowpwd then
                WepKey[wep_instance_id]  = vv["WEPKey"]
            end
        end
        wpakey_conf_str = k.."PreSharedKey.1."
        local err_wpakey, wap_key_data = dm.GetParameterValues(wpakey_conf_str, { "PreSharedKey", "KeyPassphrase" });
        for kk, vv in pairs(wap_key_data) do
            WpaPreSharedKey = _G["defaultPasswd"]
            if needshowpwd then
                WpaPreSharedKey = vv["KeyPassphrase"]
            end
            break
        end

        local wlanInstance = {}

        wlanInstance.ID = k
        wlanInstance.FrequencyBand = v["X_OperatingFrequencyBand"]
        wlanInstance.WifiEnable = get_WifiEnable(v["X_OperatingFrequencyBand"])
        wlanInstance.RegulatoryDomain = v["RegulatoryDomain"]
        if utils.toboolean(v["AutoChannelEnable"]) == true then 
            wlanInstance.Channel = 0
        else
            wlanInstance.Channel = v["Channel"]
        end    
        wlanInstance.WlanStandard = v["X_WlanStandard"]
        if "Auto" == v["MaxBitRate"] then
            wlanInstance.MaxBitRate = "0"
        else
            wlanInstance.MaxBitRate = v["MaxBitRate"]
        end    
        wlanInstance.TransmitPower = v["TransmitPower"]
        wlanInstance.WifiSsidEnable = utils.toboolean(v["Enable"])

        wlanInstance.WifiSsid = v["SSID"]
        wlanInstance.WifiHideBroadcast = utils.opsiteBoolVal(v["SSIDAdvertisementEnabled"])
        wlanInstance.X_WlanIsolateControl = utils.toboolean(v["X_WlanIsolateControl"])
        wlanInstance.WMMEnable = utils.toboolean(v["WMMEnable"])
        if 33 == v["X_Wlan11NHtMcs"] then
            wlanInstance.X_Wlan11NHtMcs = "auto"
        else    
            wlanInstance.X_Wlan11NHtMcs = v["X_Wlan11NHtMcs"]
        end
        if "20/40" == v["X_Wlan11NBWControl"] then
            wlanInstance.X_Wlan11NBWControl = "20_40"
        else    
            wlanInstance.X_Wlan11NBWControl = v["X_Wlan11NBWControl"]
        end    
        wlanInstance.X_Wlan11NGIControl = v["X_Wlan11NGIControl"]
        wlanInstance.X_AssociateDeviceNum = v["X_AssociateDeviceNum"]
        wlanInstance.X_Wlan11NTxRxStream = v["X_Wlan11NTxRxStream"]
        if "Basic" == v["BeaconType"] and "None" == v["BasicEncryptionModes"] then
            wlanInstance.BeaconType = "None"
        else
            wlanInstance.BeaconType = v["BeaconType"]
        end    
        wlanInstance.BasicEncryptionMode = v["BasicEncryptionModes"]
        wlanInstance.BasicAuthMode = v["BasicAuthenticationMode"]
        if "40-bit" == v["WEPEncryptionLevel"] then
            wlanInstance.WepKeyLen = 64
        end
        if "104-bit" == v["WEPEncryptionLevel"] then
            wlanInstance.WepKeyLen = 128  
        end 
        wlanInstance.WepKeyIndex = v["WEPKeyIndex"]
        wlanInstance.WepKey = WepKey
        wlanInstance.WpaPreSharedKey = WpaPreSharedKey
        wlanInstance.WpaPreSharedKeyTemp = WpaPreSharedKey
        if "TKIPEncryption" == v["WPAEncryptionModes"] then
            wlanInstance.WpaEncryptionMode = "TKIP"
        end
        if "AESEncryption" == v["WPAEncryptionModes"] then
            wlanInstance.WpaEncryptionMode = "AES"
        end    
        if "TKIPandAESEncryption" == v["WPAEncryptionModes"] then
            wlanInstance.WpaEncryptionMode = "TKIP+AES"
        end    
        if "TKIPEncryption" == v["IEEE11iEncryptionModes"] then
            wlanInstance.IEEEiEncryptionMode = "TKIP"
        end
        if "AESEncryption" == v["IEEE11iEncryptionModes"] then
            wlanInstance.IEEEiEncryptionMode = "AES"
        end    
        if "TKIPandAESEncryption" == v["IEEE11iEncryptionModes"] then
            wlanInstance.IEEEiEncryptionMode = "TKIP+AES"
        end 
        if "TKIPEncryption" == v["X_MixedEncryptionModes"] then
            wlanInstance.MixedEncryptionModes = "TKIP"
        end
        if "AESEncryption" == v["X_MixedEncryptionModes"] then
            wlanInstance.MixedEncryptionModes = "AES"
        end    
        if "TKIPandAESEncryption" == v["X_MixedEncryptionModes"] then
            wlanInstance.MixedEncryptionModes = "TKIP+AES"
        end 

        --just for quick set wifi
        if wlanInstance.BeaconType == "None" then 
        	wlanInstance.WpaPreSharedKey = ""
        	wlanInstance.isUnEncytion = true
        else
        	wlanInstance.isUnEncytion = false
        end
        table.insert(wlanCfgs, wlanInstance)
    end   
end

utils.multiObjSortByID(wlanCfgs)

local i2G = 1
local i5G = 1
for k, v in pairs(wlanCfgs) do
    if v['FrequencyBand'] == '5GHz' then
        v['SSIDIndex'] = i5G
        i5G = i5G + 1
    else
        v['SSIDIndex'] = i2G
        i2G = i2G + 1
    end
end

sys.print(json.encode(wlanCfgs))local utils = require('utils')
local print = print
local tostring = tostring
local maps = {
    wpakey = "KeyPassphrase",
    WifiEnable = "Enable",
    WifiSsid = "SSID",
    WifiHideBroadcast = "SSIDAdvertisementEnabled",
    X_WlanIsolateControl = "X_WlanIsolateControl",
    BeaconType = "BeaconType",
    BasicEncryptionMode = "BasicEncryptionModes",
    BasicAuthMode = "BasicAuthenticationMode",
    WpaEncryptionMode = "WPAEncryptionModes",
    IEEEiEncryptionMode = "IEEE11iEncryptionModes",
    MixedEncryptionModes = "X_MixedEncryptionModes",
    RadiusServer = "RadiusServer",
    Radiusport = "RadiusPort"
};

local sendsetting_maps = {
    RegulatoryDomain = "RegulatoryDomain",
    TransmitPower = "TransmitPower",
    WlanStandard = "X_WlanStandard",
    X_Wlan11NHtMcs = "X_Wlan11NHtMcs",
    X_Wlan11NBWControl = "X_Wlan11NBWControl",
    X_Wlan11NTxRxStream = "X_Wlan11NGIControl",
    MaxBitRate = "MaxBitRate",
    WMMEnable = "WMMEnable",
    Channel = "Channel"
};

--this is for "??" bug
function DecodeParas(curData)
    if nil ~= curData["ssid"] and nil ~= curData["Encodessid"] and true == curData["Encodessid"] then
        curData["ssid"] = web.decodeURIComponent(curData["ssid"])
    end
    if nil ~= curdata["CurrentWepKey"] and nil ~= curData["Encodewepkey"] and true == curData["Encodewepkey"] then
        curdata["CurrentWepKey"] = web.decodeURIComponent(curdata["CurrentWepKey"])
    end
    if nil ~= curData["wpakey"] and nil ~= curData["Encodewpakey"] and true == curData["Encodewpakey"] then
        curData["wpakey"] = web.decodeURIComponent(curData["wpakey"])
    end
end

function add_one_wlan_parameter(paras, name, value)
    if nil == value then
        return
    end
    table.insert(paras, {name, value})
end



function add_wlan_wpakey_parameter(paras, wlancfg, value)
    if nil == value or _G["defaultPasswd"] == value then
        return
    end
    if 64 == string.len(value) then
        add_one_wlan_parameter(paras, wlancfg.."PreSharedKey.1.PreSharedKey", value)
    else
        add_one_wlan_parameter(paras, wlancfg.."PreSharedKey.1.KeyPassphrase", value)
    end
end

function add_wlan_wepkey_parameter(paras, wlancfg, curdata)
    local wepkeyvalue = curdata["CurrentWepKey"]
    if _G["defaultPasswd"] ~= wepkeyvalue then
        add_one_wlan_parameter(paras, wlancfg.."WEPKey."..tostring(curdata["wepkeyindex"])..".WEPKey", wepkeyvalue)
    end
end

function submit_ssidsetting_data(paras, curdata)
    if nil == curdata then
        return
    end
    if nil == curdata["ID"] then
        return
    end
    if 0 ~= curdata["ID"] then
        local wlancfg = curdata["ID"]
        add_one_wlan_parameter(paras, wlancfg.."SSID", curdata["ssid"])
        add_one_wlan_parameter(paras, wlancfg.."Enable", curdata["ssidenable"])
        add_one_wlan_parameter(paras, wlancfg.."X_WlanIsolateControl", curdata["isolate"])
        add_one_wlan_parameter(paras, wlancfg.."SSIDAdvertisementEnabled", curdata["advertisement"])
        add_one_wlan_parameter(paras, wlancfg.."BeaconType", curdata["beacontype"])
        if curdata["mode"] ~= nil then
            add_one_wlan_parameter(paras, wlancfg.."X_WlanStandard", curdata["mode"])
        end
        if curdata["beacontype"] == "Basic" then
            add_one_wlan_parameter(paras, wlancfg.."BasicEncryptionModes", curdata["basicencmode"])
            if curdata["basicencmode"] == "WEPEncryption" then
                add_one_wlan_parameter(paras, wlancfg.."BasicAuthenticationMode", curdata["basicauthmode"])
                add_one_wlan_parameter(paras, wlancfg.."WEPKeyIndex", curdata["wepkeyindex"])
                add_wlan_wepkey_parameter(paras, wlancfg, curdata)
            else
                add_one_wlan_parameter(paras, wlancfg.."BasicAuthenticationMode", "None")
            end
        end
        if curdata["beacontype"] == "WPA" then
            if curdata["wpaencmode"] == "TKIP" then
                add_one_wlan_parameter(paras, wlancfg.."WPAEncryptionModes", "TKIPEncryption")
            end
            if curdata["wpaencmode"] == "AES" then
                add_one_wlan_parameter(paras, wlancfg.."WPAEncryptionModes", "AESEncryption")
            end
            if curdata["wpaencmode"] == "TKIP+AES"  then
                add_one_wlan_parameter(paras, wlancfg.."WPAEncryptionModes", "TKIPandAESEncryption")
            end
            add_wlan_wpakey_parameter(paras, wlancfg, curdata["wpakey"])
        end
        if curdata["beacontype"] == "11i" then
            if curdata["wpaencmode"] == "TKIP" then
                add_one_wlan_parameter(paras, wlancfg.."IEEE11iEncryptionModes", "TKIPEncryption")
            end
            if curdata["wpaencmode"] == "AES" then
                add_one_wlan_parameter(paras, wlancfg.."IEEE11iEncryptionModes", "AESEncryption")
            end
            if curdata["wpaencmode"] == "TKIP+AES"  then
                add_one_wlan_parameter(paras, wlancfg.."IEEE11iEncryptionModes", "TKIPandAESEncryption")
            end
            add_wlan_wpakey_parameter(paras, wlancfg, curdata["wpakey"])
        end
        if curdata["beacontype"] == "WPAand11i" then
            if curdata["wpaencmode"] == "TKIP" then
                add_one_wlan_parameter(paras, wlancfg.."X_MixedEncryptionModes", "TKIPEncryption")
            end
            if curdata["wpaencmode"] == "AES" then
                add_one_wlan_parameter(paras, wlancfg.."X_MixedEncryptionModes", "AESEncryption")
            end
            if curdata["wpaencmode"] == "TKIP+AES"  then
                add_one_wlan_parameter(paras, wlancfg.."X_MixedEncryptionModes", "TKIPandAESEncryption")
            end
            add_wlan_wpakey_parameter(paras, wlancfg, curdata["wpakey"])
        end
    end
end

function submit_sendsetting_data(paras, curdata, is5g)
    if nil == curdata then
        return
    end
    if nil == curdata["ID"] then
        return
    end
    if 0 ~= curdata["ID"] then
        local wlancfg = curdata["ID"]

        add_one_wlan_parameter(paras, wlancfg.."RegulatoryDomain", curdata["country"])
        add_one_wlan_parameter(paras, wlancfg.."TransmitPower", curdata["power"])
        add_one_wlan_parameter(paras, wlancfg.."X_WlanStandard", curdata["mode"])
        if "b/g/n" == curdata["mode"] or "a/n/ac" == curdata["mode"] or "g/n" == curdata["mode"] or "n" == curdata["mode"] then
            if "auto" == curdata["nmcs"] then
                add_one_wlan_parameter(paras, wlancfg.."X_Wlan11NHtMcs", 33)
            else
                add_one_wlan_parameter(paras, wlancfg.."X_Wlan11NHtMcs", curdata["nmcs"])
            end
            add_one_wlan_parameter(paras, wlancfg.."X_Wlan11NGIControl", curdata["ngi"])
            if curdata["nbw"] == "20_40"  then
                add_one_wlan_parameter(paras, wlancfg.."X_Wlan11NBWControl", "20/40")
            else
                add_one_wlan_parameter(paras, wlancfg.."X_Wlan11NBWControl", curdata["nbw"])
            end
                add_one_wlan_parameter(paras, wlancfg.."MaxBitRate", "Auto")
        else
            if "0" == curdata["rate"] then
                add_one_wlan_parameter(paras, wlancfg.."MaxBitRate", "Auto")
            else
                add_one_wlan_parameter(paras, wlancfg.."MaxBitRate", curdata["rate"])
            end
            add_one_wlan_parameter(paras, wlancfg.."X_Wlan11NHtMcs", 33)
        end
        if "n" == curdata["mode"] then
            add_one_wlan_parameter(paras, wlancfg.."IEEE11iEncryptionModes", "AESEncryption")
            add_one_wlan_parameter(paras, wlancfg.."BeaconType", curdata["beacontype"])
        end
        add_one_wlan_parameter(paras, wlancfg.."WMMEnable", curdata["wmm"])
        if curdata["channel"] == 0 then
            add_one_wlan_parameter(paras, wlancfg.."AutoChannelEnable", 1)
            if 0 == is5g then
                add_one_wlan_parameter(paras, wlancfg.."Channel", 6)
            else
                if curdata["country"] == "JP" then
                    add_one_wlan_parameter(paras, wlancfg.."Channel", 36)
                else
                    if curdata["country"] == "CN" then
                        add_one_wlan_parameter(paras, wlancfg.."Channel", 157)
                    else
                        if curdata["country"] == "US" or curdata["country"] == "CA" or curdata["country"] == "MX" then
                            add_one_wlan_parameter(paras, wlancfg.."Channel", 48)
                        else
                            add_one_wlan_parameter(paras, wlancfg.."Channel", 100)
                        end
                    end
                end
            end
        else
            add_one_wlan_parameter(paras, wlancfg.."AutoChannelEnable", 0)
            add_one_wlan_parameter(paras, wlancfg.."Channel", curdata["channel"])
        end
    end
end

local errcode,wifiConf = dm.GetParameterValues("InternetGatewayDevice.X_WiFi.Radio.{i}.",
{
    "OperatingFrequencyBand"
}
);

local paras = {}

if action == "BasicSettings" then
    for k, v in pairs(wifiConf) do
        if "2.4GHz" == v["OperatingFrequencyBand"] then
            curdata = data["config2g"]
            if curdata ~= nil then
                add_one_wlan_parameter(paras, k.."Enable", curdata["enable"])
            end
        else
            if "5GHz" == v["OperatingFrequencyBand"] then
                curdata = data["config5g"]
                if curdata ~= nil then
                    add_one_wlan_parameter(paras, k.."Enable", curdata["enable"])
                end
            end
        end
    end
end

if action == "SsidSettings" then
    curdata = data["config2g"]
    if "cloudc" ~= cfgtool then 
        if true == utils.toboolean(web.getparaenc()) then
            if curdata["wpakey"] ~= nil then
                local pass = web.decodepara(curdata["wpakey"])
                if "" ~= pass then
                    curdata["wpakey"] = pass
                end
            end
            if curdata["CurrentWepKey"] ~= nil then
                local pass = web.decodepara(curdata["CurrentWepKey"])
                if "" ~= pass then
                    curdata["CurrentWepKey"] = pass
                end
            end
        end
    end
    DecodeParas(curdata)
    submit_ssidsetting_data(paras, curdata)
    curdata = data["config5g"]
    if "cloudc" ~= cfgtool then 
        if true == utils.toboolean(web.getparaenc()) then
            if curdata["wpakey"] ~= nil then
                local pass = web.decodepara(curdata["wpakey"])
                if "" ~= pass then
                    curdata["wpakey"] = pass
                end
            end
            if curdata["CurrentWepKey"] ~= nil then
                local pass = web.decodepara(curdata["CurrentWepKey"])
                if "" ~= pass then
                    curdata["CurrentWepKey"] = pass
                end
            end
        end
    end
    submit_ssidsetting_data(paras, curdata)
    for k, v in pairs(wifiConf) do
        if "2.4GHz" == v["OperatingFrequencyBand"] then
            curdata = data["config2g"]
            if curdata ~= nil and curdata["enable"] ~= nil then
                add_one_wlan_parameter(paras, k.."Enable", curdata["enable"])
            end
        else
            if "5GHz" == v["OperatingFrequencyBand"] then
                curdata = data["config5g"]
                if curdata ~= nil and curdata["enable"] ~= nil then
                    add_one_wlan_parameter(paras, k.."Enable", curdata["enable"])
                end
            end
        end
    end
end

if action == "SendSettings" then
    curdata = data["config2g"]
DecodeParas(curdata)
    submit_sendsetting_data(paras, curdata, 0)
    curdata = data["config5g"]
    submit_sendsetting_data(paras, curdata, 1)
end

function submit_change_wpsstate(paras, curdata)
    if nil == curdata then
        return
    end
    if nil == curdata["ID"] then
        return
    end
    if 0 ~= curdata["ID"] then
        local wlancfg = curdata["ID"]
        if action == "SsidSettings" or action == "AllSsidSettings" then
            if curdata["beacontype"] == "Basic" or curdata["beacontype"] == "WPA" then
                add_one_wlan_parameter(paras, wlancfg.."WPS.Enable", false)
            end
            if curdata["beacontype"] == "11i" or curdata["beacontype"] == "WPAand11i" then
                if curdata["wpaencmode"] == "TKIP" then
                    add_one_wlan_parameter(paras, wlancfg.."WPS.Enable", false)
                else
                    if wlancfg == "InternetGatewayDevice.LANDevice.1.WLANConfiguration.1." then
                    add_one_wlan_parameter(paras, wlancfg.."WPS.Enable", true)
                    end
                    if wlancfg == "InternetGatewayDevice.LANDevice.1.WLANConfiguration.2." then
                        add_one_wlan_parameter(paras, wlancfg.."WPS.Enable", true)
                    end
                end
            end
            if curdata["advertisement"] == 0 then
                add_one_wlan_parameter(paras, wlancfg.."WPS.Enable", false)
            end
        end
    end
end

    curdata = data["config2g"]
    if nil ~= curdata then
        submit_change_wpsstate(paras, curdata)
    end
    curdata = data["config5g"]
    if nil ~= curdata then
        submit_change_wpsstate(paras, curdata)
    end

if "AllSsidSettings" == action then
    for k, v in pairs(data) do
        if "cloudc" ~= cfgtool then 
            if true == utils.toboolean(web.getparaenc()) then
                if v["wpakey"] ~= nil then
                    local pass = web.decodepara(v["wpakey"])
                    if "" ~= pass then
                        v["wpakey"] = pass
                    end
                end
                if v["CurrentWepKey"] ~= nil then
                    local pass = web.decodepara(v["CurrentWepKey"])
                    if "" ~= pass then
                        v["CurrentWepKey"] = pass
                    end
                end
            end
        end
        DecodeParas(v)
        submit_ssidsetting_data(paras, v)
        submit_change_wpsstate(paras, v)
    end
end

local errcode = 0
errcode, NeedReboot, paramerr = dm.SetParameterValues(paras)

if action == "SendSettings" then
    utils.responseErrorcode(errcode, paramerr, sendsetting_maps)
else
    utils.responseErrorcode(errcode, paramerr, maps)
end
require('dm')
require('json')
require('utils')

local tostring = tostring
local tonumber = tonumber

if FormData["frequency"] ~= nil then
    frequency = FormData["frequency"]
end

local isShowPass = true
local isShowPass5g = true
if FormData["type"] ~= nil then
    if FormData["type"]  == "notshowpass24" then
        isShowPass = false
    end
    if FormData["type"]  == "notshowpass5g" then
        isShowPass5g = false
    end

    if FormData["type"]  == "notshowpassall" then
        isShowPass = false
        isShowPass5g = false
    end
end

local errcode,wifiConf = dm.GetParameterValues("InternetGatewayDevice.X_WiFi.Radio.{i}.",
    {
        "Enable",
        "OperatingFrequencyBand"
    }
);

function get_WifiEnable(frequency)
    for k, v in pairs(wifiConf) do
        if frequency == v["OperatingFrequencyBand"] then
            return utils.toboolean(v["Enable"])
        end
    end

    return false
end

local domain = "InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}."

local errcode,wlanBasic = dm.GetParameterValues(domain,
    {
        "Enable",
        "X_OperatingFrequencyBand",
        "RegulatoryDomain",
        "Channel",
        "AutoChannelEnable",
        "X_WlanStandard",
        "MaxBitRate",
        "TransmitPower",
        "SSID",
        "SSIDAdvertisementEnabled",
        "X_WlanIsolateControl",
        "WMMEnable",
        "X_Wlan11NHtMcs",
        "X_Wlan11NBWControl",
        "X_Wlan11NGIControl",
        "X_AssociateDeviceNum",
        "BeaconType",
        "BasicEncryptionModes",
        "BasicAuthenticationMode",
        "WEPEncryptionLevel",
        "WEPKeyIndex",
        "WPAEncryptionModes",
        "IEEE11iEncryptionModes",
        "X_MixedEncryptionModes",     
        "X_Wlan11NTxRxStream"
    }
);

local wlanCfgs = {}
function calc_min(frequency)
    local array = {}
    i = 1
    if wlanBasic ~= nil then 
        for k,v in pairs(wlanBasic) do
            if frequency == v["X_OperatingFrequencyBand"] then
                wlan_instance_id, wlan_suffix = utils.findLastPoint(k)
                array[i] = tonumber(wlan_instance_id)
                i = i+1
            end    
        end
        table.sort(array)
        if 1 == i  then
            return -1    
        else
            return array[1]
        end    
    end    
    return -1
end

wlan_index = calc_min("2.4GHz")

wlan_5gindex = calc_min("5GHz")

function isValidIndex(k)
    local  wlan_instance_id, wlan_suffix = utils.findLastPoint(k)
    if wlan_index == tonumber(wlan_instance_id)  or wlan_5gindex == tonumber(wlan_instance_id)  then
        return true
    end

    return false
end

for k,v in pairs(wlanBasic) do
    local needshowpwd = false
    if (v["X_OperatingFrequencyBand"] == "2.4GHz" and isShowPass) then
        needshowpwd = true
    end
    if (v["X_OperatingFrequencyBand"] == "5GHz" and isShowPass5g) then
        needshowpwd = true
    end
    if isValidIndex(k) then
        local WepKey = {}
        wepkey_conf_str = k.."WEPKey."..tostring(v["WEPKeyIndex"]).."."
        local err_wep, wep_data = dm.GetParameterValues(k.."WEPKey.{i}.", { "WEPKey" });
        for kk, vv in pairs(wep_data) do
            wep_instance_id, wep_suffix = utils.findLastPoint(kk)
            WepKey[wep_instance_id] = _G["defaultPasswd"]
            if needshowpwd then
                WepKey[wep_instance_id]  = vv["WEPKey"]
            end
        end
        wpakey_conf_str = k.."PreSharedKey.1."
        local err_wpakey, wap_key_data = dm.GetParameterValues(wpakey_conf_str, { "PreSharedKey", "KeyPassphrase" });
        for kk, vv in pairs(wap_key_data) do
            WpaPreSharedKey = _G["defaultPasswd"]
            if needshowpwd then
                WpaPreSharedKey = vv["KeyPassphrase"]
            end
            break
        end

        local wlanInstance = {}

        wlanInstance.ID = k
        wlanInstance.FrequencyBand = v["X_OperatingFrequencyBand"]
        wlanInstance.WifiEnable = get_WifiEnable(v["X_OperatingFrequencyBand"])
        wlanInstance.RegulatoryDomain = v["RegulatoryDomain"]
        if utils.toboolean(v["AutoChannelEnable"]) == true then 
            wlanInstance.Channel = 0
        else
            wlanInstance.Channel = v["Channel"]
        end    
        wlanInstance.WlanStandard = v["X_WlanStandard"]
        if "Auto" == v["MaxBitRate"] then
            wlanInstance.MaxBitRate = "0"
        else
            wlanInstance.MaxBitRate = v["MaxBitRate"]
        end    
        wlanInstance.TransmitPower = v["TransmitPower"]
        wlanInstance.WifiSsidEnable = utils.toboolean(v["Enable"])

        wlanInstance.WifiSsid = v["SSID"]
        wlanInstance.WifiHideBroadcast = utils.opsiteBoolVal(v["SSIDAdvertisementEnabled"])
        wlanInstance.X_WlanIsolateControl = utils.toboolean(v["X_WlanIsolateControl"])
        wlanInstance.WMMEnable = utils.toboolean(v["WMMEnable"])
        if 33 == v["X_Wlan11NHtMcs"] then
            wlanInstance.X_Wlan11NHtMcs = "auto"
        else    
            wlanInstance.X_Wlan11NHtMcs = v["X_Wlan11NHtMcs"]
        end
        if "20/40" == v["X_Wlan11NBWControl"] then
            wlanInstance.X_Wlan11NBWControl = "20_40"
        else    
            wlanInstance.X_Wlan11NBWControl = v["X_Wlan11NBWControl"]
        end    
        wlanInstance.X_Wlan11NGIControl = v["X_Wlan11NGIControl"]
        wlanInstance.X_AssociateDeviceNum = v["X_AssociateDeviceNum"]
        wlanInstance.X_Wlan11NTxRxStream = v["X_Wlan11NTxRxStream"]
        if "Basic" == v["BeaconType"] and "None" == v["BasicEncryptionModes"] then
            wlanInstance.BeaconType = "None"
        else
            wlanInstance.BeaconType = v["BeaconType"]
        end    
        wlanInstance.BasicEncryptionMode = v["BasicEncryptionModes"]
        wlanInstance.BasicAuthMode = v["BasicAuthenticationMode"]
        if "40-bit" == v["WEPEncryptionLevel"] then
            wlanInstance.WepKeyLen = 64
        end
        if "104-bit" == v["WEPEncryptionLevel"] then
            wlanInstance.WepKeyLen = 128  
        end 
        wlanInstance.WepKeyIndex = v["WEPKeyIndex"]
        wlanInstance.WepKey = WepKey
        wlanInstance.WpaPreSharedKey = WpaPreSharedKey
        wlanInstance.WpaPreSharedKeyTemp = WpaPreSharedKey
        if "TKIPEncryption" == v["WPAEncryptionModes"] then
            wlanInstance.WpaEncryptionMode = "TKIP"
        end
        if "AESEncryption" == v["WPAEncryptionModes"] then
            wlanInstance.WpaEncryptionMode = "AES"
        end    
        if "TKIPandAESEncryption" == v["WPAEncryptionModes"] then
            wlanInstance.WpaEncryptionMode = "TKIP+AES"
        end    
        if "TKIPEncryption" == v["IEEE11iEncryptionModes"] then
            wlanInstance.IEEEiEncryptionMode = "TKIP"
        end
        if "AESEncryption" == v["IEEE11iEncryptionModes"] then
            wlanInstance.IEEEiEncryptionMode = "AES"
        end    
        if "TKIPandAESEncryption" == v["IEEE11iEncryptionModes"] then
            wlanInstance.IEEEiEncryptionMode = "TKIP+AES"
        end 
        if "TKIPEncryption" == v["X_MixedEncryptionModes"] then
            wlanInstance.MixedEncryptionModes = "TKIP"
        end
        if "AESEncryption" == v["X_MixedEncryptionModes"] then
            wlanInstance.MixedEncryptionModes = "AES"
        end    
        if "TKIPandAESEncryption" == v["X_MixedEncryptionModes"] then
            wlanInstance.MixedEncryptionModes = "TKIP+AES"
        end 

        --just for quick set wifi 
        if wlanInstance.BeaconType == "None" then 
        	wlanInstance.WpaPreSharedKey = ""
        end
        wlanInstance.isUnEncytion = false
        table.insert(wlanCfgs, wlanInstance)
    end   
end

utils.multiObjSortByID(wlanCfgs)

local i2G = 1
local i5G = 1
for k, v in pairs(wlanCfgs) do
    if v['FrequencyBand'] == '5GHz' then
        v['SSIDIndex'] = i5G
        i5G = i5G + 1
    else
        v['SSIDIndex'] = i2G
        i2G = i2G + 1
    end
end

sys.print(json.encode(wlanCfgs))local utils = require('utils')
local print = print
local tostring = tostring
local maps = {
    wpakey = "KeyPassphrase",
    WifiEnable = "Enable",
    WifiSsid = "SSID",
    WifiHideBroadcast = "SSIDAdvertisementEnabled",
    X_WlanIsolateControl = "X_WlanIsolateControl",
    BeaconType = "BeaconType",
    BasicEncryptionMode = "BasicEncryptionModes",
    BasicAuthMode = "BasicAuthenticationMode",
    WpaEncryptionMode = "WPAEncryptionModes",
    IEEEiEncryptionMode = "IEEE11iEncryptionModes",
    MixedEncryptionModes = "X_MixedEncryptionModes",
    RadiusServer = "RadiusServer",
    Radiusport = "RadiusPort"
};

local sendsetting_maps = {
    RegulatoryDomain = "RegulatoryDomain",
    TransmitPower = "TransmitPower",
    WlanStandard = "X_WlanStandard",
    X_Wlan11NHtMcs = "X_Wlan11NHtMcs",
    X_Wlan11NBWControl = "X_Wlan11NBWControl",
    X_Wlan11NTxRxStream = "X_Wlan11NGIControl",
    MaxBitRate = "MaxBitRate",
    WMMEnable = "WMMEnable",
    Channel = "Channel"
};

--this is for "??" bug
function DecodeParas(curData)
    if nil ~= curData["ssid"] and nil ~= curData["Encodessid"] and true == curData["Encodessid"] then
        curData["ssid"] = web.decodeURIComponent(curData["ssid"])
    end
    if nil ~= curdata["CurrentWepKey"] and nil ~= curData["Encodewepkey"] and true == curData["Encodewepkey"] then
        curdata["CurrentWepKey"] = web.decodeURIComponent(curdata["CurrentWepKey"])
    end
    if nil ~= curData["wpakey"] and nil ~= curData["Encodewpakey"] and true == curData["Encodewpakey"] then
        curData["wpakey"] = web.decodeURIComponent(curData["wpakey"])
    end
end

function add_one_wlan_parameter(paras, name, value)
    if nil == value then
        return
    end
    table.insert(paras, {name, value})
end



function add_wlan_wpakey_parameter(paras, wlancfg, value)
    if nil == value or _G["defaultPasswd"] == value then
        return
    end
    if 64 == string.len(value) then
        add_one_wlan_parameter(paras, wlancfg.."PreSharedKey.1.PreSharedKey", value)
    else
        add_one_wlan_parameter(paras, wlancfg.."PreSharedKey.1.KeyPassphrase", value)
    end
end

function add_wlan_wepkey_parameter(paras, wlancfg, curdata)
    local wepkeyvalue = curdata["CurrentWepKey"]
    if _G["defaultPasswd"] ~= wepkeyvalue then
        add_one_wlan_parameter(paras, wlancfg.."WEPKey."..tostring(curdata["wepkeyindex"])..".WEPKey", wepkeyvalue)
    end
end

function submit_ssidsetting_data(paras, curdata)
    if nil == curdata then
        return
    end
    if nil == curdata["ID"] then
        return
    end
    if 0 ~= curdata["ID"] then
        local wlancfg = curdata["ID"]
        add_one_wlan_parameter(paras, wlancfg.."SSID", curdata["ssid"])
        add_one_wlan_parameter(paras, wlancfg.."Enable", curdata["ssidenable"])
        add_one_wlan_parameter(paras, wlancfg.."X_WlanIsolateControl", curdata["isolate"])
        add_one_wlan_parameter(paras, wlancfg.."SSIDAdvertisementEnabled", curdata["advertisement"])
        add_one_wlan_parameter(paras, wlancfg.."BeaconType", curdata["beacontype"])
        if curdata["mode"] ~= nil then
            add_one_wlan_parameter(paras, wlancfg.."X_WlanStandard", curdata["mode"])
        end
        if curdata["beacontype"] == "Basic" then
            add_one_wlan_parameter(paras, wlancfg.."BasicEncryptionModes", curdata["basicencmode"])
            if curdata["basicencmode"] == "WEPEncryption" then
                add_one_wlan_parameter(paras, wlancfg.."BasicAuthenticationMode", curdata["basicauthmode"])
                add_one_wlan_parameter(paras, wlancfg.."WEPKeyIndex", curdata["wepkeyindex"])
                add_wlan_wepkey_parameter(paras, wlancfg, curdata)
            else
                add_one_wlan_parameter(paras, wlancfg.."BasicAuthenticationMode", "None")
            end
        end
        if curdata["beacontype"] == "WPA" then
            if curdata["wpaencmode"] == "TKIP" then
                add_one_wlan_parameter(paras, wlancfg.."WPAEncryptionModes", "TKIPEncryption")
            end
            if curdata["wpaencmode"] == "AES" then
                add_one_wlan_parameter(paras, wlancfg.."WPAEncryptionModes", "AESEncryption")
            end
            if curdata["wpaencmode"] == "TKIP+AES"  then
                add_one_wlan_parameter(paras, wlancfg.."WPAEncryptionModes", "TKIPandAESEncryption")
            end
            add_wlan_wpakey_parameter(paras, wlancfg, curdata["wpakey"])
        end
        if curdata["beacontype"] == "11i" then
            if curdata["wpaencmode"] == "TKIP" then
                add_one_wlan_parameter(paras, wlancfg.."IEEE11iEncryptionModes", "TKIPEncryption")
            end
            if curdata["wpaencmode"] == "AES" then
                add_one_wlan_parameter(paras, wlancfg.."IEEE11iEncryptionModes", "AESEncryption")
            end
            if curdata["wpaencmode"] == "TKIP+AES"  then
                add_one_wlan_parameter(paras, wlancfg.."IEEE11iEncryptionModes", "TKIPandAESEncryption")
            end
            add_wlan_wpakey_parameter(paras, wlancfg, curdata["wpakey"])
        end
        if curdata["beacontype"] == "WPAand11i" then
            if curdata["wpaencmode"] == "TKIP" then
                add_one_wlan_parameter(paras, wlancfg.."X_MixedEncryptionModes", "TKIPEncryption")
            end
            if curdata["wpaencmode"] == "AES" then
                add_one_wlan_parameter(paras, wlancfg.."X_MixedEncryptionModes", "AESEncryption")
            end
            if curdata["wpaencmode"] == "TKIP+AES"  then
                add_one_wlan_parameter(paras, wlancfg.."X_MixedEncryptionModes", "TKIPandAESEncryption")
            end
            add_wlan_wpakey_parameter(paras, wlancfg, curdata["wpakey"])
        end
    end
end

function submit_sendsetting_data(paras, curdata, is5g)
    if nil == curdata then
        return
    end
    if nil == curdata["ID"] then
        return
    end
    if 0 ~= curdata["ID"] then
        local wlancfg = curdata["ID"]

        add_one_wlan_parameter(paras, wlancfg.."RegulatoryDomain", curdata["country"])
        add_one_wlan_parameter(paras, wlancfg.."TransmitPower", curdata["power"])
        add_one_wlan_parameter(paras, wlancfg.."X_WlanStandard", curdata["mode"])
        if "b/g/n" == curdata["mode"] or "a/n/ac" == curdata["mode"] or "g/n" == curdata["mode"] or "n" == curdata["mode"] then
            if "auto" == curdata["nmcs"] then
                add_one_wlan_parameter(paras, wlancfg.."X_Wlan11NHtMcs", 33)
            else
                add_one_wlan_parameter(paras, wlancfg.."X_Wlan11NHtMcs", curdata["nmcs"])
            end
            add_one_wlan_parameter(paras, wlancfg.."X_Wlan11NGIControl", curdata["ngi"])
            if curdata["nbw"] == "20_40"  then
                add_one_wlan_parameter(paras, wlancfg.."X_Wlan11NBWControl", "20/40")
            else
                add_one_wlan_parameter(paras, wlancfg.."X_Wlan11NBWControl", curdata["nbw"])
            end
                add_one_wlan_parameter(paras, wlancfg.."MaxBitRate", "Auto")
        else
            if "0" == curdata["rate"] then
                add_one_wlan_parameter(paras, wlancfg.."MaxBitRate", "Auto")
            else
                add_one_wlan_parameter(paras, wlancfg.."MaxBitRate", curdata["rate"])
            end
            add_one_wlan_parameter(paras, wlancfg.."X_Wlan11NHtMcs", 33)
            if "n" == curdata["mode"] then
                add_one_wlan_parameter(paras, wlancfg.."WPAEncryptionModes", "AESEncryption")
                add_one_wlan_parameter(paras, wlancfg.."IEEE11iEncryptionModes", "AESEncryption")
                add_one_wlan_parameter(paras, wlancfg.."X_MixedEncryptionModes", "AESEncryption")
            end
        end
        add_one_wlan_parameter(paras, wlancfg.."WMMEnable", curdata["wmm"])
        if curdata["channel"] == 0 then
            add_one_wlan_parameter(paras, wlancfg.."AutoChannelEnable", 1)
            if 0 == is5g then
                add_one_wlan_parameter(paras, wlancfg.."Channel", 6)
            else
                if curdata["country"] == "JP" then
                    add_one_wlan_parameter(paras, wlancfg.."Channel", 36)
                else
                    if curdata["country"] == "CN" then
                        add_one_wlan_parameter(paras, wlancfg.."Channel", 157)
                    else
                        if curdata["country"] == "US" or curdata["country"] == "CA" or curdata["country"] == "MX" then
                            add_one_wlan_parameter(paras, wlancfg.."Channel", 48)
                        else
                            add_one_wlan_parameter(paras, wlancfg.."Channel", 100)
                        end
                    end
                end
            end
        else
            add_one_wlan_parameter(paras, wlancfg.."AutoChannelEnable", 0)
            add_one_wlan_parameter(paras, wlancfg.."Channel", curdata["channel"])
        end
    end
end

local errcode,wifiConf = dm.GetParameterValues("InternetGatewayDevice.X_WiFi.Radio.{i}.",
{
    "OperatingFrequencyBand"
}
);

local paras = {}

if action == "BasicSettings" then
    for k, v in pairs(wifiConf) do
        if "2.4GHz" == v["OperatingFrequencyBand"] then
            curdata = data["config2g"]
            if curdata ~= nil then
                add_one_wlan_parameter(paras, k.."Enable", curdata["enable"])
            end
        else
            if "5GHz" == v["OperatingFrequencyBand"] then
                curdata = data["config5g"]
                if curdata ~= nil then
                    add_one_wlan_parameter(paras, k.."Enable", curdata["enable"])
                end
            end
        end
    end
end

function submit_gusetssid_data(paras, curdata)
   add_one_wlan_parameter(paras,curdata["ID"].."SSID", curdata["WifiSsid"])
end

if utils.toboolean(data["guide"]) then
    add_one_wlan_parameter(paras, "InternetGatewayDevice.X_WiFi.Radio.1.WLANMode","4")
end

local upassword = ""

if action == "SsidSettings" then
    curdata = data["config2g"]
    if "cloudc" ~= cfgtool then 
        if true == utils.toboolean(web.getparaenc()) then
            if curdata["wpakey"] ~= nil then
                local pass = web.decodepara(curdata["wpakey"])
                if "" ~= pass then
                    curdata["wpakey"] = pass
                end
            end
            if curdata["upassword"] ~= nil then
                local pass = web.decodepara(curdata["upassword"])
                if "" ~= pass then
                    curdata["upassword"] = pass
                end
            end
            if curdata["CurrentWepKey"] ~= nil then
                local pass = web.decodepara(curdata["CurrentWepKey"])
                if "" ~= pass then
                    curdata["CurrentWepKey"] = pass
                end
            end
        end
    end
    DecodeParas(curdata)
    if curdata["notsetwebpassword"] ~= nil and curdata["notsetwebpassword"] == true then
    else
        if curdata["upassword"] ~= nil and curdata["upassword"] ~= "" then
            upassword = curdata["upassword"]
        else
            upassword = curdata["wpakey"]
        end
    end

    submit_ssidsetting_data(paras, curdata)
    curdata = data["config5g"]
    if "cloudc" ~= cfgtool then 
        if true == utils.toboolean(web.getparaenc()) then
            if curdata["wpakey"] ~= nil then
                local pass = web.decodepara(curdata["wpakey"])
                if "" ~= pass then
                    curdata["wpakey"] = pass
                end
            end
            if curdata["CurrentWepKey"] ~= nil then
                local pass = web.decodepara(curdata["CurrentWepKey"])
                if "" ~= pass then
                    curdata["CurrentWepKey"] = pass
                end
            end
        end
    end
    submit_ssidsetting_data(paras, curdata)
end

if nil ~= data["setGuestSsid"] and data["setGuestSsid"] == true then
    curdata = data["guest2g"]
    submit_gusetssid_data(paras, curdata)
end

if action == "SendSettings" then
    curdata = data["config2g"]
DecodeParas(curdata)
    submit_sendsetting_data(paras, curdata, 0)
    curdata = data["config5g"]
    submit_sendsetting_data(paras, curdata, 1)
end

function submit_change_wpsstate(paras, curdata)
    if nil == curdata then
        return
    end
    if nil == curdata["ID"] then
        return
    end
    if 0 ~= curdata["ID"] then
        local wlancfg = curdata["ID"]
        if action == "SsidSettings" or action == "AllSsidSettings" then
            if curdata["beacontype"] == "Basic" or curdata["beacontype"] == "WPA" then
                add_one_wlan_parameter(paras, wlancfg.."WPS.Enable", false)
            end
            if curdata["beacontype"] == "11i" or curdata["beacontype"] == "WPAand11i" then
                if curdata["wpaencmode"] == "TKIP" then
                    add_one_wlan_parameter(paras, wlancfg.."WPS.Enable", false)
                else
                    if wlancfg == "InternetGatewayDevice.LANDevice.1.WLANConfiguration.1." then
                    add_one_wlan_parameter(paras, wlancfg.."WPS.Enable", true)
                    end
                    if wlancfg == "InternetGatewayDevice.LANDevice.1.WLANConfiguration.2." then
                        add_one_wlan_parameter(paras, wlancfg.."WPS.Enable", true)
                    end
                end
            end
            if curdata["advertisement"] == 0 then
                add_one_wlan_parameter(paras, wlancfg.."WPS.Enable", false)
            end
        end
    end
end

    curdata = data["config2g"]
    if nil ~= curdata then
        submit_change_wpsstate(paras, curdata)
    end
    curdata = data["config5g"]
    if nil ~= curdata then
        submit_change_wpsstate(paras, curdata)
    end

if "AllSsidSettings" == action then
    for k, v in pairs(data) do
        if "cloudc" ~= cfgtool then 
            if true == utils.toboolean(web.getparaenc()) then
                if v["wpakey"] ~= nil then
                    local pass = web.decodepara(v["wpakey"])
                    if "" ~= pass then
                        v["wpakey"] = pass
                    end
                end
                if v["CurrentWepKey"] ~= nil then
                    local pass = web.decodepara(v["CurrentWepKey"])
                    if "" ~= pass then
                        v["CurrentWepKey"] = pass
                    end
                end
            end
        end
        DecodeParas(v)
        for k,v in pairs(data) do
            if v["notsetwebpassword"] ~= nil and v["notsetwebpassword"] == true then
                break;
            end
            if v["wpakey"] then
                upassword = v["wpakey"]
                break;
            end
        end
        submit_ssidsetting_data(paras, v)
        submit_change_wpsstate(paras, v)
    end
end

function submit_Channel(paras, curdata)
    if 0 ~= curdata["ID"] then
        local wlancfg = curdata["ID"]
        if curdata["Channel"] == 0 then
            add_one_wlan_parameter(paras, wlancfg.."AutoChannelEnable", 1)
            if 0 == is5g then
                add_one_wlan_parameter(paras, wlancfg.."Channel", 6)
            else
                if curdata["country"] == "JP" then
                    add_one_wlan_parameter(paras, wlancfg.."Channel", 36)
                else
                    if curdata["country"] == "CN" then
                        add_one_wlan_parameter(paras, wlancfg.."Channel", 157)
                    else
                        if curdata["country"] == "US" or curdata["country"] == "CA" or curdata["country"] == "MX" then
                            add_one_wlan_parameter(paras, wlancfg.."Channel", 48)
                        else
                            add_one_wlan_parameter(paras, wlancfg.."Channel", 100)
                        end
                    end
                end
            end
        else
            add_one_wlan_parameter(paras, wlancfg.."AutoChannelEnable", 0)
            add_one_wlan_parameter(paras, wlancfg.."Channel", curdata["Channel"])
        end
        add_one_wlan_parameter(paras, wlancfg.."SSIDAdvertisementEnabled", curdata["advertisement"])
    end
end
if "ChannelSettings" == action then
    for k, v in pairs(data) do
        submit_Channel(paras, v)
    end
end
if utils.toboolean(data["guide"]) then
    print("pc set ChannelWanGuide")
    add_one_wlan_parameter(paras, "InternetGatewayDevice.UserInterface.X_Web.ChannelWanGuide", "1")
    print("pc set ChannelWanGuide return =",guideconfig)
end

if upassword ~= "" and _G["defaultPasswd"] ~= upassword then
    local userdomain = ""
    local errcode,uservalues = dm.GetParameterValues("InternetGatewayDevice.UserInterface.X_Web.UserInfo.{i}.",{"Userlevel"})
    for u,l in pairs(uservalues) do
        if l["Userlevel"] == 2 then
            userdomain = u
            break
        end
    end
    if "" ~= userdomain then
        add_one_wlan_parameter(paras,userdomain.."Userpassword",upassword)
    end
end

add_one_wlan_parameter(paras, "InternetGatewayDevice.UserInterface.X_Web.QuickConfigured", "1")
print("Set QuickConfigured 1")

local errcode = 0
errcode, NeedReboot, paramerr = dm.SetParameterValues(paras)
print("set wlan...",errcode)
if errcode == 0 and  utils.toboolean(data["guide"]) then
    print("set password success then delete iptables and ebtables")
    sys.exec("iptables -D FORWARD -j DROP")
    sys.exec("ebtables -D FORWARD 1 -o !br+ -j DROP")
    sys.exec("ebtables -D FORWARD 1 -i !br+ -j DROP")
    print("iptables and ebtables delete over...")
end


if action == "SendSettings" then
    utils.responseErrorcode(errcode, paramerr, sendsetting_maps)
else
    utils.responseErrorcode(errcode, paramerr, maps)
endrequire('dm')
require('web')
require('json')
require('utils')

local errcode, objs = dm.GetParameterValues("InternetGatewayDevice.ACL.{i}.", {"X_Service","X_Direction","X_StartIpAddr","X_MacAddr","X_EndIpAddr"})

local errcode, hostobjs = dm.GetParameterValues("InternetGatewayDevice.LANDevice.{i}.Hosts.Host.{i}.", {"MACAddress"});

function parsemacs(key, srcmac)
    local start = 1
    local devices = {}
    while true do
        ip, fp = string.find(srcmac, "|", start)
        if not ip then
            local mac = {}
            mac[key] = string.sub(srcmac, start)
            table.insert(devices, mac)
            break
        end
        local mac = {}
        mac[key] = string.sub(srcmac, start, ip-1)
        table.insert(devices, mac)
        start = fp +1
    end
    return devices
end

--return mac address list in host
function getmacinHost(key, srcmac)
    local start = 1
    local devices = {}
    while true do
        ip, fp = string.find(srcmac, "|", start)
        if not ip then
            local mac = {}
            mac[key] = string.sub(srcmac, start)
            for k,v in pairs(hostobjs) do
                if v["MACAddress"] == mac[key] then
                    table.insert(devices, mac)
                end 
            end
            break
        end
        local mac = {}
        mac[key] = string.sub(srcmac, start, ip-1)
        for kk,vv in pairs(hostobjs) do
            if vv["MACAddress"] == mac[key] then
                table.insert(devices, mac)
            end 
        end
        start = fp +1
    end
    return devices
end

--return mac address not in host mac list
function isMenualSetACLMac(hostobj,aclmac)
    --first get all mac address in aclmac
    local menusetmac = ""
    
    if aclmac == "" then
        return false
    end
    local allAclMacAddr = parsemacs("MACAddress",aclmac)
    --second get one aclmac not in  allAclMacAddr
    local exitflag = false
    --if true explain not in host
    for kk,vv in pairs(allAclMacAddr) do
        for k,v in pairs(hostobj) do
            if vv["MACAddress"] == v["MACAddress"] then
                exitflag = true
                break
            end
        end
    end
    return true
end

local acllist = {}
for k,v in pairs(objs) do
    local aclitem = {}
    aclitem["ID"] = k
    aclitem["ServiceType"] = v["X_Service"]
    aclitem["AccessDirection"] = v["X_Direction"]
    aclitem["StartIpAddr"] = v["X_StartIpAddr"]
    aclitem["EndIpAddr"] = v["X_EndIpAddr"]
    --get lan host in hosts nodes
    aclitem["MacAddrinHost"] = getmacinHost("MACAddress",v["X_MacAddr"])
    --get user if setmenual setmac address and mac address
    aclitem["isMenualSetMac"] = isMenualSetACLMac(hostobjs,v["X_MacAddr"])
    table.insert(acllist, aclitem)
end
sys.print(json.encode(acllist))local utils = require('utils')
require('dm')
local string = string

local maps = {
             ServiceType="X_Service", 
             AccessDirection="X_Direction",
             StartIpAddr="X_StartIpAddr",
             ManualDevMac="X_MacAddr",
             EndIpAddr = "X_EndIpAddr"}

function get_MacAddress()
    local joinmac = ""
    for k,v in pairs(data['MacAddrinHost']) do
        if joinmac == "" then
            joinmac = v["MACAddress"]
        else
            joinmac = joinmac.."|"..v["MACAddress"]
        end
    end
    return joinmac
end

function Generate_ACL_Submit_content(domain)
    local paras 
    if utils.toboolean(data["isMenualSetMac"]) then
        paras = {
                    {domain.."X_MacAddr", get_MacAddress()},
                    {domain.."X_Service", data["ServiceType"]}, 
                    {domain.."X_Direction",data["AccessDirection"]},
                    {domain.."X_StartIpAddr",""}, 
                    {domain.."X_EndIpAddr",""},
                  }
    else
        paras = {
                    {domain.."X_Service", data["ServiceType"]}, 
                    {domain.."X_Direction",data["AccessDirection"]},
                    {domain.."X_StartIpAddr",data["StartIpAddr"]}, 
                    {domain.."X_EndIpAddr",data["EndIpAddr"]},
                    {domain.."X_MacAddr", ""},
                  }
    end
    return paras
end

function create()
    -- add
    local errcode, instnum, NeedReboot, paramerr= dm.AddObjectWithValues("InternetGatewayDevice.ACL.", Generate_ACL_Submit_content(""));
    utils.responseErrorcode(errcode, paramerr, maps)
    return errcode
end

function delete()
    return dm.DeleteObject(data["ID"])
end

function update()
    if "null" == data["StartIpAddr"] then
        data["StartIpAddr"] = ""
    end

    if "null" == data["EndIpAddr"] then
        data["EndIpAddr"] = ""
    end

    local errcode, NeedReboot, paramerr =  dm.SetParameterValues(Generate_ACL_Submit_content(data["ID"]))
    utils.responseErrorcode(errcode, paramerr, maps)
    return errcode
end

if action == 'create' then
    err = create()
elseif action == 'update' then
    err = update()
elseif action == 'delete' then
    err = delete()
else
    return
end
utils.appenderror("errcode", err)require('web')
