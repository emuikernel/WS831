local tostring = tostring
local tonumber = tonumber

local showpass = true
local index = ''
if FormData["showpass"] ~= nil and FormData["showpass"] == "false" then
    showpass = false
end
if FormData["ID"] ~= nil then
    index = FormData["ID"]
end

local wlan_domain = "InternetGatewayDevice.LANDevice.{i}.WLANConfiguration.{i}."

local errcode,ssids = dm.GetParameterValues(wlan_domain, {
    "SSID",
    "Name",
    "X_OperatingFrequencyBand",
    "Enable",
    "BasicEncryptionModes",
    "BeaconType",

    "WEPEncryptionLevel",
    "WEPKeyIndex",
    "BasicAuthenticationMode",

    "WPAEncryptionModes",
    "IEEE11iEncryptionModes",
    "X_MixedEncryptionModes",

    "SSIDAdvertisementEnabled",
    "X_WlanIsolateControl"
});

function get_wlan_alias(wlan_name)
    if "wl0" == wlan_name then
        return "SSID1"
    end
    if "wl1" == wlan_name then
        return "SSID5"
    end
    if "wl0.1" == wlan_name then
        return "SSID2"
    end
    if "wl1.1" == wlan_name then
        return "SSID6"
    end
    if "wl0.2" == wlan_name then
        return "SSID3"
    end
    if "wl1.2" == wlan_name then
        return "SSID7"
    end
    if "wl0.3" == wlan_name then
        return "SSID4"
    end
    if "wl1.3" == wlan_name then
        return "SSID8"
    end
end

function get_wep_keys(ssid_domain)
    local wep_keys = {}
    local err,keys = dm.GetParameterValues(ssid_domain.."WEPKey.{i}.", {
        "WEPKey"
    });

    for k,v in pairs(keys) do
        key_index, prefix = utils.findLastPoint(k)
        if index == ssid_domain and showpass == true then
            wep_keys[key_index] = v["WEPKey"]
        else
            wep_keys[key_index] = _G["defaultPasswd"]
        end
    end

    return wep_keys
end

function get_wpa_key_passphrase(ssid_domain)
    local err, value = dm.GetParameterValues(ssid_domain.."PreSharedKey.1.", {"KeyPassphrase"})
    if nil == value then
        return ""
    end

    local obj = value[ssid_domain.."PreSharedKey.1."]
    if index == ssid_domain and showpass == true then 
        return obj["KeyPassphrase"]
    else
        return _G["defaultPasswd"]
    end
end


function get_ssid_by_key_value(k, v)
    local ssid = {}
    ssid.ID = k
    ssid.Name = get_wlan_alias(v["Name"])
    ssid.Ssid = v["SSID"]
    ssid.Enable = utils.toboolean(v["Enable"])
    ssid.Frequency = v["X_OperatingFrequencyBand"]

    if "Basic" == v["BeaconType"] and "None" == v["BasicEncryptionModes"] then
        ssid.SecMode = "None"
    else
        ssid.SecMode = v["BeaconType"]
    end

    ssid.BasicAuthMode = v["BasicAuthenticationMode"]
    ssid.WepEncLevel = v["WEPEncryptionLevel"]
    ssid.WepKeyIndex = v["WEPKeyIndex"]
    ssid.WepKeys = get_wep_keys(k)

    ssid.WpaPreSharedKey = get_wpa_key_passphrase(k)
    ssid.WpaEncModes = v["WPAEncryptionModes"]
    ssid.WpaIEEEEncModes = v["IEEE11iEncryptionModes"]
    ssid.WpaMixEncModes = v["X_MixedEncryptionModes"]

    ssid.HideBroadcast = utils.opsiteBoolVal(v["SSIDAdvertisementEnabled"])
    ssid.IsolateControl = utils.toboolean(v["X_WlanIsolateControl"])

    return ssid
end

function is_primary_ssid(k)
    local primary_ssid_domain = "InternetGatewayDevice.LANDevice.1.WLANConfiguration.1."
    if primary_ssid_domain == k then
        return true
    end
    primary_ssid_domain = "InternetGatewayDevice.LANDevice.1.WLANConfiguration.2."
    if primary_ssid_domain == k then
        return true
    end

    return false
end

function is_guest_ntwk_ssid(k)
    local guest_ntwk_ssid = "InternetGatewayDevice.LANDevice.2.WLANConfiguration."
    if guest_ntwk_ssid == string.sub(k, 1, string.len(guest_ntwk_ssid)) then
        return true
    end

    return false
end

local wlan_ssids = {}
if nil ~= ssids then
    for k,v in pairs(ssids) do
        if not is_primary_ssid(k) then
            local ssid = get_ssid_by_key_value(k, v)
            table.insert(wlan_ssids, ssid)
        end
    end
    utils.multiObjSortByID(wlan_ssids)
end

sys.print(json.encode(wlan_ssids))require('dm')
