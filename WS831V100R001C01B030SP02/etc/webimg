var diagnoserefresh = false;
var stopConnect = false;
$(function(){
Atp.WanWizardController = Atp.SingleObjController.extend({
objName: "ntwk/wan?type=active",
contentType:"application/json;charset=UTF-8;enp",
submit: function(data, callback){
var content = this.content;
content.linkdata = data.linkdata;
content.NATType        = 1;
if (data.type == "pppoe"){
content.Password = this.enc(data.wan_pppoe_password);
content.Username = data.wan_pppoe_username;
content.MACColoneEnable = data.mac_clnone_enable;
content.MACColone = (data.macclone_mac).toUpperCase();
content.ConnectionType = "PPP_Routed";
}
else if (data.type == "static"){
content.ConnectionType = "IP_Routed";
content.IPv4AddrType   = "Static";
content.IPv4Addr       = data.wan_wizard_ipaddress;
content.IPv4Mask       = data.wan_wizard_ipmask;
content.IPv4Gateway    = data.wan_wizard_gateway;
content.IPv4DnsServers = data.wan_wizard_dns1 + "," + data.wan_wizard_dns2;
}
else{
content.ConnectionType = "IP_Routed";
content.IPv4AddrType   = "DHCP";
}
this.postData(content, callback);
}
});
Atp.CurrentMacShowController = Atp.SingleObjController.extend({
objName: 'ntwk/getmacaddress'
});
Atp.LinkController = Atp.MultiObjController.extend({
objName: 'ntwk/link'
});
Atp.WanDetectController = Atp.SingleObjController.extend({
objName: "ntwk/wandetect",
detectCnt: 0,
detectStatus: 0,
connectedPage: "diagnose_page_connected",
unknownErrPage: "diagnose_page_unknownErr",
layer2downPage: "diagnose_page_layer2down",
authfailPage:  "diagnose_page_authfail",
finishDetect: function(result){
console.log("in finishDetect...result=",result);
Atp.WanDetectController.detectStatus = 0;
if(true == stopConnect){
window.location.href = "/small/html/home.html#home_page";
return;
}
if (result.Status == "Connected"){
utilchangePage(this.connectedPage);
}
else{
utilchangePage(this.unknownErrPage);
}
},
isConnecting: function(result){
if ("Connecting" == result["ConnectionStatus"]) {
return true;
}
if ("Authenticating" == result["ConnectionStatus"]) {
return true;
}
if ("Disconnecting" == result["ConnectionStatus"]) {
return true;
}
if ("PendingDisconnecting" == result["ConnectionStatus"]) {
return true;
}
return false;
},
isConnected: function(result){
if ("Bridged" == result["ConnectionType"]) {
if ("Connected" == result["ConnectionStatus"]) {
return true;
}
return false;
}
if ("Connected" != result["ConnectionStatus"]) {
return false;
}
return true;
},
detectWanConnected: function(result, self){
console.log("in detectWanConnected...");
console.log("self.detectStatus=",self.detectStatus);
console.log("result=",result);
var detailStr = "";
if(true == stopConnect){
return;
}
self.detectStatus = self.detectStatus + 1;
if (false == self.isConnected(result)){
if (self.detectStatus <= 5){
setTimeout(function() {
Atp.WanDetectController.load(Atp.WanDetectController.detectWanConnected);
}, 2000);
}
}
if ("Down" == result.AccessStatus) {
self.detectStatus = 6;
utilchangePage(self.layer2downPage);
return;
}
var detailStr = Em.I18n.get("guide.wan.detail");
if(result["ConnectionType"] == "PPP_Routed"){
if("Unknown" == result.ErrReason){
self.detectStatus = 6;
utilchangePage(self.authfailPage);
$("#diagnose_pppoe_error").show();
$("#diagnose_pppoe_error_info").html(Em.I18n.get("guide.diagnose.unknownerr"));
return;
}
}
if("ErrAuthFail" == result.ErrReason){
self.detectStatus = 6;
utilchangePage(self.authfailPage);
$("#diagnose_pppoe_error").show();
$("#diagnose_pppoe_error_info").html(Em.I18n.get("guide.diagnose.pppaccounterror"));
return;
}
if ("ErrAuthUserLimit" == result.ErrReason){
self.detectStatus = 6;
utilchangePage(self.authfailPage);
$("#diagnose_pppoe_error").show();
$("#diagnose_pppoe_error_info").html(Em.I18n.get("guide.userlimit.error")+detailStr);
$("#diagnose_pppoe_error_info").attr("name","userlimit");
return;
}
if ("ErrAuthMacLimit" == result.ErrReason){
self.detectStatus = 6;
utilchangePage(self.authfailPage);
$("#diagnose_pppoe_error").show();
$("#diagnose_pppoe_error_info").html(Em.I18n.get("guide.macclone.error")+detailStr);
$("#diagnose_pppoe_error_info").attr("name","macclone");
return;
}
if ((self.detectStatus > 5) || self.isConnected(result)){
self.finishDetect(result);
}
},
scanningWanType: function(result, self){
if(true == stopConnect){
return;
}
console.log("in scanningWanType...result=",result);
console.log("self.detectCnt=",self.detectCnt);
var flag = false;
if ("Down" == result.AccessStatus) {
self.detectCnt = 0;
utilchangePage(self.layer2downPage);
return;
}
if("ErrAuthFail" == result.ErrReason){
self.detectCnt = 0;
utilchangePage(self.authfailPage);
$("#diagnose_pppoe_error_info").html(Em.I18n.get("guide.diagnose.pppaccounterror"));
$("#diagnose_pppoe_error_info").attr("name","");
if("" != Atp.WanDetectController.content.DetailErr){
$("#diagnose_pppoe_error_info").html(Em.I18n.get("guide.diagnose.pppaccounterror")+Em.I18n.get("guide.wan.detail"));
}
return;
}
if ("Finished" == result.SearchingStatus){
self.detectCnt = 0;
if ("1" == result["WanResult"]){
Atp.WanWizardController.content.ConnectionType = "PPP_Routed";
}
else if("2" == result["WanResult"]){
Atp.WanWizardController.content.ConnectionType = "IP_Routed";
}
flag = true;
if("Connected" ==result.ConnectionStatus){
console.log("go finishDetect...");
self.finishDetect(result);
}else{
Atp.WanWizardController.postData(Atp.WanWizardController.content, function(){
self.detectStatus = 0;
Atp.WanDetectController.load(Atp.WanDetectController.detectWanConnected);
});
}
return;
}
if (self.detectCnt > 10 || flag){
self.detectCnt = 0;
self.finishDetect(result);
return;
}
setTimeout(function() {
var detectCount = self.detectCnt;
self.detectCnt = detectCount + 1;
Atp.WanDetectController.load(Atp.WanDetectController.scanningWanType);
}, 1000);
},
detect: function(){
var self = this;
console.log("diagnose  detect..self..",self);
var data = {};
data.Type = "connType";
data.wan  = Atp.WanWizardController.content.ID;
self.submitItem("trigger", data, function(){
self.detectCnt = 0;
Atp.WanDetectController.load(Atp.WanDetectController.scanningWanType);
});
}
});
});
function diagnose_continue(){
console.log("in diagnose_continue...");
utilchangePage("diagnose_page");
Atp.WanDetectController.detectStatus = 0;
Atp.WanDetectController.load(Atp.WanDetectController.detectWanConnected);
}
function exitDiagnose(){
window.location.href = "/small/html/home.html#home_page";
}
function backToPCpage(){
window.location.href = "/html/index.html";
}
$("#diagnose_mac-choose").click(function(){
$(".choose-more").css("overflow","visible");
if($(".mac-list").hasClass("hide")){
$(".mac-list").removeClass("hide").addClass("show");
}else{
$(".mac-list").removeClass("show").addClass("hide");
}
});
$(".mac-list .mac-item").click(function(){
var cur_item = $(".mac-list .active");
cur_item.removeClass("active");
$(this).addClass("active");
$("#diagnose_mac-choose").attr("name",$(this).index());
$("#diagnose_mac-choose").html($(this).html());
$(".mac-list").removeClass("show").addClass("hide");
if($(".mac-list .otherclone").hasClass("active")){
$("#diagnose_macconet_type_manual").show();
}
else{
$("#diagnose_macconet_type_manual").hide();
}
});
$("#diagnose_pppoe_error").click(function(){
if("macclone" == $(".more_detail").parent().attr("name")){
$("#diagnose_pppoe_error_info").html(Em.I18n.get("guide.macclone.error")+Em.I18n.get("guide.wan.detail"));
$("#diagnose_pppoe_error_help").html(Em.I18n.get("guide.macclone.error.help"));
$("#diagnose_pppoe_error_help").toggle();
}
if("userlimit" == $(".more_detail").parent().attr("name")){
$("#diagnose_pppoe_error_info").html(Em.I18n.get("guide.userlimit.error")+Em.I18n.get("guide.wan.detail"));
$("#diagnose_pppoe_error_help").html(Em.I18n.get("guide.userlimit.error.help"));
$("#diagnose_pppoe_error_help").toggle();
}
});
$('#diagnose_pppoe_form').on('submit', function (e) {
var $this = $(this);
e.preventDefault();
var data = $this.serializeObject();
$("#diagnose_pppoe_error").hide();
$("#diagnose_pppoe_error_info").html("");
if(data){
var max_ppp_password_length = 64;
if(data.wan_pppoe_password.length > max_ppp_password_length){
$("#diagnose_pppoe_error").show();
$("#diagnose_pppoe_error_info").html(Em.I18n.get("wan.pppoe.passworderror"));
return;
}
if(0 == $("#diagnose_mac-choose").attr("name")){
data.mac_clnone_enable = false;
data.macclone_mac = Atp.CurrentMacShowController.content.macaddress;
}else if(1 == $("#diagnose_mac-choose").attr("name")){
data.mac_clnone_enable = true;
data.macclone_mac = Atp.CurrentMacShowController.content.macaddress;
}else{
data.mac_clnone_enable = true;
data.macclone_mac = $("#diagnose_macconet_manual").val();
if(!isValidMacAddr(data.macclone_mac)){
$("#diagnose_pppoe_error").show();
$("#diagnose_pppoe_error_info").html(Em.I18n.get("guide.mac.error"));
return;
}
}
}
console.log("diagnose_pppoe_form",data);
utilchangePage("diagnose_page");
Atp.WanWizardController.submit(data, function(result){
if(result["errcode"] != 0){
console.log("===in errcode!=0");
utilchangePage("diagnose_page_authfail");
$("#diagnose_pppoe_error").show();
$("#diagnose_pppoe_error_info").html(Em.I18n.get("wan.pppoe.error"));
}
else{
console.log("===in errcode==0");
$("#diagnose_pppoe_error").hide();
$("#diagnose_pppoe_error_info").html("");
setTimeout(function(){
Atp.WanWizardController.load(function(){
Atp.WanDetectController.detectStatus = 0;
Atp.WanDetectController.load(Atp.WanDetectController.detectWanConnected);
});
},100);
}
});
});
function stopConnecting_OnClick(){
stopConnect = true;
window.location.href = "/small/html/home.html#home_page";
}
